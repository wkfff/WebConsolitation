<?xml version="1.0" encoding="utf-8"?>
<!--<PresetInfo PresetName="HorizontalWithNeedle.xml" PresetDescription="Custom Preset Created 30.05.2011 12:02:47" PresetType="All" />-->
<igGauge:UltraGauge>
  <Gauges>
    <igGaugeProp:LinearGauge Bounds="0, 20, 0, 60" BoundsMeasure="Percent">
      <Scales>
        <igGaugeProp:LinearGaugeScale EndExtent="95" StartExtent="5">
          <MajorTickmarks StartExtent="10">
            <StrokeElement Color="Black" />
          </MajorTickmarks>
          <MinorTickmarks Frequency="0.2" StartExtent="95">
            <StrokeElement Color="Black" />
          </MinorTickmarks>
          <Labels ZPosition="AboveMarkers" Extent="5" Font="Arial, 14pt">
            <Shadow Depth="2">
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="100, 0, 0, 0" />
              </BrushElements>
            </Shadow>
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="White" />
            </BrushElements>
          </Labels>
          <Markers>
            <igGaugeProp:LinearGaugeNeedle StartWidth="5" Value="23">
              <Response />
              <StrokeElement Color="Black" />
              <BrushElements>
                <igGaugeProp:SimpleGradientBrushElement StartColor="Pink" EndColor="Red" />
              </BrushElements>
            </igGaugeProp:LinearGaugeNeedle>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="100" TickmarkInterval="10" />
          </Axes>
        </igGaugeProp:LinearGaugeScale>
      </Scales>
      <BrushElements>
        <igGaugeProp:SimpleGradientBrushElement GradientStyle="BackwardDiagonal" StartColor="LightSlateGray" EndColor="White" />
      </BrushElements>
    </igGaugeProp:LinearGauge>
  </Gauges>
  <Annotations>
    <igGaugeProp:BoxAnnotation Bounds="0, 5, 100, 0" BoundsMeasure="Percent">
      <Label FormatString="" Font="Arial, 16px, style=Bold">
        <BrushElements>
          <igGaugeProp:SolidFillBrushElement Color="Black" />
        </BrushElements>
      </Label>
    </igGaugeProp:BoxAnnotation>
  </Annotations>
</igGauge:UltraGauge>