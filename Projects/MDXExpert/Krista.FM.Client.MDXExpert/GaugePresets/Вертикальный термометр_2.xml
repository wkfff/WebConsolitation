<?xml version="1.0" encoding="utf-8"?>
<!--<PresetInfo PresetName="VerticalThermometer.xml" PresetDescription="Custom Preset Created 30.05.2011 12:18:09" PresetType="All" />-->
<igGauge:UltraGauge>
  <Gauges>
    <igGaugeProp:LinearGauge Orientation="Vertical" BoundsMeasure="Percent">
      <Scales>
        <igGaugeProp:LinearGaugeScale EndExtent="90" StartExtent="5">
          <MajorTickmarks StartExtent="30" EndExtent="70">
            <StrokeElement Color="Black" />
          </MajorTickmarks>
          <MinorTickmarks Frequency="0.2" StartExtent="65" EndExtent="70">
            <StrokeElement Color="Black" />
          </MinorTickmarks>
          <Labels ZPosition="AboveMarkers" Extent="15" Font="Arial, 14pt">
            <Shadow Depth="2">
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="100, 0, 0, 0" />
              </BrushElements>
            </Shadow>
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="White" />
            </BrushElements>
          </Labels>
          <Markers>
            <igGaugeProp:LinearGaugeBarMarker BulbSpan="15" Value="77" Precision="0">
              <Background>
                <StrokeElement Color="Black" />
                <BrushElements>
                  <igGaugeProp:SolidFillBrushElement Color="150, 220, 220, 220" />
                </BrushElements>
              </Background>
              <Response />
              <StrokeElement Color="Black" />
              <BrushElements>
                <igGaugeProp:SimpleGradientBrushElement StartColor="Pink" EndColor="Red" />
              </BrushElements>
            </igGaugeProp:LinearGaugeBarMarker>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="100" TickmarkInterval="10" />
          </Axes>
        </igGaugeProp:LinearGaugeScale>
      </Scales>
      <BrushElements>
        <igGaugeProp:SimpleGradientBrushElement GradientStyle="BackwardDiagonal" StartColor="LightSlateGray" EndColor="White" />
      </BrushElements>
    </igGaugeProp:LinearGauge>
  </Gauges>
  <Annotations>
    <igGaugeProp:BoxAnnotation Bounds="0, 2, 100, 0" BoundsMeasure="Percent">
      <Label FormatString="" Font="Arial, 16px, style=Bold">
        <BrushElements>
          <igGaugeProp:SolidFillBrushElement Color="Black" />
        </BrushElements>
      </Label>
    </igGaugeProp:BoxAnnotation>
  </Annotations>
</igGauge:UltraGauge>