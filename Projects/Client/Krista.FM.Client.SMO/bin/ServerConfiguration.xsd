<?xml version="1.0" encoding="windows-1251"?>
<!-- edited with XMLSpy v2010 rel. 2 (http://www.altova.com) by Krista (Ru-Board) -->
<xs:schema xmlns="xmluml" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="xmluml" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:simpleType name="DBName">
		<xs:annotation>
			<xs:documentation>Имя в БД</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NCName">
			<xs:minLength value="1"/>
			<xs:maxLength value="26"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VersionType">
		<xs:annotation>
			<xs:documentation>Версия</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,9}(\.\d{1,9}){0,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DBMSVendorType">
		<xs:annotation>
			<xs:documentation>Тип СУБД</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="Oracle"/>
			<xs:enumeration value="FireBird"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DataSourceParametersTypes">
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="ГодФинорган"/>
			<xs:enumeration value="ГодМесяц"/>
			<xs:enumeration value="ГодМесяцВариант"/>
			<xs:enumeration value="ГодВариант"/>
			<xs:enumeration value="Год"/>
			<xs:enumeration value="ГодКвартал"/>
			<xs:enumeration value="ГодТерритория"/>
			<xs:enumeration value="ГодКварталМесяц"/>
			<xs:enumeration value="БезПараметров"/>
			<xs:enumeration value="Вариант"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ClassEnumType">
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="f"/>
			<xs:enumeration value="d"/>
			<xs:enumeration value="fx"/>
			<xs:enumeration value="b"/>
			<xs:enumeration value="t"/>
			<xs:enumeration value="r"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DocumentEnumType">
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="DocumentEntityDiagram"/>
			<xs:enumeration value="Diagram"/>
			<xs:enumeration value="URL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="AttributeAttributes">
		<xs:annotation>
			<xs:documentation>Атрибуты атрибутов</xs:documentation>
		</xs:annotation>
		<xs:attribute name="objectKey" type="xs:string" use="required"/>
		<xs:attribute name="name" type="DBName" use="required"/>
		<xs:attribute name="caption" type="xs:string" use="required"/>
		<xs:attribute name="type" type="DBFieldTypeEnum" use="required"/>
		<xs:attribute name="size" type="xs:nonNegativeInteger" use="optional" default="0"/>
		<xs:attribute name="scale" type="xs:nonNegativeInteger" use="optional" default="0"/>
		<xs:attribute name="default" type="xs:string" use="optional"/>
		<xs:attribute name="divide" type="xs:string" use="optional"/>
		<xs:attribute name="mask" type="xs:string" use="optional"/>
		<xs:attribute name="nullable" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="stringIdentifier" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="isDocumentAttribute" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>
		<xs:attribute name="lookupObject" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="5"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="lookupAttribute" type="DBName" use="optional"/>
		<xs:attribute name="visible" type="xs:boolean" use="optional" default="true"/>
		<xs:attribute name="class" type="xs:string" use="optional"/>
		<xs:attribute name="kind" type="xs:string" use="optional"/>
		<xs:attribute name="lookupType" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:enumeration value="0"/>
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="isReadOnly" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="position" type="xs:integer" use="optional"/>
		<xs:attribute name="groupParentAttribute" type="xs:string" use="optional"/>
		<xs:attribute name="positionCalc" type="xs:string" use="optional"/>
		<xs:attribute name="groupTags" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="AssociationAttributes">
		<xs:annotation>
			<xs:documentation>Атрибуты ассоциаций</xs:documentation>
		</xs:annotation>
		<xs:attribute name="name" type="DBName" use="required">
			<xs:annotation>
				<xs:documentation>Наименование аттрибута-ссылки</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="shortName" type="DBName" use="optional">
			<xs:annotation>
				<xs:documentation>Укороченное наименование аттрибута-ссылки используется для сокращений в базе данных</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="caption" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Русское наименование ассоциации</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="objectKey" type="xs:string" use="optional"/>
		<xs:attribute name="mask" type="xs:string" use="optional"/>
		<xs:attribute name="visible" type="xs:boolean" use="optional" default="true"/>
		<xs:attribute name="lookupAttribute" type="DBName" use="optional"/>
		<xs:attribute name="isReadOnly" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="position"/>
		<xs:attribute name="groupTags"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="RoleAttributes">
		<xs:annotation>
			<xs:documentation>Атрибуты роли в ассоциации</xs:documentation>
		</xs:annotation>
		<xs:attribute name="objectKey" type="xs:string" use="optional"/>
		<xs:attribute name="package" type="xs:string" use="optional"/>
		<xs:attribute name="class" type="ClassEnumType" use="required"/>
		<xs:attribute name="semantic" type="DBName" use="required"/>
		<xs:attribute name="name" type="DBName" use="required"/>
		<xs:attribute name="multiplicity" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="0..1"/>
					<xs:enumeration value="0..*"/>
					<xs:enumeration value="1..1"/>
					<xs:enumeration value="1..*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="default" type="xs:integer" use="optional"/>
	</xs:attributeGroup>
	<xs:simpleType name="DBFieldTypeEnum">
		<xs:annotation>
			<xs:documentation>Тип поля в БД</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NCName">
			<xs:enumeration value="ftInteger"/>
			<xs:enumeration value="ftDouble"/>
			<xs:enumeration value="ftChar"/>
			<xs:enumeration value="ftString"/>
			<xs:enumeration value="ftBoolean"/>
			<xs:enumeration value="ftDate"/>
			<xs:enumeration value="ftDateTime"/>
			<xs:enumeration value="ftBLOB"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="ClassAttribetes">
		<xs:annotation>
			<xs:documentation>Аттрибуты классов</xs:documentation>
		</xs:annotation>
		<xs:attribute name="semantic" type="DBName" use="required"/>
		<xs:attribute name="name" type="DBName" use="required">
			<xs:annotation>
				<xs:documentation>Английское имя объекта. Используется для идентификации и в БД</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="shortName" type="DBName" use="optional"/>
		<xs:attribute name="caption" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Русское наименорание объекта. Используется в кубах и в интерфейсе</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="shortCaption" type="xs:string" use="optional"/>
		<xs:attribute name="objectKey" type="xs:string" use="optional"/>
		<xs:attribute name="fixed" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Признак определяющий где хданится объект в БД: в фиксированной части или в универсальной</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="version" type="VersionType" use="optional">
			<xs:annotation>
				<xs:documentation>Версия объекта. В общем случае должна соответствовать версии системы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="changed" type="xs:boolean" use="optional" default="false">
			<xs:annotation>
				<xs:documentation>Признак определяющий был ли объект изменен пользователем</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="description" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ClassAtributesDoc">
		<xs:annotation>
			<xs:documentation>Дополнительные атрибуты класса для документации</xs:documentation>
		</xs:annotation>
		<xs:attribute name="fullDBName" type="xs:string" use="required"/>
		<xs:attribute name="semanticCaption" type="xs:string" use="required"/>
		<xs:attribute name="tablePrefix" type="xs:string" use="required"/>
	</xs:attributeGroup>
	<xs:element name="ServerConfiguration">
		<xs:annotation>
			<xs:documentation>Корневой элемент</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Repository" type="RepositoryType">
					<xs:annotation>
						<xs:documentation>Конфигурация репозитория сервера</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Scheme">
					<xs:annotation>
						<xs:documentation>Описание схемы</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="SchemeType">
								<xs:attribute name="version" type="VersionType" use="required"/>
								<xs:attribute name="name" type="xs:string" use="required"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Package" type="PackageType">
					<xs:annotation>
						<xs:documentation>Описание пакета схемы</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FixedValues">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Data">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Values" type="ValuesType"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="name" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Semantics">
					<xs:annotation>
						<xs:documentation>Семантическая принадлежность классификаторов</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Semantic" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Семантический элемент</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="name" type="DBName" use="required"/>
									<xs:attribute name="caption" type="xs:string" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Parameters">
					<xs:annotation>
						<xs:documentation>Список параметров схемы</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Parameter" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Параметр</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="name" type="DBName" use="required">
										<xs:annotation>
											<xs:documentation>Имя параметра</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="value" type="xs:string" use="required">
										<xs:annotation>
											<xs:documentation>Значение параметра</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ParameterChecks">
					<xs:annotation>
						<xs:documentation>Описание корректности параметров</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Check" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Value" type="xs:string" minOccurs="0"/>
										<xs:element name="ErrorMessage" type="xs:string"/>
									</xs:sequence>
									<xs:attribute name="parametr" type="xs:string" use="required"/>
									<xs:attribute name="class" type="xs:string" use="required"/>
									<xs:attribute name="invalid" type="xs:boolean"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Suppliers" type="Suppliers">
					<xs:annotation>
						<xs:documentation>Описание поступающей информации</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="DatabaseConfiguration">
		<xs:annotation>
			<xs:documentation>Корневой элемент для метаданных хранимых в базе данных</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="FixedCls" type="FixedClsType"/>
				<xs:element name="BridgeCls" type="BridgeClsType"/>
				<xs:element name="DataCls" type="DataClsType"/>
				<xs:element name="DataTable" type="DataTableType"/>
				<xs:element name="Table" type="TableType"/>
				<xs:element name="DocumentEntity" type="DocumentEntityType"/>
				<xs:element name="Data2Bridge" type="Classifier2BridgeType"/>
				<xs:element name="Reference" type="Data2ClassifierType"/>
				<xs:element name="MasterDetail" type="Data2ClassifierType"/>
				<xs:element name="Attribute">
					<xs:complexType>
						<xs:attributeGroup ref="AttributeAttributes"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Package" type="PackageType"/>
				<xs:element name="Document">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="DocumentType">
								<xs:choice>
									<xs:element name="Diagram" type="DiagramType"/>
								</xs:choice>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Presentation" type="PresentationType"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="RepositoryType">
		<xs:sequence>
			<xs:element name="Schemes">
				<xs:annotation>
					<xs:documentation>Набор схем репозитория</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Scheme" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Схема</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="privatePath" type="xs:string" use="required"/>
								<xs:attribute name="startupType" default="Auto">
									<xs:simpleType>
										<xs:restriction base="xs:NCName">
											<xs:enumeration value="Auto"/>
											<xs:enumeration value="Manual"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="version" type="xs:string" use="required"/>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="SchemeType">
		<xs:sequence>
			<xs:element name="DWH" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Хранилище данных</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ConnectionString">
							<xs:annotation>
								<xs:documentation>UDL строка подключения к базе данных</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="vendor" type="DBMSVendorType" use="required"/>
					<xs:attribute name="provider" type="xs:Name" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="MDStore" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Многомерная база данных</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ConnectionString">
							<xs:annotation>
								<xs:documentation>UDL строка подключения к базе данных</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="server" type="xs:string" use="required"/>
					<xs:attribute name="catalog" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Archive" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Архивные папки</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="path" type="xs:string" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="DataSources" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описание источников данных</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="path" type="xs:string" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Semantics" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Семантическая принадлежность классификаторов</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Semantic" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Семантический элемент</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="name" type="DBName" use="required"/>
								<xs:attribute name="caption" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Packages">
				<xs:annotation>
					<xs:documentation>Набор пакетов схемы</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Package" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Пакет</xs:documentation>
								<xs:documentation>Пакет</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="privatePath" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Tasks" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Task" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Actions">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Action" maxOccurs="unbounded">
													<xs:complexType>
														<xs:attribute name="name" type="xs:NCName" use="required"/>
														<xs:attribute name="type" use="required">
															<xs:simpleType>
																<xs:restriction base="xs:NCName">
																	<xs:enumeration value="Create"/>
																	<xs:enumeration value="Modify"/>
																	<xs:enumeration value="Delete"/>
																	<xs:enumeration value="ChangeState"/>
																	<xs:enumeration value="Request"/>
																	<xs:enumeration value="Accept"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="States">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="State" maxOccurs="unbounded">
													<xs:complexType>
														<xs:attribute name="name" type="xs:NCName" use="required"/>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="name" type="xs:NCName" use="required"/>
								<xs:attribute name="parent" type="xs:NCName" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PackageType">
		<xs:annotation>
			<xs:documentation>Структура пакета</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeveloperDescription" minOccurs="0"/>
			<xs:element name="Packages" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Package" type="PackageType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Classes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Описания UML классов </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice maxOccurs="unbounded">
						<xs:element name="FixedCls" type="FixedClsType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Фиксированный классификатор</xs:documentation>
							</xs:annotation>
						</xs:element>
