<RegionSettings>
  <!--
  // уровень бюджета СКИФ для муниципальных районов
  RegionBudgetLevel,
  //  уровень бюджета СКИФ для городских округов
  DistrictBudgetLevel,
  // тип документа СКИФ для муниципальных районов
  RegionDocumentSKIFType,
  // тип документа СКИФ для городских округов
  DistrictDocumentSKIFType,
  // использовать для предыдущих лет данные из годовой отчетности
  UseYearReporting  
  -->
  <Region id="Samara">
    <RegionBudgetLevel>[Уровни бюджета].[СКИФ].[Все].[Бюджет района]</RegionBudgetLevel>
    <DistrictBudgetLevel>[Уровни бюджета].[СКИФ].[Все].[Бюджет гор.округа]</DistrictBudgetLevel>
    <RegionDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Собственный отчет района]</RegionDocumentSKIFType>
    <DistrictDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Собственный отчет городского округа]</DistrictDocumentSKIFType>
    <UseYearReporting>true</UseYearReporting>
  </Region>
  <Region id="HMAO">
    <RegionBudgetLevel>[Уровни бюджета].[СКИФ].[Все].[Конс.бюджет МО]</RegionBudgetLevel>
    <DistrictBudgetLevel>[Уровни бюджета].[СКИФ].[Все].[Конс.бюджет МО]</DistrictBudgetLevel>
    <RegionDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Данные конс. отчета субъекта в разрезе городов, районов и поселений (5+6+7+8)]</RegionDocumentSKIFType>
    <DistrictDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Данные конс. отчета субъекта в разрезе городов, районов и поселений (5+6+7+8)]</DistrictDocumentSKIFType>
    <UseYearReporting>true</UseYearReporting>
  </Region>  
</RegionSettings>
