    <!--Ширина второй калонки калонки-->
    <widthForSecondColumnsGrid>0,1</widthForSecondColumnsGrid>

    <!--Название первой калонки-->
    <nameForFirstColumnsGrid>Показатель</nameForFirstColumnsGrid>

