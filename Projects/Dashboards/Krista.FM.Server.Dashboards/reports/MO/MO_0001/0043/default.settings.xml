    <!--в запросах где будет что то типа <%CurrentRegion%> это будет заменено на выбраныйй теритоия в параметре-->
    <CurrentRegion>CurrentRegion</CurrentRegion>

    <!--Строчка выбранная по умолчанию-->
    <SelectItemGridDefault>0</SelectItemGridDefault>

    <!--Ширина диограммы с динамикой-->
    <widthForChartDynamics>0,5</widthForChartDynamics>

    <!--Ширина грида-->
    <widthForGrid>0,5</widthForGrid>

    <!--Ширина первой калонки-->
