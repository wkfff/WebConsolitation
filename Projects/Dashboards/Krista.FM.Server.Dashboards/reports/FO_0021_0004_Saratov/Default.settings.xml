<RegionSettings>
  <!--
  // имя папки с картами региона
  MapFolderName,  
  // загрузка карты из бинарика
  MapLoadFromBin,
  // масштаб карты
  MapZoomValue,
  // сдвиг по вертикали текста городов-выносок карты
  MapCalloutOffsetY  
  -->
  <Region id="Vologda">
    <MapFolderName>Вологда</MapFolderName>
    <MapLoadFromBin>false</MapLoadFromBin>
    <MapZoomValue>80</MapZoomValue>
