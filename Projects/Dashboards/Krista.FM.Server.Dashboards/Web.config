            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
    <roleManager enabled="true"/>
    <trace enabled="false"/>
    <compilation debug="false">
      <assemblies>
        <add assembly="Krista.FM.Server.Dashboards.Core"/>
        <add assembly="Infragistics35.WebUI.UltraWebChart.v11.1, Version=11.1.20111.2036, Culture=neutral, PublicKeyToken=7dd5c3163f2cd0cb"/>
