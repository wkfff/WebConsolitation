<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">                               
    <configSections>
    <section name="regionSettings" type="Krista.FM.Server.Dashboards.Core.RegionSettings" />
    <section name="contactInformation" type="Krista.FM.Server.Dashboards.Core.ContactInformationHelper" />
	</configSections>
	<contactInformation title="Контактная информация">                
		<elements>
			<add name="text" type="Text" value="Техническая поддержка и обслуживание&lt;br/&gt;"/>
			<add name="link" type="Hyperlink" value="НПО &#171;Криста&#187;" link="http://www.krista.ru"/>
	 		<add name="support" type="Email" value="fmsupport@krista.ru"/>
			<add name="phone" type="Phone" value="8(4855) 29-17-50, 29-17-51"/>
		</elements>
	</contactInformation>
  <regionSettings id="HMAO" name="Амурская область" />    
  <appSettings>
    <add key="SchemeServerName" value="gpkrista9:8016"/>  
    <add key="SiteName" value="iМониторинг"/>
    <add key="SitePresentation" value="Информационная система мониторинга и анализа социально-экономического развития Ханты-Мансийского автономного округа - Югры АИС  &#171;Мониторинг ЮГРА&#187;." />
    <!--  <add key="width_size" value="1240" />   
    <add key="height_size" value="1024" /> -->
    <add key="BrowserCompatibility" value="Firefox:2;IE:5;AppleMAC-Safari:2"/>    
    <add key="GuestUser" value="test;test"/>
    <add key="ExternalRef" value="ХМАО - Югра;http://www.admhmao.ru/"/>
    <add key="BootloadServiceName" value="virtualfmserv:7999"/>
    <add key="OpenDocuments" value="true"/>
    <!--
		Ключ StatisticScriptPath определяет адрес сайта сбора статистики.
		Если ключ указан, то на каждой странице будут размещены js-скрипты 
		для сбора статистики, иначе статистика собираться не будет.

		<add key="StatisticScriptPath" value="http://hmao.ifinmon.ru/stat/stats_js.asp"/>   -->
	

    <!--
		Параметр MDXQueryRenameList определяет путь к файлу со списком переименований.
		Сприсок переименований используется для преобразования текстов MDX-запросов
		из формата 2000 в формат 2005.-->
 
    <add key="MDXQueryRenameList" value="RenameList_2000_to_2005.xml"/> 

  </appSettings>
  <connectionStrings>
    <add name="mas" connectionString="Provider=MSOLAP.3;Initial Catalog=Анализ и планирование_ХМАО;Data Source=gpkrista9\sql2008;MDX Unique Name Style=2;Auto Synch Period=100000;ConnectTo=9.0"/>
    <add name="mas_secondary" connectionString="Provider=MSOLAP.3;Initial Catalog=Анализ и планирование_Федеральные данные;Data Source=gpkrista9\sql2008;MDX Unique Name Style=2;Auto Synch Period=100000;ConnectTo=9.0"/>
    <add name="mas_spare" connectionString="Provider=MSOLAP.3;Initial Catalog=Югра;Data Source=gpkrista9\sql2008;MDX Unique Name Style=2;Auto Synch Period=100000;ConnectTo=9.0"/>
    <add name="warehouse" connectionString="Provider=OraOLEDB.Oracle.1;Password=dv;Persist Security Info=True;User ID=dv;Data Source=novosib"/>
    <add name="WebPartDB" connectionString="Password=dv;Persist Security Info=True;User ID=dv;Data Source=dv"/>
    <add name="SGMBase" connectionString="c:\base"/>
  </connectionStrings>
  <system.web>
<customErrors mode="Off"/>
    <httpModules>
   <!--   <add name="HTTPModulePageCleaner" type="Krista.FM.Server.Dashboards.Core.HttpModules.HTTPModulePageCleaner"/>
      <add name="HTTPModuleScriptCleaner" type="Krista.FM.Server.Dashboards.Core.HttpModules.HTTPModuleScriptCleaner"/>-->
    </httpModules>
    <webParts enableExport="true">
      <personalization defaultProvider="OraclePersonalizationProvider">
        <providers>
          <add name="OraclePersonalizationProvider" connectionStringName="WebPartDB" type="Krista.FM.Providers.OraclePersonalizationProvider.OraclePersonalizationProvider"/>
        </providers>
      </personalization>
    </webParts>
    <pages masterPageFile="~/Reports.Master"/>
    <sessionState cookieless="AutoDetect" regenerateExpiredSessionId="true"/>
