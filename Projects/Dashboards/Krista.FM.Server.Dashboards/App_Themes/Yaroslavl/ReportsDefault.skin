        </Y2>
        <X LineThickness="1" TickmarkInterval="0" TickmarkStyle="Smart" Visible="True">
            <MinorGridLines AlphaLevel="255" Color="LightGray" DrawStyle="Dot" Thickness="1"
                Visible="False" />
