                Visible="True" />
            <Labels Font="Verdana, 7pt" FontColor="Gray" HorizontalAlign="Near" ItemFormatString="&lt;DATA_VALUE:00.##&gt;"
                Orientation="Horizontal" VerticalAlign="Center">
                <Layout Behavior="Auto">
                </Layout>
