	</webReportsSettingsSection>

	<system.diagnostics>
		<sources>
			<source name="Krista.FM.Client.MobileReports.Core" switchValue="Verbose">
				<listeners>
					<add name="ColoredConsole"/>
					<add name="FileTextWriter"/>
					<remove name="Default"/>
				</listeners>
			</source>
			<source name="Krista.FM.Client.MobileReports.Uploader" switchValue="Verbose">
				<listeners>
					<add name="ColoredConsole"/>
					<add name="FileTextWriter"/>
					<remove name="Default"/>
				</listeners>
			</source>
			<source name="Krista.FM.Client.MobileReports.Bootloader" switchValue="Verbose">
				<listeners>
					<add name="ColoredConsole"/>
					<add name="FileTextWriter"/>
					<remove name="Default"/>
				</listeners>
			</source>
		</sources>
		<sharedListeners>
			<add name="ColoredConsole" type="Krista.Diagnostics.ColoredConsoleTraceListener, Krista.Diagnostics"/>
			<add name="FileTextWriter" type="System.Diagnostics.TextWriterTraceListener, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" initializeData="WebReportsHelper.log"/>
			<!--
      <add name="ChatServer" type="Krista.Diagnostics.ChatServerTraceListener, Krista.Diagnostics" initializeData="tcp://fmserv:889/Krista.ChatServer/Server.rem"/>
      -->
		</sharedListeners>
	</system.diagnostics>

	<system.runtime.remoting>
		<application name="Krista.FM.Client.MobileReports.BootloaderService">
			<service>
				<wellknown mode="Singleton" type="Krista.FM.Client.MobileReports.Core.SnapshotServer, Krista.FM.Client.MobileReports.Core" objectUri="Server.rem"/>
			</service>
			<channels>
				<channel ref="tcp" port="7999">
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full"/>
					</serverProviders>
				</channel>
			</channels>
		</application>
		<customErrors mode="Off"/>
	</system.runtime.remoting>

<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
