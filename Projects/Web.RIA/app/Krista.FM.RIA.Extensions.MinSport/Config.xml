<?xml version="1.0" encoding="utf-8" ?>
<Extension>
	<!-- Register types -->
	<Types>
		<!-- Repositoryes -->
		
	</Types>

	<!-- Область навигации -->
	<Navigation type="list" ID="acMinSportExtension" icon="Vector" title="Спортивная карта России" order="400" group="Сбор данных" dashboardIcon="/Krista.FM.RIA.Extensions.MinSport/Presentation/Content/Images/minsport.png/extention.axd" defaultItemId="nidbeSubject">
    <Items>
	    <Item ID="nidbeSubject" title="Субъект РФ" icon="Book">
        <Params>
          <Param name="url">'/View/Subject'</Param>
        </Params>
	    </Item>
      <Item ID="nidbeOIV" title="ОИВ в сфере ФК и спорта" icon="Book">
        <Params>
          <Param name="url">'/View/OIV'</Param>
        </Params>
      </Item>
      <Item ID="nidbeDocuments" title="Документы" icon="Book">
      </Item>
      <Item ID="nidbeKinds" title="Виды спорта" icon="Book">
      </Item>
      <Item ID="nidbeOrg" title="Юридические лица, осуществляющие работу по физической культуре и спорту" icon="Book">
      </Item>
      <Item ID="nidbeMedOrg" title="Научные и медицинские организации" icon="Book">
      </Item>
      <Item ID="nidbeOtherOrg" title="Физкультурно-спортивные и иные организации" icon="Book">
      </Item>
      <Item ID="nidbeSportBuild" title="Спортивные сооружения" icon="Book">
      </Item>
      <Item ID="nidbePersonals" title="Персоналии" icon="Book">
      </Item>
      <Item ID="nidbePropaganda" title="Пропаганда ФК и ЗОЖ" icon="Book">
        <Params>
          <Param name="url">'/View/Propaganda'</Param>
        </Params> 
      </Item>
		</Items>
		<Action>MdiTab.addTab({ title: menuItem.text, url: menuItem.url, icon: menuItem.iconCls, passParentSize: menuItem.passParentSize});</Action>
		<ButtomBar></ButtomBar>
	</Navigation>

	<Views>

		<!-- Навигационные представления-->

    <View id="Subject" title="Субъекты"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.MinSport.SubjectsView, Krista.FM.RIA.Extensions.MinSport">
    </View>

    <View id="tabTerritory" title="Муниципальные образования"
			    type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
			    controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Subject.TerritoryView, Krista.FM.RIA.Extensions.MinSport">
    </View>

    <View id="tabPersonCurator" title="Кураторы"
         type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
         controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Subject.PersonCuratorView, Krista.FM.RIA.Extensions.MinSport">
    </View>

    <View id="OIV" title="Органы исполнительной власти"
         type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
         controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Oiv.OivView, Krista.FM.RIA.Extensions.MinSport">
    </View>

    <View id="Propaganda" title="Пропаганда"
         type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
         controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Propaganda.PropagandaView, Krista.FM.RIA.Extensions.MinSport">
    </View>

    <View id="tabPrints" title="Печатные издания"
			    type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
			    controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Propaganda.PrintsView, Krista.FM.RIA.Extensions.MinSport">
    </View>

        <View id="tabMedia" title="Медиа издания"
			    type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
			    controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Propaganda.MediaView, Krista.FM.RIA.Extensions.MinSport">
        </View>

        <View id="tabInternet" title="Интернет издания"
			    type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
			    controlType="Krista.FM.RIA.Extensions.MinSport.Presentation.Views.Propaganda.InternetView, Krista.FM.RIA.Extensions.MinSport">
        </View>
    
    
	</Views>
</Extension>
