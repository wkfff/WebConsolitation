<?xml version="1.0" encoding="utf-8" ?>
<Extension>
  <Params>
  </Params>

  <!-- Register types -->
  <Types>
  </Types>

  <!-- Область навигации -->
  <Navigation type="list" ID="acTargetPrograms" icon="Vector"
              title="Целевые программы" order="200" group="Сбор данных"
              dashboardIcon="/Krista.FM.RIA.Extensions.EO15TargetPrograms/Presentation/Content/Images/dashboardIcon.png/extention.axd"
              defaultItemId="nidbeTrgProgPrograms">
    <Items>
      <Item ID="nidbeTrgProgPrograms" title="Целевые программы" icon="ApplicationViewList" >
        <Params>
          <Param name="url">'/View/TrgProgPrograms'</Param>
        </Params>
      </Item>
    </Items>
    <Action>MdiTab.addTab({ title: menuItem.text, url: menuItem.url, icon: menuItem.iconCls, passParentSize: menuItem.passParentSize});</Action>
    <ButtomBar></ButtomBar>
  </Navigation>

  <Views>
    <!--Корневые представления-->
    <View id="TrgProgPrograms" title="Целевые программы"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.ProgramsView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    
    <!--Остальные формы-->
    <View id="TrgProgProgram" title="Целевая программа"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.ProgramView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>

    <View id="TrgProgDetailNPA" title="Нормативные правовые акты"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailNPAListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailTargets" title="Цели"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailTargetListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailTasks" title="Задачи"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailTaskListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailActions" title="Мероприятия"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailActionListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailFinances" title="Финансирование"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailFinancesListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailTargetRatings" title="Целевые показатели"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailTargetRatingsListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgDetailSubsidy" title="Субсидии"
              type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
              controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.DetailSubsidyListView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>

    <View id="TrgProgReportFinanceUsage" title="Отчет об использовании финансовых средств"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.ReportFinanceUsageView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgReportProgramExecuting" title="Отчет о ходе реализации целевой программы"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.ReportProgramExecutingView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
    <View id="TrgProgReportEffectivityEstimate" title="Оценка целевой программы"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO15TargetPrograms.Presentation.Views.ReportEffectivityEstimateView, Krista.FM.RIA.Extensions.EO15TargetPrograms">
    </View>
  </Views>
</Extension>
