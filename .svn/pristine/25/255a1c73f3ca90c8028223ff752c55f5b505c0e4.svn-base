<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="SEP_0005_0002_date">
    <![CDATA[ -- SEP_0005_0002_date
with
    member [Measures].[Данные]
    as 'Sum
        (
            {
                [Measures].[Значение ],
                [Measures].[Темп роста ],
                [Measures].[Оценка ]
            }
        )'
    member [Measures].[Значение ]
    as '
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[Темп роста ]
    as '
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.DataMember,
            [Measures].[Темп роста]
        )'
    member [Measures].[Оценка ]
    as '
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.DataMember,
            [Measures].[Оценка]
        )'
    member [Measures].[Для сортировки]
    as 'iif
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Level is [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name + "9999",
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Properties("MEMBER_KEY")
        )'
    set [Даты]
    as 'order
        (
            filter
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].Members + [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].Members,
                not IsEmpty([Measures].[Данные])
            ),
            [Measures].[Для сортировки],
            BASC
        )'
    member [Measures].[Имя]
    as 'iif
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Level is [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name + " год",
            "Январь-" + [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name + " " + Ancestor
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember,
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год]
            ).Name + " года"
        )'
    member [Measures].[Уникальное имя]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.UniqueName'
    member [Measures].[Уровень]
    as '0'
    member [Measures].[Тип]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Level.Name'
    member [Measures].[Краткое имя]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name + " год"'
    member [Measures].[Полное имя]
    as '[Measures].[Имя]'
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень],
        [Measures].[Тип],
        [Measures].[Краткое имя],
        [Measures].[Полное имя]
    } on columns,
    [Даты] on rows
from [ЭО_Мониторинг СЭП_Итоги]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Мониторинг СЭП],
        {
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация],
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ]
        },
        [СЭП__Итоги].[СЭП__Итоги].[Все показатели]
    )
    ]]>
  </query>

  <query id="SEP_0005_0002_indicators">
    <![CDATA[ -- SEP_0005_0002_indicators
with
    set [Показатели]
    as 'filter
        (
            Descendants
            (
                [СЭП__Итоги].[СЭП__Итоги].[Все показатели],
                [СЭП__Итоги].[СЭП__Итоги].[Уровень 1],
                SELF_AND_BEFORE
            ),
            not [СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Parent.DataMember
        )'
    member [Measures].[Уникальное имя]
    as '[СЭП__Итоги].[СЭП__Итоги].CurrentMember.UniqueName'
    member [Measures].[Имя]
    as 'iif
        (
            [СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].[Все показатели],
            "Все",
            iif([СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по полному кругу производителей промышленной продукции],
            "Объем отгруженных товаров",
            [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Name)
        )'
    member [Measures].[Уровень]
    as 'iif
        (
            [СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].[Все показатели],
            0,
            1
        )'
    member [Measures].[Тип]
    as '[СЭП__Итоги].[СЭП__Итоги].CurrentMember.Level.Name'
    member [Measures].[Краткое имя]
    as '[СЭП__Итоги].[СЭП__Итоги].CurrentMember.Properties("Краткое наименование")'
    member [Measures].[Полное имя]
    as '[Measures].[Имя]'
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень],
        [Measures].[Тип],
        [Measures].[Краткое имя],
        [Measures].[Полное имя]
    } on columns,
    [Показатели] on rows
from [ЭО_Мониторинг СЭП_Итоги]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Мониторинг СЭП]
    )
    ]]>
  </query>
  
  <query id="SEP_0005_0002_grid_year">
    <![CDATA[ -- SEP_0005_0002_grid_year
with
    set [Показатели]
    as 'filter
        (
            <%sphere%>,
            not [СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Parent.DataMember
        )'
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Два года назад]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].PrevMember.PrevMember'
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].PrevMember'
    member [Measures].[РФ два года назад]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Два года назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ два года назад темп роста]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Два года назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[РФ год назад]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ год назад темп роста]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[РФ сейчас]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ сейчас темп роста]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО два года назад]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Два года назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО два года назад темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Два года назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО год назад]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО год назад темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО сейчас]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО сейчас темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[Единица измерения]
    as 'iif([СЭП__Итоги].[СЭП__Итоги].CurrentMember.Level is [СЭП__Итоги].[СЭП__Итоги].[Уровень 1],
    "Уровень 1",
    [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Properties("Единица измерения"))'
select
    {
        [Measures].[Единица измерения],
        [Measures].[РФ два года назад],
        [Measures].[РФ два года назад темп роста],
        [Measures].[РФ год назад],
        [Measures].[РФ год назад темп роста],
        [Measures].[РФ сейчас],
        [Measures].[РФ сейчас темп роста],
        [Measures].[ХМАО два года назад],
        [Measures].[ХМАО два года назад темп роста],
        [Measures].[ХМАО год назад],
        [Measures].[ХМАО год назад темп роста],
        [Measures].[ХМАО сейчас],
        [Measures].[ХМАО сейчас темп роста]
    } on columns,
    [Показатели] on rows
from [ЭО_Мониторинг СЭП_Итоги]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Мониторинг СЭП]
    )
    ]]>
  </query>

  <query id="SEP_0005_0002_grid_month">
    <![CDATA[ -- SEP_0005_0002_grid_month
with
    set [Показатели]
    as 'filter
        (
            <%sphere%>,
            not [СЭП__Итоги].[СЭП__Итоги].CurrentMember is [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Parent.DataMember
        )'
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад]
    as 'StrToMember("[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[" +
    <%selected_period%>.Parent.Parent.Parent.PrevMember.Name +
    Right(<%selected_period%>.UniqueName, Len(<%selected_period%>.UniqueName) - 84))'
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Предыдущий год]
    as '<%selected_period%>.Parent.Parent.Parent.PrevMember'
    member [Measures].[РФ год назад]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ год назад темп роста]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[РФ сейчас]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ сейчас темп роста]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[РФ оценка]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Оценка]
        )'
    member [Measures].[РФ предыдущий год]
    as '
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Предыдущий год].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[РФ темп роста расчетный]
    as 'iif(not IsEmpty([Measures].[РФ оценка]) and ([Measures].[РФ оценка] <> 0) and not IsEmpty([Measures].[РФ предыдущий год]) and ([Measures].[РФ предыдущий год] <> 0),
    [Measures].[РФ оценка] / [Measures].[РФ предыдущий год],
    null)'
    member [Measures].[ХМАО год назад]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО год назад темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Год назад].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО сейчас]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО сейчас темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО оценка]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            <%selected_period%>.DataMember,
            [Measures].[Оценка]
        )'
    member [Measures].[ХМАО предыдущий год]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Предыдущий год].DataMember,
            [Measures].[Значение]
        )'
    member [Measures].[ХМАО предыдущий год темп роста]
    as '
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[Предыдущий год].DataMember,
            [Measures].[Темп роста]
        ) / 100'
    member [Measures].[ХМАО темп роста расчетный]
    as 'iif(not IsEmpty([Measures].[ХМАО оценка]) and ([Measures].[ХМАО оценка] <> 0) and not IsEmpty([Measures].[ХМАО предыдущий год]) and ([Measures].[ХМАО предыдущий год] <> 0),
    [Measures].[ХМАО оценка] / [Measures].[ХМАО предыдущий год],
    null)'
    member [Measures].[Единица измерения]
    as 'iif([СЭП__Итоги].[СЭП__Итоги].CurrentMember.Level is [СЭП__Итоги].[СЭП__Итоги].[Уровень 1],
    "Уровень 1",
    [СЭП__Итоги].[СЭП__Итоги].CurrentMember.Properties("Единица измерения"))'
select
    {
        [Measures].[Единица измерения],
        [Measures].[РФ год назад],
        [Measures].[РФ год назад темп роста],
        [Measures].[РФ сейчас],
        [Measures].[РФ сейчас темп роста],
        [Measures].[РФ оценка],
        [Measures].[РФ темп роста расчетный],
        [Measures].[ХМАО год назад],
        [Measures].[ХМАО год назад темп роста],
        [Measures].[ХМАО предыдущий год],
        [Measures].[ХМАО предыдущий год темп роста],
        [Measures].[ХМАО сейчас],
        [Measures].[ХМАО сейчас темп роста],
        [Measures].[ХМАО оценка],
        [Measures].[ХМАО темп роста расчетный]
    } on columns,
    [Показатели] on rows
from [ЭО_Мониторинг СЭП_Итоги]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Мониторинг СЭП]
    )
    ]]>
  </query>

