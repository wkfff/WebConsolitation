                [Measures].[Факт]
            )
        )'
    member [Период].[Период].[Темп роста ]
    as 'IIF
        (
            Measures.CurrentMember is [Measures].[План],
            [Период].[Период].[Темп роста плана],
            IIF
            (
                Measures.CurrentMember is [Measures].[Уточненный план],
                [Период].[Период].[Темп роста уточненного плана],
