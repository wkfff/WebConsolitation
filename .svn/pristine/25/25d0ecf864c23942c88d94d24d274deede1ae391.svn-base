            IsEmpty
            (
                (
                    [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                    [Measures].[Факт]
                )
            ) or
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Факт]
            ) = 0,
            null,
            (
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
