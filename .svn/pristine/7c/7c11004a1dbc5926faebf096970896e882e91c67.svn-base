        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[cur-out-subject-invest-portfel],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[cur-out-subject-invest-line],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[cur-out-subject-invest-sum],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-in-subject-invest-line],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-in-subject-invest-other] ,
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-in-subject-invest-portfel],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-in-subject-invest-sum],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-out-subject-invest-other],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-out-subject-invest-portfel],
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-out-subject-invest-line] ,
        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-out-subject-invest-sum]
