        [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Все виды инвестиций].[prev-out-subject-invest-sum]'
    member [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[ParentName]
    as '   [ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.uniquename '
    member [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[LevelName]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.level_unique_name'
    member [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Levelnumber]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.level_number'
member [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[empty?]
    as 'iif((not isempty(([ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.datamember,
    [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[AllSum]))
    or
    [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Levelnumber] >=5
    ), 1,1)'
    
        member [Инвестиции__Виды инвестиций].[Инвестиции__Виды инвестиций].[Razdel]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.Properties("Раздел")'
    
    
    member [Нарастающий итог ]
    as '(iif([ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.children.count > 0,
             [ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.datamember,
             [ОК__ОКВЭД].[ОК__ОКВЭД].currentmember),
             [Нарастающий итог])'
    
     member [Нарастающий итог  ]    
    as '
        
                    Aggregate
                    (
                        filter([ОК__ОКВЭД].[ОК__ОКВЭД].[Подраздел].members,
                        [ОК__ОКВЭД].[ОК__ОКВЭД].currentmember.is_datamember),
                        [Нарастающий итог]
                    )'  
    
select
