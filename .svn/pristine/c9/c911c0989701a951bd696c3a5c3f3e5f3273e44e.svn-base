<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="PM_0001_0003_date">
    <![CDATA[ -- PM_0001_0003_date
with
    set [Даты]
    as 'filter
        (
            [Период__Период].[Период__Период].[Год].Members,
            not IsEmpty([Measures].[Значение])
        )'
    member [Measures].[Имя]
    as '[Период__Период].[Период__Период].CurrentMember.Name'
    member [Measures].[Уникальное имя]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
    member [Measures].[Уровень]
    as '0'
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень]
    } on columns,
    [Даты] on rows
from [ЭО_Мониторинг закупок_Показатели работы]
where
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ],
        {
            [Мониторинг закупок__Виды заказа].[Мониторинг закупок__Виды заказа].[Все виды заказа].[Государственный заказ],
            [Мониторинг закупок__Виды заказа].[Мониторинг закупок__Виды заказа].[Все виды заказа].[Муниципальный заказ]
        },
        [Мониторинг закупок__Направления].[Мониторинг закупок__Направления].[Все направления],
        {
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество рассмотренных обращений],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество согласованных обращений],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Всего],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Отказано в согласовании],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Возвращено],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество обращений, по которым установлено, что согласование не требуется],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество поданных жалоб - Всего],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество поданных жалоб, отклоненных или не удовлетворенных по результатам рассмотрения],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество удовлетворенных жалоб]
        }
    )
    ]]>
  </query>
  
  <query id="PM_0001_0003_grid">
    <![CDATA[ -- PM_0001_0003_grid
with
    set [Даты]
    as 'filter
        (
            descendants
            (
                <%selected_period%>,
                [Период__Период].[Период__Период].[Квартал],
                SELF
            ),
            Left
            (
                [Период__Период].[Период__Период].CurrentMember.Name,
                7
            ) = "Квартал"
        ) + [Период__Период].[Период__Период].[Данные всех периодов].[Всего]'
    member [Период__Период].[Период__Период].[Данные всех периодов].[Всего]
    as '<%selected_period%>'
    set [Показатели]
    as '
        {
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Согласование возможности заключения государственного (муниципального) контракта  с единственным поставщиком (исполнителем, подрядчиком)],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество рассмотренных обращений],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество согласованных обращений],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Всего],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Отказано в согласовании],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество отклонений обращений - Возвращено],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество обращений, по которым установлено, что согласование не требуется],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Рассмотрение жалоб],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество поданных жалоб - Всего],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество поданных жалоб, отклоненных или не удовлетворенных по результатам рассмотрения],
            [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Количество удовлетворенных жалоб]
        }'
    member [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Согласование возможности заключения государственного (муниципального) контракта  с единственным поставщиком (исполнителем, подрядчиком)]
    as 'null'
    member [Мониторинг закупок__Показатели].[Мониторинг закупок__Показатели].[Все показатели].[Рассмотрение жалоб]
    as 'null'
select
    { [Даты] } on columns,
    [Показатели] on rows
from [ЭО_Мониторинг закупок_Показатели работы]
where
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ],
        {
            [Мониторинг закупок__Виды заказа].[Мониторинг закупок__Виды заказа].[Все виды заказа].[Государственный заказ],
            [Мониторинг закупок__Виды заказа].[Мониторинг закупок__Виды заказа].[Все виды заказа].[Муниципальный заказ]
        },
        [Мониторинг закупок__Направления].[Мониторинг закупок__Направления].[Все направления]
    )
    ]]>
  </query>

</root>