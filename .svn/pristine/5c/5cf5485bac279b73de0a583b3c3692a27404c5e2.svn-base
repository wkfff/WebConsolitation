                (
                    [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Сибирский федеральный округ].[Читинская область],
                    [КД].[Сопоставимый].[Численность экономически активного населения ]
                ),
                IIF 
                (
                    [Период].[Период].CurrentMember.Properties("PKID") < [Период].[Период].[Данные всех периодов].[2009].[Полугодие 2].[Квартал 4].[Октябрь].Properties("PKID")or [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Приволжский федеральный округ].[Пензенская область],
                    LookupCube 
                    (
                        "[СТАТ_Труд_Трудовые ресурсы]",
                        " 
                        (
                            [Период].[Год Квартал Месяц].[Данные всех периодов].[2008],
                            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                            [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                            [Measures].[За период],
                            [Территории].[РФ].[" + [Территории].[Сопоставимый].CurrentMember.Name + "] 
                        )" 
                    ),
                    LookupCube 
                    (
                        "[СТАТ_Труд_Трудовые ресурсы]",
                        " 
                        (
                            [Период].[Год Квартал Месяц].[Данные всех периодов].[<%period_year%>],
                            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                            [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                            [Measures].[Нарастающий итог],
                            [Территории].[РФ].[" + [Территории].[Сопоставимый].CurrentMember.Name + "] 
                        )" 
                    )
                ) 
            ) 
        )' 
    member [КД].[Сопоставимый].[Налоговая нагрузка]  
    as '[КД].[Сопоставимый].[Налог на доходы физических лиц ] + [КД].[Сопоставимый].[Единый социальный налог (страховые взносы с 2010г) ] + [КД].[Сопоставимый].[Налог на имущество физических лиц ] + [КД].[Сопоставимый].[Транспортный налог с физических лиц ]'  
    member [КД].[Сопоставимый].[Налоговая нагрузка на ед.акт.населения]  
    as 'IIF  
        (
            IsEmpty  
            (
                [КД].[Сопоставимый].[Численность экономически активного населения ]  
            ) or [КД].[Сопоставимый].[Численность экономически активного населения ]  = 0,
            null,
            [КД].[Сопоставимый].[Налоговая нагрузка]  / [КД].[Сопоставимый].[Численность экономически активного населения ]  
        )'  
    member [КД].[Сопоставимый].[Ранг налоговой нагрузки на ед. экономически активного населения] 
    as 'IIF 
        (
            IsEmpty 
            (
                [КД].[Сопоставимый].[Налоговая нагрузка на ед.акт.населения]
            ) or [Территории].[Сопоставимый].CurrentMember.Level.Name = "РФ" or
            [Территории].[Сопоставимый].CurrentMember.Level.Name = "Федеральный округ",
            null,
            Rank
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order
                (
                    [Территории ],
                    [КД].[Сопоставимый].[Налоговая нагрузка на ед.акт.населения],
                    BASC
                ),
                [КД].[Сопоставимый].[Налоговая нагрузка на ед.акт.населения]
            )
        )' 
    member [КД].[Сопоставимый].[Худший ранг налоговой нагрузки на ед. экономически активного населения] 
    as 'max  
        (
            [Территории ],
            [КД].[Сопоставимый].[Ранг налоговой нагрузки на ед. экономически активного населения]
        )'  
    set [Территории ] 
    as 'Filter  
        (
            Descendants  
            (
                <%selected_fo%>,
                [Территории].[Сопоставимый].[Субъект РФ],
                SELF  
            ),
            not  
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember  
            ) and not IsEmpty  
            (
                [КД].[Сопоставимый].[Налоговая нагрузка]  
            )  
        )'  
SELECT
    {
        [КД].[Сопоставимый].[Налоговая нагрузка на ед.акт.населения],
        [КД].[Сопоставимый].[Ранг налоговой нагрузки на ед. экономически активного населения],
        [КД].[Сопоставимый].[Худший ранг налоговой нагрузки на ед. экономически активного населения],
        [КД].[Сопоставимый].[Численность экономически активного населения ],
        [КД].[Сопоставимый].[Налоговая нагрузка],
        [КД].[Сопоставимый].[Налог на доходы физических лиц ],
        [КД].[Сопоставимый].[Единый социальный налог (страховые взносы с 2010г) ],
        [КД].[Сопоставимый].[Налог на имущество физических лиц ],
        [КД].[Сопоставимый].[Транспортный налог с физических лиц ] 
    } on columns,
    {
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация]    
        } + Filter    
        (
            Generate    
            (
                Descendants    
                (
                    <%selected_fo%>,
                    [Территории].[Сопоставимый].[Федеральный округ],
                    SELF    
                ),
                {
                    [Территории].[Сопоставимый].CurrentMember    
                } +  Descendants    
                (
                    [Территории].[Сопоставимый].CurrentMember,
                    [Территории].[Сопоставимый].[Субъект РФ],
                    SELF    
                )    
            ),
            not    
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember    
            ) and not IsEmpty    
            (
                [КД].[Сопоставимый].[Налоговая нагрузка]    
            )    
        )   
    } on rows    
FROM [ФК_МесОтч_Доходы]  
WHERE  
    (
        [Measures].[<%selected_measure%>],
        [Период].[Период].[Данные всех периодов].[<%period_year%>].[<%period_half_year%>].[<%period_quater%>].[<%period_month%>]
    ) 	            
	]]>
  </query>

  <query id="STAT_0001_0011_chart_dynamic">
    <![CDATA[
-- STAT_0001_0011_chart_dynamic
-- <#[ФК_МесОтч_Доходы];[СТАТ_Труд_Трудовые ресурсы]#>
with   
    member [КД].[Сопоставимый].[Налог на доходы физических лиц ]   
    as '   
        (
            [Measures].[<%selected_measure%>],
            [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ПРИБЫЛЬ, ДОХОДЫ].[Налог на доходы физических лиц],
            [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта]   
        ) / 1000'   
    member [КД].[Сопоставимый].[Взносы на социальные нужды]  
    as 'Aggregate  
        (
            {
              [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[ЗАДОЛЖЕННОСТЬ И ПЕРЕРАСЧЕТЫ ПО ОТМЕНЕННЫМ НАЛОГАМ, СБОРАМ И ИНЫМ ОБЯЗАТЕЛЬНЫМ ПЛАТЕЖАМ].[Единый социальный налог[ii]]].[Единый социальный налог, зачисляемый в бюджеты территориальных фондов обязательного медицинского страхования]            
            }
        )'    
    member [КД].[Сопоставимый].[Единый социальный налог (страховые взносы с 2010г) ]   
    as '   
        ((
            [Measures].[<%selected_measure%>],
            [КД].[Сопоставимый].[Взносы на социальные нужды],
            [Уровни бюджета].[СКИФ].[Все].[Внебюдж.фонд]   
        ) * 13) / 1000'   
    member [КД].[Сопоставимый].[Налог на имущество физических лиц ]   
    as '   
        (
            [Measures].[<%selected_measure%>],
            [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ИМУЩЕСТВО].[Налог на имущество физических лиц],
            [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта]   
        )/ 1000'   
    member [КД].[Сопоставимый].[Транспортный налог с физических лиц ]  
    as '  
        (
            [Measures].[<%selected_measure%>],
            [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ИМУЩЕСТВО].[Транспортный налог].[Транспортный налог с физических лиц],
            [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта]  
        ) / 1000'    
    member [Measures].[Численность экономически активного населения ] 
    as 'IIF 
        (
            [Территории].[Сопоставимый].CurrentMember.Level.Name = "РФ"  or [Территории].[Сопоставимый].CurrentMember.Level.Name = "Федеральный округ",
            Sum 
            (
                Filter 
                (
                    Descendants 
                    (
                        [Территории].[Сопоставимый].CurrentMember,
                        [Территории].[Сопоставимый].[Субъект РФ],
                        SELF 
                    ) - 
                    {
                        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Сибирский федеральный округ].[Забайкальский край] 
                    },
                    not 
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
                    ) 
                ),
                [Measures].[Численность экономически активного населения ] 
            ),
            IIF 
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Сибирский федеральный округ].[Забайкальский край],
                (
                    [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Сибирский федеральный округ].[Читинская область],
                    [Measures].[Численность экономически активного населения ]
                ),
                IIF 
                (
                    [Период].[Период].CurrentMember.Properties("PKID") < [Период].[Период].[Данные всех периодов].[2009].[Полугодие 2].[Квартал 4].[Октябрь].Properties("PKID")or 
                    [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Приволжский федеральный округ].[Пензенская область],
                    LookupCube 
                    (
                        "[СТАТ_Труд_Трудовые ресурсы]",
                        " 
                        (
                            [Период].[Год Квартал Месяц].[Данные всех периодов].[2008],
                            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                            [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                            [Measures].[За период],
                            [Территории].[РФ].[" + [Территории].[Сопоставимый].CurrentMember.Name + "] 
                        )" 
                    ),
                    LookupCube 
                    (
                        "[СТАТ_Труд_Трудовые ресурсы]",
                        " 
                        (
                            [Период].[Год Квартал Месяц].[Данные всех периодов].[<%period_year%>],
                            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                            [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                            [Measures].[Нарастающий итог],
                            [Территории].[РФ].[" + [Территории].[Сопоставимый].CurrentMember.Name + "] 
                        )" 
                    )
                ) 
            ) 
        )' 
    member [Measures].[Налоговая нагрузка]
    as '([КД].[Сопоставимый].[Налог на доходы физических лиц ], [Measures].[<%selected_measure%>]) +
        ([КД].[Сопоставимый].[Единый социальный налог (страховые взносы с 2010г) ], [Measures].[<%selected_measure%>]) +
        ([КД].[Сопоставимый].[Налог на имущество физических лиц ], [Measures].[<%selected_measure%>]) +
        ([КД].[Сопоставимый].[Транспортный налог с физических лиц ], [Measures].[<%selected_measure%>])'
    
    member [Measures].[Налоговая нагрузка на ед.акт.населения]   
    as 'IIF   
        (
            IsEmpty   
            (
                [Measures].[Численность экономически активного населения ]   
            ) or [Measures].[Численность экономически активного населения ]  = 0,
            null,
            [Measures].[Налоговая нагрузка]  / [Measures].[Численность экономически активного населения ]   
        )' 
    member [Measures].[Средняя налоговая нагрузка по ФО] 
    as 'IIF 
        (
            Sum 
            (
                Filter 
                (
                    Descendants 
                    (
                        [Территории].[Сопоставимый].CurrentMember.Parent,
                        [Территории].[Сопоставимый].[Субъект РФ],
                        SELF 
                    ),
                    not 
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
                    ) and not IsEmpty 
                    (
                        [Measures].[Налоговая нагрузка] 
                    ) 
                ),
                [Measures].[Численность экономически активного населения ] 
            ) = 0,
            null,
            Sum 
            (
                Filter 
                (
                    Descendants 
                    (
                        [Территории].[Сопоставимый].CurrentMember.Parent,
                        [Территории].[Сопоставимый].[Субъект РФ],
                        SELF 
                    ),
                    not 
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
                    ) and not IsEmpty 
                    (
                        [Measures].[Налоговая нагрузка] 
                    ) 
                ),
                [Measures].[Налоговая нагрузка] 
            ) / Sum 
            (
                Filter 
                (
                    Descendants 
                    (
                        [Территории].[Сопоставимый].CurrentMember.Parent,
                        [Территории].[Сопоставимый].[Субъект РФ],
                        SELF 
                    ),
                    not 
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
                    ) and not IsEmpty 
                    (
                        [Measures].[Налоговая нагрузка] 
                    ) 
                ),
                [Measures].[Численность экономически активного населения ] 
            ) 
        )' 
    member [Measures].[Средняя налоговая нагрузка по РФ] 
    as 'IIF 
        (
            (
                [Территории].[Сопоставимый].[Все территории].[Российская  Федерация],
                [Measures].[Численность экономически активного населения ] 
            ) = 0,
            null,
            Sum 
            (
                Filter 
                (
                    Descendants 
                    (
                        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация],
                        [Территории].[Сопоставимый].[Субъект РФ],
                        SELF 
                    ),
                    not 
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
                    ) and not IsEmpty 
                    (
                        [Measures].[Налоговая нагрузка] 
                    ) 
                ),
                [Measures].[Налоговая нагрузка] 
            ) / 
            (
                [Территории].[Сопоставимый].[Все территории].[Российская  Федерация],
                [Measures].[Численность экономически активного населения ] 
            ) 
        )'        
    member [Measures].[Год]
    as '[Период].[Период].CurrentMember.Parent.Parent.Parent.Name'
   
SELECT
    {
             [Measures].[Год],
             <%chart_column_set%>
    } on columns,
    {
        Filter
        (
            Descendants
            (
                [Период].[Период].[Данные всех периодов].[2008],
                [Период].[Период].[Месяц],
                SELF
            ) +
            Descendants
            (
                [Период].[Период].[Данные всех периодов].[2009],
                [Период].[Период].[Месяц],
                SELF
            ),
            not
            (
                [Период].[Период].CurrentMember is [Период].[Период].CurrentMember.Parent.DataMember
            ) and not IsEmpty
            (
                [Measures].[Налоговая нагрузка на ед.акт.населения]
            )
        )
    } on rows   
FROM [ФК_МесОтч_Доходы]   
WHERE   
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация<%selected_subject%>
    )
  ]]>
  </query>

  <query id="STAT_0001_0011_regions">
    <![CDATA[
-- STAT_0001_0011_regions
-- <#[ФК_МесОтч_Доходы]#>
With 
    member [Measures].[UniqName]
    as '[Территории].[Сопоставимый].CurrentMember.UniqueName'
SELECT
    { [Measures].[UniqName] } on columns,
    Filter 
    (
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация]
        } + [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].Children,
        (
            not 
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember 
            ) 
        ) 
    )  on rows 
FROM [ФК_МесОтч_Доходы]  
  ]]>
  </query>

</root>

