<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[SELECT
    { [Measures].[За период]  } on columns,
    --#доделать эту шляпу ;
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[За период]  > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [СТАТ_Туризм_Основные показатели]
where
    (
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Туризм_Основные показатели].[<%way_last_year%>],
        [Территории].[РФ Карта].[Субъект РФ].[<%1%>],
        [Туризм].[Основные показатели].[Число организаций гостиничного типа]
    )]]>
  </query>
  <query id="G">
    <![CDATA[WITH
    MEMBER [Measures].[Единица измерения]    
    AS  '[Туризм].[Основные показатели].CurrentMember.Properties("Единица измерения")'
select
    { [Measures].[Единица измерения],
        [Measures].[За период]
          
    }  on columns ,
    non empty   
    {
        [Туризм].[Основные показатели].[Число организаций гостиничного типа],
        [Туризм].[Основные показатели].[Единовременная вместимость],
        [Туризм].[Основные показатели].[Предоставлено ночёвок в КСР гостиничного типа]   
    }on rows 
FROM  [СТАТ_Туризм_Основные показатели]    
where    
    (
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Туризм_Основные показатели].[Без группировки],
        [Территории].[РФ Карта].[Субъект РФ].[<%1%>],
        [Период].[Год Квартал Месяц].[<%lastdate%>]
    )]]>
  </query>
  <query id="TC">
    <![CDATA[WITH  
    MEMBER [Период].[Год Квартал Месяц].[Slicer] 
    AS 'AGGREGATE
        (
            {
                {

                    [Период].[Год Квартал Месяц].[Данные всех периодов].[<%lastdate%>]
                }
            }
        )' 
    SET [NonFilterColumnSet] 
    AS 'Intersect
        (
            {
                {
                    Descendants
                    (
                        [Территории].[РФ Карта].[Все территории],
                        [Территории].[РФ Карта].[Все территории].Level,
                        SELF_AND_AFTER
                    )
                }
            },
            {
                [Территории].[РФ Карта].[МР ГО].[г. Саратов],
                [Территории].[РФ Карта].[ГП СП ВТГФЗ].AllMembers
            }
        )' 
    SET [NonFilterRowSet] 
    AS 'Intersect
        (
            {
                {
                    Descendants
                    (
                        [Туризм].[Основные показатели].[<%pokaz%>],
                        [Туризм].[Основные показатели].[<%pokaz%>].Level,
                        SELF_AND_AFTER
                    )
                }
            },
            {
                [Туризм].[Основные показатели].[Показатель].AllMembers
            }
        )' 
SELECT
    NON EMPTY{[NonFilterColumnSet]} on columns,
    NON EMPTY{[NonFilterRowSet]} on rows 
FROM [СТАТ_Туризм_Основные показатели] 
WHERE 
    (
        [Measures].[За период],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Облстат],
        [Период].[Год Квартал Месяц].[Slicer],
        [Группировки].[Туризм_Основные показатели].[По городам и районам]
    )   ]]>
  </query>

  <query id="BC">
    <![CDATA[select
    NON EMPTY     
    {            [Туризм].[Виды средств размещения].[Вид средства размещения].[Гостиницы (включая туристские)],
            [Туризм].[Виды средств размещения].[Вид средства размещения].[Общежития для приезжих],
            [Туризм].[Виды средств размещения].[Вид средства размещения].[Прочие средства размещения]
       
    }  on rows ,
    non empty  
    {
 [Период].[Год Квартал Месяц].[<%lastdate%>]
          --[Территории].[РФ].[ГП СП ВТГФЗ].Members
    }  on columns
FROM  [СТАТ_Туризм_Основные показатели]  
where  
    (
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Туризм_Основные показатели].[Группировка].[По видам средств размещения],
        [Территории].[РФ Карта].[Субъект РФ].[<%1%>],
        [Measures].[За период]  ,
  [Туризм].[Основные показатели].[<%pokaz%>]
    )  ]]>
  </query>

</root>