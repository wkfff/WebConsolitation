<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[SELECT
    { [Measures].[За период]  } on columns,
    {
        Tail 
        (
            Filter 
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[За период] > 0 
            ) 
        ).Item(0) 
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows 
FROM  [СТАТ_Платные услуги_Основные показатели]
where 
    (
        [Территории].[РФ Карта].[Федеральный округ].[<%1%>],
         [Источники данных].[Источник].[<%2%>],
        [Группировки].[Платные услуги_Основные показатели].[Без группировки],
        [Платные услуги].[Основные показатели].[Объем платных услуг населению] 
    )  ]]>
  </query> 
  <query id="last_date2">
    <![CDATA[
    SELECT
    { [Measures].[За период]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[За период] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM  [СТАТ_Платные услуги_Основные показатели]
where
    (
        [Территории].[РФ Карта].[Субъект РФ].[Саратовская обл.] ,
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Платные услуги_Основные показатели].[Без группировки],
        [Платные услуги].[Основные показатели].[Объем платных услуг населению к предыдущему году в сопоставимых ценах]
    )]]>
  </query>
  <query id="BC">
    <![CDATA[select
    non empty 
    {
        [Территории].[РФ Карта].[Субъект РФ].Members
    }on columns   ,
    non empty 
    {
       [Период].[Год Квартал Месяц].[Год].[<%lastdate%>]   
    }   on rows 
FROM   [СТАТ_Платные услуги_Основные показатели] 
where 
    (
        [Measures].[За период] ,
        [Источники данных].[Источник].[<%2%>],
        [Группировки].[Платные услуги_Основные показатели].[Без группировки],
        [Платные услуги].[Основные показатели].[Объем платных услуг населению] 
    )  ]]>
  </query>

  <query id="TC">
    <![CDATA[select
    non empty  
    {
        [Территории].[РФ Карта].[Субъект РФ].[Саратовская обл.],
        [Территории].[РФ Карта].[Субъект РФ].[<%pokaz%>] 
    }on rows    ,
    non empty  
    {
        -- надоб разобраться как это нармально написать!! 
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 1].[Январь],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 1].[Февраль],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 1].[Март],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 2].[Апрель],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 2].[Май],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 1].[Квартал 2].[Июнь],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 3].[Июль],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 3].[Август],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 3].[Сентябрь],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 4].[Октябрь],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 4].[Ноябрь],
        [Период].[Год Квартал Месяц].[Год].[<%lastdate%>].[Полугодие 2].[Квартал 4].[Декабрь]   
    }  on columns  
FROM   [СТАТ_Платные услуги_Основные показатели]  
where  
    (
        [Measures].[За период] ,
        [Источники данных].[Источник].[<%2%>],
        [Группировки].[Платные услуги_Основные показатели].[Без группировки],
        [Платные услуги].[Основные показатели].[Объем платных услуг населению к предыдущему году в сопоставимых ценах] 
    ) ]]>
  </query>
