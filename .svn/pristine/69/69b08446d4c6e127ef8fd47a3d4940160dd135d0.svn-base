<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="ORG_0003_0008_list_of_dates">
    <![CDATA[ -- name: ORG_0003_0008_list_of_dates
select
    {[Measures].[Средняя цена]} on columns,
    {
        filter
        (
            [Период].[День].[День].members,
            not IsEmpty
            (
                (
                    [Организации].[Товары и услуги].[Все товары и услуги].[Транспортные услуги].[Стоимость проезда пассажиров в общественном транспорте по городу (1 поездка)]
                )) or
               not isEmpty ((
                    [Организации].[Товары и услуги].[Все товары и услуги].[Транспортные услуги].[Стоимость проезда пассажиров автотранспортом внутрирайонного сообщения (1 км пути)]
                )
            ) and
            [Measures].[Средняя цена] > 0
        )
    } on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        [Территории].[РФ].[Все территории].[Российская Федерация].[Северо-Западный федеральный округ].[Вологодская область].[Вологодская область],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - МО Вологодской области],
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена]
    )
    ]]>
  </query>

  <query id = "ORG_0003_0008_grid">
    <![CDATA[ -- ORG_0003_0008_grid
select
    crossjoin
    (
        {[Measures].[Средняя цена]},
        {
            [Организации].[Товары и услуги].[Все товары и услуги].[Транспортные услуги].[Стоимость проезда пассажиров в общественном транспорте по городу (1 поездка)],
            [Организации].[Товары и услуги].[Все товары и услуги].[Транспортные услуги].[Стоимость проезда пассажиров автотранспортом внутрирайонного сообщения (1 км пути)]
        }
    ) on columns,
    non empty
    {
        filter
        (
            [Территории].[РФ].[Все территории].[Российская Федерация].[Северо-Западный федеральный округ].[Вологодская область].[Вологодская область].Children,
            [Measures].[Средняя цена] > 0
        )
    } on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - МО Вологодской области],
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена],
        <%selected_date%>
    )
    ]]>
  </query>

  <query id = "ORG_0003_0008_chart_by_time">
    <![CDATA[ -- ORG_0003_0008_chart_by_time
select
    Tail
    (
        Filter
        (
            { <%selected_date%> : <%first_date%> },
            [Measures].[Средняя цена] > 0
        ),
        12
    ) on columns,
    {[Measures].[Средняя цена]} on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        <%selected_district%>,
        <%selected_parameter%>,
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена]
    )

    ]]>
  </query>

  <query id = "ORG_0003_0008_chart_by_time_names">
    <![CDATA[ -- ORG_0003_0008_chart_by_time_names
with
member [Measures].[День]
as '[Период].[День].CurrentMember.UniqueName'
select
    Tail
    (
        Filter
        (
            { <%selected_date%> : <%first_date%> },
            [Measures].[Средняя цена] > 0
        ),
        12
    ) on columns,
    {[Measures].[День]} on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        <%selected_district%>,
        <%selected_parameter%>,
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена]
    )

    ]]>
  </query>

  <query id = "ORG_0003_0008_chart_by_district">
    <![CDATA[ -- ORG_003_0008_chart_by_district
select
    {[Measures].[Средняя цена]} on columns,
    non empty order(
    {
        filter
        (
            [Территории].[РФ].[Все территории].[Российская Федерация].[Северо-Западный федеральный округ].[Вологодская область].[Вологодская область].Children,
            [Measures].[Средняя цена] > 0
        )
    }, [Measures].[Средняя цена], asc) on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - МО Вологодской области],
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена],
        <%selected_parameter%>,
        <%selected_date%>
    )
    ]]>
  </query>

</root>