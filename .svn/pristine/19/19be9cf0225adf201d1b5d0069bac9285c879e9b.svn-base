<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="years">
    <![CDATA[
--years
select
   non empty {
      filter(
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members,
Aggregate(
   [ОК__МКБ10].[ОК__МКБ10].[Класс].members,[Measures].[За период]) > 0)
        --[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members
    }  on rows,
    non empty 
    {
           [Measures].[За период]
    }on columns 
from  [СТАТ_Здравоохранение_Заболеваемость населения]
where ([Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
[ОК__Отдельные возрастные группы],
[Группировки__Здравоохранение_Заболеваемость].[Группировки__Здравоохранение_Заболеваемость].[Все группировки].[По МКБ10],
[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
)
]]>
  </query>

  <query id="chart1">
    <![CDATA[
--chart1
with  
    member [Measures].[Ед]  
    as '[Здравоохранение__Заболеваемость населения].[Здравоохранение__Заболеваемость населения].Currentmember.Properties("Единица измерения")'
select
   non empty {
        --[ОК__МКБ10].[ОК__МКБ10].[Класс].members
        filter({[ОК__МКБ10].[ОК__МКБ10].[Класс].members},Right
            (
                [ОК__МКБ10].[ОК__МКБ10].Currentmember.UniqueName,
                21
            ) <> '[Значение не указано]'
            and not isempty( [Measures].[За период] )
            )
    }  on rows,
    non empty 
    {
        [Measures].[За период]  ,
        [Measures].[Ед]  
    }on columns 
from  [СТАТ_Здравоохранение_Заболеваемость населения] 
where ([Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],

[ОК__Отдельные возрастные группы],
<%year%>,
[Группировки__Здравоохранение_Заболеваемость].[Группировки__Здравоохранение_Заболеваемость].[Все группировки].[По МКБ10],
[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
[Здравоохранение__Заболеваемость населения].[Здравоохранение__Заболеваемость населения].[Все показатели].[Зарегистрировано больных с диагнозом, установленным впервые в жизни, на 1000 человек населения]
)

]]>
</query>

<query id="grid">
<![CDATA[
--grid
with member [Measures].[Прирост цепной1]
as 'Round([Measures].[Прирост цепной],3)'
select
   non empty {
        filter({[ОК__МКБ10].[ОК__МКБ10].[Класс].members},Right
            (
                [ОК__МКБ10].[ОК__МКБ10].Currentmember.UniqueName,
                21
            ) <> '[Значение не указано]')
    }  on rows,

    {
    
        {
           [Measures].[За период],
           [Measures].[Прирост цепной1],
           [Measures].[Темп прироста цепной]
        }
        * 
        {
            [Здравоохранение__Заболеваемость населения].[Здравоохранение__Заболеваемость населения].[Все показатели].[Зарегистрировано больных с диагнозом, установленным впервые в жизни, на 1000 человек населения]
        }
    }on columns
from  [СТАТ_Здравоохранение_Заболеваемость населения]
where (
[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],

[ОК__Отдельные возрастные группы],
<%year%>,
[Группировки__Здравоохранение_Заболеваемость].[Группировки__Здравоохранение_Заболеваемость].[Все группировки].[По МКБ10],
[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
)
]]>
  </query>

  <query id="chart2">
    <![CDATA[
--chart2
select
    non empty 
    {

        filter
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members,
            not isempty([Measures].[За период])
        ) 
    }  on rows,
 
    {
       [Здравоохранение__Заболеваемость населения].[Здравоохранение__Заболеваемость населения].[Все показатели].[Зарегистрировано больных с диагнозом, установленным впервые в жизни, на 1000 человек населения]
    }on columns 
from  [СТАТ_Здравоохранение_Заболеваемость населения] 
where 
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Measures].[За период],
        [ОК__Отдельные возрастные группы],
        [Группировки__Здравоохранение_Заболеваемость].[Группировки__Здравоохранение_Заболеваемость].[Все группировки].[По МКБ10] ,
        [ОК__МКБ10].[ОК__МКБ10].[Все виды заболеваний].[<%currentWay%>],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    ) 
]]>
  </query>
</root>