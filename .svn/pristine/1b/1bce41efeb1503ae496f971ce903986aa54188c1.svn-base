<?xml version="1.0" encoding="windows-1251"?>
<root>


  <query id = "Period">
    <![CDATA[with 

    member [Death] 
    as '
        (
            [Население__Причины смерти].[Население__Причины смерти].[Все показатели].[Число умерших],
                [Measures].[Нарастающий итог]

        )' 
    member [UniqueName]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.UniqueName'
    member [Mounth]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name'
    member [Year]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.parent.parent.parent.Name'
    member [Child]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name'
    member [Parent]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.parent.parent.parent.Name'

select
    non empty{

[UniqueName],
[Mounth],
[Year],
[Child],
[Parent]

    } on columns ,
    non empty    {
filter(
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].members,
        not isempty([Death]))

    } on rows
from [СТАТ_Население_Причины смерти] 
where 
    (
            [ОК__МКБ10].[ОК__МКБ10],

        [Источники данных].[Источники данных].[Источник].[СТАТ Отчетность - Статистический раздел],
        [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Основная возрастная группа].[Значение не указано],
        --[Тип местности].[Тип местности].[Все типы местности].[Значение не указано],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].datamember,--.[Сахалинская обл.],
        [Группировки__Население_Причины смерти].[Группировки__Население_Причины смерти].[Группировка].[По МКБ10]  
    )   
]]>
  </query>


  <query id = "ChartDynamic">
    <![CDATA[with 
    member [isDataMember] 
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.IS_DATAMEMBER' 
    member [Death] 
    as ' 
        (
            [Население__Причины смерти].[Население__Причины смерти].[Все показатели].[Число умерших],
                [Measures].[Нарастающий итог]
        )' 
    member [Mounth]
    as '
        (
         [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.Name
        )'
    member [Year]
    as '
        (
         [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.parent.Name
        )'
   
select
    {
        filter 
        (

            <%CurrentDate%>:<%CurrentDate%>.lag(48) ,

            not ([isDataMember])and
            not IsEmpty([Death]) 
        ) 
    } on rows,
    {

        [Death],
        [Mounth],
        [Year]

    } on columns 
from [СТАТ_Население_Причины смерти] 
where 
    (
        <%SelectUniqName%>,
        [Источники данных].[Источники данных].[Источник].[СТАТ Отчетность - Статистический раздел],
        [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Основная возрастная группа].[Значение не указано],
        --[Тип местности].[Тип местности].[Все типы местности].[Значение не указано],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].datamember,--.[Сахалинская обл.],
        [Группировки__Население_Причины смерти].[Группировки__Население_Причины смерти].[Группировка].[По МКБ10] 
    )   
]]>
  </query>

  <query id = "ChartPoint">
    <![CDATA[with 
    member [isDataMember]
    as '[ОК__МКБ10].[ОК__МКБ10].currentmember.IS_DATAMEMBER'
    member [Death] 
    as '
        (
            [Население__Причины смерти].[Население__Причины смерти].[Все показатели].[Число умерших],
                [Measures].[Нарастающий итог],
--[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].[2010].[Полугодие 2].[Квартал 4].[Октябрь]
<%CurrentDate%>
        )' 
    member [DeathComparison]  
    as '([Население__Причины смерти].[Население__Причины смерти].[Все показатели].[Число умерших] ,
            [Measures].[Нарастающий итог],
        --[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].[2010].[Полугодие 2].[Квартал 4].[Ноябрь]
        <%ComparisonDate%>
    )' 
    member [DeathChildren] 
    as '([Население__Причины смерти].[Население__Причины смерти].[Показатель].[Число умерших детей в возрасте до 1 года] ,
            [Measures].[Нарастающий итог],
--        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].[2010].[Полугодие 2].[Квартал 4].[Октябрь]
<%CurrentDate%>
    )' 
    member [DeathChildrenComparison]  
    as '([Население__Причины смерти].[Население__Причины смерти].[Показатель].[Число умерших детей в возрасте до 1 года] ,
            [Measures].[Нарастающий итог],
                --[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].[2010].[Полугодие 2].[Квартал 4].[Ноябрь]
                <%ComparisonDate%>
    )'
    member [DiseaseUniqueName]
    as '[ОК__МКБ10].[ОК__МКБ10].currentmember.MEMBER_UNIQUE_NAME'
    member [DiseaseLevel]
    as '
iif([isDataMember],
    0,[ОК__МКБ10].[ОК__МКБ10].currentmember.LEVEL_NUMBER)'

    member [Disease]
    as '
iif([isDataMember],
    [ОК__МКБ10].[ОК__МКБ10].currentmember.parent.Name,[ОК__МКБ10].[ОК__МКБ10].currentmember.Name)'

    member [parentName]
    as '
iif([isDataMember],
    [ОК__МКБ10].[ОК__МКБ10].currentmember.parent.parent.Name,[ОК__МКБ10].[ОК__МКБ10].currentmember.parent.Name)'


select
    {
    filter
    (

                [ОК__МКБ10].[ОК__МКБ10].[Все виды заболеваний].children ,
        ([DiseaseLevel]>0)and
        ([DiseaseLevel]<2)and
not IsEmpty([Death])
        )
    } on rows,
    { 

        [Death],
        [DeathComparison]


    } on columns
from [СТАТ_Население_Причины смерти]
where
    (
        [Источники данных].[Источники данных].[Источник].[СТАТ Отчетность - Статистический раздел],
        [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Основная возрастная группа].[Значение не указано],
        --[Тип местности].[Тип местности].[Все типы местности].[Значение не указано],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].datamember,--.[Сахалинская обл.],
        [Группировки__Население_Причины смерти].[Группировки__Население_Причины смерти].[Группировка].[По МКБ10]
    )
]]>
  </query>
