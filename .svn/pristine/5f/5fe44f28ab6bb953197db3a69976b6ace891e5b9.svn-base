<?xml version="1.0" encoding="Windows-1251"?>
<root>
--FNS_0004_0002_Date
<query id="FNS_0004_0002_Date">
    <![CDATA[
with
    member [Measures].[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период].[Период].Members,
                not IsEmpty([Measures].[С начала года]) and
                [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                [Период].[Период].CurrentMember.Name <> "Данные года" and
                [Период].[Период].CurrentMember.Name <> "Данные месяца"
            ),
            1
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [ФНС РФ_4НМ]
  ]]>

 </query>
--FNS_0004_0002_Grid
  <query id="FNS_0004_0002_Grid">
   <![CDATA[
With
    member [Measures].[ShortName]
    as '[Территории].[Сопоставимый].CurrentMember.Properties("Сокращенное наименование")'
    member [Measures].[ParentShortName]
    as '[Территории].[Сопоставимый].CurrentMember.Parent.Properties("Сокращенное наименование")'
    member [Measures].[ФО]
    as 'IIF
        (
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ" or [Территории].[Сопоставимый].CurrentMember.Level.Name = "Федеральный округ"
            ),
            IIF
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ",
                [Measures].[ParentShortName],
                [Measures].[ShortName]
            ),
            null
        )'
    set [Субъекты]
    as 'Filter
        (
            Generate
            (
                Descendants
                (
                    [Территории].[Сопоставимый].[Все территории].[Российская  Федерация]<%selected_fo%>,
                    [Территории].[Сопоставимый].[Федеральный округ],
                    SELF
                ),
                {
                    [Территории].[Сопоставимый].CurrentMember
                } +  Descendants
                (
                    [Территории].[Сопоставимый].CurrentMember,
                    [Территории].[Сопоставимый].[Субъект РФ],
                    SELF
                )
            ),
            not
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
            ) and not IsEmpty
            (
                 [Measures].[Задолженность по налогам] 
            )
            and ( [Measures].[Задолженность по налогам])  <> 0
        )'
    Member [Measures].[Задолженность по налогам] 
    as '
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел I. Задолженность по налогам и сборам в бюджетную систему Российской Федерации] 
        )' 
    Member [Measures].[Недоимка по налогам] 
    as '
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел I. Задолженность по налогам и сборам в бюджетную систему Российской Федерации].[ЗАДОЛЖЕННОСТЬ ПЕРЕД БЮДЖЕТОМ ПО НАЛОГАМ И СБОРАМ ВСЕГО В ТОМ ЧИСЛЕ:].[ВОЗМОЖНАЯ К ВЗЫСКАНИЮ ЗАДОЛЖЕННОСТЬ].[НЕДОИМКА] 
        )' 
    Member [Measures].[Отсроченные платежи по налогам] 
    as '
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел I. Задолженность по налогам и сборам в бюджетную систему Российской Федерации].[ЗАДОЛЖЕННОСТЬ ПЕРЕД БЮДЖЕТОМ ПО НАЛОГАМ И СБОРАМ ВСЕГО В ТОМ ЧИСЛЕ:].[ВОЗМОЖНАЯ К ВЗЫСКАНИЮ ЗАДОЛЖЕННОСТЬ].[УРЕГУЛИРОВАННАЯ ЗАДОЛЖЕННОСТЬ ВСЕГО:].[ОТСРОЧЕННЫЕ (РАССРОЧЕННЫЕ) ПЛАТЕЖИ - ВСЕГО] 
        )' 
    Member [Measures].[Задолженность по пени] 
    as '
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел II. Задолженность по уплате пеней и налоговых санкций в бюджетную систему Российской Федерации] 
        )'  
    Member [Measures].[отсроченные пени] 
    as '
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел II. Задолженность по уплате пеней и налоговых санкций в бюджетную систему Российской Федерации].[ЗАДОЛЖЕННОСТЬ ПЕРЕД БЮДЖЕТОМ ПО ПЕНЯМ И НАЛОГОВЫМ САНКЦИЯМ - ВСЕГО].[УРЕГУЛИРОВАННАЯ ЗАДОЛЖЕННОСТЬ ВСЕГО:].[ОТСРОЧЕННЫЕ (РАССРОЧЕННЫЕ) ПЕНИ И НАЛОГОВЫЕ САНКЦИИ - ВСЕГО] 
        )' 
SELECT
    {
        {
            {
                [Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы] 
            } 
            * 
            {
                [Measures].[ФО],
                [Measures].[Задолженность по налогам],
                [Measures].[Недоимка по налогам],
                [Measures].[Отсроченные платежи по налогам],
                [Measures].[Задолженность по пени],
                [Measures].[отсроченные пени] 
            } 
        } + 
        {
            {
                [Доходы].[Группы].[Все показатели].[Всего].[Региональные налоги и сборы]
            } 
            * 
            {
                [Measures].[Задолженность по налогам],
                [Measures].[Недоимка по налогам],
                [Measures].[Отсроченные платежи по налогам],
                [Measures].[Задолженность по пени],
                [Measures].[отсроченные пени]
            } 
        } + 
        {
            {
                [Доходы].[Группы].[Все показатели].[Всего].[Местные налоги и сборы]
            } 
            * 
            {
                [Measures].[Задолженность по налогам],
                [Measures].[Недоимка по налогам],
                [Measures].[Отсроченные платежи по налогам],
                [Measures].[Задолженность по пени],
                [Measures].[отсроченные пени]
            } 
        } + 
        {
            {
                [Доходы].[Группы].[Все показатели].[Всего].[Налоги со специальным налоговым режимом]
            } 
            * 
            {
                [Measures].[Задолженность по налогам],
                [Measures].[Недоимка по налогам],
                [Measures].[Отсроченные платежи по налогам],
                [Measures].[Задолженность по пени],
                [Measures].[отсроченные пени]
            } 
        } 
    } on columns,
    {
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация] 
        } + [Субъекты] 
    }   on rows 
FROM [ФНС РФ_4НМ] 
WHERE 
    (
      [Период].[Период].[Данные всех периодов].[<%period_year%>].[<%period_half_year%>].[<%period_quater%>].[<%period_month%>]
    ) 

   ]]>
 </query>
 --FNS_0004_0002_Chart
  <query id="FNS_0004_0002_Chart">
    <![CDATA[
With      
    Member [Measures].[Задолженность по налогам]     
    as '     
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел I. Задолженность по налогам и сборам в бюджетную систему Российской Федерации]     
        )'      
    Member [Measures].[Задолженность по пени]     
    as '     
        (
            [Measures].[С начала года],
            [Задолженность].[ФНС Сопоставимый].[Все показатели].[Раздел II. Задолженность по уплате пеней и налоговых санкций в бюджетную систему Российской Федерации]     
        )'   
    Member [Measures].[Итого]   
    as '  
        (
           ( [Measures].[Задолженность по налогам] + [Measures].[Задолженность по пени]   )/1000
        )'   
SELECT
    { [Measures].[Итого] } on columns ,
    {
        {
            [Доходы].[Группы].[Все показатели].[Всего].[Региональные налоги и сборы],
            [Доходы].[Группы].[Все показатели].[Всего].[Местные налоги и сборы],
            [Доходы].[Группы].[Все показатели].[Всего].[Налоги со специальным налоговым режимом],
            [Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы].[Налог на прибыль организаций],
            [Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы].[Налог на добавленную стоимость],
            [Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы].[Платежи за пользование природными ресурсами],
            [Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы].[Остальные федеральные налоги и сборы]
            //[Доходы].[Группы].[Все показатели].[Всего].[Федеральные налоги и сборы] 
        } 
    }   on rows      
FROM [ФНС РФ_4НМ]     
WHERE     
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация<%subject%>,
        [Период].[Период].[Данные всех периодов].[<%period_year%>].[<%period_half_year%>].[<%period_quater%>].[<%period_month%>]    
    )    
   ]]>
 </query>
 
 
 
