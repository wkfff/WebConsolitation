<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="STAT_0001_0010_date">
    <![CDATA[
-- STAT_0001_0010_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin({[Период].[Год Квартал Месяц].[Квартал].Members})
        )
    } on rows
from [СТАТ_Инвестиции_Нефинансовые активы]
]]>
  </query>

  <query id="STAT_0001_0010_date_money">
    <![CDATA[
-- STAT_0001_0010_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            Filter
            (
                NonEmptyCrossjoin
                (
                    {
                        [Период].[Год Квартал Месяц].[Месяц].Members
                    }
                ),
                [Период].[Год Квартал Месяц].CurrentMember.Properties("PKID") <= <%period_month%>.Properties("PKID")
            )
        )
    } on rows
from [СТАТ_Труд_Заработная плата]
]]>
  </query>
  
  <query id="STAT_0001_0010_date_ser">
    <![CDATA[
  with
  member Measures.[ДанныеНа]
  as '[Период].[Период].CurrentMember.UniqueName'
  select
  {Measures.[ДанныеНа]} on columns,
  {
  Tail
  (
  NonEmptyCrossjoin({[Период].[Период].[День].Members})
  )
  } on rows
  from [СТАТ_Мониторинг СЭР]
  ]]>
</query>
  
  <query id="STAT_0001_0010_chart1">
    <![CDATA[
-- STAT_0001_0010_chart1
SELECT
    NON EMPTY Order
    (
        Descendants
        (
            [ОК].[ОКВЭД].[Все виды экономической деятельности],
            [ОК].[ОКВЭД].[Раздел],
            SELF
        ),
        [Measures].[За период],
        BASC
    ) ON COLUMNS
FROM [СТАТ_Труд_Заработная плата]
WHERE
    (
        [Труд].[Заработная плата].[Все показатели].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        <%period_month%>,
        [Measures].[За период],
        [Группировки].[Труд_Заработная плата].[Все группировки].[По видам экономической деятельности]
    )
]]>
  </query>

  <query id="STAT_0001_0010_chart1_2">
    <![CDATA[
-- STAT_0001_0010_chart1_2
SELECT
 NON EMPTY Order
    (
        Descendants
        (
            [ОК].[ОКВЭД].[Все виды экономической деятельности],
            [ОК].[ОКВЭД].[Раздел],
            SELF
        ),
        [Measures].[За период],
        BASC
    ) ON COLUMNS
FROM [СТАТ_Труд_Трудовые ресурсы]
WHERE
  (
    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[По видам экономической деятельности]
   ,[Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%subject_FO%>
   ,[Труд].[Трудовые ресурсы].[Все показатели]
   ,[Период].[Год Квартал Месяц].[Данные всех периодов].[<%period_year%>],
   [Measures].[За период]
  )
]]>
  </query>
  <query id="STAT_0001_0010_chart1_average">
    <![CDATA[
-- STAT_0001_0010_chart1_average  
SELECT
  NON EMPTY
  {[Труд].[Заработная плата].[Все показатели].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника]} ON COLUMNS
  FROM [СТАТ_Труд_Заработная плата]
  WHERE
  (
  [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>
  ,[Группировки].[Труд_Заработная плата].[Все группировки].[Без группировки]
  , <%period_month%>
  ,[ОК].[ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
  [Measures].[За период]
  )
  ]]>
  </query>

  <query id="STAT_0001_0010_chart2">
    <![CDATA[
-- STAT_0001_0010_chart2
with
member [Труд].[Трудовые ресурсы].[Среднегодовая численность занятого населения в <%period_year%> году, человек]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Среднегодовая численность занятого населения]'
member [Труд].[Трудовые ресурсы].[заработная плата на вновь созданных рабочих местах]
    as 'LookupCube
        (
            "[СТАТ_Мониторинг СЭР]",
            "
            (
                <%period_cur_date%>,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - Субъект РФ] ,
                [Measures].[Значение],
                [Показатели].[Мониторинг СЭР].[Все показатели].[Среднемесячная заработная плата работников на вновь созданных рабочих местах (в целом и в разрезе отраслей) (по заявленным работадателями в органы занятости населения вакансиям)].[" + [ОК].[ОКВЭД].CurrentMember.Name + "],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>
            )"
        )'
    member [Труд].[Трудовые ресурсы].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника]
    as 'LookupCube
        (
            "[СТАТ_Труд_Заработная плата]",
            "
            (
                [Труд].[Заработная плата].[Все показатели].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника],
                [Measures].[За период],
                <%period_month%>,
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
                 " + MemberToStr
                (
                    [ОК].[ОКВЭД].CurrentMember
                ) + "
            )"
        )'
    member [Труд].[Трудовые ресурсы].[Среднемесячная заработная плата и выплаты социального характера в <%period_year%> году]
    as 'LookupCube
        (
            "[СТАТ_Труд_Заработная плата]",
            "
            (
                [Труд].[Заработная плата].[Все показатели].[Среднемесячная номинальная начисленная заработная плата и выплаты социального характера в расчете на одного работника],
                [Measures].[За период],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
                " + MemberToStr
                (
                    [Период].[Год Квартал Месяц].CurrentMember
                ) + ",
                 " + MemberToStr
                (
                    [ОК].[ОКВЭД].CurrentMember
                ) + "
            )"
        )'
SELECT
    NON EMPTY
    {
        {
           -- [Труд].[Трудовые ресурсы].[Среднегодовая численность занятого населения в <%period_year%> году, человек],
            [Труд].[Трудовые ресурсы].[Среднемесячная заработная плата и выплаты социального характера в <%period_year%> году],
            [Труд].[Трудовые ресурсы].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника],
            [Труд].[Трудовые ресурсы].[заработная плата на вновь созданных рабочих местах]
        }
    } ON rows ,
    NON EMPTY filter  
    (
        Descendants  
        (
            [ОК].[ОКВЭД].[Все виды экономической деятельности],
            [ОК].[ОКВЭД].[Раздел],
            SELF   
        ),
        not 
        (
            [ОК].[ОКВЭД].CurrentMember is [ОК].[ОКВЭД].[Все виды экономической деятельности].[Значение не указано]
        ) 
    )  ON columns
FROM [СТАТ_Труд_Трудовые ресурсы]
WHERE
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[По видам экономической деятельности] ,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%subject_FO%>,
        [Measures].[За период],
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2008]
    )
]]>
  </query>
  <query id="STAT_0001_0010_grid">
    <![CDATA[
-- STAT_0001_0010_chart2_grid
with
member [Труд].[Трудовые ресурсы].[Среднегодовая численность занятого населения в <%period_year%> году, человек]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Среднегодовая численность занятого населения]'
member [Труд].[Трудовые ресурсы].[заработная плата на вновь созданных рабочих местах]
    as 'LookupCube
        (
            "[СТАТ_Мониторинг СЭР]",
            "
            (
                <%period_cur_date%>,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - Субъект РФ] ,
                [Measures].[Значение],
                [Показатели].[Мониторинг СЭР].[Все показатели].[Среднемесячная заработная плата работников на вновь созданных рабочих местах (в целом и в разрезе отраслей) (по заявленным работадателями в органы занятости населения вакансиям)].[" + [ОК].[ОКВЭД].CurrentMember.Name + "],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>
            )"
        )'
    member [Труд].[Трудовые ресурсы].[Среднемесячная номинальная начисленная заработная плата в расчете на одного работника]
    as 'LookupCube
        (
            "[СТАТ_Труд_Заработная плата]",
