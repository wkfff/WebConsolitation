<?xml version="1.0" encoding="windows-1251"?>
<root>



  <query id="Years"> 
    <![CDATA[
    --STAT_0028_0017 Years
with
    
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Число родившихся]
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Число родившихся],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[Без группировки],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Число умерших]
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Число умерших],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[Без группировки],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост]
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Естественный прирост (убыль)],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[Без группировки],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
         member  [Население__Миграция].[Население__Миграция].[Все показатели].[Прирост (убыль) населения]
    as ' [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)]+
    [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост]  '
select
    non empty
    {
     
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members

    }on rows,
   -- NON EMPTY
    {
        {
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число родившихся],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число умерших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число прибывших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число выбывших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Прирост (убыль) населения]
        }
        *
        {
            [Measures].[Нарастающий итог]
        }
    } on columns
from   [СТАТ_Население_Миграция]
where
    (
       [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область],
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
           [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[Без группировки]
    )

]]>
  </query>

  <query id="Regions"> 
    <![CDATA[
    --STAT_0028_0017 Regions 
select
    non empty
    {
        [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Число родившихся]
    }on columns,
    NON EMPTY
    {
     [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область],
        filter 
        (
            [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].children,
            right
            (
                [Территории__РФ].[Территории__РФ].currentmember.uniquename,
                10
            )<>"DATAMEMBER" 
        ) 
    } on rows 
from   [СТАТ_Население_Естественное движение]
where 
    (
        [Measures].[Нарастающий итог],
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат]
      
    ) 
]]>
  </query>

  <query id = "ChartRFLine">
    <![CDATA[

SELECT
    non empty
    {
        filter
        ( 
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].members,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.parent.name="<%selectedPeriod%>" 
        )
    }on columns,
    {
    [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Население].[<%selectedPokRF%>]
      
       
    }on rows 
FROM [СТАТ_СЭП_Годовой сборник]
where
( 
[Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская  Федерация].DATAMEMBER,
[Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Без варианта],
[Measures].[За период]
)]]>
  </query>
  
    <query id="Grid">
    <![CDATA[
    --STAT_0028_0017 Grid
with 
     member [Measures].[Прирост]
    as ' iif
        (
            (
                ParallelPeriod
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ),
                [Measures].[Нарастающий итог]
            )<>0 and not isempty([Measures].[Нарастающий итог]),
            [Measures].[Нарастающий итог]-
            (
                ParallelPeriod
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ),
                [Measures].[Нарастающий итог]
            ) ,
            null
        ) '
    member [Measures].[Темп прироста]
    as ' iif
        (
            (
                ParallelPeriod
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ),
                [Measures].[Нарастающий итог]
            )<>0 and not isempty([Measures].[Нарастающий итог]),
            [Measures].[Нарастающий итог]/
            (
                ParallelPeriod
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ),
                [Measures].[Нарастающий итог]
            )  -1 ,
            null
        ) '
    
    
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Число родившихся]  
    as 'LookUpCube  
        (
            "СТАТ_Население_Естественное движение",
            " 
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Число родившихся],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"
                 
        )'   
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Число умерших]  
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Число умерших],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
    member [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост]  
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Естественный прирост (убыль)],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
    member  [Население__Миграция].[Население__Миграция].[Все показатели].[Прирост (убыль) населения] 
    as ' IIF 
        (
            not isempty
            (
                [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)]
            ) and not isempty
            (
                [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост]
            ),
            [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)]+ [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост],
            null  
        )'
select
    non empty           
    {
        filter    
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].members,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.parent.name="<%selectedPeriod%>" and right   
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.uniquename,
                10   
            )<>"datamember"    
          --  and not isempty([Measures].[Нарастающий итог])
        )    
    }on columns,
   -- NON EMPTY 
    {
        {
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число родившихся],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число умерших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Естественный прирост],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число прибывших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число выбывших],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)],
            [Население__Миграция].[Население__Миграция].[Все показатели].[Прирост (убыль) населения]     
        }         
        *      
        {
            [Measures].[Нарастающий итог],
            [Measures].[Темп прироста],
            [Measures].[Прирост]    
        }       
    } on rows             
from   [СТАТ_Население_Миграция]            
where         
    (
       <%selectedRegion%>,
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
           [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[<%groupType%>]
    )
]]>
  </query>

  
  <query id="dynamicText">
    <![CDATA[
    --STAT_0028_0017 Novosib dynamicText
with
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Средний возраст населения]
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
               [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Средний возраст населения],
                [Measures].[За период],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[Без группировки],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[<%prevPeriod%>],
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'

    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Ожидаемая продолжительность жизни при рождении]
    as 'LookUpCube
        (
            "СТАТ_Население_Естественное движение",
            "
            (
                [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Ожидаемая продолжительность жизни при рождении],
                 [Measures].[За период],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[Без группировки],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[<%prevPeriod%>],"
                 + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население детьми 0-15 лет (на 1000 трудоспособного населения) на начало]
    as ' [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население детьми 0-15 лет на начало года]'
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население лицами старше трудоспособного возраста (на 1000 трудоспособного населения) на начало]
    as ' [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население лицами пенсионного возраста на начало года]'
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Ед]
    as '
IIF (
 [Население__Численность и состав].[Население__Численность и состав].currentmember.name="Ожидаемая продолжительность жизни при рождении" or
 [Население__Численность и состав].[Население__Численность и состав].currentmember.name="Средний возраст населения",
 "лет",
    [Население__Численность и состав].[Население__Численность и состав].currentmember.properties("Единица измерения")
)
    '
select
    non empty
    {
         [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[<%selectedPeriod%>],
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Ед]
    }on columns,
    NON EMPTY
    {
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Средний возраст населения],
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Ожидаемая продолжительность жизни при рождении],
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население детьми 0-15 лет на начало года],
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Коэффициент нагрузки на трудоспособное население лицами пенсионного возраста на начало года]
    } on rows
from    [СТАТ_Население_Численность и состав]
where
    (
        [Measures].[За период],
        <%selectedRegion%>,
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
        [Группировки__Население_Численность и состав].[Группировки__Население_Численность и состав].[Все группировки].[<%groupType%>]
    )
]]>
  </query>

  <query id="Chart2">
    <![CDATA[
       --STAT_0028_0017 Chart2
with
    member [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Коэффициент миграционного прироста (убыли)]
    as 'LookUpCube
        (
            "[СТАТ_Население_Миграция]",
            "
            (
                [Население__Миграция].[Население__Миграция].[Все показатели].[Коэффициент миграционного прироста],
                [Measures].[Нарастающий итог],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[<%groupType%>],"
                + MemberToStr
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
                ) + ",
                " + MemberToStr
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember
                ) + "
            )"

        )'

select
    non empty
    {
        filter
        ( 
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].members,
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.parent.name="<%selectedPeriod%>" 
        )
    }on columns,
    --NON EMPTY
    {
        {
            <%chart2_region%>,
            <%selectedRegion%>
        }
        *
        {
            [Measures].[Нарастающий итог]
        }
    } on rows
from    [СТАТ_Население_Естественное движение]
where
    (

        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
         [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],
        <%selectedPok%>
    )
]]>
  </query>
 
  <query id="Chart1">
    <![CDATA[
       --STAT_0028_0016 Chart1
with         
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Естественный прирост (убыль)]       
    as ' 
            IIF  
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.name="Январь",
                LookUpCube  
                (
                    "СТАТ_Население_Естественное движение",
                    "  
                    (
                        [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Естественный прирост (убыль)],
                        [Measures].[Нарастающий итог],
                        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                        [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],
                        " + MemberToStr  
                        (
                            ParallelPeriod  
                            (
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц],
                                2,
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember  
                            )  
                        ) + ",
                        " + MemberToStr  
                        (
                            [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember  
                        ) + "  
                    )"  
                )  ,
                LookUpCube       
                (
                    "СТАТ_Население_Естественное движение",
                    "       
                    (
                        [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Естественный прирост (убыль)],
                        [Measures].[Нарастающий итог],
                        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                        [Группировки__Население_Естественное движение].[Группировки__Население_Естественное движение].[Все группировки].[<%groupType%>],
                        " + MemberToStr       
                        (
                            ParallelPeriod     
                            (
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц],
                                2,
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember     
                            )     
                        ) + ",
                        " + MemberToStr       
                        (
                            [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember       
                        ) + "       
                    )"        
                )  
            )  
         '       
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Миграционный прирост (убыль)]       
    as ' 
            IIF  
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.name="Январь" ,
                LookUpCube  
                (
                    "СТАТ_Население_Миграция",
                    "  
                    (
                        [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)],
                        [Measures].[Нарастающий итог],
                        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                        [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[<%groupType%>],
                        " + MemberToStr  
                        (
                            ParallelPeriod  
                            (
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц],
                                2,
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember  
                            )  
                        ) + ",
                        " + MemberToStr  
                        (
                            [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember  
                        ) + "  
                    )"  
                ) ,
                LookUpCube       
                (
                    "СТАТ_Население_Миграция",
                    "       
                    (
                         [Население__Миграция].[Население__Миграция].[Все показатели].[Миграционный прирост (убыль)],
                        [Measures].[Нарастающий итог],
                        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
                        [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[<%groupType%>],
                        " + MemberToStr       
                        (
                            ParallelPeriod       
                            (
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц],
                                2,
                                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember       
                            )       
                        ) + ",
                        " + MemberToStr       
                        (
                            [Территории__Сопоставимый].[Территории__Сопоставимый].CurrentMember       
                        ) + "       
                    )"       
                )   
            )  
        '   
        member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Прирост (убыль) населения] 
    as ' IIF 
        (
            not isempty
            (
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Миграционный прирост (убыль)]
            ) and not isempty
            (
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Естественный прирост (убыль)]
            ), [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Миграционный прирост (убыль)]  + [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Естественный прирост (убыль)],
            null  
        ) ' 
    member [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность]        
    as ' IIF 
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.name="Январь" and [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.parent.parent.parent.name="<%selectedPeriod%>",
            (
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>],
                [Measures].[За период] 
            ) ,
            IIF
            (
                not isempty
                (
                    (
                        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Прирост (убыль) населения],
                        [Measures].[Нарастающий итог] 
                    )
                ),
                (
                    [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>],
                    [Measures].[За период] 
                ) + 
                (
                    [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Прирост (убыль) населения],
                    [Measures].[Нарастающий итог] 
                ),
                null 
            ) 
        )'     
    member [Measures].[1]        
    as '0'        
    member [Measures].[2]        
    as '0'  
    member [Measures].[3]        
    as '0' 
select
    non empty        
    {
        {
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>].[Полугодие 1].[Квартал 1].[Январь],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>].[Полугодие 1].[Квартал 2].[Апрель],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>].[Полугодие 2].[Квартал 3].[Июль],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selectedPeriod%>].[Полугодие 2].[Квартал 4].[Октябрь],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%nextPeriod%>].[Полугодие 1].[Квартал 1].[Январь]  
        }        
        *        
        {
            [Measures].[Нарастающий итог]
        }        
    }on rows,
    NON EMPTY        
    {
        filter       
        (
            {
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность]         
            },
            not isempty([Measures].[Нарастающий итог]) or [Measures].[Нарастающий итог]<>0        
        )        
    } on columns        
from    [СТАТ_Население_Численность и состав]        
where        
    (
        <%selectedRegion%>,
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
        [Группировки__Население_Численность и состав].[Группировки__Население_Численность и состав].[Все группировки].[<%groupType%>]        
    )  
]]>
  </query>
</root>
