<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="table">
    <![CDATA[
with      
    member Measures.[С начала месяца]    
    as 'Measures.[По данным УФК] + Measures.[Сегодня]'    
    member Measures.[% исполнения]   
    as 'IIF 
        (
            IsEmpty(Measures.[План]),
            null,
            Measures.[С начала месяца]/Measures.[План] 
        )'  
    member Measures.[Сегодня]   
    as 'LookupCube    
        (
            "[ФО_Доходы сегодня]",
            "   
            (
                [Measures].[Сумма за период],
                " + MemberToStr
                (
                    [Доходы].[Группы КД].CurrentMember
                ) + ",
                [Период].[День].[Данные всех периодов].[2008].[Полугодие 2].[Квартал 3].[Июль].[26]   
            )"    
        )'    
    member Measures.[По данным УФК]   
    as 'LookupCube    
        (
            "[УФК_Реестр перечисленных поступлений]",
            "Sum   
            (
                {
                    [Период].[День].[Данные всех периодов].[2008].[Полугодие 2].[Квартал 3].[Июль].[1]:[Период].[День].[Данные всех периодов].[2008].[Полугодие 2].[Квартал 3].[Июль].[25]   
                },
                (
                    Measures.[За период],
                    " + MemberToStr
                    (
                        [Доходы].[Группы КД].CurrentMember
                    ) + "
                )   
            )"   
        )'    
    member Measures.[Name] as '[Доходы].[Группы КД].CurrentMember.UniqueName'
    SELECT
    {
        Measures.[План],
        Measures.[С начала месяца],
        Measures.[Сегодня],
        Measures.[% исполнения],
        Measures.[Name]
    } on columns,
    Except 
    (
        {
            [Доходы].[Группы КД].[Все группы КД].[Итого доходов].Children,
            [Доходы].[Группы КД].[Все группы КД].[Итого доходов],
            [Доходы].[Группы КД].[Все группы КД].[Безвозмездные поступления],
            [Доходы].[Группы КД].[Все группы КД].[Безвозмездные поступления].Children 
        },
        {
            [Доходы].[Группы КД].[Все группы КД].[Итого доходов].DATAMEMBER,
            [Доходы].[Группы КД].[Все группы КД].[Безвозмездные поступления].DATAMEMBER 
        } 
    ) on rows  
FROM [ФО_АС Бюджет_План доходов]       
WHERE       
    (
        [Источники данных].[Все источники данных].[ФО АС Бюджет - Минфин Новосибирской области 2008],
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2008].[Полугодие 2].[Квартал 3].[Июль]     
    )        

	]]>
</query>

<query id="chartIncoming">
	<![CDATA[
SELECT
    {
        [Measures].[С начала года]    
    } on columns,
    non empty [Доходы].[Группы КД].[Все группы КД].[Итого доходов].Children on rows
FROM [УФК_Реестр перечисленных поступлений] 
WHERE        
    (
        [Период].[День].[Данные всех периодов].[2008]
    )   

	]]>
</query>

<query id="chartDynamics">
  <![CDATA[
with 
    member [Доходы].[Группы КД].[Slicer] 
    as 'Aggregate
        (
            {
                [Доходы].[Группы КД].[Все группы КД].[Итого доходов],
                [Доходы].[Группы КД].[Все группы КД].[Безвозмездные поступления]
            }
        )' 
        member [Measures].[За период0] as 'IIF(IsEmpty([Measures].[За период]), 0, [Measures].[За период])'
SELECT
    { [Measures].[За период0] } on columns,
    [Период].[День].[Данные всех периодов].[2008].[Полугодие 2].[Квартал 3].[Июль].Children on rows 
FROM [УФК_Реестр перечисленных поступлений] 
WHERE 
    (
        <%kd_group%>
    ) 
	]]>
</query>
</root>