  <query id="IT_0002_0001_incomes">
    <![CDATA[
      -- IT_0002_0001_incomes
with
     member [Measures].[Доля в бюджете ФГУП]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [СвязьБезопасность].[Показатели].[Все].[Итого доходов],
                    [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                    [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                    [Measures].[Факт]
                )
            ) or
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов] ,
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                [Measures].[Факт]
            ) = 0 or
            (
                [СвязьБезопасность].[Филиалы].CurrentMember is [СвязьБезопасность].[Филиалы].[Все].[Консолидированный]
            ),
            null,
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов] ,
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].CurrentMember,
                [Measures].[Факт]
            )/
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов],
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                [Measures].[Факт]
            )
        )'
        member [Measures].[Доля плана в бюджете ФГУП]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [СвязьБезопасность].[Показатели].[Все].[Итого доходов],
                    [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                    [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                    [Measures].[План]
                )
            ) or
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов] ,
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                [Measures].[План]
            ) = 0 or
            (
                [СвязьБезопасность].[Филиалы].CurrentMember is [СвязьБезопасность].[Филиалы].[Все].[Консолидированный]
            ),
            null,
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов] ,
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].CurrentMember,
                [Measures].[План]
            )/
            (
                [СвязьБезопасность].[Показатели].[Все].[Итого доходов],
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
                [Measures].[План]
            )
        )'
    member [Measures].[Темп роста к прошлому году]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                    [Measures].[Факт]
                )
            ) or
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Факт]
            ) = 0 ,
            null,
            (
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Факт]
            )/
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Факт]
            )
        )'
    member [Measures].[Темп роста к прошлому году (план)]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                    [Measures].[План]
                )
            ) or
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[План]
            ) = 0 ,
            null,
            (
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[План]
            )/
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[План]
            )
        )'
    member [Measures].[Темп роста к прошлому году (план уточненный)]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                    [Measures].[Уточненный план]
                )
            ) or
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Уточненный план]
            ) = 0 ,
            null,
            (
                [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Уточненный план]
            )/
            (
                [Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь],
                [Measures].[Уточненный план]
            )
        )'
    member [Measures].[План, тыс.руб.]
    as '  ( [Measures].[План] ) / 1000'
    member [Measures].[Уточненный план, тыс.руб.]
    as '  ( [Measures].[Уточненный план] ) / 1000'
    member [Measures].[Факт, тыс.руб.]
    as '    ( [Measures].[Факт] ) / 1000'
    member [Measures].[% исполнения ]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь],
            [Measures].[% исполнения]
        )'
    member [Период].[Период].[1 п/г 2009 года]
    as '[Период].[Период].[Данные всех периодов].[2009].[Полугодие 1].[Квартал 2].[Июнь]'
    member [Период].[Период].[1 п/г 2010 года]
    as '[Период].[Период].[Данные всех периодов].[2010].[Полугодие 1].[Квартал 2].[Июнь]'
    member [Measures].[Ранг ]
    as 'IIF
        (
            IsEmpty
            (
                [Measures].[Доля в бюджете ФГУП]
            ),
            null,
            Rank
            (
                [СвязьБезопасность].[Филиалы].CurrentMember,
                Order
                (
                    Filter
                    (
                        [Филиалы] ,
                        not IsEmpty
                        (
                            [Measures].[Доля в бюджете ФГУП]
                        )
                    ),
                    [Measures].[Доля в бюджете ФГУП],
                    BDESC
                ),
                [Measures].[Доля в бюджете ФГУП]
            )
        )'
    member [Measures].[Худший ранг ]
    as 'max
        (
            [Филиалы] ,
            [Measures].[Ранг ]
        )'
    set [Филиалы]
    as '
        {
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ЦФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[СЗФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ЮФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ПФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[УФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[СФО],
            [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ДФО]
        }'
select
    {
        {
            {
                [Период].[Период].[1 п/г 2009 года]
            }
            *
            {
                [Measures].[План, тыс.руб.],
                [Measures].[Уточненный план, тыс.руб.],
                [Measures].[Факт, тыс.руб.],
                [Measures].[% исполнения]
            }
        } +
        {
            {
                [Период].[Период].[1 п/г 2010 года]
            }
            *
            {
                [Measures].[План, тыс.руб.] ,
                [Measures].[Уточненный план, тыс.руб.],
                [Measures].[Факт, тыс.руб.],
                [Measures].[% исполнения],
                [Measures].[Темп роста к прошлому году],
                [Measures].[Темп роста к прошлому году (план)],
                [Measures].[Темп роста к прошлому году (план уточненный)],
                [Measures].[Доля в бюджете ФГУП],
                [Measures].[Ранг ],
                [Measures].[Худший ранг ],
                [Measures].[Доля плана в бюджете ФГУП]
            }
        }
    } on columns,
    {
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[АУП],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ЦФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[СЗФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ЮФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ПФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[УФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[СФО],
        [СвязьБезопасность].[Филиалы].[Все].[Консолидированный].[ДФО]
    } on rows
from  [СВЯЗЬБЕЗОП_БюджетДоходовРасходов]
where
    (
        [СвязьБезопасность].[Показатели].[Все].[Итого доходов]
    ) 
          ]]>
  </query>

