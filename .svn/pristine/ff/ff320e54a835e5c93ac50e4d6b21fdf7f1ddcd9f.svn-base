<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FO_0003_0006_date">
    <![CDATA[
-- FO_0003_0006_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin
            (
                {
                    Filter
                    (
                        [Период].[Период].Members,
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года"
                    )
                }
            )
        )
    } on rows
from  [ФК_Паспорт субъекта]
  ]]>
  </query>

    <query id="FO_0003_0006_date_debts">
        <![CDATA[
-- FO_0003_0006_date_debts
with    
    member Measures.[ДанныеНа]    
    as '[Период].[Период].CurrentMember.UniqueName'    
    member [Показатели].[Паспорт субъекта].[Значение ] 
    as 'aggregate 
        (
                
            {
                [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в федеральный бюджет],
                [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в консолидированный бюджет региона],
                [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в федеральный бюджет],
                [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в консолидированный бюджет региона]
            } 
        )' 
        member Measures.[PKID]
    as '[Период].[Период].CurrentMember.Properties("PKID") > [Период].[Период].[Данные всех периодов].[<%period_year%>].Properties("PKID")'
select
    {
        Measures.[ДанныеНа]
    } 
    on columns,
    {
        Tail    
        (
            NonEmptyCrossjoin    
            (
                {
                    Filter    
                    (
                        Descendants
                        (
                            [Период].[Период].[Данные всех периодов],
                            [Период].[Период].[Месяц],
                            SELF
                        ),
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года" and not 
                        (
                            isEmpty 
                            (
                                [Показатели].[Паспорт субъекта].[Значение ]
                            )
                        ) and
                        Measures.[PKID] 
                    )    
                }    
            )    
        )    
    } on rows    
from  [ФК_Паспорт субъекта]    
where   
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )
  ]]>
</query>

<query id="FO_0003_0006_date_taxes">
    <![CDATA[
-- FO_0003_0006_date_taxes
with
    member Measures.[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
    member [Показатели].[Паспорт субъекта].[Значение ]
    as 'aggregate
        (
          {
           [Показатели].[Паспорт субъекта].[Все].[Недоимка по налогам и сборам, в т.ч.],
                [Показатели].[Паспорт субъекта].[Все].[по региональным и местным налогам]}
        )'
    member Measures.[PKID]
    as '[Период].[Период].CurrentMember.Properties("PKID") > [Период].[Период].[Данные всех периодов].[<%period_year%>].Properties("PKID")'
select
    {
        Measures.[ДанныеНа]
    }
    on columns,
    {
        Tail
        (
            NonEmptyCrossjoin
            (
                {
                    Filter
                    (
                        Descendants
                        (
                            [Период].[Период].[Данные всех периодов],
                            [Период].[Период].[Месяц],
                            SELF
                        ),
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года" and not
                        (
                            isEmpty
                            (
                                [Показатели].[Паспорт субъекта].[Значение ]
                            )
                        ) and
                        Measures.[PKID]
                    )
                }
            )
        )
    } on rows
from  [ФК_Паспорт субъекта]
where   
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )
  ]]>
    </query>

  <query id="FO_0003_0006_grid1">
    <![CDATA[
-- FO_0003_0006_grid1  
WITH      
member [Measures].[Уровень]
as '[Показатели].[Паспорт субъекта].Properties("Примечание")'
member [Measures].[Исполнение консолидированного бюджета ]
as '[Measures].[Исполнение консолидированного бюджета] / 1000'
member [Measures].[Исполнение бюджета субъекта ]
as '[Measures].[Исполнение бюджета субъекта] / 1000'
    
    SET [NonFilterRowSet]      
    AS '    
        {
            [Показатели].[Паспорт субъекта].[Все].[Просроченная кредиторская задолженность, в т.ч.],
            [Показатели].[Паспорт субъекта].[Все].[по заработной плате],
            [Показатели].[Паспорт субъекта].[Все].[по начислением на заработную плату],
            [Показатели].[Паспорт субъекта].[Все].[по предоставлению субсидий населению на оплату жилья и коммунальных услуг],
            [Показатели].[Паспорт субъекта].[Все].[по оплате коммунальных услуг],
            [Показатели].[Паспорт субъекта].[Все].[по обеспечению мер социальной поддержки отдельных категорий граждан],
            [Показатели].[Паспорт субъекта].[Все].[по расходам на обязательное медицинское страхования неработающего населения]
        }'     
SELECT      
    {
        {
            {
                [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>],
                [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
                [Период].[Период].[Данные всех периодов].[<%period_year%>] 
            }
            *      
            {
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ]
            }     
        } +
        {
        {
            {
                [Период].[Период].[Данные всех периодов].[2000]
            }
            *      
            {
                [Measures].[Уровень]
            }     
        } 
        }
    } on columns,
    { [NonFilterRowSet]} on rows    
FROM [ФК_Паспорт субъекта]      
WHERE      
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]     
    )
  ]]>
  </query>

    <query id="FO_0003_0006_grid2">
        <![CDATA[
-- FO_0003_0006_grid2 
WITH  
    member measures.[Единица измерения]  
    as '[Показатели].[Паспорт субъекта].CurrentMember.Properties("Единица измерения")'  
    member measures.[Значение ]  
    as 'IIF  
        (
            [Показатели].[Паспорт субъекта].CurrentMember.Properties("Единица измерения") = "ТЫС.РУБ.",
            measures.[Значение] / 1000,
            IIF 
            (
                [Показатели].[Паспорт субъекта].CurrentMember.Properties("Единица измерения") = "ПРОЦЕНТОВ",
                measures.[Значение],
                measures.[Значение] 
            ) 
        )'  
    member [Measures].[Уровень]  
    as '[Показатели].[Паспорт субъекта].Properties("Примечание")'  
    SET [NonFilterColumnSet]        
    AS '        
        {
            [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>],
            [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
            [Период].[Период].[Текущий период]       
        }'        
    member [Период].[Период].[Текущий период]         
    as 'IIF     
        (
            [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в федеральный бюджет] or
            [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в консолидированный бюджет региона] or
            [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в федеральный бюджет] or
            [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в консолидированный бюджет региона],
            <%period_taxes_date%>,
            IIF  
            (
                [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Недоимка по налогам и сборам, в т.ч.] or
                [Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[по региональным и местным налогам],
                <%period_debt_date%>,
                <%period_cur_date%>
            )
        )'   
    SET [NonFilterRowSet]  
    AS '  
        {
            [Показатели].[Паспорт субъекта].[Все].[Численность населения],
            [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в федеральный бюджет],
            [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в консолидированный бюджет региона],
            [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в федеральный бюджет],
            [Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в консолидированный бюджет региона],
            [Показатели].[Паспорт субъекта].[Все].[Недоимка по налогам и сборам, в т.ч.],
            [Показатели].[Паспорт субъекта].[Все].[по региональным и местным налогам],
            [Показатели].[Паспорт субъекта].[Все].[Государственный долг субъекта],
            [Показатели].[Паспорт субъекта].[Все].[Объем просроченной задолженности по долговым обязательствам субъекта Российской Федерации],
            [Показатели].[Паспорт субъекта].[Все].[Муниципальный долг субъекта],
            [Показатели].[Паспорт субъекта].[Все].[Объем просроченной задолженности по муниципальным долговым обязательствам],
            [Показатели].[Паспорт субъекта].[Все].[Объем незавершенного строительства],
            [Показатели].[Паспорт субъекта].[Все].[Численность работающих в органах государственного управления и местного самоуправления],
            [Показатели].[Паспорт субъекта].[Все].[Численность работающих в бюджетной сфере, в т.ч.],
            [Показатели].[Паспорт субъекта].[Все].[в здравоохранении],
            [Показатели].[Паспорт субъекта].[Все].[в образовании],
            [Показатели].[Паспорт субъекта].[Все].[Установленный уровень возмещения затрат за предоставленные жилищно-коммунальные услуги],
            [Показатели].[Паспорт субъекта].[Все].[Фактический уровень возмещения затрат за предоставленные жилищно-коммунальные услуги],
            [Показатели].[Паспорт субъекта].[Все].[Достижение наилучших значений показателей деятельности органов исполнительной власти субъектов Российской Федерации],
            [Показатели].[Паспорт субъекта].[Все].[Степень качества организации бюджетного процесса в соответствии с приказом Минфина РФ №552 от 3 декабря 2010г. "О порядке осуществления мониторинга и оценки качества управления региональными финансами"]  
        }'  
SELECT
    NON EMPTY  
    {
        {
            {
                [Период].[Период].[Данные всех периодов].[2000]  
            }  
            * {measures.[Единица измерения] }  
        } +  
        {
            {
                [NonFilterColumnSet]  
                *  { [Measures].[Значение ] }  
            }  
        } +  
        {
            {
                [Период].[Период].[Данные всех периодов].[2000]  
            }  
            * {[Measures].[Уровень] }  
        }  
    } on columns,
    { [NonFilterRowSet]} on rows  
FROM [ФК_Паспорт субъекта]     
WHERE         
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]        
    )         
    ]]>  
    </query>
</root>