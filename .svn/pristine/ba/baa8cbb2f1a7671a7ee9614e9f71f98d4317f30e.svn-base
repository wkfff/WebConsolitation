<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id = "WRP_0001_0001_list_of_dates">
    <![CDATA[ -- WRP_0001_0001_list_of_dates
select
    [Measures].[За период] on columns,
    filter
    (
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].Members,
        [Measures].[За период] > 0
    ) on rows
from [СТАТ_ВРП_Основные показатели]
where
    (
        [ВРП__Виды потребления].[ВРП__Виды потребления].[Все виды потребления].[Значение не указано],
        [ВРП__Источники доходов].[ВРП__Источники доходов].[Все источники доходов].[Значение не указано],
        [ВРП__Элементы использования].[ВРП__Элементы использования].[Все элементы использования].[Значение не указано],
        [Группировки__ВРП_Основные показатели].[Группировки__ВРП_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__Виды затрат].[ОК__Виды затрат].[Все виды затрат],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    )
    ]]>
  </query>

  <query id = "WRP_0001_0001_grid">
    <![CDATA[ -- WRP_0001_0001_grid
with
    member [Measures].[ПП_Значение]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Промежуточное потребление],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
        )'
    member [Measures].[ПП_Значение_Предыдущее]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Промежуточное потребление],
            Tail
            (
                filter
                (
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%start_year%>]: [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    } -
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    },
                    [Measures].[За период] > 0
                )
            ).Item(0)
        )'
    member [Measures].[ПП_ТП]
    as 'IIF
        (
            IsEmpty ( [Measures].[ПП_Значение] ) or ( [Measures].[ПП_Значение] ) = 0 or IsEmpty
            (
                [Measures].[ПП_Значение_Предыдущее]
            ) or
            (
                [Measures].[ПП_Значение_Предыдущее]
            ) = 0,
            null,
            [Measures].[ПП_Значение] / [Measures].[ПП_Значение_Предыдущее]  - 1
        )'
    member [Measures].[ПП_Сумма]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Промежуточное потребление],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>],
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности]
        )'
    member [Measures].[ПП_Доля]
    as 'IIF
        (
            IsEmpty ( [Measures].[ПП_Значение] ) or ( [Measures].[ПП_Значение] ) = 0 or IsEmpty ( [Measures].[ПП_Сумма] ) or ( [Measures].[ПП_Сумма] ) = 0,
            null,
            [Measures].[ПП_Значение] / [Measures].[ПП_Сумма]
        )'
    member [Measures].[ВРП_Значение]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Валовой региональный продукт в текущих ценах],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
        )'
    member [Measures].[ВРП_Значение_Предыдущее]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Валовой региональный продукт в текущих ценах],
            Tail
            (
                filter
                (
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%start_year%>]: [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    } -
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    },
                    [Measures].[За период] > 0
                )
            ).Item(0)
        )'
    member [Measures].[ВРП_ТП]
    as 'IIF
        (
            IsEmpty ( [Measures].[ВРП_Значение] ) or ( [Measures].[ВРП_Значение] ) = 0 or IsEmpty
            (
                [Measures].[ВРП_Значение_Предыдущее]
            ) or
            (
                [Measures].[ВРП_Значение_Предыдущее]
            ) = 0,
            null,
            [Measures].[ВРП_Значение] / [Measures].[ВРП_Значение_Предыдущее]  - 1
        )'
    member [Measures].[ВРП_Сумма]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Валовой региональный продукт в текущих ценах],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>],
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности]
        )'
    member [Measures].[ВРП_Доля]
    as 'IIF
        (
            IsEmpty ( [Measures].[ВРП_Значение] ) or ( [Measures].[ВРП_Значение] ) = 0 or IsEmpty ( [Measures].[ВРП_Сумма] ) or ( [Measures].[ВРП_Сумма] ) = 0,
            null,
            [Measures].[ВРП_Значение] / [Measures].[ВРП_Сумма]
        )'
    member [Measures].[Выпуск_Значение]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Выпуск],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
        )'
    member [Measures].[Выпуск_Значение_Предыдущее]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Выпуск],
            Tail
            (
                filter
                (
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%start_year%>]: [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    } -
                    {
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
                    },
                    [Measures].[За период] > 0
                )
            ).Item(0)
        )'
    member [Measures].[Выпуск_ТП]
    as 'IIF
        (
            IsEmpty ( [Measures].[Выпуск_Значение] ) or ( [Measures].[Выпуск_Значение] ) = 0 or IsEmpty
            (
                [Measures].[Выпуск_Значение_Предыдущее]
            ) or
            (
                [Measures].[Выпуск_Значение_Предыдущее]
            ) = 0,
            null,
            [Measures].[Выпуск_Значение] / [Measures].[Выпуск_Значение_Предыдущее]  - 1
        )'
    member [Measures].[Выпуск_Сумма]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Выпуск],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>],
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности]
        )'
    member [Measures].[Выпуск_Доля]
    as 'IIF
        (
            IsEmpty ( [Measures].[Выпуск_Значение] ) or ( [Measures].[Выпуск_Значение] ) = 0 or IsEmpty ( [Measures].[Выпуск_Сумма] ) or ( [Measures].[Выпуск_Сумма] ) = 0,
            null,
            [Measures].[Выпуск_Значение] / [Measures].[Выпуск_Сумма]
        )'
set [ОКВЭД]
as '
    filter
    (
        except
        (
            {
                [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].children
            },
            {
                [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано]
            }
        ),
        [Measures].[Выпуск_Значение] > 0
    )
'
member [ОК__ОКВЭД].[ОК__ОКВЭД].[Всего ]
as 'aggregate({[ОКВЭД]})'
select
    {
        [Measures].[Выпуск_Значение],
        [Measures].[Выпуск_Доля],
        [Measures].[Выпуск_ТП],
        [Measures].[ПП_Значение],
        [Measures].[ПП_Доля],
        [Measures].[ПП_ТП],
        [Measures].[ВРП_Значение],
        [Measures].[ВРП_Доля],
        [Measures].[ВРП_ТП]
    } on columns,
 {[ОКВЭД] + {[ОК__ОКВЭД].[ОК__ОКВЭД].[Всего ]}} on rows
from [СТАТ_ВРП_Основные показатели]
where
    (
        [ВРП__Виды потребления].[ВРП__Виды потребления].[Все виды потребления].[Значение не указано],
        [ВРП__Источники доходов].[ВРП__Источники доходов].[Все источники доходов].[Значение не указано],
        [ВРП__Элементы использования].[ВРП__Элементы использования].[Все элементы использования].[Значение не указано],
        [Группировки__ВРП_Основные показатели].[Группировки__ВРП_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__Виды затрат].[ОК__Виды затрат].[Все виды затрат],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    )
    ]]>
  </query>

  <query id = "WRP_0001_0001_chart1">
    <![CDATA[ -- WRP_0001_0001_chart1
with
    member [Measures].[Промежуточное потребление]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Промежуточное потребление]
        )'
    member [Measures].[Валовой региональный продукт в текущих ценах]
    as '
        (
            [Measures].[За период],
            [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Валовой региональный продукт в текущих ценах]
        )'
    set [Years]
    as ' (
    filter
    (
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].Members,
        [Measures].[За период] > 0
    )
    ) '
select
    {
        [Measures].[Промежуточное потребление],
        [Measures].[Валовой региональный продукт в текущих ценах]
    } on columns,
    { [Years] } on rows
from [СТАТ_ВРП_Основные показатели]
where
    (
        [ВРП__Виды потребления].[ВРП__Виды потребления].[Все виды потребления].[Значение не указано],
        [ВРП__Источники доходов].[ВРП__Источники доходов].[Все источники доходов].[Значение не указано],
        [ВРП__Элементы использования].[ВРП__Элементы использования].[Все элементы использования].[Значение не указано],
        [Группировки__ВРП_Основные показатели].[Группировки__ВРП_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__Виды затрат].[ОК__Виды затрат].[Все виды затрат],
        <%selected_action%>,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    )
  ]]>
  </query>

  <query id = "WRP_0001_0001_units">
    <![CDATA[ -- WRP_0001_0001_units
with
    member [Measures].[Единица измерения]
    as ' [ВРП__Основные показатели].[ВРП__Основные показатели].CurrentMember.Properties("Единица измерения") '
select
    {
        [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Выпуск],
        [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Промежуточное потребление],
        [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[Валовой региональный продукт в текущих ценах]
    } on columns,
    {[Measures].[Единица измерения]} on rows
from [СТАТ_ВРП_Основные показатели]
  ]]>
  </query>

  <query id = "WRP_0001_0001_charts">
    <![CDATA[ -- WRP_0001_0001_charts
select
    { [Measures].[За период] } on columns,
    {except(
            {[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].children},
            {[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано]})
    } on rows 
from [СТАТ_ВРП_Основные показатели] 
where 
    (
        [ВРП__Виды потребления].[ВРП__Виды потребления].[Все виды потребления].[Значение не указано],
        [ВРП__Источники доходов].[ВРП__Источники доходов].[Все источники доходов].[Значение не указано],
        [ВРП__Элементы использования].[ВРП__Элементы использования].[Все элементы использования].[Значение не указано],
        [Группировки__ВРП_Основные показатели].[Группировки__ВРП_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__Виды затрат].[ОК__Виды затрат].[Все виды затрат],
        [ВРП__Основные показатели].[ВРП__Основные показатели].[Все показатели].[<%selected_parameter%>],
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра] 
    )     ]]>
  </query>
  
</root>