<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="CTT">
    <![CDATA[
    WITH 
           

    
    MEMBER [Measures].[Единица измерения] 
    AS  '[Паспорт МО].[Показатели].CurrentMember.Properties("Единица измерения")'
    select
    { [Measures].[Значение],
    [Measures].[Единица измерения]} on columns,
    {
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Общий товарооборот  в действующих ценах],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Товарооборот предприятий общественного питания],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обслуживаемый контингент],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Общий товарооборот в расчете на 1 жителя],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Товарооборот предприятий общественного питания в расчете на 1 посадочное место],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Общий товарооборот в расчете на 1 м2 торговой площади]   
    } on rows  
from [МО_Паспорт МО]  
where (<%period_year_quater%>
,<%current_region%> 
)
    ]]>
  </query>

  <query id="CTB">
    <![CDATA[
    WITH 
           

 
    MEMBER [Measures].[Единица измерения]
    AS  '[Паспорт МО].[Показатели].CurrentMember.Properties("Единица измерения")'
    select
    { [Measures].[Значение],
    [Measures].[Единица измерения]} on columns,
    {
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[Магазины],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[Хранилища],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[Холодильники],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[Склады],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[Предприятия общепита]
    } on rows
from [МО_Паспорт МО]
where (<%period_year_quater%>
,<%current_region%> 
)
    ]]>
      </query>
  <query id ="CCT">
    <![CDATA[--asdkasldad
 
    select
    non empty 
    {
        [Период].[Год Квартал Месяц].[Год].Members 
    } on columns,
    {
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[<%kd_group%>]
    } on rows 
from [МО_Паспорт МО]
where (<%current_region%>) ]]>
</query>
            <query id="CCB">
              <![CDATA[

    select
    non empty  
    {
        [Период].[Год Квартал Месяц].[Год].Members  
    } on columns,
    {
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[<%kd_group%>],
        [Паспорт МО].[Показатели].[Торговля и общественное питание].[Обеспеченность на 1000 жителей торгово - складской сетью].[<%period_last_date%>] 
    } on rows  
from [МО_Паспорт МО]  
where <%current_region%> 
    ]]>
    
    
    
  </query>
  <query id ="STT">
    <![CDATA[
--STT
    select
    {
    <%marks%>
        
    } on columns ,
    non empty 
    {
    <%last_year%>:<%last_year%>.Lag(9)
    } on rows 
from [МО_Паспорт МО]  
where <%current_region%> 
]]>
</query>
            <query id ="CT">
              <![CDATA[
 
    select
    {
    
        <%marks%>.[<%kd_group%>]
    } on rows ,
    non empty 
    {
    [Период].[Год Квартал Месяц].[Год].members
        --<%last_year%>:<%last_year%>.Lag(11)
    } on columns 
from [МО_Паспорт МО]    
where <%current_region%> 
    ]]>
  </query>
  <query id ="CBL" >
    <![CDATA[ --cbl
                 select
    {filter
        (
            {
                <%marks%>
            },
            Right 
            (
                [Паспорт МО].[Показатели].Currentmember.UniqueName,
                10 
            ) <> "DATAMEMBER" 
        ) 
   }  on rows,
    non empty
    {
        <%last_year%>    }on columns
from [МО_Паспорт МО]
where <%current_region%>

]]>
  </query>

  <query id ="LD" >
    <![CDATA[

    select
    { [Measures].[Значение] } on columns,
    {
        Tail  
        (
            Filter  
            (
                [Период].[Год Квартал Месяц].[Год].members,
                [Measures].[Значение]>0 
            )  
        ).Item(0)  
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows  
from [МО_Паспорт МО] 
where
    (
        [Паспорт МО].[Показатели].[Торговля и общественное питание],
        <%current_region%>
    ) 

    ]]>
  </query>

  <query id ="LD_" >
    <![CDATA[----ld

    select
    { [Measures].[Значение] } on columns,
    {
        Tail  
        (
            Filter  
            (
                [Период].[Год Квартал Месяц].[Год].members,
                [Measures].[Значение]>0 
            )  
        ).Item(0)  
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows  
from [МО_Паспорт МО] 
where
    (
        [Паспорт МО].[Показатели].[Реализация платных услуг населению],
        <%current_region%>
    ) 

    ]]>
  </query>
  
</root>
