	INN CDATA #IMPLIED
	KPP CDATA #IMPLIED
	BIK CDATA #REQUIRED
	NUM_CONTRACT CDATA #IMPLIED
	DTSTART CDATA #IMPLIED
	DTEND CDATA #IMPLIED
>
<!ELEMENT VID_ACCOUNT EMPTY>
<!ATTLIST VID_ACCOUNT
