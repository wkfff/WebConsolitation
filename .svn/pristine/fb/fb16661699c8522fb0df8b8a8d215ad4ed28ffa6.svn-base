	DTREG CDATA #REQUIRED
>
<!ATTLIST UCHR
	CNT_RUL CDATA #REQUIRED
	CNT_IUL CDATA #REQUIRED
	CNT_UCHRFL CDATA #REQUIRED
>
<!ELEMENT UCHR (RUL*, IUL*, UCHRFL*)>
<!ELEMENT RUL (REGORG?, ADDRESS?)>
<!ATTLIST RUL
	DTSTART CDATA #REQUIRED
	NAMEP CDATA #REQUIRED
	OGRN CDATA #IMPLIED
	INN CDATA #IMPLIED
	KPP CDATA #IMPLIED
	NUMST CDATA #IMPLIED
	DTREG CDATA #IMPLIED
	SUMMA CDATA #REQUIRED
>
<!ELEMENT VIDREG EMPTY>
<!ATTLIST VIDREG
	ID CDATA #REQUIRED
	NAME CDATA #REQUIRED
>
<!ELEMENT REGORG EMPTY>
<!ATTLIST REGORG
	ID CDATA #REQUIRED
	NAME CDATA #REQUIRED
>
<!ATTLIST OKSM
	ID CDATA #REQUIRED
	KOD_OKSM CDATA #IMPLIED
	NAME CDATA #REQUIRED
>
<!ELEMENT OKSM EMPTY>
<!ELEMENT IUL (OKSM)>
<!ATTLIST IUL
	DTSTART CDATA #REQUIRED
	NAMEP CDATA #REQUIRED
	SUMMA CDATA #REQUIRED
