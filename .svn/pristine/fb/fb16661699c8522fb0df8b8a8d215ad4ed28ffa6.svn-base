<?xml version="1.0" encoding="windows-1251"?>
<!ATTLIST EGRUL_UL_DATA
	VER (1.0) #REQUIRED
>
<!ELEMENT EGRUL_UL_DATA (HEADER, UL+)>
<!ATTLIST HEADER
	IDFILE CDATA #REQUIRED
	S_ID CDATA #REQUIRED
	S_CODE (MNS) #REQUIRED
	S_NAME CDATA #REQUIRED
	TIMESTAMP CDATA #REQUIRED
	PRG_VER CDATA #IMPLIED
	CNT_DOK CDATA #REQUIRED
>
<!ELEMENT HEADER (USER)>
<!ELEMENT USER EMPTY>
<!ATTLIST USER
