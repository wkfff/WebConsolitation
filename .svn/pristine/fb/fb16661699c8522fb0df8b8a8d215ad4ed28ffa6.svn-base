	FAM_FL CDATA #REQUIRED
	NAME_FL CDATA #IMPLIED
	OTCH_FL CDATA #IMPLIED
	TEL CDATA #IMPLIED
	EMAIL CDATA #IMPLIED
>
<!ELEMENT UL (OKVED+, STATUS, REGORG, UL_NAME, UL_ADDRESS, UL_CAPITAL?, UL_START, UL_FINISH?, UCHR?, REESTR_AO?, DOLGNFL*, PREDSH*, PREEM*, LICENZ*, UL_OB*, UL_UPR?, REGEGRUL+, MNS?, PF?, FSS?, FOMS?, ACCOUNT*)>
<!ATTLIST UL
	IDUL CDATA #REQUIRED
	IDDOK CDATA #REQUIRED
	OGRN CDATA #REQUIRED
	INN CDATA #REQUIRED
	KPP CDATA #REQUIRED
>
<!ELEMENT OKVED EMPTY>
<!ATTLIST OKVED
