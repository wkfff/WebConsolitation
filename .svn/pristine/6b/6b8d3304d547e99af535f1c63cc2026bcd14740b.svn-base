using System;
using System.Runtime.Serialization;

namespace Krista.FM.Client.ViewObjects.FinSourcePlanningUI
{
    [Serializable]
    public class FinSourcePlanningException : Exception
    {
        public FinSourcePlanningException()
        {
        }

        public FinSourcePlanningException(string message)
            : base(message)
        {
        }

        protected FinSourcePlanningException(SerializationInfo info, StreamingContext context)
            : base(info, context)
        {
        }

        public FinSourcePlanningException(string message, Exception innerException)
            : base(message, innerException)
        {
        }
    }
}
