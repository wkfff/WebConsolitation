<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="STAT_0001_0012_date">
    <![CDATA[
-- STAT_0001_0012_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin({[Период].[Год Квартал Месяц].[Квартал].Members})
        )
    } on rows
from [СТАТ_Инвестиции_Нефинансовые активы]
]]>
  </query>
  <query id="STAT_0001_0012_chart1">

    <![CDATA[
-- STAT_0001_0012_chart1
with  
    member [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы УрФО ]  
    as ' IIF
        (
            not
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER  
            ),
            null
        )' 
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ]  
    as ' IIF
        (
            not
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER
            ),
            null
        )'  
    member [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы ]  
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)]'  
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ ]  
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)]'  
    member [Труд].[Трудовые ресурсы].[Период ]  
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'  
    member [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы РФ ]  
    as '  
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DataMember  
        )'  
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ]  
    as '  
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DataMember  
        )'  
    member [Measures].[Нарастающий итог ]  
    as 'IIF  
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Территории].[РФ].CurrentMember,
                    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]  
                )  
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]  
            )  
        )'  
SELECT
    NON EMPTY  
    {
        [Труд].[Трудовые ресурсы].[Период ],        
        [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы РФ ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ],
        [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ ]
       
    } ON columns ,
    filter  
    (
        Descendants  
        (
            [Период].[Год Квартал Месяц].[Данные всех периодов],
            [Период].[Год Квартал Месяц].[Месяц],
            SELF  
        ),
        not  
        (
            IsEmpty  
            (
                [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы ]  
            ) and IsEmpty  
            (
                [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ ]  
            ) or [Период].[Год Квартал Месяц].CurrentMember is [Период].[Год Квартал Месяц].CurrentMember.Parent.DataMember  
        )  
    ) ON rows  
FROM [СТАТ_Труд_Трудовые ресурсы]  
WHERE        
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        [Measures].[Нарастающий итог ]
    ) 
]]>
  </query>

  <query id="STAT_0001_0012_chart2">
    <![CDATA[
-- STAT_0001_0012_chart2
with
    member [Труд].[Трудовые ресурсы].[Число безработных, зарегистрированных в службе занятости ]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Число безработных, зарегистрированных в службе занятости (на конец месяца)]'
    member [Труд].[Трудовые ресурсы].[Общая численность безработных по методологии МОТ ]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]'    
   
    member [Труд].[Трудовые ресурсы].[Период ]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
    
    member [Measures].[Нарастающий итог ]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Measures].[Нарастающий итог],
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Территории].[РФ].CurrentMember,
                    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
            )
        )'
SELECT
    NON EMPTY
    {
        [Труд].[Трудовые ресурсы].[Период ],
        [Труд].[Трудовые ресурсы].[Число безработных, зарегистрированных в службе занятости ],
        [Труд].[Трудовые ресурсы].[Общая численность безработных по методологии МОТ ]
    } ON columns ,
    filter
    (
        Descendants
        (
            [Период].[Год Квартал Месяц].[Данные всех периодов],
            [Период].[Год Квартал Месяц].[Месяц],
            SELF
        ),
        not
        (
            IsEmpty
            (
                [Труд].[Трудовые ресурсы].[Число безработных, зарегистрированных в службе занятости ]
            ) and IsEmpty
            (
                [Труд].[Трудовые ресурсы].[Общая численность безработных по методологии МОТ ]
            ) or [Период].[Год Квартал Месяц].CurrentMember is [Период].[Год Квартал Месяц].CurrentMember.Parent.DataMember
        )
    ) ON rows
FROM [СТАТ_Труд_Трудовые ресурсы]
WHERE 
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],        
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        [Measures].[Нарастающий итог ] 
    ) 
]]>
  </query>

  <query id="STAT_0001_0012_chart3">
    <![CDATA[
-- STAT_0001_0012_chart3
with member [Труд].[Трудовые ресурсы].[Численность занятого населения ]
    as '
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения]
        ) - 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]
        )' 
    member [Труд].[Трудовые ресурсы].[Численность безработных по методологии МОТ ]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]' 
SELECT    
       { <%period_year%> }
     ON COLUMNS ,
    NON EMPTY 
    {
        [Труд].[Трудовые ресурсы].[Численность безработных по методологии МОТ ],
        [Труд].[Трудовые ресурсы].[Численность занятого населения ] 
    } ON ROWS 
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE 
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        <%data_source%>,
        [Measures].[Нарастающий итог] 
    )
  ]]>
  </query>

  <query id="STAT_0001_0012_grid">
    <![CDATA[
-- STAT_0001_0012_grid
with 
    member [Труд].[Трудовые ресурсы].[Период ]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
    member [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы УрФО ] 
    as ' IIF 
        (
            not 
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
            ),
            null 
        )' 
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ] 
    as ' IIF 
        (
            not 
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
                [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
            ),
            null 
        )' 
    member [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы РФ ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DataMember 
        )' 
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DataMember 
        )' 
    member [Measures].[Нарастающий итог ] 
    as 'IIF 
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Территории].[РФ].CurrentMember,
                    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
                ) 
            ),
            (
                IIF 
                (
                    [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DataMember,
                    (
                        [Measures].[Нарастающий итог],
                        [Труд].[Трудовые ресурсы].CurrentMember,
                        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DataMember
                    ),
                    (
                        [Measures].[Нарастающий итог],
                        [Труд].[Трудовые ресурсы].CurrentMember,
                        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                        [Территории].[РФ].CurrentMember
                    )
                )  
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
            ) 
        )' 
SELECT
    NON EMPTY 
    {
        [Труд].[Трудовые ресурсы].[Все показатели].[Число безработных, зарегистрированных в службе занятости (на конец месяца)],
        [Труд].[Трудовые ресурсы].[Все показатели].[Уровень зарегистрированной безработицы (от экономически активного населения)],
        [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы РФ ],
        [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)],
        [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ],
        [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
        [Труд].[Трудовые ресурсы].[Период ]
    }ON COLUMNS,
    NON EMPTY filter
    (
        Descendants
        (
            [Период].[Год Квартал Месяц].[Данные всех периодов],
            [Период].[Год Квартал Месяц].[Данные всех периодов].Level ,
            AFTER
        ),
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 1" and
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 2" and
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 1" and
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 2" and
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 3" and
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 4"  and
        not ([Период].[Год Квартал Месяц].CurrentMember is [Период].[Год Квартал Месяц].CurrentMember.Parent.DataMember)
         and not IsEmpty ([Труд].[Трудовые ресурсы].[Уровень зарегистрированной безработицы РФ ])
    ) ON ROWS
FROM [СТАТ_Труд_Трудовые ресурсы]   
WHERE     
    (       
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        [Measures].[Нарастающий итог ] 
    ) 
]]>
  </query>

  <query id="STAT_0001_0012_regions">
    <![CDATA[
-- STAT_0001_0012_regions
With
    member [ОК].[ОКВЭД].[FK]
    as '[Территории].[РФ].CurrentMember.Parent.Name'
SELECT
    { [ОК].[ОКВЭД].[FK] }
     on columns,
    Filter
    (
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].Children,
        (
            not
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].CurrentMember.Parent.DataMember
            )
        )
    )  on rows
FROM  [СТАТ_Труд_Заработная плата]  
  ]]>
  </query>

  <query id="STAT_0001_0012_period">
    <![CDATA[ 
    -- STAT_0001_0012_period
with   
    member [Труд].[Трудовые ресурсы].[Период]   
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'  
    member [Источники данных].[Все источники данных].[Фильтр]  
    as 'Aggregate   
        (
            {
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            } 
        )'   
SELECT
    NON EMPTY Filter   
    (
        Descendants   
        (
            [Период].[Год Квартал Месяц].[Данные всех периодов],
            [Период].[Год Квартал Месяц].[Данные всех периодов].Level ,
            AFTER   
        ),
        [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 1" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 2" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 1" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 2" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 3" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 4" and not IsEmpty   
        (
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[Фильтр]
            )   
        )   
    ) ON rows ,
    NON EMPTY   
    {
        [Труд].[Трудовые ресурсы].[Период]   
    } ON COLUMNS   
FROM [СТАТ_Труд_Трудовые ресурсы]   
WHERE   
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DataMember,
        [Measures].[Нарастающий итог]
    )   
     ]]>
  </query>

  <query id="STAT_0001_0012_text">
    <![CDATA[ 
    -- STAT_0001_0012_text    
    with 
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
        ) 
    ' 
    member    [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DATAMEMBER
            ,
            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]
        ) 
    ' 
    member [Период].[Год Квартал Месяц].[Предыдущий месяц] 
    as 'IIF 
        (
            <%period_year%>.PrevMember is 
            <%period_year%>.PrevMember.Parent.DataMember,
            <%period_year%>.PrevMember.PrevMember,
            <%period_year%>.PrevMember 
        )' 
    member [Труд].[Трудовые ресурсы].[Численность экономически активного населения ] 
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения] / 1000' 
    member [Труд].[Трудовые ресурсы].[Общая численность безработных (на конец месяца) ] 
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)] / 1000' 
        member [Measures].[Нарастающий итог ]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Measures].[Нарастающий итог],
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Территории].[РФ].CurrentMember,
                    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
            )
        )'
SELECT
    {
        <%period_year%>,
        [Период].[Год Квартал Месяц].[Предыдущий месяц]
        
    } ON COLUMNS ,
    
      {  [Труд].[Трудовые ресурсы].[Численность экономически активного населения ],
        [Труд].[Трудовые ресурсы].[Общая численность безработных (на конец месяца) ],
        [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ]   
    } ON ROWS 
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE  
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        [Measures].[Нарастающий итог ] 
    )
     ]]>
  </query>

  <query id="STAT_0001_0012_source">
    <![CDATA[ 
    -- STAT_0001_0012_source
   with
    member [Measures].[Источник]
    as 'IIF
        (
            IsEmpty (([Measures].[Нарастающий итог], [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)])),
            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС].UniqueName,
            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области].UniqueName
        )'
    member [Measures].[Имя источника]
    as 'IIF
        (
            IsEmpty (([Measures].[Нарастающий итог], [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)])),
            "ФСГС",
            "ФСГС по Свердловской области"
        )'
SELECT
    {
        [Measures].[Источник],
        [Measures].[Имя источника]
    } ON COLUMNS,
    {
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
    } ON ROWS
FROM [СТАТ_Труд_Трудовые ресурсы]
WHERE
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%filter%>,
        <%period_year%> 
    )    
     ]]>
  </query>

  <query id="STAT_0001_0012_unjobedOperative">
    <![CDATA[ 
--STAT_0001_0012_unjobedOperative
with

    member [Показатели].[Рынок труда].[Период ]
    as '[Период].[Период].CurrentMember.Parent.UniqueName'
    member [Показатели].[Рынок труда].[Уровень регистрируемой безработицы УрФО ]
    as '
    IIF
        (
            not
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]
            ),
            (
                [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы],
            [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ],
            [Measures].[Значение ]
            ),
            null
        )'
    member [Показатели].[Рынок труда].[Уровень регистрируемой безработицы ]
    as '[Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы] / 100'
    member [Показатели].[Рынок труда].[Уровень регистрируемой безработицы РФ ]
    as '
        (
            [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы],
            [Территории].[РФ].[Все территории].[Российская Федерация].DataMember,
            [Measures].[Значение ]
        )'
    member [Measures].[Значение ]
    as '[Measures].[Значение] / 100'
SELECT
    non Empty{
        [Показатели].[Рынок труда].[Период ],
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан],
        [Показатели].[Рынок труда].[Уровень регистрируемой безработицы ],
        [Показатели].[Рынок труда].[Уровень регистрируемой безработицы РФ ],
        [Показатели].[Рынок труда].[Уровень регистрируемой безработицы УрФО ]        
    } ON columns,
    filter
    (
        Descendants
        (
            [Период].[Период].[Данные всех периодов],
             [Период].[Период].[День],
            SELF
        ),
        not isEmpty ([Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные месяца"
        ) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные года"
        )
    ) ON rows
FROM  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE 
    (
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ]<%subject_FO%>,
        [Measures].[Значение] 
    )
     ]]>
  </query>

  <query id="STAT_0001_0012_compare">
    <![CDATA[ 
    -- STAT_0001_0012_compare
   with
    member [Труд].[Трудовые ресурсы].[Численность занятого населения ]   
    as '   
        (
            [Труд].[Трудовые ресурсы].[Численность экономически активного населения ]
        ) -   
        (
            [Труд].[Трудовые ресурсы].[Численность безработных по методологии МОТ ]
        )'   
    member [Труд].[Трудовые ресурсы].[Численность безработных по методологии МОТ ]   
    as ' IIF  
        (
            
            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация] 
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)],
                [Территории].[РФ].[Все территории].[Российская Федерация].DataMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]   
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)],
                [Территории].[РФ].CurrentMember   
            )  
        )'   
    member [Труд].[Трудовые ресурсы].[Численность экономически активного населения ]
    as ' IIF
        (

            (
                [Территории].[РФ].CurrentMember is [Территории].[РФ].[Все территории].[Российская Федерация]
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                [Территории].[РФ].[Все территории].[Российская Федерация],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС],
                [Период].[Год Квартал Месяц].CurrentMember.Parent
            ),
            (
                [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                [Территории].[РФ].CurrentMember
            )
        )'
        member [Measures].[Нарастающий итог ]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Measures].[Нарастающий итог],
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Территории].[РФ].CurrentMember,
                    [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]
            ),
            (
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].CurrentMember,
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
            )
        )'
    member [Территории].[РФ].[Уральский федеральный округ ]   
    as ' [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DataMember'   
SELECT
    {
        [Территории].[РФ].[Все территории].[Российская Федерация],
        [Территории].[РФ].[Уральский федеральный округ ],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Курганская область],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Свердловская область],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ямало-Ненецкий автономный округ],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Челябинская область]   
    } ON columns ,
    NON EMPTY   
    {
        [Труд].[Трудовые ресурсы].[Численность безработных по методологии МОТ ],
        [Труд].[Трудовые ресурсы].[Численность занятого населения ]   
    } ON rows   
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,       
        [Measures].[Нарастающий итог ],
        <%period_year%> 
    )    
     ]]>
  </query>

</root>

