<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FO_0016_0001_date">
    <![CDATA[
-- FO_0016_0001_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin({[Период].[Год Квартал Месяц].Members})
        )
    } on rows 
from [ФО_БККУ_Показатели]
  ]]>
</query>

<query id="FO_0016_0001_compare_grid">
<![CDATA[
-- FO_0016_0001_compare_grid
-- <#[ФО_БККУ_Показатели]#>
SELECT
    {{
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[Группа МО],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[ОКН],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 1],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 2],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 3_обл],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 3_мест],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 4],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 5],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 6_а],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 6_б],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 6_в],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 7],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 8],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 9],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 10],
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[БК 11]
    } * {[Measures].[Значение], [Measures].[Количество нарушений]}} on columns,
    NON EMPTY Except
    (
        [Районы].[Сопоставимый].[Районы].AllMembers,
        {
            [Районы].[Сопоставимый].[Все районы].[Несопоставленные данные],
            [Районы].[Сопоставимый].[Все районы].[Консолидированный бюджет - Новосибирская область],
            [Районы].[Сопоставимый].[Все районы].[Новосибирская Область (Бюджет Субъекта)]
        }
    )  on rows
FROM [ФО_БККУ_Показатели]
WHERE
    (
        [Период].[Год Квартал Месяц].[Данные всех периодов].<%period_day_fo%>
    )
	
  ]]>
  </query>

  <query id="FO_0016_0001_compare_chart">
    <![CDATA[
-- FO_0016_0001_compare_chart
SELECT
    {
        [Measures].[Значение],
        [Measures].[Количество нарушений]
    }  on columns,
    NON EMPTY 
    {
        Except 
        (
            [Районы].[Сопоставимый].[Районы].AllMembers,
            {
                [Районы].[Сопоставимый].[Все районы].[Несопоставленные данные] 
            } 
        ) 
    }  on rows 
FROM [ФО_БККУ_Показатели] 
WHERE 
    (
        [Период].[Год Квартал Месяц].[Данные всех периодов].<%period_day_fo%>,
        [Показатели].[БККУ_Сопоставимый].[Данные всех источников].[<%selectedItem%>]
    )     
  ]]>
</query>

<query id="FO_0016_0001_compare_indicators">
<![CDATA[
-- FO_0016_0001_compare_indicators
WITH  
    Member [Районы].[Сопоставимый].[Наименование]  
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Наименование")'  
    Member [Районы].[Сопоставимый].[Содержание]  
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Содержание")'  
    Member [Районы].[Сопоставимый].[Формула]  
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Формула")'  
    Member [Районы].[Сопоставимый].[Условия проверки]  
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Условия проверки")'  
    Member [Районы].[Сопоставимый].[Пороговое значение]  
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Пороговое значение")' 
    Member [Районы].[Сопоставимый].[Минимальное значение]
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Минимальное значение")'
    Member [Районы].[Сопоставимый].[Максимальное значение]
    as '[Показатели].[БККУ_Сопоставимый].CurrentMember.Properties("Максимальное значение")'    
SELECT
    {[Показатели].[БККУ_Сопоставимый].[Данные всех источников].[<%selectedItem%>]}  on columns,
    NON EMPTY  
    {
        {
            [Районы].[Сопоставимый].[Наименование],
            [Районы].[Сопоставимый].[Содержание],
            [Районы].[Сопоставимый].[Формула],
            [Районы].[Сопоставимый].[Условия проверки],
            [Районы].[Сопоставимый].[Пороговое значение],
            [Районы].[Сопоставимый].[Минимальное значение],
            [Районы].[Сопоставимый].[Максимальное значение]            
        } 
    }  on rows  
FROM [ФО_БККУ_Показатели]  
  ]]>
</query>  

</root>