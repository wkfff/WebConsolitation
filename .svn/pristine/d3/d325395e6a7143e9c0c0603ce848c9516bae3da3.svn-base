<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="ComboDates">
    <![CDATA[with
    member [Население__Естественное движение].[Население__Естественное движение].[Численность занятых индивидуальным трудом и по найму у отдельных граждан, в % к численности трудовых ресурсов]
    as '[Население__Естественное движение].[Население__Естественное движение].[Все].[Численность занятых индивидуальным трудом и по найму у отдельных граждан] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)] * 100'
    member [Население__Естественное движение].[Население__Естественное движение].[Численность граждан, занятых в домашнем хозяйстве производством товаров и услуг для реализации, в % к численности трудовых ресурсов]
    as '[Население__Естественное движение].[Население__Естественное движение].[Все].[Численность граждан, занятых в домашнем хозяйстве производством товаров и услуг для реализации] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)] * 100'
    member [Население__Естественное движение].[Население__Естественное движение].[Трудоспособное население в трудоспособном возрасте, не занятое в экономике и обучением, в % к численности трудовых ресурсов]
    as '[Население__Естественное движение].[Население__Естественное движение].[Все].[Трудоспособное население в трудоспособном возрасте, не занятое в экономике и обучением] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)] * 100'
    member [Население__Естественное движение].[Население__Естественное движение].[Среднесписочная численность работников организаций и предприятий, в % к численности трудовых ресурсов]
    as '[Население__Естественное движение].[Население__Естественное движение].[Все].[Среднесписочная численность работников организаций и предприятий] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)] * 100'
    member [Население__Естественное движение].[Население__Естественное движение].[sum-report-field]   
    as 'sum    
        (
            Crossjoin  
            (
                {
                    Except  
                    (
                        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].children,
                        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].datamember  
                    )  
                },
                {
                    [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Коэффициент рождаемости],
                    [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Коэффициент смертности],
                    [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Коэффициент естественного прироста (убыли)]   
                }   
            ),
            [Measures].[Нарастающий итог]   
        )'  
    member [UniqueName]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.uniquename'
        member [PUniqueName]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.uniquename'
    member [Mounth]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.name'
    member [Year]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.parent.name'
select
    non empty
    { 
[Mounth] ,
        [Year],
        [UniqueName],
        [PUniqueName]
    } on columns,
    non empty
    {
        Filter
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц].members,
            [Население__Естественное движение].[Население__Естественное движение].[sum-report-field]>0
        )
    }on rows
from [СТАТ_Население_Естественное движение]
    ]]>
  </query>
  <query id = "GridFO">
    <![CDATA[with 
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurPeriod] 
    as '<%ChosenCurPeriod%>' 
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[ComparePeriod] 
    as '<%ChosenPrevPeriod%>'  
    
SELECT
    -- non empty 
    {
        Crossjoin 
        (
            {
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность экономически активного населения ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность официально зарегистрированных безработных ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень регистрируемой безработицы, % от численности экономически активного населения ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность незанятых граждан, состоящих на учете ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Число заявленных вакансий ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень напряженности на рынке труда ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень трудоустройства ищущих работу граждан ]
            },
            {
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurPeriod],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[ComparePeriod] 
            } 
        ) 
    } on columns ,
    non empty
    {
--       [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].datamember
    [Measures].[Значение]
    }on rows 
FROM [СТАТ_Население_Естественное движение] 
    ]]>
  </query>

  <query id = "Grid">
    <![CDATA[with 
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurPeriod] 
    as '<%ChosenCurPeriod%>' 
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[ComparePeriod] 
    as '<%ChosenPrevPeriod%>'  
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень регистрируемой безработицы, % от численности экономически активного населения ]
    as '([Население__Естественное движение].[Население__Естественное движение].[Уровень регистрируемой безработицы, % от численности экономически активного населения],
    [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember)'
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень напряженности на рынке труда ]
    as '([Население__Естественное движение].[Население__Естественное движение].[Уровень напряженности на рынке труда],
    [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember)'
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень трудоустройства ищущих работу граждан ]
    as '([Население__Естественное движение].[Население__Естественное движение].[Уровень трудоустройства ищущих работу граждан],
    [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember)'
SELECT
   -- non empty  
    {
        Crossjoin
        (
            {
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность экономически активного населения],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность официально зарегистрированных безработных],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень регистрируемой безработицы, % от численности экономически активного населения ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность незанятых граждан, состоящих на учете],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Число заявленных вакансий],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень напряженности на рынке труда ],
                [Население__Естественное движение].[Население__Естественное движение].[Все].[Уровень трудоустройства ищущих работу граждан ]
            },
            {
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurPeriod],[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[ComparePeriod]
            }
        )  
    } on columns ,
    non empty{
        --[Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].children  
        Filter(
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].children,
        not [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.is_datamember)
    }on rows 
FROM [СТАТ_Население_Естественное движение]
    ]]>
  </query>
  <query id = "Map">
    <![CDATA[
    
    with  
    member [Население__Естественное движение].[Население__Естественное движение].[ShortName]  
    as ' [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.properties("Краткое наименование")'  
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень регистрируемой безработицы, % от численности экономически активного населения ]  
    as ' 
        (
            [Население__Естественное движение].[Население__Естественное движение].[Уровень регистрируемой безработицы, % от численности экономически активного населения],
            [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember 
        )'  
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень напряженности на рынке труда ]  
    as ' 
        (
            [Население__Естественное движение].[Население__Естественное движение].[Уровень напряженности на рынке труда],
            [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember 
        )'  
    member [Население__Естественное движение].[Население__Естественное движение].[Уровень трудоустройства ищущих работу граждан ]  
    as ' iif((
            [Население__Естественное движение].[Население__Естественное движение].[Уровень трудоустройства ищущих работу граждан],
            [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember
        )<>0,
        ( 
            [Население__Естественное движение].[Население__Естественное движение].[Уровень трудоустройства ищущих работу граждан],
            [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.datamember 
        ),null)'
    member [Население__Естественное движение].[Население__Естественное движение].[Численность занятых индивидуальным трудом и по найму у отдельных граждан, в % к численности трудовых ресурсов]  
    as ' iif
        (
            [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]<>0,
            [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность занятых индивидуальным трудом и по найму у отдельных граждан] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]  
            * 100,
            null
        )' 
    member [Население__Естественное движение].[Население__Естественное движение].[Численность граждан, занятых в домашнем хозяйстве производством товаров и услуг для реализации, в % к численности трудовых ресурсов]  
    as '  iif 
        (
             [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]<>0,
            [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность граждан, занятых в домашнем хозяйстве производством товаров и услуг для реализации] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]  
            * 100,
            null 
        )'  
    member [Население__Естественное движение].[Население__Естественное движение].[Трудоспособное население в трудоспособном возрасте, не занятое в экономике и обучением, в % к численности трудовых ресурсов]
    as 'iif([Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]<>0,
    [Население__Естественное движение].[Население__Естественное движение].[Все].[Трудоспособное население в трудоспособном возрасте, не занятое в экономике и обучением]
     / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]  
        * 100,null)'
    member [Население__Естественное движение].[Население__Естественное движение].[Среднесписочная численность работников организаций и предприятий, в % к численности трудовых ресурсов]  
    as 'iif([Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]<>0,
    [Население__Естественное движение].[Население__Естественное движение].[Все].[Среднесписочная численность работников организаций и предприятий] / [Население__Естественное движение].[Население__Естественное движение].[Все].[Численность трудовых ресурсов (без учёта маятниковой миграции)]* 100,null)'
    
        member [Население__Естественное движение].[Население__Естественное движение].[Все показатели].[Коэффициент миграционного прироста (убыли)] 
    as 'LookupCube
        (
            "[СТАТ_Население_Миграция]" ,
            "( [Население__Миграция].[Население__Миграция].[Все показатели].[Коэффициент миграционного прироста],"
            +[Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.unique_name+
            ",<%ChosenCurPeriod%>)"
        ) '  
    
    member [Население__Естественное движение].[Население__Естественное движение].[Value]
    as '<%SelectMapAndChartParam%>'
SELECT
    {
    
[Население__Естественное движение].[Население__Естественное движение].[Value],
[Население__Естественное движение].[Население__Естественное движение].[ShortName]

    } on columns ,
    {
        Filter(
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].children,
        not [Территории__Сопоставимый].[Территории__Сопоставимый].currentmember.is_datamember
        and
        not isempty([Население__Естественное движение].[Население__Естественное движение].[Value]))
    }on rows
FROM [СТАТ_Население_Естественное движение]
where
(<%ChosenCurPeriod%>)]]>
  </query>
