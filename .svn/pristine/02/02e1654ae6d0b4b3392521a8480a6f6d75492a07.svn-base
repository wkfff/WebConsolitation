                <BrushElements>
                  <igGaugeProp:SolidFillBrushElement Color="20, 0, 0, 0" />
                </BrushElements>
              </Shadow>
              <Response />
              <StrokeElement Color="106, 154, 172" />
              <BrushElements>
                <igGaugeProp:SimpleGradientBrushElement StartColor="185, 217, 231" EndColor="135, 189, 214" />
              </BrushElements>
            </igGaugeProp:LinearGaugeNeedle>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="100" TickmarkInterval="20" />
          </Axes>
        </igGaugeProp:LinearGaugeScale>
      </Scales>
      <BrushElements>
        <igGaugeProp:SimpleGradientBrushElement StartColor="239, 243, 246" EndColor="182, 215, 230" />
      </BrushElements>
