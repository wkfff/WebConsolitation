<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="years">
    <![CDATA[
--STAT_0003_0007_Novosib years
with
member [Measures].[Предок]
as '[Период__Период].[Период__Период].currentmember.parent.parent.parent.name'
SELECT
    NON EMPTY { [Measures].[Значение],
    [Measures].[Предок] } ON COLUMNS ,
    non empty 
    {
    filter
    (
        [Период__Период].[Период__Период].[Месяц].members,
        not isempty([Measures].[Значение])
        )
    } ON ROWS 
FROM  [СТАТ_Мониторинг ситуации на рынке труда] 
WHERE 
    (
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат] ,
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].datamember 
    ) 
 ]]>
</query>

  <query id="compare_years">
    <![CDATA[
--STAT_0003_0007_Novosib compare_years

SELECT
    NON EMPTY
    {
       [Measures].[Значение]
    } ON COLUMNS ,
   non empty {
       
       [Период__Период].[Период__Период].[Год].members
    } ON ROWS
FROM  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE
    (
    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
    [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат] ,
     [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].datamember
    )
 ]]>
  </query>
  
<query id="grid">
    <![CDATA[
--STAT_0003_0007_Novosib Grid
with     
    member [Measures].[Сравнение]     
    as ' IIF     
        (
            (
               <%comparePeriod%> ,
                [Measures].[Значение]     
            )<>0,
            (
                [Measures].[Значение]/     
                (
                   <%comparePeriod%>,
                    [Measures].[Значение]     
                )    
            )    
            *100,
            null     
        ) '     
    member [Measures].[Рассчетное значение]     
    as ' IIF     
        (
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
                [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
                [Measures].[Значение]     
            )<>0,
            (
                [Measures].[Значение] /     
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
                    [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
                    [Measures].[Значение]     
                )    
            )    
            *100,
            null     
        ) '     
    member [ОК__ОКВЭД].[ОК__ОКВЭД].[В том числе по видам деятельности:]    
    as '1'     
    set  [Подчиненные]     
    as ' [ОК__ОКВЭД].[ОК__ОКВЭД].[В том числе по видам деятельности:]+ Descendants     
        (
            [ОК__ОКВЭД].[ОК__ОКВЭД],
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Раздел],
            SELF    
        ) '   
SELECT
    NON EMPTY     
    {
        [Measures].[Значение],
        [Measures].[Сравнение],
        [Measures].[Рассчетное значение] 
    } ON COLUMNS ,
    non empty    
    {
        {
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)] 
        }  
        * { [Подчиненные]}     
    } ON ROWS     
FROM  [СТАТ_Мониторинг ситуации на рынке труда]     
WHERE     
    (
        <%SelectedPeriod%>,
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат] ,
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].datamember   
    )  
   ]]>
  </query>
 

  <query id="Chart">
    <![CDATA[
    --STAT_0003_0007_Novosib map
with 
         
           
    member [ОК__ОКВЭД].[ОК__ОКВЭД].[В том числе по видам деятельности:]       
    as '1'   
    set  [Подчиненные]        
    as 'Descendants
        (
            [ОК__ОКВЭД].[ОК__ОКВЭД],
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Раздел],
            SELF       
        ) '      
      
    member [Measures].[Численность работников]  
    as '(
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Среднесписочная численность работников организаций и предприятий (по видам деятельности)],
                    [ОК__ОКВЭД].[ОК__ОКВЭД].currentmember,
                    [Measures].[Значение]
                )
         '
         member [Measures].[Значение1]
as '
(
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
                    [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
                    [Measures].[Значение]
                )'
SELECT
    NON EMPTY       { [Measures].[Значение],[Measures].[Численность работников],[Measures].[Значение1] } ON rows,
    non empty 
    {
        filter 
        (
            order 
            (
                { [Подчиненные] },
                [Measures].[Значение],
                asc 
            ),
            not isempty([Measures].[Значение]) 
        ) 
    } ON columns      
FROM  [СТАТ_Мониторинг ситуации на рынке труда]        
WHERE        
    (
       <%SelectedPeriod%>,
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат] ,
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].datamember      ,
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)]
        
    )  
 ]]>
  </query>

  
</root>