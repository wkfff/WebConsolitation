  <query id="Oil_0002_0001_chart">
    <![CDATA[
    --Oil_0002_0001_chart
WITH             
    member measures.[Текущая цена]                      
    as '                     
        (
            <%period_cur_date%>,
            [Measures].[Средняя цена]                     
        )'
SELECT
    {
        {
            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80],
            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-92],
            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-95 (98)],
            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Дизельное топливо]     
        }
       
