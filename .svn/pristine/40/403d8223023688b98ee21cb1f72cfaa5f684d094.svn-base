<root>
  <query id="Oil_0002_0001_incomes_date">
    <![CDATA[
    --Oil_0002_0001_incomes_date
with      
    member [Measures].[ДанныеНа]      
    as '[Период].[День].CurrentMember.UniqueName' 
    member [Measures].[Год ] 
    as 'Tail 
        (
            Filter 
            (
                [Период].[День].[День].Members,
                not IsEmpty 
                (
                    (
                        [Measures].[Средняя цена],
                        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы] 
                    ) 
                ) 
            ),
            1 
        ).Item(0).Parent.Parent.Parent.Parent.Properties("Name") ' 
    set [NonEmptySet]      
    as ' Head      
        (
            Filter      
            (
                [Период].[День].[День].Members,
                not IsEmpty      
                (
                    (
                        [Measures].[Средняя цена],
                        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы]      
                    )      
                ) and 
                (
                    [Период].[День].CurrentMember.Parent.Parent.Parent.Parent.Name = Tail 
                    (
                        Filter 
                        (
                            [Период].[День].[День].Members,
                            not IsEmpty 
                            (
                                (
                                    [Measures].[Средняя цена],
                                    [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы] 
                                ) 
                            ) 
                        ),
                        1 
                    ).Item(0).Parent.Parent.Parent.Parent.Properties("Name")
                ) 
            ),
            1      
        ) + Tail      
        (
            Filter      
            (
                [Период].[День].[День].Members,
                not IsEmpty      
                (
                    (
                        [Measures].[Средняя цена],
                        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы]      
                    )      
                )      
            ),
            2      
        )'      
select
    {
        [Measures].[ДанныеНа]
    } on columns,
    { [NonEmptySet] } on rows      
from   [ОРГАНИЗАЦИИ_Цены и тарифы]
WHERE 
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Вологдастат],
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена]
    ) 
  ]]>
  </query>

  <query id="Oil_0002_0001_table1">
    <![CDATA[
    --Oil_0002_0001_table1
WITH    
    member measures.[Текущая цена] 
    as ' 
        (
            <%period_cur_date%>,
            [Measures].[Средняя цена] 
        )' 
    member measures.[Прошлая цена] 
    as ' 
        (
            <%period_last_date%>,
            [Measures].[Средняя цена] 
        )' 
    member measures.[Начало года цена] 
    as ' 
        (
            <%period_first_year%>,
            [Measures].[Средняя цена] 
        )' 
    member measures.[Абсолютное отклонение к прошлой дате] 
    as 'measures.[Текущая цена]  - measures.[Прошлая цена]' 
    member measures.[Темп прироста (динамика к предыдущему периоду)] 
    as 'IIF 
        (
            not IsEmpty(measures.[Прошлая цена]) and not (measures.[Прошлая цена] = 0),
            (
                measures.[Текущая цена] / measures.[Прошлая цена] - 1 
            ) 
            * 100,
            null 
        )' 
    member measures.[Темп прироста (динамика за период с начала года) ] 
    as 'IIF 
        (
            not IsEmpty(measures.[Начало года цена]) and not 
            (
                measures.[Начало года цена] = 0 
            ),
            (
                measures.[Текущая цена] / measures.[Начало года цена] - 1 
            ) 
            * 100,
            null 
        )' 
    member measures.[Абсолютное отклонение к началу года] 
    as 'measures.[Текущая цена]  - measures.[Начало года цена]' 
     member measures.[Территория ]
    as 'Tail
        (
            Order
            (
                Filter
                (
                    [Территории].[РФ].[МР ГО].Members,
                    not IsEmpty
                    (
                        (
                            measures.[Текущая цена],
                            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80]
                        )
                    )
                ),
                measures.[Текущая цена],
                BASC
            )
        ).Item(0).Name'
    member [Measures].[Максимум ]
    as '
        (
            Tail
            (
                Order
                (
                    Filter
                    (
                        [Территории].[РФ].[МР ГО].Members,
                        not IsEmpty
                        (
                            (
                                measures.[Текущая цена],
                                [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80]
                            )
                        )
                    ),
                    measures.[Текущая цена],
                    BASC
                )
            ).Item(0),
            measures.[Текущая цена]
        )'
    member [Measures].[Минимум ]
    as '
        (
            Tail
            (
                Order
                (
                    Filter
                    (
                        [Территории].[РФ].[МР ГО].Members,
                        not IsEmpty
                        (
                            (
                                measures.[Текущая цена],
                                [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80]
                            )
                        )
                    ),
                    measures.[Текущая цена],
                    BDESC
                )
            ).Item(0),
            measures.[Текущая цена]
        ) '
    member [Measures].[Минимум город]
    as ' Tail
        (
            Order
            (
                Filter
                (
                    [Территории].[РФ].[МР ГО].Members,
                    not IsEmpty
                    (
                        (
                            measures.[Текущая цена],
                            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80]
                        )
                    )
                ),
                measures.[Текущая цена],
                BDESC
            )
        ).Item(0).Properties("Name") '
    member [Measures].[Максимум город]
    as ' Tail
        (
            Order
            (
                Filter
                (
                    [Территории].[РФ].[МР ГО].Members,
                    not IsEmpty
                    (
                        (
                            measures.[Текущая цена],
                            [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80]
                        )
                    )
                ),
                measures.[Текущая цена],
                BASC
            )
        ).Item(0).Properties("Name")'
SELECT
    {
        measures.[Текущая цена],
        measures.[Абсолютное отклонение к прошлой дате],
        measures.[Темп прироста (динамика к предыдущему периоду)],
        measures.[Абсолютное отклонение к началу года],
        measures.[Темп прироста (динамика за период с начала года) ],
        [Measures].[Минимум ],
        [Measures].[Минимум город],
        [Measures].[Максимум ],
        [Measures].[Максимум город] 
    } on columns,
    {
        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-80],
        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-92],
        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Бензин АИ-95 (98)],
        [Организации].[Товары и услуги].[Все товары и услуги].[Непродовольственные товары].[Горюче-смазочные материалы].[Дизельное топливо] 
    } on rows 
FROM [ОРГАНИЗАЦИИ_Цены и тарифы] 
WHERE 
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Вологдастат],
        [Организации].[Предприятия торговли].[Все организации].[Значение не указано],
        [Организации].[Тип цены].[Все типы цен].[Розничная цена],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Северо-Западный федеральный округ].[Вологодская область].[Вологодская область].[Город Вологда] 
    ) 
  ]]>
  </query>

