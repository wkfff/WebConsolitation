<?xml version="1.0" encoding="utf-8"?>
<Theme xmlns="http://wixtoolset.org/schemas/thmutil/2010">
    <Window Width="505" Height="390" HexStyle="100a0000" FontId="0">Программа установки Криста MDX Эксперт 3.13 с провайдерами</Window>
    <Font Id="0" Height="-11" Weight="510" Foreground="000000" Background="FFFFFF">Tahoma</Font>
    <Font Id="1" Height="-15" Weight="500" Foreground="000000">Tahoma</Font>
    <Font Id="2" Height="-15" Weight="500" Foreground="000000">Tahoma</Font>
    <Font Id="3" Height="-12" Weight="500" Foreground="000000" Background="FFFFFF">Tahoma</Font>
    <Font Id="4" Height="-10" Weight="500" Foreground="000000" Background="FFFFFF" Underline="yes">Tahoma</Font>
    <Font Id="5" Height="-10" Weight="500" Background="D4D0C8"></Font>
      
    <Image X="0" Y="0" Width="160" Height="314" ImageFile="logo.png" Visible="yes"/>
    <Text X="80" Y="11" Width="-11" Height="64" FontId="1" Visible="yes">[WixBundleName]</Text>

    <Page Name="Install">
        <Richedit Name="EulaRichedit" X="175" Y="30" Width="-11" Height="-70" FontId="1"/>
        <Static X="0" Y="314" Width="505" Height="40" />
        <Button Name="InstallButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Установить</Button>
        <Button Name="WelcomeCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Отмена</Button>
    </Page>
    <Page Name="Options">
        <Text X="11" Y="80" Width="-11" Height="30" FontId="2">Setup Options</Text>
        <Text X="11" Y="121" Width="-11" Height="17" FontId="3">Install location:</Text>
        <Editbox Name="FolderEditbox" X="11" Y="143" Width="-91" Height="21" TabStop="yes" FontId="3" FileSystemAutoComplete="yes" />
        <Button Name="BrowseButton" X="-11" Y="142" Width="75" Height="23" TabStop="yes" FontId="3">&amp;Browse</Button>
        <Button Name="OptionsOkButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;OK</Button>
        <Button Name="OptionsCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Cancel</Button>
    </Page>
    <Page Name="Progress">
        <Text X="170" Y="150" Width="-55" Height="17" FontId="1">Установка:</Text>
        <Text Name="OverallProgressPackageText" X="170" Y="170" Width="-11" Height="17" FontId="3">[ProgressPackageName]</Text>
        <Progressbar Name="OverallCalculatedProgressbar" X="170" Y="190" Width="-11" Height="15" />
        <Static X="0" Y="314" Width="505" Height="40" />
        <Button Name="ProgressCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Отмена</Button>
    </Page>
    <Page Name="Modify">
        <Text X="11" Y="80" Width="-11" Height="30" FontId="2">Изменение</Text>
        
        <Button Name="UninstallButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Удалить</Button>
        <Button Name="ModifyCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Выход</Button>
    </Page>
    <Page Name="Success">
        <Text X="170" Y="50" Width="-11" Height="40" FontId="2">Установка Криста MDX Эксперт успешно завершена.</Text>
        <Text X="170" Y="90" Width="-11" Height="30" FontId="0">Нажмите кнопку "Готово" для выхода из программы.</Text>
        <Static X="0" Y="314" Width="505" Height="40" />
        <Button Name="LaunchButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">&amp;Launch</Button>
        <Button Name="SuccessRestartButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">&amp;Restart</Button>
        <Button Name="SuccessCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Готово</Button>

    </Page>
    <Page Name="Failure">
        <Text X="170" Y="80" Width="-11" Height="30" FontId="2">Установка завершена с ошибками</Text>
        <Text Name="FailureLogFileText" X="170" Y="120" Width="-11" Height="27" FontId="3" HideWhenDisabled="yes">Для дополнительной информации см. лог установки</Text>
        <Hyperlink Name="FailureLogFileLink" X="170" Y="140" Width="40" Height="17" TabStop="yes" FontId="4" HoverFontId="4" SelectedFontId="4" HideWhenDisabled="yes">log file.</Hyperlink>
        <Text Name="FailureMessageText" X="170" Y="163" Width="-11" Height="51" FontId="3" HideWhenDisabled="yes" />
        <Static X="0" Y="314" Width="505" Height="40" />
        <Button Name="FailureRestartButton" X="-91" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0" HideWhenDisabled="yes">&amp;Restart</Button>
        <Button Name="FailureCancelButton" X="-11" Y="-11" Width="75" Height="23" TabStop="yes" FontId="0">&amp;Выход</Button>
    </Page>
</Theme>