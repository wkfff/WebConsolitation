<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[ -- name: last_date   
SELECT
    { [Measures].[Значение]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[Значение] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        <%baseRegion%>,
        [Источники данных].[Все источники данных],
        [Показатели].[ВедСтат_Свод].[Все показатели].[<%way_last_year%>]
    )  
	  ]]>
  </query>

  <query id="grid1">
    <![CDATA[ -- name: grid1
select
    non empty
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Использование водных ресурсов].children   
    } on columns,
    non empty   
    {
        <%last_year%>.Lag(9):<%last_year%>
    } on rows
FROM [ВЕДСТАТ_Свод]   
where   
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]   
    )        
    ]]>
  </query>


  <query id="grid2">
    <![CDATA[ -- name: grid2
WITH   
    MEMBER [Период].[Год Квартал Месяц].[ЕдИзм]   
    AS  '[Показатели].[ВедСтат_Свод].CurrentMember.Properties("Единица измерения")'   
select
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Добыча нефти (произведено)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Добыча нефти, включая газовый конденсат],
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Газовый конденсат],
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Добыча газа всего],
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Выработка компремированного газа]  
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows,
    non empty  UNION
    (
        {
            [Период].[Год Квартал Месяц].[ЕдИзм]
        },
        {
            <%last_year%>.Lag(4):<%last_year%>
        }
    ) on columns  
FROM [ВЕДСТАТ_Свод]  
where  
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]  
    )      
    ]]>
  </query>
  
  
  <query id="chart1">
    <![CDATA[ -- name: chart1
select
    non empty
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[РЕГУЛИРОВАНИЕ ИСПОЛЬЗОВАНИЯ ВОДНЫХ ОБЪЕКТОВ МЕСТНОГО ЗНАЧЕНИЯ, МЕСТОРОЖДЕНИЙ ОБЩЕРАСПРОСТРАНЕННЫХ ПОЛЕЗНЫХ ИСКОПАЕМЫХ].[Использование водных ресурсов].children
    } on rows,
    non empty   
    {
        [Период].[Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
    } on columns
FROM [ВЕДСТАТ_Свод]   
where   
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]   
    )   
    ]]>
  </query>

 
  <query id="gauge">
    <![CDATA[ -- name: gauge
select
    {
        <%current_way1%>
    } on columns,
    {
        <%last_year%>.Lag(4):<%last_year%>
    } on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]
    )    
    ]]>
  </query>

</root>