<RegionSettings>
  <!--
  // имя папки с картами региона
  MapFolderName,  
  // загрузка карты из бинарика
  MapLoadFromBin,
  // масштаб карты
  MapZoomValue,
  // сдвиг по вертикали текста городов-выносок карты
  MapCalloutOffsetY  
  -->
  <Region id="Vologda">
    <MapFolderName>Вологда</MapFolderName>
    <MapLoadFromBin>false</MapLoadFromBin>
    <MapZoomValue>80</MapZoomValue>
    <MapCalloutOffsetY>-800</MapCalloutOffsetY>
  </Region>

  <Region id="HMAO">
    <MapFolderName>ХМАО</MapFolderName>
    <MapLoadFromBin>false</MapLoadFromBin>
    <MapZoomValue>100</MapZoomValue>
    <MapCalloutOffsetY>0,5</MapCalloutOffsetY>
  </Region>
</RegionSettings>
