    member [ОКВЭД].[Сопоставимый].[Оптовая и розничная торговля; ремонт автотранспортных средств, мотоциклов, бытовых изделий и предметов личного пользования ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Оптовая и розничная торговля; ремонт автотранспортных средств, мотоциклов, бытовых изделий и предметов личного пользования]' 
    member [ОКВЭД].[Сопоставимый].[Гостиницы и рестораны ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Гостиницы и рестораны]' 
    member [ОКВЭД].[Сопоставимый].[Транспорт и связь ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Транспорт и связь]' 
    member [ОКВЭД].[Сопоставимый].[Финансовая деятельность ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Финансовая деятельность]' 
    member [ОКВЭД].[Сопоставимый].[Операции с недвижимым имуществом, аренда и предоставление услуг ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Операции с недвижимым имуществом, аренда и предоставление услуг]' 
    member [ОКВЭД].[Сопоставимый].[<%fns_okved_government%> ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[<%fns_okved_government%>]' 
    member [ОКВЭД].[Сопоставимый].[Образование ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Образование]' 
    member [ОКВЭД].[Сопоставимый].[Здравоохранение и предоставление социальных услуг ]
    as '[ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Здравоохранение и предоставление социальных услуг]' 
    member [ОКВЭД].[Сопоставимый].[Предоставление прочих коммунальных, социальных и персональных услуг, Деятельность домашних хозяйств, Деятельность экстерриториальных организаций ]
    as 'Aggregate 
        (
            {
                [ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Предоставление прочих коммунальных, социальных и персональных услуг],
                [ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[<%fns_okved_household%>],
                [ОКВЭД].[Сопоставимый].[Все коды ОКВЭД].[Деятельность экстерриториальных организаций]
            }
        ) '

    ----
    set [Группы ОКВЭД]
    as ' 
        {
            [ОКВЭД].[Сопоставимый].[Нет кода ОКВЭД],
            [ОКВЭД].[Сопоставимый].[Сельское хозяйство, охота и лесное хозяйство ],
            [ОКВЭД].[Сопоставимый].[Рыболовство, рыбоводство ],
            [ОКВЭД].[Сопоставимый].[Добыча полезных ископаемых ],
            [ОКВЭД].[Сопоставимый].[Производство и распределение электроэнергии, газа и воды ],
            [ОКВЭД].[Сопоставимый].[Оптовая и розничная торговля; ремонт автотранспортных средств, мотоциклов, бытовых изделий и предметов личного пользования ],
            [ОКВЭД].[Сопоставимый].[Гостиницы и рестораны ],
            [ОКВЭД].[Сопоставимый].[Транспорт и связь ],
            [ОКВЭД].[Сопоставимый].[Финансовая деятельность ],
            [ОКВЭД].[Сопоставимый].[Операции с недвижимым имуществом, аренда и предоставление услуг ],
            [ОКВЭД].[Сопоставимый].[<%fns_okved_government%> ],
            [ОКВЭД].[Сопоставимый].[Образование ],
            [ОКВЭД].[Сопоставимый].[Здравоохранение и предоставление социальных услуг ],
            [ОКВЭД].[Сопоставимый].[Предоставление прочих коммунальных, социальных и персональных услуг, Деятельность домашних хозяйств, Деятельность экстерриториальных организаций ]
        } ' 

    member [Measures].[Поступило, итого, тыс.руб.]
    as '
        (
            [ФНС_65н_Показатели].[Все].[Поступило - всего],
            [Measures].[С начала года]
        )/1000'
    member [Measures].[Начислено, всего, тыс.руб.]
    as '
        (
            [ФНС_65н_Показатели].[Все].[Начислено - всего],
            [Measures].[С начала года]
        )/1000'
    member [Measures].[Уровень собираемости,%*]
    as '
        (
            [ФНС_65н_Показатели].[Все].[Поступило - всего],
            [Measures].[С начала года]
        )/
        (
            [ФНС_65н_Показатели].[Все].[Начислено - всего],
            [Measures].[С начала года]
        )'
    member [Measures].[КодОКАТО]
    as '[ОКАТО].[28н].CurrentMember.name'
    member [Measures].[Муниципальные образования]
    as '[Районы].[Сопоставимый].CurrentMember.name'
    set [Районы]
    as 'Filter
        (
            Descendants
            (
                [Районы].[Сопоставимый].[Все районы],
                <%regions_level%>,
                SELF
            ),
            not
            (
                [Районы].[Сопоставимый].CurrentMember is [Районы].[Сопоставимый].CurrentMember.Parent.DataMember
            )
        )'
    member [Measures].[Ранг МО по уровню собираемости]
    as 'IIF
        (
            IsEmpty
            (
                (
                    [Measures].[Уровень собираемости,%*],
                    [Районы].[Сопоставимый].CurrentMember,
                    [ОКАТО].[28н].CurrentMember
                )
            ),
            null,
            Rank
            (
                [ОКАТО].[28н].CurrentMember,
                Order
                (
                    Filter
                    (
                        [Коды ОКАТО],
                        not IsEmpty
                        (
                            (
                                [Measures].[Уровень собираемости,%*],
                                [Районы].[Сопоставимый].CurrentMember,
                                [ОКАТО].[28н].CurrentMember
                            )
                        )
                    ),
                    (
                        [Measures].[Уровень собираемости,%*],
                        [Районы].[Сопоставимый].CurrentMember,
                        [ОКАТО].[28н].CurrentMember
                    ),
                    BDESC
                ),
                (
                    [Measures].[Уровень собираемости,%*],
                    [Районы].[Сопоставимый].CurrentMember,
                    [ОКАТО].[28н].CurrentMember
                )
            )
        )'
    set [Коды ОКАТО]
    as ' [ОКАТО].[28н].[Данные всех источников].[ФНС 28н - <%period_year%> <%nMonth%>].Children'
    member [Measures].[Худший ранг]
    as 'Max
        (
            Filter
            (
                [Коды ОКАТО],
                not IsEmpty
                (
                    (
                        [Measures].[Уровень собираемости,%*],
                        [Районы].[Сопоставимый].CurrentMember,
                        [ОКАТО].[28н].CurrentMember
                    )
                )
            ),
            [Measures].[Ранг МО по уровню собираемости]
        )'
SELECT
    NON EMPTY
    {
        [Measures].[Начислено, всего, тыс.руб.],
		[Measures].[Поступило, итого, тыс.руб.]
    } ON COLUMNS,NON EMPTY
    
         [Районы]
        
    ON ROWS
FROM [ФНС_28н_с расщеплением]
WHERE
    (
    [Период].[Период].[Данные всех периодов].[<%period_year%>].[<%period_half_year%>].[<%period_quater%>].[<%period_month%>],  
   [Уровни бюджетов].[<%budget_level%>],
    [КД].[Сопоставимый].[<%fns_kd_group%>]    
    )
    ]]>
</query>





