        ) '

    ----
    set [Группы ОКВЭД]
    as ' 
        {
            [ОКВЭД].[Сопоставимый].[Нет кода ОКВЭД],
            [ОКВЭД].[Сопоставимый].[Сельское хозяйство, охота и лесное хозяйство ],
            [ОКВЭД].[Сопоставимый].[Рыболовство, рыбоводство ],
            [ОКВЭД].[Сопоставимый].[Добыча полезных ископаемых ],
            [ОКВЭД].[Сопоставимый].[Производство и распределение электроэнергии, газа и воды ],
            [ОКВЭД].[Сопоставимый].[Оптовая и розничная торговля; ремонт автотранспортных средств, мотоциклов, бытовых изделий и предметов личного пользования ],
            [ОКВЭД].[Сопоставимый].[Гостиницы и рестораны ],
            [ОКВЭД].[Сопоставимый].[Транспорт и связь ],
            [ОКВЭД].[Сопоставимый].[Финансовая деятельность ],
            [ОКВЭД].[Сопоставимый].[Операции с недвижимым имуществом, аренда и предоставление услуг ],
            [ОКВЭД].[Сопоставимый].[<%fns_okved_government%> ],
            [ОКВЭД].[Сопоставимый].[Образование ],
            [ОКВЭД].[Сопоставимый].[Здравоохранение и предоставление социальных услуг ],
            [ОКВЭД].[Сопоставимый].[Предоставление прочих коммунальных, социальных и персональных услуг, Деятельность домашних хозяйств, Деятельность экстерриториальных организаций ]
        } ' 

    member [Measures].[Поступило, итого, тыс.руб.]  
    as ' 
        (
            [ФНС_65н_Показатели].[Все].[Поступило - итого],
            [Measures].[С начала года] 
        )/1000'  
    member [Measures].[Начислено, всего, тыс.руб.]  
    as ' 
        (
            [ФНС_65н_Показатели].[Все].[Начислено - всего],
            [Measures].[С начала года] 
        )/1000'  
   member [Measures].[Уровень собираемости,%*]
    as 'IIF(([Measures].[Начислено, всего, тыс.руб.] < 0) OR ([Measures].[Поступило, итого, тыс.руб.] < 0), null, IIF ([Районы].[Сопоставимый].CurrentMember is [Районы].[Сопоставимый].[Все территории],null,
        (
            [ФНС_65н_Показатели].[Все].[Поступило - итого],
            [Measures].[С начала года]
        )/
        (
            [ФНС_65н_Показатели].[Все].[Начислено - всего],
            [Measures].[С начала года]
        )*100))'
    member [Measures].[КодОКАТО]
    as '[Районы].[Сопоставимый].CurrentMember.properties("ОКАТО")'
    member [Measures].[Муниципальные образования] 
    as '[Районы].[Сопоставимый].CurrentMember.name' 
    set [Районы]  
    as 'Filter  
        (
            Descendants  
            (
                [Районы].[Сопоставимый].[Все районы],
                <%regions_level%>,
                SELF  
            ),
            not  
            (
                [Районы].[Сопоставимый].CurrentMember is [Районы].[Сопоставимый].CurrentMember.Parent.DataMember  
            )  
        )' 
        
        set [Район с поселениями]
    as '{<%selectedRegion%>}+Filter
        (
            Descendants
            (
                <%selectedRegion%>,
                <%settlement_level%>,
                SELF
            ),
            not
            (
                [Районы].[Сопоставимый].CurrentMember is [Районы].[Сопоставимый].CurrentMember.Parent.DataMember
            )
        )'
        
    member [Measures].[Ранг МО по уровню собираемости]
    as 'IIF(([Measures].[Начислено, всего, тыс.руб.] < 0) OR ([Measures].[Поступило, итого, тыс.руб.] < 0), null, IIF
        (
            IsEmpty
            (
                (
                    [Measures].[Уровень собираемости,%*]
                 
                )
            ) or [Районы].[Сопоставимый].CurrentMember is [Районы].[Сопоставимый].[Все территории],
            null,
            Rank
            (
                [Районы].[Сопоставимый].CurrentMember,
                Order
                (
                    Filter
                    (
                        <%regType%>,
                        not IsEmpty
                        (
                            (
                                [Measures].[Уровень собираемости,%*]
                                
                            )
                        )
                    ),
                    (
                        [Measures].[Уровень собираемости,%*]
                        
                    ),
                    BDESC
                ),
                (
                    [Measures].[Уровень собираемости,%*]
                    
                )
            )
        ) )'
    set [Коды ОКАТО]  
    as ' [ОКАТО].[28н].[Данные всех источников].[ФНС 28н - <%period_year%> <%nMonth%>].Children' 
    member [Measures].[Худший ранг]
    as 'Max
        (
            Filter
            (
                <%regType%>,
                not IsEmpty
                (
                    (
                        [Measures].[Уровень собираемости,%*]
                    )
                )
            ),
            [Measures].[Ранг МО по уровню собираемости]
        )'
        member [Период].[Период].[Периоды] 
    as   'aggregate({<%selectedPeriod%>})'
    member [Районы].[Сопоставимый].[Все территории]
     as 'IIF ([Measures].CurrentMember is [Measures].[КодОКАТО] or [Measures].CurrentMember is [Measures].[Муниципальные образования] or [Measures].CurrentMember is [Measures].[Уровень собираемости,%*] or [Measures].CurrentMember is [Measures].[Ранг МО по уровню собираемости] or [Measures].CurrentMember is [Measures].[Худший ранг],null,
     Aggregate
            (
             {   [Районы]}
            ))'
SELECT
    NON EMPTY  
    {
        [Measures].[КодОКАТО],
        [Measures].[Муниципальные образования],
        [Measures].[Начислено, всего, тыс.руб.],
        [Measures].[Поступило, итого, тыс.руб.],
        [Measures].[Уровень собираемости,%*],
        [Measures].[Ранг МО по уровню собираемости],
        [Measures].[Худший ранг] 
    } ON COLUMNS,
    {<%measStr%>filter
    (
        { 
         { <%regType%> } },
        not isempty([Measures].[Поступило, итого, тыс.руб.])and not isempty([Measures].[Начислено, всего, тыс.руб.]) 
    )}ON ROWS 
FROM [ФНС_28н_с расщеплением] 
WHERE  
    (
        [Период].[Период].[Данные всех периодов].[<%period_year%>].[<%period_half_year%>].[<%period_quater%>].[<%period_month%>],  
       [Уровни бюджетов].[<%budget_level%>],
        [КД].[Сопоставимый].[<%fns_kd_group%>]    
    )  
  ]]>
</query>
</root>

