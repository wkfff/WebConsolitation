<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="FNS_0001_0007_date">
    <![CDATA[
-- FNS_0001_0001_regions_date   
-- <#[ФНС_28н_с расщеплением]#>
with      
    member Measures.[ДанныеНа]      
    as '[Период].[Период].CurrentMember.UniqueName'      
select
    {Measures.[ДанныеНа]} on columns,
    Tail      
    (
        NonEmptyCrossjoin
        (
            OpeningPeriod    
            (
                [Период].[Период].[Месяц],
                [Период].[Период].[Данные всех периодов].[2009]
            ) : ClosingPeriod    
            (
                [Период].[Период].[Месяц],
                [Период].[Период].[Данные всех периодов].[2012]
            )
        )
    ) on rows  
from [ФНС_28н_с расщеплением]
]]>
  </query>
