<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="STAT_0001_0019_date">
    <![CDATA[
-- STAT_0001_0019_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        tail
        (
            filter
            (
                Descendants
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    SELF
                ),
                not isEmpty ([Measures].[За период])
            ) ,
            1
        )
    } on rows
FROM [СТАТ_Производство_Основные показатели]
WHERE
    (
        [Группировки__Производство_Основные показатели].[Группировки__Производство_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Добыча полезных ископаемых].DataMember,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    )
]]>
  </query>

  <query id="STAT_0001_0019_grid">
    <![CDATA[
-- STAT_0001_0019_grid
with  
    member measures.[Единицы изменения]  
    as '[Производство__Основные показатели].[Производство__Основные показатели].CurrentMember.Properties("Единица измерения")'  
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Пустой]  
    as 'null'  
SELECT
    {
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Пустой] 
    } 
    *{measures.[Единицы изменения]} +  
    {
    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%period_first_year%>],
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%period_last_year%>],
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[<%period_year%>]
    }  
    *  
    {
        [Measures].[За период],
        [Measures].[Прирост цепной],
        [Measures].[Темп роста цепной]  
    }on columns ,
    filter 
    (
        {
            [Производство__Основные показатели].[Производство__Основные показатели].Members -  
            {
                [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Индекс промышленного производства],
                [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели]  
            }  
        },
        not isEmpty ([Measures].[За период]) 
    ) ON ROWS 
FROM [СТАТ_Производство_Основные показатели]  
WHERE  
    (
        [Группировки__Производство_Основные показатели].[Группировки__Производство_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Добыча полезных ископаемых].DataMember ,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]  
    ) 
  ]]>
  </query>

  <query id="STAT_0001_0019_chart1">
    <![CDATA[
-- STAT_0001_0019_chart1
with 
    member [Measures].[За период ] 
    as '[Measures].[За период] / 1000' 
    member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Пустой] 
    as 'null' 
    SET [FilterColumnSet] 
    AS 'tail 
        (
            filter 
            (
                Descendants 
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    SELF 
                ),
                not isEmpty ([Measures].[За период]) 
            ) ,
            10 
        )  ' 
SELECT
    {[FilterColumnSet]} on columns ,
    {
        [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами] 
    } ON ROWS 
FROM [СТАТ_Производство_Основные показатели] 
WHERE 
    (
        [Measures].[За период ],
        [Группировки__Производство_Основные показатели].[Группировки__Производство_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Добыча полезных ископаемых].DataMember,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра] 
    ) 
]]>
</query>

  <query id="STAT_0001_0019_chart2">
    <![CDATA[
-- STAT_0001_0019_chart2
with          
    member measures.[Единицы изменения]          
    as '[Производство__Основные показатели].[Производство__Основные показатели].CurrentMember.Properties("Единица измерения")'          
    member [Производство__Основные показатели].[Производство__Основные показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами на одного работника]  
    as 'IIF 
        (
            isEmpty 
            (
                [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Среднегодовая численность работников организаций]
            ),
            null,
            [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами]/ [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Среднегодовая численность работников организаций]
        )' 
    SET [FilterColumnSet]        
    AS 'tail        
        (
            filter    
            (
                Descendants    
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    SELF        
                ),
                not isEmpty ([Measures].[За период])  
            ) ,
            10      
        )  '      
SELECT
    {[FilterColumnSet]}      
     on columns ,
    {
        [Производство__Основные показатели].[Производство__Основные показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами на одного работника]
    } ON ROWS        
FROM [СТАТ_Производство_Основные показатели]          
WHERE          
    (
    [Measures].[За период],
        [Группировки__Производство_Основные показатели].[Группировки__Производство_Основные показатели].[Все группировки].[По видам экономической деятельности],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Добыча полезных ископаемых].DataMember,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]     
    )
]]>
  </query>

