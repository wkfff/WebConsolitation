  <query id="STAT_0001_0019_chart4">
    <![CDATA[
-- STAT_0001_0019_chart4
with
    member [ОК__ОКП].[ОК__ОКП].[Нефть, млн.тонн.]
    as '[ОК__ОКП].[ОК__ОКП].[Все виды продукции].[Нефть. Нефтепродукты. Альтернативные виды топлива. Газ].[Нефть добытая]'
    member [ОК__ОКП].[ОК__ОКП].[Газ естественный, млрд. кубических метров]
    as '[ОК__ОКП].[ОК__ОКП].[Все виды продукции].[Нефть. Нефтепродукты. Альтернативные виды топлива. Газ].[Газ естественный]'
    member [ОК__ОКП].[ОК__ОКП].[Материалы строительные нерудные, тыс. кубических метров]
    as '[ОК__ОКП].[ОК__ОКП].[Все виды продукции].[Материалы строительные, кроме сборных железобетонных конструкций и деталей].[Материалы строительные нерудные]'
    SET [FilterColumnSet]
    AS 'tail
        (
            filter
            (
                Descendants
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    SELF
                ),
                not isEmpty ([Measures].[За период])
            ) ,
            10
        )  '
SELECT
    {[FilterColumnSet]}
     on columns ,
    {
        [ОК__ОКП].[ОК__ОКП].[Нефть, млн.тонн.],
        [ОК__ОКП].[ОК__ОКП].[Газ естественный, млрд. кубических метров],
        [ОК__ОКП].[ОК__ОКП].[Материалы строительные нерудные, тыс. кубических метров]
    } ON ROWS
FROM [СТАТ_Производство_Основные показатели]
WHERE
    (
    [Measures].[За период],
        [Группировки__Производство_Основные показатели].[Группировки__Производство_Основные показатели].[Все группировки].[По видам продукции, по видам экономической деятельности],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Добыча полезных ископаемых].DataMember,
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
        [Производство__Основные показатели].[Производство__Основные показатели].[Все показатели].[Производство основных видов продукции]
    )
]]>
  </query>
