                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Изменение остатков средств на счетах по учету  средств бюджета],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Иные источники внутреннего финансирования  дефицитов бюджетов] 
            }
        )' 
    SET [NonFilterRowSet]    
    AS '  
        {
            [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации],
            [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации],
            [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации],
            [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Изменение остатков средств на счетах по учету  средств бюджета],
            [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Иные источники внутреннего финансирования  дефицитов бюджетов]  
        }   ' 
SELECT
    {[Measures].[Факт ]} ON COLUMNS ,
    NON EMPTY       
    {
        {
            [NonFilterRowSet] ,
            [КИФ].[Сопоставимый].[Итого:]
        }       
    } ON ROWS     
FROM [ФО_МесОтч_ИсточникВнутрФинансирования]       
WHERE       
    (
        [ТипДокумента].[СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
        [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта],
        <%period_cur_date%>       
    ) 
  ]]>
    </query>

    <query id="FO_0007_0001_table_debt">
        <![CDATA[
    --FO_0007_0001_table_debt
  WITH    
     member [Measures].[Годовые назначения ]   
    as 'IIF ( isEmpty ([Measures].[Годовые назначения]), 0, [Measures].[Годовые назначения]  / 1000)'  
     member [Measures].[Факт ]  
    as 'IIF ( isEmpty ([Measures].[Факт]), 0, [Measures].[Факт]  / 1000)'  
    member [Measures].[% исполнения ] 
    as 'IIF ( isEmpty ([Measures].[% выполнения годовых назначений]), 0, [Measures].[% выполнения годовых назначений])'    
    SET [NonFilterColumnSet]  
    AS '  
        {
            [Measures].[Годовые назначения ],
            [Measures].[Факт ],
            [Measures].[% исполнения ]
        }'  
    SELECT
        [NonFilterColumnSet] ON COLUMNS  
    FROM [ФО_МесОтч_Расходы]  
    WHERE  
    (
        [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта] ,
        [ТипДокумента].[СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов] ,
        [РзПр].[Сопоставимый].[Все коды ФКР].[ОБСЛУЖИВАНИЕ ГОСУДАРСТВЕННОГО И МУНИЦИПАЛЬНОГО ДОЛГА] ,
        <%period_cur_date%>  
    )
  ]]>
    </query>

    <query id="FO_0007_0001_result">
        <![CDATA[
    --FO_0007_0001_result
  WITH 
    member [Measures].[Факт ]  
    as 'IIF ( isEmpty ([Measures].[Факт]), 0, [Measures].[Факт]  / 1000)'  
SELECT
    {[Measures].[Факт ]} ON COLUMNS 
FROM [ФО_МесОтч_Дефицит Профицит] 
WHERE 
    (        
        [ТипДокумента].[СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов] ,
        [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта],
        <%period_cur_date%>  
    )
  ]]>
    </query>

</root>
