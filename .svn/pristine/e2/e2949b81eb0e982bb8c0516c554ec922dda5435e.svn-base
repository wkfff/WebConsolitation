            ),
            0,
            [Measures].[Факт]  / -1000
        )' 
    member [КИФ].[Сопоставимый].[ ]
    as 'sum         
        (
            {
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации].[Погашение государственных  (муниципальных)ценных бумаг, номинальная  стоимость которых указана в валюте Российской  Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации].[Погашение кредитов, предоставленных кредитными организациями в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации].[Погашение бюджетных кредитов, полученных от других бюджетов бюджетной системы Российской Федерации в валюте Российской Федерации]  
            }        
        )'        
    member [КИФ].[Сопоставимый].[в том числе]        
    as 'null'       
    member [КИФ].[Сопоставимый].[Кредиты кредитных организаций]
    as '[КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации].[Погашение кредитов, предоставленных кредитными организациями в валюте Российской Федерации]'  
    member [КИФ].[Сопоставимый].[Бюджетные кредиты]
    as '[КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации].[Погашение бюджетных кредитов, полученных от других бюджетов бюджетной системы Российской Федерации в валюте Российской Федерации]'  
    member [КИФ].[Сопоставимый].[Государственные ценные бумаги]
    as '[КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации].[Погашение государственных  (муниципальных)ценных бумаг, номинальная  стоимость которых указана в валюте Российской  Федерации]'  
SELECT
    {[Measures].[Погашение/списание]} ON COLUMNS,
    { 
        [КИФ].[Сопоставимый].[ ],
        [КИФ].[Сопоставимый].[в том числе],
        [КИФ].[Сопоставимый].[Кредиты кредитных организаций],
        [КИФ].[Сопоставимый].[Бюджетные кредиты],
        [КИФ].[Сопоставимый].[Государственные ценные бумаги]    
    } ON ROWS         
FROM  [ФО_МесОтч_ИсточникВнутрФинансирования]          
WHERE                    
    (
        [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта] ,
        [ТипДокумента].[СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов] ,
        <%period_cur_date%>                    
    )
  ]]>
    </query>

    <query id="FO_0007_0001_table_saldo">
        <![CDATA[
    --FO_0007_0001_table_saldo
 WITH 
    member [Measures].[Факт ]  
    as 'IIF ( isEmpty ([Measures].[Факт]), 0, [Measures].[Факт]  / 1000)'  
    member [КИФ].[Сопоставимый].[Сальдо] 
    as 'sum 
        (
            {
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации].[Размещение государственных (муниципальных)  ценных бумаг, номинальная стоимость которых  указана в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации].[Получение кредитов от кредитных организаций в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации].[Получение бюджетных кредитов от других бюджетов бюджетной системы Российской Федерации в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Государственные (муниципальные) ценные бумаги,  номинальная стоимость которых указана в валюте  Российской Федерации].[Погашение государственных  (муниципальных)ценных бумаг, номинальная  стоимость которых указана в валюте Российской  Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Кредиты кредитных организаций в валюте Российской Федерации].[Погашение кредитов, предоставленных кредитными организациями в валюте Российской Федерации],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации].[Погашение бюджетных кредитов, полученных от других бюджетов бюджетной системы Российской Федерации в валюте Российской Федерации] 
            } 
        )'  
SELECT
    {[Measures].[Факт ]}  ON COLUMNS ,
    {
        [КИФ].[Сопоставимый].[Сальдо] 
    } ON ROWS 
FROM  [ФО_МесОтч_ИсточникВнутрФинансирования] 
WHERE 
    (
        [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта] ,
        [ТипДокумента].[СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов] ,
        <%period_cur_date%> 
    ) 
  ]]>
    </query>

