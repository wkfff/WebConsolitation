<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="Grid">
    <![CDATA[with 
    member [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Процент неоплаченных коммунальных услуг]
as 'iif([ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Начислено (предъявлено) жилищно-коммунальных платежей населению]>0,
([ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Начислено (предъявлено) жилищно-коммунальных платежей населению] - [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Фактически оплачено])/[ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Начислено (предъявлено) жилищно-коммунальных платежей населению]
,null)'
member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[null]
as 'null'

member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurYear]
as '<%ChosenCurPeriod%>'

member [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[PrevYear]
as '<%ChosenPrevPeriod%>'

SELECT
    --non empty 
    {
    Crossjoin(
    {[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[CurYear],
    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[PrevYear]},
    {   [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Начислено (предъявлено) жилищно-коммунальных платежей населению],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Фактически оплачено],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Процент неоплаченных коммунальных услуг],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Фактические объемы финансирования из бюджетов всех уровней на предоставление отдельным категориям граждан социальной поддержки по оплате жилищно-коммунальных услуг],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Фактические объемы финансирования из бюджетов всех уровней на предоставление отдельным категориям граждан субсидий по оплате жилищно-коммунальных услуг],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Стоимость предоставленных населению услуг, рассчитанная по экономически обоснованным тарифам],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Возмещение населением затрат за предоставление услуг по установленным для населения тарифам],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Возмещение населением затрат за предоставление услуг фактическое],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Обслуживаемый жилищный фонд],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Показатель].[Число проживающих в обслуживаемом жилищном фонде, которым оказываются ЖКУ]})
    }on columns,    
    {        [ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[жилищные],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[найм жилого помещения],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[ремонт жилого помещения],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[ремонт жилого помещения со всеми видами благоустройства,включая лифты и мусопроводы],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[ремонт жилого помещения со всеми видами благоустройства, кроме лифтов и мусопроводов],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[коммунальные],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[водоснабжение],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[электроснабжение],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[электроснабжение в домах с газовыми плитами],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[электроснабжение в домах с электропитанием],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[водоотведение],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[газоснабжение сетевым газом],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[газоснабжение сжиженным газом],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[поставка бытового газа в баллонах],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[отопление],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[горячее водоснабжение],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[поставка твердого топлива (уголь)],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[поставка твердого топлива (дрова)],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[жилищно-коммунальные услуги],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Услуга ЖКХ].[норма площади жилого помещения на 1 гражданина, м2]}
    on rows 
FROM [ФСТ_ЖКХ по МО_Форма 22ЖКХ]
where
(
<%ChosenRegionGrid%>
--,
--[Источники данных].[Источники данных].[Источник].[ФСТ ЖКХ по МО - ЕИАС_Форма22]
)
]]>
  </query>
  <query id="ComboRegion">
    <![CDATA[with
    set [region]
    as '
    Filter([Территории__РФ].[Территории__РФ].members,
[Территории__РФ].[Территории__РФ].parent is <%RegionBaseDimension%> )'
    member [UniqueName]
    as '[Территории__РФ].[Территории__РФ].currentmember.unique_name'
    member [LevelNumber]
    as '[Территории__РФ].[Территории__РФ].currentmember.LEVEL_NUMBER - 5'

SELECT
    non empty
    {
    filter(
[region],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Все услуги]> 0)
    }on rows,
    {
    [LevelNumber],
[UniqueName]
    }  on columns
FROM   [ФСТ_ЖКХ по МО_Форма 22ЖКХ]
--where
--(
--[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал],
--[Источники данных].[Источники данных].[Источник].[ФСТ ЖКХ по МО - ЕИАС_Форма22]
--)
 
]]>
  </query>
  <query id="ComboRegionFirst">
    <![CDATA[with
    set [region]
    as '
    Filter([Территории__РФ].[Территории__РФ].members,
[Территории__РФ].[Территории__РФ].parent is <%RegionBaseDimension%>
or
[Территории__РФ].[Территории__РФ].parent.parent is <%RegionBaseDimension%> )'
    member [UniqueName]
    as '[Территории__РФ].[Территории__РФ].currentmember.unique_name'
    member [LevelNumber]
    as '[Территории__РФ].[Территории__РФ].currentmember.LEVEL_NUMBER - 5'

SELECT
    non empty
    {
    filter(
[region],
[ФСТ__Услуги ЖКХ по форме 22].[ФСТ__Услуги ЖКХ по форме 22].[Все услуги]> 0)
    }on rows,
    {
    [LevelNumber],
[UniqueName]
    }  on columns
FROM   [ФСТ_ЖКХ по МО_Форма 22ЖКХ]
where
(
--[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал],
--[Источники данных].[Источники данных].[Источник].[ФСТ ЖКХ по МО - ЕИАС_Форма22],
<%ChosenCurPeriod%>
)
 
]]>
  </query>
  <query id="ComboYear">
    <![CDATA[with
    member [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[UniqueName]
    as 'iif([ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[LevelNumber] = 1,
    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.datamember.UniqueName,
    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.UniqueName)'
    member [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[LevelNumber]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.Level_Number-1'
    member [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Year]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.name'
SELECT
    non empty
    {
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[UniqueName],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[LevelNumber],
        [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[Year]
    }on columns  ,
    non empty
    {
        Filter
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].members,
            [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[(All)].[Все показатели] > 0
            and
            not [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.is_datamember
            and ([ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[LevelNumber] = 0
            or
            [ФСТ__Показатели Формы 22ЖКХ].[ФСТ__Показатели Формы 22ЖКХ].[LevelNumber] = 2)
        )
    }on rows
FROM [ФСТ_ЖКХ по МО_Форма 22ЖКХ]
--where ([Источники данных].[Источники данных].[Источник].[ФСТ ЖКХ по МО - ЕИАС_Форма22])
where
(
--[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ]
<%RegionBaseDimension%>
)
]]>
  </query>
</root>