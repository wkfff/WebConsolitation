<?xml version="1.0" encoding="utf-8" ?>
<Extension>
  <Params>
    <Param name="UserRoleGKH" type="string">
      <ValueProvider type="Krista.FM.RIA.Extensions.OrgGKH.UserRoleGKHValueProvider, Krista.FM.RIA.Extensions.OrgGKH"></ValueProvider>
    </Param>
  </Params>

  <Types>
    <Register from="Krista.FM.RIA.Extensions.OrgGKH.Services.IExportService, Krista.FM.RIA.Extensions.OrgGKH"
              to="Krista.FM.RIA.Extensions.OrgGKH.Services.ExportService, Krista.FM.RIA.Extensions.OrgGKH"
              lifetimeManager="Microsoft.Practices.Unity.PerResolveLifetimeManager, Microsoft.Practices.Unity"></Register>
  </Types>

  <!-- Область навигации -->
  <Navigation type="list" ID="acOrgGKH" icon="Vector" title="Сбор по ЖКХ" order="200" group="Сбор данных" dashboardIcon="/Krista.FM.RIA.Extensions.Tasks/Presentation/Content/images/task-72.png/extention.axd" defaultItemId="">
    <Items>
      <Item ID="nidbePlanProceeds" title="Реестр организаций ЖКХ" icon="ApplicationViewList" condition="$(UserRoleGKH) == 'Audit' || $(UserRoleGKH) == 'MO'">
        <Params>
          <Param name="url">'/View/PlanProceeds'</Param>
        </Params>
      </Item>
      <Item ID="nidbeMonthForm" title="Ежемесячный сбор" icon="ApplicationViewList" condition="$(UserRoleGKH) == 'Org'">
        <Params>
          <Param name="url">'/View/MonthForm'</Param>
        </Params>
      </Item>
      <Item ID="nidbeWeekForm" title="Еженедельный сбор" icon="ApplicationViewList" condition="$(UserRoleGKH) == 'Org'">
        <Params>
          <Param name="url">'/View/WeekForm'</Param>
        </Params>
      </Item>
      <Item ID="nidbeMarksForm" title="Ввод общих показателей" icon="ApplicationViewList" condition="$(UserRoleGKH) == 'Audit' || $(UserRoleGKH) == 'Iogv'">
        <Params>
          <Param name="url">'/View/MarksForm'</Param>
        </Params>
      </Item>
    </Items>
    <Action>MdiTab.addTab({ title: menuItem.text, url: menuItem.url, icon: menuItem.iconCls, passParentSize: menuItem.passParentSize});</Action>
    <ButtomBar></ButtomBar>
  </Navigation>

  <Views>

    <View id="PlanProceeds" title="Реестр организаций ЖКХ"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.OrgGKH.Presentation.Views.PlanProceedsView, Krista.FM.RIA.Extensions.OrgGKH">
    </View>
    <View id="MonthForm" title="Ежемесячный сбор"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.OrgGKH.Presentation.Views.MonthlyOrgView, Krista.FM.RIA.Extensions.OrgGKH">
    </View>
    <View id="WeekForm" title="Еженедельный сбор"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.OrgGKH.Presentation.Views.WeeklyOrgView, Krista.FM.RIA.Extensions.OrgGKH">
    </View>
    <View id="MarksForm" title="Ввод общих показателей"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.OrgGKH.Presentation.Views.MarksView, Krista.FM.RIA.Extensions.OrgGKH">
    </View>
  </Views>
</Extension>
