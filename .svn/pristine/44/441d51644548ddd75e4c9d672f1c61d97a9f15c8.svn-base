    <query id="STAT_0005_0001_tendencies_grid">
        <![CDATA[
-- STAT_0005_0001_tendencies_grid
with    
    set [ПоследниеМесяцы]   
    as 'tail   
        (
            filter   
            (
                descendants   
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Месяц],
                    SELF   
                ),
                not isEmpty ( [Measures].[Значение] )   
            ),
            13  
        )'   
    member [Measures].[Показатель текущего периода] 
    as 'aggregate    
        (
            [ПоследниеМесяцы].item(12),
            [Measures].[Значение]   
        )'      
    member [Measures].[Показатель предыдущего года] 
    as 'aggregate  
        (
            [ПоследниеМесяцы].item(0),
            [Measures].[Значение]  
        )'  
    member [Measures].[Показатель на начало года]  
    as 'aggregate  
        (
            filter  
            (
                [ПоследниеМесяцы],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentMember.name = "Январь"  
            ),
            [Measures].[Значение]  
        )'  
    member [Measures].[Абсолютное отклонение к предыдущему периоду]  
    as '[Measures].[Показатель текущего периода] - [Measures].[Показатель предыдущего года]' 
    member [Measures].[Темп роста к предыдущему периоду%]  
    as '[Measures].[Показатель текущего периода] / [Measures].[Показатель предыдущего года]' 
    member [Measures].[Абсолютное отклонение к начау года]  
    as '[Measures].[Показатель текущего периода] - [Measures].[Показатель на начало года]' 
    member [Measures].[Темп роста к начау года%]  
    as '[Measures].[Показатель текущего периода] / [Measures].[Показатель на начало года]'  
    
    member [Measures].[ТипПоказателяОбратный]
    as 'iif
       (
           [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Число убыточных организаций, в % к общему количеству]
           OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Стоимость минимального набора продуктов питания (на конец месяца)]
           OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Численность не занятых трудовой деятельностью граждан, состоящих на учете в органах государственной службы занятости (на конец месяца)]
           OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[из них безработных]
           OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Уровень зарегистрированной безработицы, в % к численности экономически активного населения],
           1,
           0
       )'
    member [Measures].[Единицы]
    as 'iif
        (
            [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Уровень зарегистрированной безработицы, в % к численности экономически активного населения]
            OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Число прибыльных организаций, в % к общему количеству]
            OR [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember is [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Число убыточных организаций, в % к общему количеству],
            null,
            [СЭП__МесМониторинг].[СЭП__МесМониторинг].currentMember.properties("Примечание")
        )'
    member [Measures].[ДанныеНа]
    as '[ПоследниеМесяцы].item(12).uniqueName'    
select
    {
        [Measures].[Показатель предыдущего года],
        [Measures].[Показатель на начало года],
        [Measures].[Показатель текущего периода],
        [Measures].[Абсолютное отклонение к предыдущему периоду],
        [Measures].[Темп роста к предыдущему периоду%],
        [Measures].[Абсолютное отклонение к начау года] ,
        [Measures].[Темп роста к начау года%],
        [Measures].[ТипПоказателяОбратный],
        [Measures].[Единицы],
        [Measures].[ДанныеНа]
    } on columns,
    {
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Добыча полезных ископаемых - С],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Производство и распределение электроэнергии, газа и воды - Е],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Объем работ, выполненных по виду деятельности "строительство"],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Строительство жилых домов],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Оборот розничной торговли],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Объем платных услуг населению],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Стоимость минимального набора продуктов питания (на конец месяца)],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Финансовые результаты деятельности крупных и средних организаций (сальдо прибылей и убытков)],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Число прибыльных организаций, в % к общему количеству],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами по видам экономической деятельности].[Число убыточных организаций, в % к общему количеству],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Денежные доходы в расчете на душу населения],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Среднемесячная номинальная начисленная заработная плата (за период)],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Численность не занятых трудовой деятельностью граждан, состоящих на учете в органах государственной службы занятости (на конец месяца)],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[из них безработных],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Уровень зарегистрированной безработицы, в % к численности экономически активного населения],
        [СЭП__МесМониторинг].[СЭП__МесМониторинг].[Все показатели].[Социальная сфера].[Потребительские расходы в расчете на душу населения]
    } on rows    
from   [ЭО_Мониторинг СЭП_МесМониторинг]    
where    
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Мониторинг СЭП],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER    
    )    
  ]]>
    </query>
</root>