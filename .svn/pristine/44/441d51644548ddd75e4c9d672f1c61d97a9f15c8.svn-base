    <query id="STAT_0005_0001_migration_count">
        <![CDATA[
-- STAT_0005_0001_migration_count
with
    member [Measures].[Прибывшие]     
    as '   
        (
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число прибывших],
            [Measures].[За период]    
        )'     
    member [Measures].[Выбывшие]
    as '
        (
            [Население__Миграция].[Население__Миграция].[Все показатели].[Число выбывших],
            [Measures].[За период]
        )'
select
    { 
        [Measures].[Прибывшие],
        [Measures].[Выбывшие] 
    } on columns,
    {
        tail          
        (
            filter            
            (
                descendants
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц],
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    SELF
                ),
                not isEmpty ( [Measures].[Прибывшие] ) OR not isEmpty ( [Measures].[Выбывшие] )
            )          
        )           
    } on rows
from   [СТАТ_Население_Миграция]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
        [Группировки__Население_Миграция].[Группировки__Население_Миграция].[Все группировки].[Без группировки]
    )
  ]]>
    </query>

