<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id = "STAT_0001_0021_list_of_regions">
    <![CDATA[ -- STAT_0001_0021_list_of_regions
with
    set [Регионы]
    as '
        {
            filter
            (
                [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
                [Measures].[Нарастающий итог] > 0
            )
        }'
    member [Measures].[Имя региона]
    as '[Территории__РФ].[Территории__РФ].CurrentMember.UniqueName'
select
    [Measures].[Имя региона] on columns,
    [Регионы] on rows
from [СТАТ_Инвестиции_Финансовые вложения]
where
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра]
    )
    ]]>
  </query>

  <query id = "STAT_0001_0021_quarters">
    <![CDATA[ -- STAT_0001_0021_quarters
with
    set [Кварталы]
    as '
        (
            filter
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал],
                (
                    [Measures].[Нарастающий итог] > 0
                ) and
                (
                    (
                        [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Краткосрочные финансовые вложения]
                    ) or
                    (
                        [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Долгосрочные финансовые вложения]
                    )
                )
            )
        )'
    member [Measures].[Уникальное имя квартала]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.UniqueName'
select
    [Measures].[Уникальное имя квартала] on columns,
    [Кварталы] on rows
from [СТАТ_Инвестиции_Финансовые вложения]
where
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
        [Все источники данных].[СТАТ Отчетность - ХМАО-Югра]
    )
  ]]>
  </query>
  
  <query id = "STAT_0001_0021_grid">
    <![CDATA[ -- STAT_0001_0021_grid
with
    set [Кварталы]
    as '
        {
            filter
            (
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал].Members,
                not [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember is [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Parent.DataMember and [Measures].[Нарастающий итог] > 0
            )
        }'
    member [Measures].[В этом году]
    as 'iif(
    [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].CurrentMember is [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Всего],
    [Measures].[В этом году всего],
    iif(
        [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
        (
            [Measures].[Нарастающий итог],
            [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По видам финансовых вложений],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
        ),
        (
            [Measures].[Нарастающий итог],
            [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По видам финансовых вложений, по городам и районам],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
        )
        )
        )'
    member [Measures].[В прошлом году]
    as 'iif(
    [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].CurrentMember is [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Всего],
    [Measures].[В прошлом году всего],
    iif(
        [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
        (
            [Measures].[Нарастающий итог],
            [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По видам финансовых вложений],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Lag(6)
        ),
        (
            [Measures].[Нарастающий итог],
            [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По видам финансовых вложений, по городам и районам],
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Lag(6)
        ))
        )'
    member [Measures].[В этом году всего]
    as 'iif(
        [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
        (
             [Measures].[Нарастающий итог],
             [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].CurrentMember,
             [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[Без группировки],
             [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
        ),
        (
             [Measures].[Нарастающий итог],
             [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].CurrentMember,
             [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По городам и районам],
             [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember
        )
    )'
    member [Measures].[В прошлом году всего]
    as 'iif(
        [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
        (
             [Measures].[Нарастающий итог],
             [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].CurrentMember,
             [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[Без группировки],
             [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Lag(6)
        ),
        (
             [Measures].[Нарастающий итог],
             [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].CurrentMember,
             [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По городам и районам],
             [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Lag(6)
        )
    )'
    member [Measures].[Прирост к АППГ]
    as 'iif
        (
            IsEmpty([Measures].[В этом году]) or IsEmpty([Measures].[В прошлом году]),
            null,
            [Measures].[В этом году] - [Measures].[В прошлом году]
        )'
    member [Measures].[Темп роста к АППГ]
    as 'iif
        (
            IsEmpty([Measures].[В этом году]) or IsEmpty([Measures].[В прошлом году]) or ([Measures].[В прошлом году]) = 0,
            null,
            [Measures].[В этом году] / [Measures].[В прошлом году]
        )'
    set [Виды вложений]
    as '
        {
            [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Долгосрочные финансовые вложения],
            [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Краткосрочные финансовые вложения]
        }'
    set [Инвестиции]
    as '
        {
            except
            (
                [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Все виды].children,
                [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Все виды].[Значение не указано]
            )
        }'
    member [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Всего]
    as '[Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Все виды]'
    member [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Год]
    as '"Год " + [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Parent.Parent.Name + + ";" + [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name'
    set [Строки грида]
    as '
        {
            [Measures].[В этом году],
            [Measures].[Прирост к АППГ],
            [Measures].[Темп роста к АППГ]
        }'
select
    {
        [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели]
        *
        {
            [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Год]
        } + [Виды вложений]
        *
        {
            [Инвестиции__Виды финансовых вложений].[Инвестиции__Виды финансовых вложений].[Всего] + [Инвестиции]
        }
    } on columns,
    [Кварталы]
    * [Строки грида] on rows
from [СТАТ_Инвестиции_Финансовые вложения]
where
    (
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [ОК__ОКФС].[ОК__ОКФС].[Все формы собственности].[Значение не указано],
        [Инвестиции__Источники финансирования].[Инвестиции__Источники финансирования].[Все источники финансирования].[Значение не указано],
        <%selected_region%>,
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра]
    )
    ]]>
  </query>

  <query id = "STAT_0001_0021_chart">
    <![CDATA[ -- STAT_0001_0021_chart
with
    set [Кварталы]    
    as '    
        {
                filter  
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал].Members,
                    [Measures].[Нарастающий итог] > 0 and not [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember is [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Parent.DataMember
                )
        }'   
    set [Инвестиции]
    as '
        {
            [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Долгосрочные финансовые вложения],
            [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Все показатели].[Краткосрочные финансовые вложения]
        }'
    member [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Год]
    as '[Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Name + ";" + [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember.Parent.Parent.Name + " год"'
    member [Measures].[НарастающийИтог]
    as 'iif(
    [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DATAMEMBER,
    ([Measures].[Нарастающий итог],
    [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[Без группировки]),
    ([Measures].[Нарастающий итог],
    [Группировки__Инвестиции_Финансовые вложения].[Группировки__Инвестиции_Финансовые вложения].[Все группировки].[По городам и районам])
    )'
select
    {
        {
            [Инвестиции__Финансовые вложения].[Инвестиции__Финансовые вложения].[Год]
        } + [Инвестиции]
    } on columns,
    [Кварталы] on rows
from [СТАТ_Инвестиции_Финансовые вложения]
where
    (
        [Measures].[НарастающийИтог],
        <%selected_region%>,
        [Инвестиции__Источники финансирования].[Инвестиции__Источники финансирования].[Все источники финансирования].[Значение не указано],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [ОК__ОКФС].[ОК__ОКФС].[Все формы собственности].[Значение не указано]
    )
    ]]>
  </query>

</root>