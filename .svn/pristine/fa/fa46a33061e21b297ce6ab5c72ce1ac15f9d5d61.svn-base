<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="STAT_0003_0008_Novosib_list_of_dates">
    <![CDATA[ -- name: STAT_0003_0008_Novosib_list_of_dates
with
    set [Показатели]
    as 'filter
        (
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].Children - [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
            not IsEmpty([Measures].[Значение])
        )'
    set [Даты]
    as 'filter
        (
            [Период__Период].[Период__Период].[Год].Members,
            not IsEmpty([Measures].[Значение])
        )'
select
    [Показатели] on columns,
    [Даты] on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].DataMember
    )
    ]]>
  </query>
  
  <query id = "STAT_0003_0008_Novosib_grid">
    <![CDATA[ -- STAT_0003_0008_Novosib_grid
with
    member [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано]'
    set [Показатели]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего] +
    filter(
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].Children - [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
            not IsEmpty([Measures].[Значение])
        )'
    member [Measures].[Мера]
    as 'null'
select
    [Measures].[Мера] on columns,
    [Показатели] on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].DataMember
    )
    ]]>
  </query>

  <query id = "STAT_0003_0008_Novosib_grid_data">
    <![CDATA[ -- STAT_0003_0008_Novosib_grid_data
with
    member [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано]'
    set [Показатели]
    as '[ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего] +
    filter(
            [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].Children - [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
            not IsEmpty([Measures].[Значение])
        )'
    set [Даты для среднегодового]
    as 'filter
        (
            Descendants
            (
                [Период__Период].[Период__Период].[Данные всех периодов].[<%period_year%>],
                [Период__Период].[Период__Период].[День],
                self
            ),
            not IsEmpty([Measures].[Значение]) and [Период__Период].[Период__Период].CurrentMember.Name = "Заключительные обороты"
        )'
    member [Measures].[Среднегодовое значение]
    as 'avg
        (
            [Даты для среднегодового],
            [Measures].[Значение ]
        )'
    member [Measures].[Значение ]
    as 'iif
        (
            [ОК__ОКВЭД].[ОК__ОКВЭД].CurrentMember is [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего],
                [Measures].[Значение]
            ),
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)],
                [Measures].[Значение]
            )
        )'
select
    [Measures].[Среднегодовое значение] on columns,
    [Показатели] on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Новосибирскстат],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Начисленная среднемесячная заработная плата в расчете на одного работника предприятий и организаций - всего (по видам деятельности)],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Сибирский федеральный округ].[Новосибирская область].DataMember
    )
    ]]>
  </query>

</root>