using System;
using System.Collections.Generic;
using System.Text;

namespace Krista.FM.Client.MDXExpert
{
    //настройки индикатора
    public class GaugeSettings
    {
        private int _innerExtent;
        private int _outerExtent;

        public int InnerExtent
        {
            get { return _innerExtent; }
        }

        public int OuterExtent
        {
            get { return _outerExtent; }
        }

        public GaugeSettings(int innerExtent, int outerExtent)
        {
            this._innerExtent = innerExtent;
            this._outerExtent = outerExtent;
        }
    }
}
