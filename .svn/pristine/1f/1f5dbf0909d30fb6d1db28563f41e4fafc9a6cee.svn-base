<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="STAT_0001_0001_date">
    <![CDATA[
    --STAT_0001_0001_date
with                  
    member [Measures].[Дата]        
    as '[Период].[Период].CurrentMember.UniqueName'  
select
    {  [Measures].[Дата]  } on columns,
    {
        Tail 
        (
            Filter  
            (
                Descendants   
                (
                    [Период].[Период].[Данные всех периодов],
                    [Период].[Период].[День],
                    SELF   
                ),
                (
                    [Период].[Период].CurrentMember.Properties("PKID") <= <%period_current_date%>.Properties("PKID")
                ) and
                [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and 
                [Период].[Период].CurrentMember.Name <> "Данные квартала" and 
                [Период].[Период].CurrentMember.Name <> "Данные полугодия" and 
                [Период].[Период].CurrentMember.Name <> "Данные года" and 
                not IsEmpty
                (
                    (
                        [Measures].[Значение],
                        [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы]
                    )
                )
            ),
            1
        )
    }  on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
  ]]>
  </query>

  <query id="STAT_0001_0001_chart1_date">
    <![CDATA[
    --STAT_0001_0001_chart1_date
with
    member [Measures].[ДанныеНа]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период__Период].[Период__Период].[День].Members,
                not IsEmpty([Measures].[Значение])
            ),
            2
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]
    )
  ]]>
  </query>

  <query id="STAT_0001_0001_Text_Hmao">
    <![CDATA[
    --STAT_0001_0001_Text_Hmao
    with  
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Занятое население ] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ] - [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]'
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность безработных ] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]' 
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Прочие незанятые ] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения] - [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]'
    member[Показатели__Рынок труда].[Показатели__Рынок труда].[Уровень безработицы]
    as 'IIF 
        (
            not isEmpty 
            (
                (
                    [Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ]
                ) 
            ) and not 
            (
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ]
            ) = 0,
            (
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан] 
            )/ 
            (
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ]
            ),
            null 
        )' 
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ] 
    as ' 
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DataMember  
        )' 
   member [Показатели__Рынок труда].[Показатели__Рынок труда].[Число безработных на 1 вакансию]  
    as 'IIF  
        (
            not isEmpty  
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
                )  
            ) and not  
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
            ) = 0,
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]  
                )  
            )/  
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
            )              ,
            null  
        )'  
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Коэффициент напряженности]  
    as 'IIF  
        (
            not isEmpty  
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
                )  
            ) and not  
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
            ) = 0,
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]  
                )  
            )/  
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]  
            ),
            null  
        )'  
    member [Measures].[Значение ] 
    as ' 
        (
            [Measures].[Значение],
            
            <%period_current_date%> 
        )' 
    member [Measures].[Значение прошлый период] 
    as ' 
        (
            [Measures].[Значение],
            <%period_last_week_date%>
        )' 
    member [Measures].[Прирост] 
    as ' 
        (
            [Measures].[Значение ] - [Measures].[Значение прошлый период] 
        )' 
    member [Measures].[Темп прироста] 
    as 'IIF 
        (
            not isEmpty 
            (
                (
                    [Measures].[Значение прошлый период] 
                ) 
            ) and not 
            (
                [Measures].[Значение прошлый период] 
            ) = 0,
            ( [Measures].[Значение ] )/ 
            (
                [Measures].[Значение прошлый период] 
            )  - 1            ,
             null 
        )' 
SELECT
    {
        {
            [Measures].[Значение ],
            [Measures].[Прирост],
            [Measures].[Темп прироста] 
        } 
    } on columns,
    {
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Занятое население ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность безработных ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Прочие незанятые ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Уровень безработицы],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность экономически активного населения ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Коэффициент напряженности],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Число безработных на 1 вакансию] 
    } ON rows  
FROM  [СТАТ_Мониторинг ситуации на рынке труда]  
WHERE  
    (
    [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]  
    )
  ]]>
  </query>

  <query id="STAT_0001_0001_chart1">    
    <![CDATA[
    --STAT_0001_0001_chart1
with  
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Занятое население ] 
    as '
        (
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DataMember
        ) - [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]' 
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность безработных ] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]'
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Прочие незанятые ]
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения] -
    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]'
SELECT
    { [Measures].[Значение] } on columns,
    {
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Занятое население ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Прочие незанятые ],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Численность безработных ]        
    } ON rows  
FROM  [СТАТ_Мониторинг ситуации на рынке труда]  
WHERE  
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
        <%period_current_date%>
    )
  ]]>
  </query>

  <query id="STAT_0001_0002_debts_date">
    <![CDATA[
-- STAT_0001_0002_debts_date
with                  
    member [Measures].[Дата]        
    as '[Период].[Период].CurrentMember.UniqueName'  
select
    {  [Measures].[Дата]  } on columns,
    {
        Tail 
        (
            Filter  
            (
                Descendants   
                (
                    [Период].[Период].[Данные всех периодов],
                    [Период].[Период].[День],
                    SELF   
                ),
                (
                    [Период].[Период].CurrentMember.Properties("PKID") <= <%period_current_date%>.Properties("PKID")
                ) and
                [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and 
                [Период].[Период].CurrentMember.Name <> "Данные квартала" and 
                [Период].[Период].CurrentMember.Name <> "Данные полугодия" and 
                [Период].[Период].CurrentMember.Name <> "Данные года" and 
                not IsEmpty   
                (
                    (
                        [Measures].[Значение],
                        [Показатели].[Рынок труда].[Все].[Количество граждан, имеющих задолженность по выплате заработной платы]  
                    )   
                ) 
            ),
            2 
        )  
    }  on rows   
from [СТАТ_Мониторинг ситуации на рынке труда]           
where           
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ]     
    )
]]>
  </query>

