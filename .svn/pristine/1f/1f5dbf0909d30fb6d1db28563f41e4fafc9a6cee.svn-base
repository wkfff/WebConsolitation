  <query id="STAT_0001_0002_commentText">
    <![CDATA[
-- STAT_0001_0002_commentText
with   

    member [Measures].[Общая численность]
    as '
        (
            [Measures].[Значение]
        )'
    member [Measures].[Общий темп прироста]
    as '
        
            IIF
            (
                not
                (
                    (
                        <%period_last_week_date%>,
                        [Measures].[Значение]
                    ) = 0
                ),
                (
                    <%period_current_date%>,
                    [Measures].[Значение]
                ) /
                (
                    <%period_last_week_date%>,
                    [Measures].[Значение]
                ) - 1,
                null
            )
        '
    member [Measures].[Общий прирост]
    as '
        (
            [Measures].[Прирост ]
        )'        
    member [Measures].[Число организаций]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Количество предприятий, на которых введены неполная рабочая неделя, неполный рабочий день]
        )'
    member [Measures].[Численность работников с неполной занятостью]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[численность работников с неполной занятостью]
        )'
    member [Measures].[Cумма задолженности]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Сумма задолженности по выплате заработной платы (млн.руб.)],
            <%period_current_date_debts%>
        )'
    member [Measures].[Cумма задолженности прошлая неделя]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Сумма задолженности по выплате заработной платы (млн.руб.)],
            <%period_last_week_date_debts%>
        )'           
    member [Measures].[Количество граждан, имеющих задолженность]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Количество граждан, имеющих задолженность по выплате заработной платы],
            <%period_current_date_debts%>
        )'
    member [Measures].[Прирост задолженности]
    as '
        (           
        [Measures].[Cумма задолженности] - [Measures].[Cумма задолженности прошлая неделя]
        )'
   member [Measures].[Численность работников, предполагаемых к увольнению]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Количество работников, предупрежденных о предстоящем увольнении в связи с ликвидацией организаций, сокращением численности или штата работников]
        )'
   member [Measures].[Прирост числа работников, предполагаемых к увольнению]
    as '
        (
            [Measures].[Прирост ],
            [Показатели].[Рынок труда].[Все].[Количество работников, предупрежденных о предстоящем увольнении в связи с ликвидацией организаций, сокращением численности или штата работников]
        )'
   member [Measures].[Численность работников, уволенных с начала мониторинга]
    as '
        (
            [Measures].[Значение],
            [Показатели].[Рынок труда].[Все].[Численность уволенных работников в связи с ликвидацией организаций, сокращением численности или штата работников всего]
        )'
   member [Measures].[Прирост числа работников, уволенных за отчетный период]
    as '
        (
            [Measures].[Прирост ],
            [Показатели].[Рынок труда].[Все].[Численность уволенных работников в связи с ликвидацией организаций, сокращением численности или штата работников всего]
        )'  
    Member [Measures].[Уровень регистрируемой безработицы ]
    as '   (
                [Measures].[Значение],
                [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы]
           )
        '
    Member [Measures].[Прирост уровня регистрируемой безработицы]
    as '(
                [Measures].[Прирост ],
                [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы]
           )       
        '     
    Member [Measures].[Потребность в работниках]
    as '(
                [Measures].[Значение],
                [Показатели].[Рынок труда].[Все].[Потребность предприятий в работниках, заявленная в службы занятости]
        )       
        '   
    Member [Measures].[Прирост потребности в работниках]
    as '(
                [Measures].[Прирост ],
                [Показатели].[Рынок труда].[Все].[Потребность предприятий в работниках, заявленная в службы занятости]
        )
        '    
    Member [Measures].[Число зарегистрированных безработных в расчёте на 1 вакансию]
    as '(
                [Measures].[Значение],
                [Показатели].[Рынок труда].[Все].[Число зарегистрированных безработных в расчёте на 1 вакансию]
           )       
        '   
    Member [Measures].[Прирост числа зарегистрированных безработных в расчёте на 1 вакансию]
    as '(
                [Measures].[Прирост ],
                [Показатели].[Рынок труда].[Все].[Число зарегистрированных безработных в расчёте на 1 вакансию]
           )       
        ' 
    Member [Measures].[Уровень регистрируемой безработицы РФ]
    as '
        (
            [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember,
            [Measures].[Значение],
            <%redundant_level_RF_date%>
        )' 
    Member [Measures].[Уровень регистрируемой безработицы УрФО]
    as '
        (
            [Показатели].[Рынок труда].[Все].[Уровень регистрируемой безработицы],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ],
            [Measures].[Значение],
            <%period_current_date%>
        )'
    Member [Measures].[Прирост ]       
    as '    (
                <%period_current_date%>,
                [Measures].[Значение]     
            ) -      
            (
                <%period_last_week_date%>,
                [Measures].[Значение]     
            )' 
    member [Measures].[Целевое значение по уровню зарегистрированных безработных ]
    as '(
                    [Measures].[Значение],
                    [Показатели].[Рынок труда].[Все].[Целевое значение по уровню зарегистрированных безработных],
                   [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - Субъект РФ],
                    [Период].[Период].CurrentMember.Parent.Parent.Parent.Parent
        )  '
         member Measures.[Численность экономически активного населения ] 
    as 'LookupCube 
        (
            "[СТАТ_Труд_Трудовые ресурсы]",
            " 
            (
                [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
                [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области],
                [Measures].[Нарастающий итог],
                [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения],
                [Период].[Год Квартал Месяц].[Данные всех периодов].[2010].[Полугодие 2].[Квартал 4].[Ноябрь]  
            )" 
        )' 
select
    {
        [Measures].[Общая численность],
        [Measures].[Общий темп прироста],
        [Measures].[Общий прирост],
        [Measures].[Число организаций],
        [Measures].[Численность работников с неполной занятостью],
        [Measures].[Cумма задолженности],
        [Measures].[Cумма задолженности прошлая неделя],
        [Measures].[Количество граждан, имеющих задолженность],
        [Measures].[Прирост задолженности],
        [Measures].[Численность работников, предполагаемых к увольнению],
        [Measures].[Прирост числа работников, предполагаемых к увольнению],
        [Measures].[Численность работников, уволенных с начала мониторинга],
        [Measures].[Прирост числа работников, уволенных за отчетный период],
        [Measures].[Уровень регистрируемой безработицы ],
        [Measures].[Прирост уровня регистрируемой безработицы],
        [Measures].[Потребность в работниках],
        [Measures].[Прирост потребности в работниках],
        [Measures].[Число зарегистрированных безработных в расчёте на 1 вакансию],
        [Measures].[Прирост числа зарегистрированных безработных в расчёте на 1 вакансию],
        [Measures].[Уровень регистрируемой безработицы РФ],
        [Measures].[Уровень регистрируемой безработицы УрФО],
        [Measures].[Целевое значение по уровню зарегистрированных безработных ],
        Measures.[Численность экономически активного населения ] 
    } on columns
from [СТАТ_Мониторинг ситуации на рынке труда]    
where    
    (
        <%period_current_date%>,
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
    )
  ]]>
</query>

<query id="STAT_0001_0003_tagCloud_data">
<![CDATA[
    --STAT_0001_0003_tagCloud_data
with   
    member Measures.[Уровень безработицы]   
    as 'IIF   
        (
            not isEmpty   
            (
                (
                    <%period_current_date%> ,
                    [Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]   
                )   
            ) and not   
            (
                <%period_current_date%> ,
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]   
            ) = 0,
            (
                <%period_current_date%> ,
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]   
            )/   
            (
                <%period_current_date%> ,
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]   
            )              ,
            null   
        )'   
    member Measures.[Численность экономически активного населения ]   
    as '   
        (
            <%period_current_date%> ,
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]   
        )'    
    member [Measures].[Прирост]  
    as '  
        (
            (
                <%period_current_date%> ,
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]  
            ) -  
            (
                <%period_last_week_date%>,
                [Measures].[Значение],
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]  
            )  
        )'  
    member Measures.[Ранг ]   
    as 'IIF   
        (
            IsEmpty   
            (
                Measures.[Уровень безработицы]   
            ),
            null,
            Rank   
            (
                [Территории__РФ].[Территории__РФ].CurrentMember,
                Order   
                (
                    Filter   
                    (
                        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
                        not   
                        (
                            [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember   
                        )  and not IsEmpty   
                        (
                            Measures.[Уровень безработицы]   
                        )   
                    ),
                    Measures.[Уровень безработицы],
                    BASC   
                ),
                Measures.[Уровень безработицы]   
            )   
        )'   
    set [Все районы]   
    as ' Filter   
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
            not   
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember   
            )   
        )'   
    member Measures.[Худший ранг ]   
    as 'max   
        (
            [Все районы]   ,
            Measures.[Ранг ]   
        )'   
    member Measures.[Численность населения ]   
    as 'LookupCube   
        (
            "[СТАТ_Население_Численность и состав]",
            "   
            (
                [Measures].[За период],
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
                [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Все возрастные группы].[Значение не указано],
                [ОК__Отдельные возрастные группы].[ОК__Отдельные возрастные группы].[Все отдельные группы].[Значение не указано],
                [Половая принадлежность].[Все типы].[Значение не указано],
                [Тип местности].[Все типы местности].[Значение не указано],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[2010],
                " + MemberToStr   
                (
                    [Территории__РФ].[Территории__РФ].CurrentMember   
                ) + "   
            )"   
        )'   
    member Measures.[Краткое наименование ]   
    as '[Территории__РФ].[Территории__РФ].CurrentMember.Properties("Краткое наименование")'   
SELECT
    {
        Measures.[Уровень безработицы],
        Measures.[Численность экономически активного населения ],
        Measures.[Ранг ],
        Measures.[Худший ранг ],
        Measures.[Краткое наименование ],
        Measures.[Численность населения ],
        [Measures].[Прирост] 
    } on columns,
    {[Все районы]} ON rows   
FROM  [СТАТ_Мониторинг ситуации на рынке труда]   
WHERE   
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]   
    )   
  ]]>
  </query>

  <query id="STAT_0001_0012_date">
    <![CDATA[
-- STAT_0001_0012_date
with 
    member [Труд].[Трудовые ресурсы].[Период] 
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName' 
SELECT
    tail
        (Filter
    (
        
            Descendants 
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов],
                [Период].[Год Квартал Месяц].[Данные всех периодов].Level ,
                AFTER 
            ),
            [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 1" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Полугодие 2" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 1" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 2" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 3" and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Квартал 4" and not IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог] 
                ) 
            )
        ) 
    ) ON rows ,
    NON EMPTY 
    {
        [Труд].[Трудовые ресурсы].[Период] 
    } ON COLUMNS 
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE 
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DataMember,
        [Measures].[Нарастающий итог],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
    ) 
]]>
  </query>

  <query id="STAT_0001_0012_text">
    <![CDATA[ 
    -- STAT_0001_0012_text    
    with 
    member [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER 
        ) 
    ' 
    member    [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ] 
    as ' 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
            [Территории].[РФ].[Все территории].[Российская Федерация].DATAMEMBER
            ,
            [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]
        ) 
    ' 
    member [Период].[Год Квартал Месяц].[Предыдущий месяц] 
    as 'IIF 
        (
            <%period_year%>.PrevMember is 
            <%period_year%>.PrevMember.Parent.DataMember,
            <%period_year%>.PrevMember.PrevMember,
            <%period_year%>.PrevMember 
        )' 
    member [Труд].[Трудовые ресурсы].[Численность экономически активного населения ] 
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения]' 
    member [Труд].[Трудовые ресурсы].[Общая численность безработных (на конец месяца) ] 
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]' 
SELECT
    {
        <%period_year%>,
        [Период].[Год Квартал Месяц].[Предыдущий месяц]
        
    } ON COLUMNS ,    
      {  [Труд].[Трудовые ресурсы].[Численность экономически активного населения ],
        [Труд].[Трудовые ресурсы].[Общая численность безработных (на конец месяца) ],
        [Труд].[Трудовые ресурсы].[Все показатели].[Уровень общей безработицы (от экономически активного населения)],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ УрФО ],
        [Труд].[Трудовые ресурсы].[Уровень общей безработицы по методологии МОТ РФ ]   
    } ON ROWS 
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE  
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Measures].[Нарастающий итог] ,
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области]
    )
     ]]>
  </query>

  <query id="STAT_0001_0012_chart3">
    <![CDATA[
-- STAT_0001_0012_chart3
with member [Труд].[Трудовые ресурсы].[Занятое население ] 
    as '
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Численность экономически активного населения]
        ) - 
        (
            [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]
        )' 
    member [Труд].[Трудовые ресурсы].[Безработные по МОТ ]
    as '[Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]' 
SELECT    
       { <%period_year%> }
     ON COLUMNS ,
    NON EMPTY 
    {
    [Труд].[Трудовые ресурсы].[Занятое население ],
        [Труд].[Трудовые ресурсы].[Безработные по МОТ ]
         
    } ON ROWS 
FROM [СТАТ_Труд_Трудовые ресурсы] 
WHERE 
    (
        [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки] ,
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области],
        [Measures].[Нарастающий итог] 
    )
  ]]>
  </query>

  <query id="STAT_0001_0012_debts_mo_date">
    <![CDATA[
-- STAT_0001_0012_debts_mo_date
  with
    member [Measures].[ДанныеНа]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период__Период].[Период__Период].[День].Members,
                not IsEmpty([Measures].[Значение])
            ),
            2
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра],
        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
    )
 ]]>
      </query>

  <query id="STAT_0001_0012_debts_Hmao">
    <![CDATA[
-- STAT_0001_0012_debts_Hmao
  with 
    member [Measures].[Cумма задолженности] 
    as '(
            [Measures].[Значение],
            <%period_current_date_debts%>,
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы] 
        )' 
    member [Measures].[Cумма задолженности прошлая неделя] 
    as ' 
        (
            [Measures].[Значение],
            <%period_last_week_date_debts%>,
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы] 
        )' 
    member Measures.[Количество граждан, имеющих задолженность] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]' 
    member [Measures].[Прирост задолженности] 
    as ' 
        (
            [Measures].[Cумма задолженности] - [Measures].[Cумма задолженности прошлая неделя]
        )'  
    member Measures.[Краткое наименование ] 
    as '[Территории__РФ].[Территории__РФ].CurrentMember.Properties("Краткое наименование")' 
SELECT
    {
        [Measures].[Cумма задолженности],
        [Measures].[Cумма задолженности прошлая неделя],
        Measures.[Количество граждан, имеющих задолженность],
         [Measures].[Прирост задолженности] 
    } on columns,
    {
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    } ON rows 
FROM  [СТАТ_Мониторинг ситуации на рынке труда] 
WHERE 
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры] 
    )
 ]]>
  </query>

  <query id="STAT_0001_0012_debts_mo">
        <![CDATA[
-- STAT_0001_0012_debts_mo
with  
   member Measures.[Задолженность] 
    as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы (млн.руб.)]'    
    member Measures.[Кол-во граждан, перед которыми имеется задолженность ]
     as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]'
    member Measures.[Краткое наименование ]  
    as '[Территории__РФ].[Территории__РФ].CurrentMember.Properties("Краткое наименование")'  
    set [Все районы] 
    as ' Filter 
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
            not 
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember 
            ) and not (isEmpty (Measures.[Задолженность] ))
        )' 
SELECT
    {
        Measures.[Задолженность],
        Measures.[Краткое наименование ],
        Measures.[Кол-во граждан, перед которыми имеется задолженность ]
    } on columns,
    {[Все районы]} ON rows  
FROM  [СТАТ_Мониторинг ситуации на рынке труда]  
WHERE  
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
       ,<%period_current_date_debts%>        
    )  
  ]]>
  </query>

  <query id="STAT_0001_0012_population">
    <![CDATA[
-- STAT_0001_0012_population
  with member [Measures].[За период ]
as '[Measures].[За период] * 1000'
SELECT
    { [Measures].[За период ] } on columns,
    {
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].DataMember
    } ON rows
FROM  [СТАТ_Население_Численность и состав]
WHERE
    (
        [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Все возрастные группы].[Значение не указано],
        [ОК__Отдельные возрастные группы].[ОК__Отдельные возрастные группы].[Все отдельные группы].[Значение не указано],
        [Половая принадлежность].[Все типы].[Значение не указано],
        [Тип местности].[Все типы местности].[Значение не указано],
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[2010]
    )
   ]]>
  </query>

  <query id="STAT_0001_0003_chart4">
    <![CDATA[
-- STAT_0001_0003_chart4
with
    member [Measures].[Сумма задолженности по выплате заработной платы]
    as '(
                    [Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
        )  '
    member [Measures].[DateUniqName]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
select
    {
        [Measures].[DateUniqName],
        [Measures].[Сумма задолженности по выплате заработной платы]
    } on columns,
    non empty
    (
        Filter
        (
            [Период__Период].[Период__Период].[День].AllMembers,
            [Период__Период].[Период__Период].CurrentMember.Name <> "Остатки на начало года" and
            [Период__Период].[Период__Период].CurrentMember.Name <> "Данные квартала" and
            [Период__Период].[Период__Период].CurrentMember.Name <> "Данные полугодия" and
            [Период__Период].[Период__Период].CurrentMember.Name <> "Данные года" and
            [Период__Период].[Период__Период].CurrentMember.Name <> "Данные месяца" and
            not IsEmpty(([Measures].CurrentMember, [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]))
        )
    ) on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    )
  ]]>
  </query>

  <query id="STAT_0001_0003_grid_v">
    <![CDATA[
-- STAT_0001_0003_grid_v
with              
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Уровень безработицы]            
    as 'IIF              
        (
            not isEmpty              
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]              
                )              
            ) and not              
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]              
            ) = 0,
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан]              
            )/              
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]              
            )              ,
            null              
        )'   
    member [Measures].[Значение ]   
    as '  
        (
            [Measures].[Значение],
            <%period_current_date%>   
        )'   
    member [Measures].[Значение прошлый период]  
    as '  
        (
            [Measures].[Значение],
            <%period_last_week_date%>  
        )'  
    member [Measures].[Прирост]  
    as '  
        (
            [Measures].[Значение ] - [Measures].[Значение прошлый период]  
        )'  
    member [Measures].[Темп прироста] 
    as 'IIF 
        (
            not isEmpty 
            (
                (
                    [Measures].[Значение прошлый период] 
                ) 
            ) and not 
            (
                [Measures].[Значение прошлый период] 
            ) = 0,
            ( [Measures].[Значение ] )/ 
            (
                [Measures].[Значение прошлый период] 
            )  - 1            ,
            null 
        )' 
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Коэффициент напряженности]      
    as 'IIF      
        (
            not isEmpty      
            (
                (
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]      
                )      
            ) and not      
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]      
            ) = 0,
            (
                (                    
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан] 
                )     
            )/      
            (
                [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность вакансий]      
            )              ,
            null      
        )'      
    member Measures.[Численность экономически активного населения ]              
    as '              
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность экономически активного населения]              
        )'              
    member [Measures].[Ранг ]
    as '[Measures].[Обратный ранг]'
    member [Measures].[Обратный ранг]
    as 'iif(IsEmpty([Measures].[Прямой ранг]), null, Measures.[Худший ранг ] + 1 - [Measures].[Прямой ранг])'
    member Measures.[Прямой ранг]              
    as 'IIF              
        (
            IsEmpty (  [Measures].[Значение]  ),
            null,
            Rank              
            (
                [Территории__РФ].[Территории__РФ].CurrentMember,
                Order              
                (
                    Filter              
                    (
                        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
                        not              
                        (
                            [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember              
                        )  and not IsEmpty         (  [Measures].[Значение] )         
                    ),
                    ( [Measures].[Значение ] ) ,
                    BASC              
                ),
                ( [Measures].[Значение ] )  
            )              
        )'              
    set [Все районы]              
    as ' Filter              
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].Children,
            not              
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember              
            )              
        )'              
    member Measures.[Худший ранг ]              
    as 'max              
        (
            [Все районы]   ,
            Measures.[Прямой ранг]              
        )'              
    member Measures.[Численность населения ]              
    as 'LookupCube              
        (
            "[СТАТ_Население_Численность и состав]",
            "              
            (
                [Measures].[За период],
                [Население__Численность и состав].[Население__Численность и состав].[Все показатели].[Численность населения на начало года],
                [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
                [ОК__Основные возрастные группы].[ОК__Основные возрастные группы].[Все возрастные группы].[Значение не указано],
                [ОК__Отдельные возрастные группы].[ОК__Отдельные возрастные группы].[Все отдельные группы].[Значение не указано],
                [Половая принадлежность].[Все типы].[Значение не указано],
                [Тип местности].[Все типы местности].[Значение не указано],
                [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Данные всех периодов].[2010],
                " + MemberToStr              
                (
                    [Территории__РФ].[Территории__РФ].CurrentMember              
                ) + "              
            )"              
        )'              
    member Measures.[Краткое наименование ]              
    as '[Территории__РФ].[Территории__РФ].CurrentMember.Properties("Краткое наименование")'              
 member [Показатели__Рынок труда].[Показатели__Рынок труда].[Сумма задолженности по выплате заработной платы ]
     as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]'
     member [Показатели__Рынок труда].[Показатели__Рынок труда].[Кол-во граждан, перед которыми имеется задолженность ]
     as '[Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность ]'
SELECT
    {
        {
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Уровень безработицы],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Численность безработных граждан],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Коэффициент напряженности],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Сумма задолженности по выплате заработной платы ],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Кол-во граждан, перед которыми имеется задолженность ]
        }         
        *        
        {
            [Measures].[Значение ],
            [Measures].[Прирост],
            [Measures].[Темп прироста],
            Measures.[Ранг ],
            Measures.[Худший ранг ]
             
        }        
    } on columns,
    {[Все районы]} ON rows              
FROM  [СТАТ_Мониторинг ситуации на рынке труда]              
WHERE              
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]   
    )              
  ]]>
  </query>
</root>