<?xml version="1.0" encoding="utf-8" ?>
<Extension>
  <Params>
    <Param name="UserRoleEO10" type="string">
      <ValueProvider type="Krista.FM.RIA.Extensions.EO10MissivePRF.UserRoleEO10ValueProvider, Krista.FM.RIA.Extensions.EO10MissivePRF"></ValueProvider>
    </Param>
  </Params>


  <!-- Область навигации -->
  <Navigation type="list" ID="acEO10MissivePRF" icon="Vector" title="Послание ПРФ" order="200" group="Сбор данных" dashboardIcon="/Krista.FM.RIA.Extensions.Tasks/Presentation/Content/images/task-72.png/extention.axd" defaultItemId="nidbeMeausures">
    <Items>
      <Item ID="nidbeMeausures" title="Исполнение мероприятий" icon="ApplicationViewList" condition="$(UserRoleEO10) == 'User'">
        <Params>
          <Param name="url">'/View/Meausures'</Param>
        </Params>
      </Item>
    </Items>
    <Action>MdiTab.addTab({ title: menuItem.text, url: menuItem.url, icon: menuItem.iconCls, passParentSize: menuItem.passParentSize});</Action>
    <ButtomBar></ButtomBar>
  </Navigation>

  <Views>

    <View id="Meausures" title="Отчет по исполнению мероприятий"
          type="Krista.FM.RIA.Core.Gui.XView, Krista.FM.RIA.Core"
          controlType="Krista.FM.RIA.Extensions.EO10MissivePRF.Presentation.Views.MeasuresView, Krista.FM.RIA.Extensions.EO10MissivePRF">
    </View>
  </Views>
</Extension>
