		public static void CustomizeUltraGridParams(UltraGrid grid)
		{
            Infragistics.Win.Appearance mainAppearance = new Infragistics.Win.Appearance();
			Infragistics.Win.Appearance cardAreaAppearance = new Infragistics.Win.Appearance();
			Infragistics.Win.Appearance headerAppearance = new Infragistics.Win.Appearance();
