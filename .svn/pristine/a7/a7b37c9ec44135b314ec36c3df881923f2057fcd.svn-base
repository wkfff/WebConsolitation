		{
			ultraButton.ButtonStyle = UIElementButtonStyle.ButtonSoft;
		}

		public static void CustomizeUltraToolbarsManager(UltraToolbarsManager ultraToolbarsManager)
		{
			ultraToolbarsManager.Style = ToolbarStyle.OfficeXP;
            ultraToolbarsManager.ToolbarSettings.Appearance.BackColor = System.Drawing.SystemColors.Control;
            ultraToolbarsManager.Appearance.BackColor = System.Drawing.SystemColors.Control;
            //ultraToolbarsManager.ToolbarSettings.Appearance.BorderColor = System.Drawing.SystemColors.Control;
            //ultraToolbarsManager.ToolbarSettings.Appearance.BorderColor3DBase = System.Drawing.SystemColors.Control;
            //ultraToolbarsManager.ToolbarSettings.BorderStyleDocked = UIElementBorderStyle.None;

			ultraToolbarsManager.ToolbarSettings.AllowCustomize = DefaultableBoolean.False;

			//ultraToolbarsManager.ToolbarSettings.AllowDockBottom = DefaultableBoolean.False;
			//ultraToolbarsManager.ToolbarSettings.AllowDockLeft = DefaultableBoolean.False;
			//ultraToolbarsManager.ToolbarSettings.AllowDockRight = DefaultableBoolean.False;
			//ultraToolbarsManager.ToolbarSettings.AllowDockTop = DefaultableBoolean.False;
			ultraToolbarsManager.ToolbarSettings.AllowFloating = DefaultableBoolean.False;
			ultraToolbarsManager.ToolbarSettings.AllowHiding = DefaultableBoolean.False;
			ultraToolbarsManager.RuntimeCustomizationOptions = RuntimeCustomizationOptions.None;
			//ultraToolbarsManager.ToolbarSettings.GrabHandleStyle = GrabHandleStyle. None;
		}

		public static void CustomizeInfragisticsControl(Component cmp)
		{
			System.Type controlType = cmp.GetType();
			if (controlType == typeof(Infragistics.Win.UltraWinGrid.UltraGrid))
			{
				CustomizeUltraGridParams((UltraGrid)cmp);
				return;
			}

			if (controlType == typeof(Infragistics.Win.UltraWinTabControl.UltraTabControl))
			{
				CustomizeUltraTabControl((UltraTabControl)cmp);
				return;
			}

			if (controlType == typeof(Infragistics.Win.Misc.UltraButton))
			{
				CustomizeUltraButton((UltraButton)cmp);
				return;
			}

			if (controlType == typeof(System.Windows.Forms.ToolStrip))
			{
				CustomizeToolStrip((ToolStrip)cmp);
				return;
			}

			if (controlType == typeof(Infragistics.Win.UltraWinToolbars.UltraToolbarsManager))
			{
				CustomizeUltraToolbarsManager((UltraToolbarsManager)cmp);
				return;
			}

		}

	}
