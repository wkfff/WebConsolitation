			headerAppearance.BackGradientStyle = Infragistics.Win.GradientStyle.Default;
			headerAppearance.ForeColor = System.Drawing.Color.Black;
			headerAppearance.TextHAlign = Infragistics.Win.HAlign.Left;
            headerAppearance.TextVAlign = Infragistics.Win.VAlign.Middle;
			headerAppearance.ThemedElementAlpha = Infragistics.Win.Alpha.Default;

			rowAppearance.BorderColor = System.Drawing.SystemColors.Control;

			rowSelectorAppearance.BackColor = System.Drawing.SystemColors.Control;
			rowSelectorAppearance.BackColor2 = System.Drawing.SystemColors.Control;
			rowSelectorAppearance.BackGradientStyle = Infragistics.Win.GradientStyle.Default;
			rowSelectorAppearance.ForeColor = System.Drawing.Color.Black;

			grid.DisplayLayout.InterBandSpacing = 5;
			grid.DisplayLayout.Override.RowSelectorWidth = 0;
			grid.DisplayLayout.Override.RowSpacingBefore = -1;
			grid.DisplayLayout.GroupByBox.Appearance.BackColor = System.Drawing.SystemColors.ControlDark;
			grid.DisplayLayout.GroupByBox.Prompt = "Перетащите сюда колонку для группировки";

			grid.DisplayLayout.Appearance = mainAppearance;
			grid.DisplayLayout.Override.CardAreaAppearance = cardAreaAppearance;
			grid.DisplayLayout.Override.HeaderAppearance = headerAppearance;
			grid.DisplayLayout.Override.RowAppearance = rowAppearance;
			grid.DisplayLayout.Override.RowSelectorAppearance = rowSelectorAppearance;
			grid.DisplayLayout.Override.SelectedRowAppearance = selectedRowAppearance;
			grid.DisplayLayout.Override.RowSizing = RowSizing.AutoFixed;
			grid.DisplayLayout.Override.RowSizingArea = RowSizingArea.RowSelectorsOnly;
			grid.DisplayLayout.Override.AllowColSizing = AllowColSizing.Free;
			grid.DisplayLayout.ScrollBounds = ScrollBounds.ScrollToFill;
            grid.DisplayLayout.AutoFitStyle = AutoFitStyle.None;
			grid.DisplayLayout.Override.CellMultiLine = DefaultableBoolean.True;
			grid.DisplayLayout.Override.AllowColSizing = AllowColSizing.Free;
			grid.DisplayLayout.ScrollBounds = ScrollBounds.ScrollToFill;

			grid.DisplayLayout.LoadStyle = LoadStyle.LoadOnDemand;
            if (grid.DisplayLayout.Override.WrapHeaderText != DefaultableBoolean.False)
                grid.DisplayLayout.Override.WrapHeaderText = DefaultableBoolean.True;
            //grid.DisplayLayout.Override.

			// Обработчики для кнопок
			grid.KeyActionMappings.Add(
				new GridKeyActionMapping(
					Keys.Z,
					UltraGridAction.UndoCell,
					0,
					0,
					Infragistics.Win.SpecialKeys.Alt,
					SpecialKeys.Ctrl));

			grid.KeyActionMappings.Add(
				new GridKeyActionMapping(
					Keys.Z,
					UltraGridAction.UndoRow,
					0,
					0,
					Infragistics.Win.SpecialKeys.Alt,
					SpecialKeys.Ctrl)); 

