<root>
<query id="EO_0001_0002_Date">
<![CDATA[
--EO_0001_0001_date_filter
  with
    member [Measures].[Имя]
    as 'IIF
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            [Период__Период].[Период__Период].CurrentMember.Name + " год",
            [Период__Период].[Период__Период].CurrentMember.Name
        )'
    member [Measures].[Уникальное имя]  
    as ' [Период__Период].[Период__Период].CurrentMember.UniqueName  ' 
	
    member [Measures].[Уровень]
    as 'IIF
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            0,
            IIF
            (
                [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Квартал],
                1,
                2
            )
        )'
    set [Периоды]
    as 'Generate
        (
            {
                [Период__Период].[Период__Период].[Данные всех периодов].[2008] : [Период__Период].[Период__Период].[Данные всех периодов].[2020]
            },
            {
                [Период__Период].[Период__Период].CurrentMember
            } + Filter
            (
                Descendants
                (
                    [Период__Период].[Период__Период].CurrentMember,
                    [Период__Период].[Период__Период].[Квартал],
                    SELF_AND_AFTER
                ),
                not
                (
                    [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].CurrentMember.Parent.DataMember
                )and
                (
                  [Период__Период].[Период__Период].CurrentMember.Name <> "Данные года"
                )and
                (
                  [Период__Период].[Период__Период].CurrentMember.Name <> "Данные квартала"
                )and
                (
                  [Период__Период].[Период__Период].CurrentMember.Name <> "Данные полугодия"
                )
            )
        )'

select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень]
    } on columns,
    Filter
    (
        [Периоды],
        not IsEmpty([Measures].[Значение])
    ) on rows
from   [ЭО_Инвестиционная привлекательность_Архангельск]
where
    (
       [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Статистический раздел],
       [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.]
    )

]]>
</query>

	<query id="EO_0001_0002_chart1">
		<![CDATA[
-- EO_0001_0001_chart1 
  WITH  
    set [Список все субъектов ]  
    as 'Filter  
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].Children,
            not  
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember  
            )  
        ) - [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].[Городской округ "Александровск-Сахалинский район"]'  
		
	member measures.[Период ]      
    as 'IIF     
        (
            [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 1" ,
            1,
            IIF    
            (
                [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 2" ,
                2,
                IIF   
                (
                    [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 3" ,
                    3,
                    4 
                )   
            )    
        )'
    member   Measures.[Наименование ]
    as '[Территории__РФ].[Территории__РФ].CurrentMember.Name'
SELECT
    { 
	  Measures.[Наименование ],
      [Measures].[Значение],
	  measures.[Период ]	  
	} ON columns,
    NON EMPTY  
    {
	    {
	    	[Список все субъектов ] + [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].[Городской округ "Александровск-Сахалинский район"]
		}
		*
        {
            <%quarts%> 
        }
    }  ON rows
FROM [ЭО_Инвестиционная привлекательность_Архангельск]  
WHERE  
    (
      [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Интегральный индекс инвестиционной привлекательности]  
    )
]]>
</query>	
	
<query id="EO_0001_0002">
<![CDATA[
-- EO_0001_0001_chart1 
  WITH  
    set [Список все субъектов ]  
    as 'Filter  
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].Children,
            not  
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember  
            )  
        ) - [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].[Городской округ "Александровск-Сахалинский район"]'  
SELECT
    { [Список все субъектов ] + [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].[Городской округ "Александровск-Сахалинский район"] } ON columns,
    NON EMPTY  
    {
        {
            <%quarts%> 
        }     
    }  ON rows  
FROM [ЭО_Инвестиционная привлекательность_Архангельск]  
WHERE  
    (

        [Measures].[Значение],
        [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Интегральный индекс инвестиционной привлекательности]  
    )
]]>
</query>
	
<query id="EO_0001_0002_grid">
    <![CDATA[
-- EO_0001_0001_grid  
WITH  
    set [Список все субъектов ]  
    as 'Filter  
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].Children,
            not  
            (
                [Территории__РФ].[Территории__РФ].CurrentMember is [Территории__РФ].[Территории__РФ].CurrentMember.Parent.DataMember  
            )  
        )'  
     member measures.[Ранг]   
    as 'IIF  
        (
            [Measures].[Значение] = null,
            null,
             Rank  
            (
                [Территории__РФ].[Территории__РФ].CurrentMember,
                Order   
                (
                    [Список все субъектов ],
                    [Measures].[Значение],
                    BASC   
                ),
                [Measures].[Значение]   
            ) 
        )' 	  
	member Measures.[Максимальный ранг]
    as 'max 
        (
            [Список все субъектов ],
            measures.[Ранг]
        )' 
		member  [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс природно-ресурсного потенциала ]
    as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс природно-ресурсного потенциала].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс трудового потенциала ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс трудового потенциала].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс производственного потенциала региона ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс производственного потенциала региона].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс уровня производственного развития региона ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс уровня производственного развития региона].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс экономической активности населения ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс экономической активности населения].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс покупательной способности населения ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс покупательной способности населения].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс состояния муниципальных финансов ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс состояния муниципальных финансов].DATAMEMBER'
	
	member [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс развития рыночной инфраструктуры ]
	as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс развития рыночной инфраструктуры].DATAMEMBER'
	
	member  [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс инфраструктурного потенциала ]
    as '[Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс инфраструктурного потенциала].DATAMEMBER'
	
	
	member  Measures.[Количество субъектов] 
    as 'Count({ [Список все субъектов ]})' 
	
	set [Природно-ресурсный потенцтал]    
    as 'Filter    
        (
            Descendants   
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс природно-ресурсного потенциала],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self  
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0  
        )'     
    set [Трудовой потенциал]  
    as 'Filter  
        (
            Descendants  
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс трудового потенциала],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self  
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0  
        )'  
    set [Производственный потенциал региона] 
    as 'Filter 
        (
            Descendants 
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс производственного потенциала региона],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self 
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0 
        )' 
    set [Уровень производственного развития региона] 
    as 'Filter 
        (
            Descendants 
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс уровня производственного развития региона],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self 
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0 
        )' 
    set [Экономическая активность региона] 
    as 'Filter 
        (
            Descendants 
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс экономической активности населения],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self 
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0 
        )' 
    set [Состояние муниципальных финансов]
    as 'Filter 
        (
            Descendants 
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс состояния муниципальных финансов],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self 
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0 
        )' 
    set [Развитие рыночной инфраструктуры]
    as 'Filter
        (
            Descendants
            (
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Индекс развития рыночной инфраструктуры],
                [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Показатель],
                self
            ),
            not [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember is [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Parent.DataMember and [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Properties("Квартальный весовой коэффициент") <> 0
        )'
    member Measures.[Уровень]
    as 'IIF
        (
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].CurrentMember.Level.Name = "Показатель",
            1,
            1
            
        )'
	
SELECT
    {
        { [Список все субъектов ] }  
        *  { [Measures].[Значение] } + { [Список все субъектов ] } 
        * { [Measures].[Ранг] }+ 
        {
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.] 
        }
        * 
        {
		    Measures.[Уровень],
            Measures.[Количество субъектов],
			Measures.[Максимальный ранг]
        } 
        
    } ON COLUMNS ,
    
    {
       {
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Все].[Интегральный индекс инвестиционной привлекательности],
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс природно-ресурсного потенциала ],
            
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс трудового потенциала ],
            
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс производственного потенциала региона ],
            
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс уровня производственного развития региона ],
            
            [Инвестиционная привлекательность__Архангельск].[Инвестиционная привлекательность__Архангельск].[Индекс экономической активности населения ],
