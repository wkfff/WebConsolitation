<query id="EO_0001_0004_map">
<![CDATA[
-- EO_0001_0001_map  
WITH     
    set [Список все субъектов ]     
    as 'Filter     
        (
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Дальневосточный федеральный округ].[Сахалинская область].[Сахалинская обл.].Children,
            not     
