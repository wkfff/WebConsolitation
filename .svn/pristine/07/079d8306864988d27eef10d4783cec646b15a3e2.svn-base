<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[ -- name: last_date   
SELECT
    { [Measures].[Значение]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[Значение] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        <%baseRegion%>,
        [Источники данных].[Все источники данных],
        [Показатели].[ВедСтат_Свод].[Все показатели].[<%way_last_year%>]
    )  
	  ]]>
  </query>

  <query id="text">
    <![CDATA[ -- name: text
select
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Плоскостные спортивные сооружения (площадки, поля, спортивные ядра)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Спортивные залы],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Плавательные бассейны],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Лыжные базы],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Стрелковые тиры],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Количество спортивных сооружений].[Физкультурно-оздоровительные центры предприятий, учреждений, организаций]
    } on rows,
    non empty 
    {
        <%last_year%>
    } on columns 
FROM [ВЕДСТАТ_Свод] 
where 
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение] 
    )  
    ]]>
  </query>
  
  <query id="grid1">
    <![CDATA[ -- name: grid1
WITH 
    MEMBER [Measures].[Единица измерения] 
    AS  '[Показатели].[ВедСтат_Свод].CurrentMember.Properties("Единица измерения")' 
select
    non empty filter 
    (
        {
          [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].children
        },
        [Measures].[Значение]>0 
    )DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows,
    non empty 
    {
        [Measures].[Значение],
        [Measures].[Единица измерения] 
    } on columns 
FROM [ВЕДСТАТ_Свод] 
where 
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
         <%last_year%>
    )  
    ]]>
  </query>

  <query id="grid2">
    <![CDATA[ -- name: grid2
select
    non empty   
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Мощность спортивных сооружений],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Число коллективов физкультуры, спортивных клубов]
    } on rows,
    non empty   
    {
        <%last_year%>.Lag(9):<%last_year%>
    } on columns   
FROM [ВЕДСТАТ_Свод]   
where   
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]   
    )     
    ]]>
  </query>
  

  <query id="chart1">
    <![CDATA[ -- name: chart1
select
    non empty 
    {
        <%current_way1%>
    } on rows,
    non empty 
    {
        <%last_year%>.Lag(9):<%last_year%>
    } on columns 
FROM [ВЕДСТАТ_Свод] 
where 
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение] 
    )   
    ]]>
  </query>

  <query id="chart2">
    <![CDATA[ -- name: chart2
