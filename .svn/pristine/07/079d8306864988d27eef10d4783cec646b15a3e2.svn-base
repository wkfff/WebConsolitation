        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Мощность спортивных сооружений].children
    } on rows,
    non empty
    {
        [Период].[Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
    } on columns
FROM [ВЕДСТАТ_Свод]
where
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]
    )    
    ]]>
  </query>
 
  <query id="chart3">
    <![CDATA[ -- name: chart3
select
    non empty
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ РАЗВИТИЯ ФИЗИЧЕСКОЙ КУЛЬТУРЫ И СПОРТА].[Число коллективов физкультуры, спортивных клубов].children
    } on rows,
    non empty
    {
        [Период].[Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
    } on columns
FROM [ВЕДСТАТ_Свод]
