<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="STAT_0003_0003_list_of_dates">
    <![CDATA[ -- name: STAT_0003_0003_list_of_dates
select
    {[Measures].[Значение]} on columns,
    {
        filter
        (
            [Период__Период].[Период__Период].[День].members,
            (
                not IsEmpty(([Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
                )) or
                not IsEmpty(([Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
                ))
            )
        )
    } on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
  </query>

  <query id ="STAT_0003_0003_list_of_regions">
    <![CDATA[ -- STAT_0003_0003_list_of_regions
select
    {[Measures].[Значение]} on columns,
    filter
    (
        except
        (
            {
                [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].Children
            },
            {
                [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].DataMember
            }
        ),
            (
                not IsEmpty(([Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
                )) or
                not IsEmpty(([Measures].[Значение],
                    [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
                ))
            )
    ) on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
</query>

<query id = "STAT_0003_0003_grid">
<![CDATA[ -- STAT_0003_0003_grid
with
    member [Measures].[к началу года]
    as '(head
        (
            filter
            (
                descendants
                (
                    [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Parent.Parent,
                    [Период__Период].[Период__Период].[День]
                ),
                not IsEmpty([Measures].[Значение])
            ),
            1
        ).Item(0), [Measures].[Значение])'
    member [Measures].[к предыдущей дате]
    as '(Tail
        (
            Filter
            (
                {
                    [Период__Период].[Период__Период].[Данные всех периодов].[1998].[Полугодие 1].[Квартал 1].[Январь].[1]:[Период__Период].[Период__Период].CurrentMember
                } -
                {
                    [Период__Период].[Период__Период].CurrentMember
                },
                not IsEmpty([Measures].[Значение])
            ),
            1
        ).Item(0), [Measures].[Значение])'
    member [Measures].[Текущая дата]
    as '([Период__Период].[Период__Период].CurrentMember, [Measures].[Значение])'
    set [Территории для ранга]
    as 'except
        (
            {
                descendants
                (
                    [Территории__Сопоставимый].[Территории__Сопоставимый].[РФ].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
                    [Территории__Сопоставимый].[Территории__Сопоставимый].[МР ГО],
                    self
                )
            },
            {
                [Территории__Сопоставимый].[Территории__Сопоставимый].[РФ].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].datamember
            }
        )'
    member [Measures].[Сумма задолженности по выплате заработной платы_Значение]
    as
        (
            [Measures].[Текущая дата],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
        )
    member [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения]
    as
        (
            [Measures].[<%compare_mode%>],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
        )
    member [Measures].[Сумма задолженности по выплате заработной платы_Прирост]
    as 'iif
        (
            IsEmpty
            (
                [Measures].[Сумма задолженности по выплате заработной платы_Значение]
            ) or IsEmpty
            (
                [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения]
            ),
            null,
            [Measures].[Сумма задолженности по выплате заработной платы_Значение] - [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения]
        )'
    member [Measures].[Сумма задолженности по выплате заработной платы_Темп прироста]
    as 'iif
        (
            IsEmpty
            (
                [Measures].[Сумма задолженности по выплате заработной платы_Значение]
            ) or IsEmpty
            (
                [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения]
            ) or [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения] = 0,
            null,
            (
                [Measures].[Сумма задолженности по выплате заработной платы_Значение] / [Measures].[Сумма задолженности по выплате заработной платы_Значение для сравнения] - 1
            )
        )'
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение]
    as
        (
            [Measures].[Текущая дата],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
        )
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения]
    as
        (
            [Measures].[<%compare_mode%>],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
        )
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность_Прирост]
    as 'iif
        (
            IsEmpty
            (
                [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение]
            ) or IsEmpty
            (
                [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения]
            ),
            null,
            [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение] - [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения]
        )'
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность_Темп прироста]
    as 'iif
        (
            IsEmpty
            (
                [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение]
            ) or IsEmpty
            (
                [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения]
            ) or [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения] = 0,
            null,
            (
                [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение] / [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение для сравнения] - 1
            )
        )'
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы, тысяч рублей]
    as 'null'
    member [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность, человек]
    as 'null'
    set [Показатели]
    as '
        {
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы, тысяч рублей],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность, человек]
        }'
    member [Measures].[Значение показателя]
    as 'iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы, тысяч рублей],
       [Measures].[Сумма задолженности по выплате заработной платы_Значение],
       iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность, человек],
       [Measures].[Кол-во граждан, перед которыми имеется задолженность_Значение],
       null
       ))'
    member [Measures].[Темп прироста показателя]
    as 'iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы, тысяч рублей],
       [Measures].[Сумма задолженности по выплате заработной платы_Темп прироста],
       iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность, человек],
       [Measures].[Кол-во граждан, перед которыми имеется задолженность_Темп прироста],
       null
       ))'
    member [Measures].[Прирост показателя]
    as 'iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы, тысяч рублей],
       [Measures].[Сумма задолженности по выплате заработной платы_Прирост],
       iif(
       [Показатели__Рынок труда].[Показатели__Рынок труда].CurrentMember is [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность, человек],
       [Measures].[Кол-во граждан, перед которыми имеется задолженность_Прирост],
       null
       ))'
select
    [Показатели] on columns,
    {{{<%selected_date%>} *
    {{[Measures].[Значение показателя]} + {[Measures].[Темп прироста показателя]} + {[Measures].[Прирост показателя]}}}} *
    {[Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ].[<%selected_region%>]} on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
  </query>

  <query id = "STAT_0003_0003_grid_description">
    <![CDATA[ -- STAT_0003_0003_grid_description
with
    member [Measures].[к началу года]
    as 'head
        (
            filter
            (
                descendants
                (
                    [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Parent.Parent,
                    [Период__Период].[Период__Период].[День]
                ),
                (
                    not IsEmpty(([Measures].[Значение],
                        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
                    )) or
                    not IsEmpty(([Measures].[Значение],
                        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
                    ))
                )
            ),
            1
        ).Item(0).Uniquename'
    member [Measures].[к предыдущей дате]
    as 'Tail
        (
            Filter
            (
                {
                    [Период__Период].[Период__Период].[Данные всех периодов].[1998].[Полугодие 1].[Квартал 1].[Январь].[1]:[Период__Период].[Период__Период].CurrentMember
                } -
                {
                    [Период__Период].[Период__Период].CurrentMember
                },
                (
                    not IsEmpty(([Measures].[Значение],
                        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы]
                    )) or
                    not IsEmpty(([Measures].[Значение],
                        [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность]
                    ))
                )
            ),
            1
        ).Item(0).Uniquename'
    member [Measures].[Текущая дата]
    as '([Период__Период].[Период__Период].CurrentMember.UniqueName)'
select
    {[Measures].[Текущая дата], [Measures].[<%compare_mode%>]} on columns,
    {<%selected_date%>} on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
  </query>
  
  <query id = "STAT_0003_0003_chart1">
    <![CDATA[ -- STAT_0003_0003_chart1
with
    member [Measures].[Сумма задолженности по выплате заработной платы, <%selected_region%>, тысяч рублей]
    as '
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].[<%selected_region%>]
        ) '
    member [Measures].[Сумма задолженности по выплате заработной платы, ХМАО, тысяч рублей]
    as '
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Сумма задолженности по выплате заработной платы],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
        ) '
    member [Measures].[Дата]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
select
    {
        [Measures].[Дата],
        [Measures].[Сумма задолженности по выплате заработной платы, <%selected_region%>, тысяч рублей],
        [Measures].[Сумма задолженности по выплате заработной платы, ХМАО, тысяч рублей]
    } on columns,
    {
        <%selected_date%>
    } on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
  </query>

  <query id = "STAT_0003_0003_chart2">
    <![CDATA[ -- STAT_0003_0003_chart2
with
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность, <%selected_region%>, человек]
    as '
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].[<%selected_region%>]
        ) '
    member [Measures].[Кол-во граждан, перед которыми имеется задолженность, ХМАО, человек]
    as '
        (
            [Measures].[Значение],
            [Показатели__Рынок труда].[Показатели__Рынок труда].[Все].[Кол-во граждан, перед которыми имеется задолженность],
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
        ) '
    member [Measures].[Дата]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
select
    {
        [Measures].[Дата],
        [Measures].[Кол-во граждан, перед которыми имеется задолженность, <%selected_region%>, человек],
        [Measures].[Кол-во граждан, перед которыми имеется задолженность, ХМАО, человек]
    } on columns,
    {
        <%selected_date%>
    } on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ДТиЗ ХМАО-Югры]
    )
    ]]>
  </query>

</root>