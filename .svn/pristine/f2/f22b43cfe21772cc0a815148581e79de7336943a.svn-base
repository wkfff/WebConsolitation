    member [КД].[Сопоставимый].[Земельный налог ]
    as '<%incomes_kd_all_level%>.[<%incomes_kd_root_name%>].[НАЛОГИ НА ИМУЩЕСТВО].[Земельный налог]'   


    member [КД].[Сопоставимый].[Акцизы ]
