<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="years">
    <![CDATA[
--years 
select
    non empty 
    {
        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members 
    }  on rows,
    non empty { [Measures].[За период] }on columns
from   [СТАТ_Экология_Водные ресурсы]
where 
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Группировки__Экология_Водные ресурсы].[Группировки__Экология_Водные ресурсы].[Все группировки].[По городам и районам],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра]
    ) 
]]>
  </query>

  <query id="chart1">
    <![CDATA[
--chart1
with   
    member [Measures].[Ед] 
    as '[Экология__Водные ресурсы].[Экология__Водные ресурсы].CurrentMember.Properties("Единица измерения")'
    member  [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Недобор объема пресной воды до установленного лимита]    
    as ' IIF 
        (
            (
                [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]- [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов]
            )<0,
            -([Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]- [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов])
            ,
            0
        ) ' 
    member [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов сверх лимита]  
    as ' IIF
        (
            (
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]- [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов]
            )>0,
            ([Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]- [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов])
            ,
            0
        ) '
select
    {
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов],
            
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов сверх лимита],
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Недобор объема пресной воды до установленного лимита] 
            
        } 
        *{[Measures].[За период] },
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов]
        }
        *{[Measures].[Ед]}
    }  on columns,
    non empty 
    {
        filter 
        (
            {
                [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].children 
            },
            Right 
            (
                [Территории__РФ].[Территории__РФ].Currentmember.UniqueName,
                10  
            ) <> 'Datamember'  
        )  
    }on rows     
from   [СТАТ_Экология_Водные ресурсы]     
where      
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Группировки__Экология_Водные ресурсы].[Группировки__Экология_Водные ресурсы].[Все группировки].[По городам и районам],
        <%year%> 
    )      

]]>
</query>

<query id="grid">
<![CDATA[
--grid
with 
    member [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Соблюдение] 
    as ' [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]- [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов] ' 
    member  [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Соблюдение1]
    as ' 
        (
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов]/ [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды]
        )*100
         '
         
   member [Measures].[Темп роста]  
    as ' IIF 
        (
            (
                ParallelPeriod 
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember 
                ),
                [Measures].[За период]
            ) <>0 and 
           not isempty (
                
                (
                    ParallelPeriod 
                    (
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                        1,
                        [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember 
                    ),
                    [Measures].[За период]
                )
            ),
            [Measures].[За период]/
            (
                ParallelPeriod 
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год],
                    1,
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].CurrentMember 
                ),
                [Measures].[За период]
            ),
            null  
        )    ' 
select
    {
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Установленный лимит забора пресной воды] 
        } 
        *  
        {
            [Measures].[За период],
            [Measures].[Прирост цепной],
            [Measures].[Темп роста] 
        },
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Объем пресной воды, забранной из природных водных объектов] 
        } 
        * 
        {
            [Measures].[За период],
            [Measures].[Прирост цепной],
           [Measures].[Темп роста] 
        },
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Соблюдение] 
        } 
        * 
        {
            [Measures].[За период]
        },
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Соблюдение1]
        }
        *
        {
            [Measures].[За период]
        },
        {
            [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Потери воды при транспортировке]
        }
        *
        {
            [Measures].[За период]
        }
    }  on columns,
    {
        filter(
        {[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].children},
        Right(

                [Территории__РФ].[Территории__РФ].Currentmember.UniqueName,
                10
            ) <> 'Datamember'
        )
    }on rows   
from   [СТАТ_Экология_Водные ресурсы]   
where    
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Группировки__Экология_Водные ресурсы].[Группировки__Экология_Водные ресурсы].[Все группировки].[По городам и районам],
        <%year%>
    ) 
]]>
  </query>

  <query id="chart2">
    <![CDATA[
--chart2
with member [Measures].[Ед]
as '[Экология__Водные ресурсы].[Экология__Водные ресурсы].CurrentMember.Properties("Единица измерения")'
select
    {
        [Measures].[За период],
        [Measures].[Ед]
    }  on columns,
    non empty  
    {
        order
        (
            {
                filter 
                (
                    {
                        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].children 
                    },
                    Right 
                    (
                        [Территории__РФ].[Территории__РФ].Currentmember.UniqueName,
                        10 
                    ) <> 'Datamember' 
                )  
            },
            ([Measures].[За период] ),
            desc
        )  
    }on rows      
from   [СТАТ_Экология_Водные ресурсы]      
where       
    (
        [Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        [Группировки__Экология_Водные ресурсы].[Группировки__Экология_Водные ресурсы].[Все группировки].[По городам и районам],
        <%year%>,
        [Экология__Водные ресурсы].[Экология__Водные ресурсы].[Все показатели].[Потери воды при транспортировке]
         
    )     
]]>
  </query>
</root>