<RegionSettings>
  <!--
  // имя папки с картами региона
  MapFolderName,  
  // загрузка карты из бинарика
  MapLoadFromBin,
  // пропорция карты
  MapSizeProportion,
  // масштаб карты
  MapZoomValue,
  // сдвиг по вертикали текста городов-выносок карты
  MapCalloutOffsetY  
  -->
  <Region id="Novosib">
    <MapFolderName>Новособл</MapFolderName>
    <MapLoadFromBin>false</MapLoadFromBin>
    <MapSizeProportion>0,7</MapSizeProportion>
    <MapZoomValue>90</MapZoomValue>
    <MapCalloutOffsetY>-15</MapCalloutOffsetY>
  </Region>
</RegionSettings>
