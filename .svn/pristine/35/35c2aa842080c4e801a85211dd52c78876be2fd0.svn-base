.bujetHeader a
{
    text-decoration: none;
    font-family: Arial;
    font-size: 18px;
    line-height: 18px;
    margin: 0px;
    padding: 0px;
    font-weight: lighter;
    color: rgb(60,60,255);
}

.bujetHeader a:visited
{
    color: rgb(170,170,255);
    text-decoration: underline;
}

.bujetHeader a:hover
{
    text-decoration: underline;
}

.bujetText
{
    font-family: Arial;
    line-height: 16px;
    height: 100%;
    font-size: 12px;
    color: rgb(85, 85, 85);
}
