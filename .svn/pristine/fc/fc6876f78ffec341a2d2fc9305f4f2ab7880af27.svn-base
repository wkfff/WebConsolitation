<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[ -- name: last_date   
SELECT
    { [Measures].[Значение]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[Значение] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        <%baseRegion%>,
        [Источники данных].[Все источники данных],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА]
    )
	  ]]>
  </query>
  <query id="grid">
    <![CDATA[ -- name: grid

select
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Число действующих предприятий на территории муниципального образования],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Отгружено товаров и услуг (без внутреннего оборота) в фактических ценах (без НДС и акциза)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Отгружено товаров и услуг (без внутреннего оборота) в фактических ценах (без НДС и акциза) (мун. предприятиями)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Среднесписочная численность работающих на предприятиях производственной и непроизводственной сферы],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Среднесписочная численность работающих на муниципальных предприятиях производственной и непроизводственной сферы]  
    } DIMENSION PROPERTIES [Показатели].[ВедСтат_Свод].[Единица измерения] on rows,
    non empty 
    {
        <%last_year%>:<%last_year%>.Lag(9)  
    } on columns 
FROM [ВЕДСТАТ_Свод] 
where 
    (
        <%baseRegion%>,
        [Measures].[Значение] 
    ) 
    ]]>
  </query>

  <query id="grid1">
    <![CDATA[ -- name: grid1
select
    non empty     
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[ПРОИЗВОДСТВЕННАЯ ДЕЯТЕЛЬНОСТЬ И СОЦИАЛЬНАЯ ИНФРАСТРУКТУРА].[Число предприятий (по отраслям)].children 
    } on rows,
    non empty  { [Measures].[Значение] } on columns  
FROM [ВЕДСТАТ_Свод]  
where  
    (
        <%baseRegion%>,
        [Период].[Год Квартал Месяц].[Данные всех периодов].[<%selected_year%>]
    )  
    ]]>
  </query>

