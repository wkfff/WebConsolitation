<!--<PresetInfo PresetName="BasicHorizontal09.xml" PresetDescription="Custom Preset Created 08.09.2011 15:51:00" PresetType="All" />-->
<igGauge:UltraGauge BackColor="Transparent">
  <Gauges>
    <igGaugeProp:LinearGauge CornerExtent="10">
      <Scales>
        <igGaugeProp:LinearGaugeScale EndExtent="90" StartExtent="25">
          <MajorTickmarks StartWidth="2" EndWidth="2" Frequency="0" StartExtent="40" EndExtent="60">
            <StrokeElement Color="DimGray" />
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="White" />
            </BrushElements>
          </MajorTickmarks>
          <MinorTickmarks Frequency="0.2" StartExtent="45" EndExtent="55">
            <StrokeElement Color="135, 135, 135" />
          </MinorTickmarks>
          <Labels ZPosition="AboveMarkers" Extent="25" Font="Segoe UI, 10pt">
            <Shadow Depth="2">
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement />
              </BrushElements>
            </Shadow>
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="White" />
            </BrushElements>
          </Labels>
          <Markers>
            <igGaugeProp:LinearGaugeBarMarker SegmentSpan="1" StartExtent="-15" BulbSpan="35" Value="58">
              <Response />
              <BrushElements>
                <igGaugeProp:SimpleGradientBrushElement GradientStyle="Horizontal" StartColor="200, 255, 0, 0" EndColor="200, 178, 34, 34" />
              </BrushElements>
            </igGaugeProp:LinearGaugeBarMarker>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="100" TickmarkInterval="10" />
          </Axes>
        </igGaugeProp:LinearGaugeScale>
        <igGaugeProp:LinearGaugeScale InnerExtent="50" OuterExtent="95" EndExtent="98" StartExtent="2">
          <BrushElements>
            <igGaugeProp:SimpleGradientBrushElement StartColor="100, 255, 255, 255" EndColor="20, 255, 255, 255" />
          </BrushElements>
        </igGaugeProp:LinearGaugeScale>
      </Scales>
      <BrushElements>
        <igGaugeProp:SolidFillBrushElement Color="15, 15, 15" />
      </BrushElements>
      <StrokeElement Thickness="0" />
    </igGaugeProp:LinearGauge>
  </Gauges>
  <Annotations>
    <igGaugeProp:BoxAnnotation Bounds="0, 10, 100, 0" BoundsMeasure="Percent">
      <Label FormatString="" Font="Arial, 16pt, style=Bold">
        <BrushElements>
          <igGaugeProp:SolidFillBrushElement Color="240, 240, 240" />
        </BrushElements>
      </Label>
    </igGaugeProp:BoxAnnotation>
  </Annotations>
</igGauge:UltraGauge>