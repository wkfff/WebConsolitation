<?xml version="1.0" encoding="windows-1251"?>
<root>  <query id="Ge">
    <![CDATA[
    --Ge
    with
    member [Показатели].[Рынок труда].[Все].[Уровень зарегистрированной безработицы, процент]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность экономически активного населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность безработных граждан]
                )
            )
            *100
        )'
    member [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность вакансий]
                )
            )            
        )'
member [Показатели].[Рынок труда].[Все].[Дефицит (-), профицит (+) рабочих мест]
    as '
        (
                    [Показатели].[Рынок труда].[Все].[Численность вакансий]-[Показатели].[Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
        )'
    member [Measures].[3]
    as '[Территории].[РФ].CurrentMember.Properties("Тип территории")'
    
        member [Территории].[РФ].[ы]
    as '[Показатели].[Рынок труда].CurrentMember.Properties("Единицы измерения")'
    
select
   non empty {
   [Территории].[РФ].[ы]   
    } on rows,
    non empty
    {
        
        
                    [Показатели].[Рынок труда].[Все].[Численность экономически активного населения],
                    [Показатели].[Рынок труда].[Все].[Численность безработных граждан],
                    [Показатели].[Рынок труда].[Все].[Уровень зарегистрированной безработицы, процент],
                    [Показатели].[Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения],
                    [Показатели].[Рынок труда].[Все].[Численность вакансий],
                    [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек],
                    [Показатели].[Рынок труда].[Все].[Дефицит (-), профицит (+) рабочих мест]
                
        
        
    } on columns
from [СТАТ_Мониторинг ситуации на рынке труда]
where ([Measures].[Значение])]]>
  </query> 
  <query id="G">
    <![CDATA[
    --G
    with
    member [Показатели].[Рынок труда].[Все].[Уровень зарегистрированной безработицы, процент]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность безработных граждан]
                    
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность экономически активного населения]
                )
            )
            *100
        )'
    member [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек]
  /*  as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность вакансий]
                )
            )             
        )'*/
            
    as 'iif([Показатели].[Рынок труда].[Показатели].[Численность вакансий]<>0,
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность вакансий]
                )
            )
        ),null)'
member [Показатели].[Рынок труда].[Все].[Дефицит (-), профицит (+) рабочих мест]
    as '
        (
                    [Показатели].[Рынок труда].[Все].[Численность вакансий]-[Показатели].[Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
        )'
member [Measures].[3]
    as '[Территории].[РФ].CurrentMember.Properties("Тип территории")'    
member [Территории].[РФ].[ы]
    as '[Показатели].[Рынок труда].CurrentMember.Properties("Единицы измерения")'
    
select
   non empty {
		<%p2%>,
        order(<%p2%>.children, [Measures].[Значение],desc)
    } on rows,
    --non empty
    {
        
            Crossjoin
            (
                {
                    [Показатели].[Рынок труда].[Все].[Численность экономически активного населения],
                    [Показатели].[Рынок труда].[Все].[Численность безработных граждан],
                    [Показатели].[Рынок труда].[Все].[Уровень зарегистрированной безработицы, процент],
                    [Показатели].[Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения],
                    [Показатели].[Рынок труда].[Все].[Численность вакансий],
                    [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек],
                    [Показатели].[Рынок труда].[Все].[Дефицит (-), профицит (+) рабочих мест]
                }   ,
                {<%p1%>}               
                
            )
            
        
    } on columns
from [СТАТ_Мониторинг ситуации на рынке труда]
where ([Measures].[Значение])]]>
  </query> 
  
  <query id="LC">
    <![CDATA[
    with  
    member [Показатели].[Рынок труда].[ydal]
    as '<%p5%>.CurrentMember.Properties("Краткое наименование")'
    select
    {
        order(<%p4%>.children , [Measures].[Значение],desc) 
    } on rows,
    non empty 
    {[Показатели].[Рынок труда].[ydal],
		[Показатели].[Рынок труда].[Показатели].[Численность экономически активного населения]
    } on columns
from [СТАТ_Мониторинг ситуации на рынке труда] 
where ([Measures].[Значение],<%p1%>) ]]>
  </query>  
