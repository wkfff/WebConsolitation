  <query id="BC0">
    <![CDATA[
    ---BC0
    select
    non empty{
        Except
        (
            <%p2%>.children ,
            {<%p2%>.datamember}
        )
    } on rows,
    non empty 
    {
      <%p1%>:<%p1%>.lag(200)
    } on columns
from [СТАТ_Мониторинг ситуации на рынке труда] 
where ([Measures].[Значение],
        [Показатели].[Рынок труда].[Все].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения])]]>
  </query> 
