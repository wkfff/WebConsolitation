    {
        [Показатели].[Рынок труда].[Все].[Уровень зарегистрированной безработицы, процент],
        [Показатели].[Рынок труда].[ydal],
        [Показатели].[Рынок труда].[ydal1],
        [Показатели].[Рынок труда].[Показатели].[Численность безработных граждан]
    }  on columns
