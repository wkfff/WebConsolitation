    as 'IIF
       (
         (Measures.[Значение недоимки]) > 0,
          Measures.[Значение недоимки],
          null
       )'
    member Measures.[Снижение недоимки]
    as 'IIF
       (
         (Measures.[Значение недоимки]) < 0,
          Measures.[Значение недоимки],
          null
       )'
