        Measures.[Прирост недоимки],
        Measures.[Снижение недоимки]
    } on columns,
    non empty
    {
      [<%chart1_region_set%>]
    } on rows
from [ФНС_28н_без расщепления]
where 
(
 <%incomes_kd_dimension%>.[<%fns_kd_group%>]
) 
