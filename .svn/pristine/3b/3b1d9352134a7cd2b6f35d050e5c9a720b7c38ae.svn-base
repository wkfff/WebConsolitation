<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FO_0003_0005_Horizontal_date">
    <![CDATA[
-- FO_0003_0005_Horizontal_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin
            (
                {
                    Filter
                    (
                        [Период].[Период].Members,
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года"
                    )
                }
            )
        )
    } on rows
from  [ФК_Паспорт субъекта]
  ]]>
  </query>

  <query id="FO_0003_0005_Horizontal_incomes">
    <![CDATA[
-- FO_0003_0005_Horizontal_incomes  
WITH                
    member [Measures].[Исполнение консолидированного бюджета ] 
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Исполнение консолидированного бюджета], [Measures].[Исполнение консолидированного бюджета] / 1000)' 
    member [Measures].[Исполнение бюджета субъекта ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Исполнение бюджета субъекта], [Measures].[Исполнение бюджета субъекта] / 1000)' 
    member [Measures].[Первоначальный план ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Первоначальный план], [Measures].[Первоначальный план] / 1000)' 
    member [Measures].[Уточненный план консолидированного бюджета ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Уточненный план консолидированного бюджета], [Measures].[Уточненный план консолидированного бюджета] / 1000)' 
    member [Measures].[Уточненный план бюджета субъекта ] 
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Уточненный план бюджета субъекта], [Measures].[Уточненный план бюджета субъекта] / 1000)' 
    member [Measures].[Оценка ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Оценка], [Measures].[Оценка] / 1000)' 
    member [Measures].[Уровень]
    as '[Показатели].[Паспорт субъекта].Properties("Примечание")'
    member [Measures].[Темп к прошлому году ]
    as 'IIF ([Measures].[Темп к прошлому году] = 0, null, [Measures].[Темп к прошлому году])'
    member [Measures].[Темп уточненного плана к исполнению прошлого года ]
    as  'IIF ([Measures].[Темп уточненного плана к исполнению прошлого года] = 0, null, [Measures].[Темп уточненного плана к исполнению прошлого года])' 
    member [Measures].[Темп оценки к исполнению прошлого года ]
    as 'IIF ([Measures].[Темп оценки к исполнению прошлого года] = 0, null, [Measures].[Темп оценки к исполнению прошлого года])' 
    member [Measures].[Исполнение к плану ]
    as 'IIF ([Measures].[Исполнение к плану] = 0, null, [Measures].[Исполнение к плану])' 
    member [Measures].[Темп к прошлому году ФО ] 
    as '
        (
            [Measures].[Темп к прошлому году ],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].DataMember
        )' 
    member [Measures].[Темп к прошлому году РФ ]
    as '
        (
            [Measures].[Темп к прошлому году ],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember
        )'
SELECT
    {   
    {
            {
                [Период].[Период].[Данные всех периодов].[2009]    
            }     
            *     
            {
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ]
            }    
        } +  
        {
            {                
                [Период].[Период].[Данные всех периодов].[2010]     
            }     
            *     
            {
                [Measures].[Первоначальный план ],
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ]
            }    
        } +     
        {
            {
                [Период].[Период].[Данные всех периодов].[2011]     
            }       
            *           
            {
                [Measures].[Первоначальный план ],
                [Measures].[Уточненный план консолидированного бюджета ],
                [Measures].[Уточненный план бюджета субъекта ],
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ],
                [Measures].[Темп к прошлому году ],
                [Measures].[Оценка ],
                [Measures].[Уровень]
            }          
        }   
    } on columns,
    {        
        [Показатели].[Паспорт субъекта].[Все].[Профицит (+) / дефицит (-)],
        [Показатели].[Паспорт субъекта].[Все].[Итого источников финансирования дефицита бюджета],
        [Показатели].[Паспорт субъекта].[Все].[Долговые обязательства и ценные бумаги],
        [Показатели].[Паспорт субъекта].[Все].[Бюджетные кредиты, полученные от других бюджетов],
        [Показатели].[Паспорт субъекта].[Все].[получение бюджетных кредитов],
        [Показатели].[Паспорт субъекта].[Все].[погашение бюджетных кредитов],
        [Показатели].[Паспорт субъекта].[Все].[Кредиты, полученные от кредитных организаций],
        [Показатели].[Паспорт субъекта].[Все].[получение кредитов от кредитных организаций],
        [Показатели].[Паспорт субъекта].[Все].[погашение кредитов от кредитных организаций],
        [Показатели].[Паспорт субъекта].[Все].[Погашение государственных и муниципальных гарантий],
        [Показатели].[Паспорт субъекта].[Все].[Акции и иные формы участия в капитале],
        [Показатели].[Паспорт субъекта].[Все].[Изменение остатков средств бюджетов],
        [Показатели].[Паспорт субъекта].[Все].[Остатки средств бюджетов],
        [Показатели].[Паспорт субъекта].[Все].[Остатки целевых средств, поступивших из федерального бюджета],
        [Показатели].[Паспорт субъекта].[Все].[Остатки нецелевых средств],
        [Показатели].[Паспорт субъекта].[Все].[Недостаток средств на финансовое обеспечение расходов (с учетом источников финансирования)]
    }on rows   
FROM [ФК_Паспорт субъекта]           
WHERE           
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]          
    )           
  ]]>
  </query>

    <query id="FO_0003_0005_Horizontal_chart">
        <![CDATA[
-- FO_0003_0005_Horizontal_chart 
with         
    member [Показатели].[Паспорт субъекта].[Социально-значимые расходы]
    as 'IIF
        (
            not isEmpty
            (
                [Показатели].[Паспорт субъекта].[Всего]
            ) and not
            (
                [Показатели].[Паспорт субъекта].[Всего] = 0
            ),
            [Показатели].[Паспорт субъекта].[Все].[Раздел 1. Социально-значимые расходы] / [Показатели].[Паспорт субъекта].[Всего] * 100,
            0
        )'
    member [Показатели].[Паспорт субъекта].[Первоочередные расходы]
    as 'IIF
        (
            not isEmpty
            (
                [Показатели].[Паспорт субъекта].[Всего]
            ) and not
            (
                [Показатели].[Паспорт субъекта].[Всего] = 0
            ),
            [Показатели].[Паспорт субъекта].[Все].[Раздел 2. Первоочередные расходы] / [Показатели].[Паспорт субъекта].[Всего] * 100,
            0
        )'
    member [Показатели].[Паспорт субъекта].[Расходы]
    as 'IIF
        (
            not isEmpty
            (
                [Показатели].[Паспорт субъекта].[Всего]
            ) and not
            (
                [Показатели].[Паспорт субъекта].[Всего] = 0
            ),
            [Показатели].[Паспорт субъекта].[Все].[Раздел 3.Расходы] / [Показатели].[Паспорт субъекта].[Всего] * 100,
            0
        )'
    member [Показатели].[Паспорт субъекта].[Всего]       
    as 'sum
        (
            {
                [Показатели].[Паспорт субъекта].[Все].[Раздел 1. Социально-значимые расходы],
                [Показатели].[Паспорт субъекта].[Все].[Раздел 2. Первоочередные расходы],
                [Показатели].[Паспорт субъекта].[Все].[Раздел 3.Расходы]
            } 
        )'        
select
    {
        [Период].[Период].[Данные всех периодов].[2009],
        [Период].[Период].[Данные всех периодов].[2010],
        [Период].[Период].[Данные всех периодов].[2011].[Полугодие 1].[Квартал 1].[Март]       
    }on columns,
    {
        [Показатели].[Паспорт субъекта].[Социально-значимые расходы],
        [Показатели].[Паспорт субъекта].[Первоочередные расходы],
        [Показатели].[Паспорт субъекта].[Расходы]
    } on rows         
from  [ФК_Паспорт субъекта]         
where       
    (
        [Measures].[Первоначальный план],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]         
    )  
    ]]>  
    </query>

