                [КИФ__Сопоставимый].[КИФ__Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации],
                " + MemberToStr     
                (
                    [Период__Период].[Период__Период].CurrentMember     
                ) + ",
                " + MemberToStr
                (
                    [Measures].CurrentMember
                ) + "                
            )"     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов]     
    as 'LookupCube     
        (
            "[ФО_МесОтч_ИсточникВнутрФинансирования]",
            "     
            (
                [Measures].[Факт],
                [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
                [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
                [КИФ__Сопоставимый].[КИФ__Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации],
                " + MemberToStr     
                (
                    [Период__Период].[Период__Период].CurrentMember     
                ) + "     
            )"     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов]     
    as '     
        (
            [Measures].CurrentMember,
            [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
            [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
            [КД__Сопоставимый].[КД__Сопоставимый].[Все коды доходов].[Доходы бюджета - Всего],
            [Период__Период].[Период__Период].CurrentMember     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
    as '     
        (
            [Measures].CurrentMember,
            [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
            [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
            [КД__Сопоставимый].[КД__Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ],
            [Период__Период].[Период__Период].CurrentMember     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем гос.долга]     
    as '     
        (
            [Measures].[Факт],
            [Период__Период].[Период__Период].CurrentMember,
            [КД__Сопоставимый].[КД__Сопоставимый].[Внутренний долг]     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Расходы на обслуживание муниципального долга]     
    as 'LookupCube     
        (
            "[ФО_МесОтч_Расходы]",
            "     
            (
                [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
                [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
                [РзПр__Сопоставимый].[РзПр__Сопоставимый].[Все коды ФКР].[ОБСЛУЖИВАНИЕ ГОСУДАРСТВЕННОГО И МУНИЦИПАЛЬНОГО ДОЛГА],
                " + MemberToStr     
                (
                    [Период__Период].[Период__Период].CurrentMember     
                ) + ",
                " + MemberToStr
                (
                    [Measures].CurrentMember
                ) + "                
            )"     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО]     
    as 'LookupCube     
        (
            "[ФО_МесОтч_Расходы]",
            "     
            (
                [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
                [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
                [РзПр__Сопоставимый].[РзПр__Сопоставимый].[Все коды ФКР].[Расходы бюджета - ИТОГО],
                " + MemberToStr     
                (
                    [Период__Период].[Период__Период].CurrentMember     
                ) + ",
                " + MemberToStr
                (
                    [Measures].CurrentMember
                ) + "
            )"     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]     
    as '     
        (
            [Measures].CurrentMember,
            [ТипДокумента__СКИФ].[ТипДокумента__СКИФ].[Все].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов],
            [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет субъекта],
            [КД__Сопоставимый].[КД__Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ ОТ ДРУГИХ БЮДЖЕТОВ БЮДЖЕТНОЙ СИСТЕМЫ РОССИЙСКОЙ ФЕДЕРАЦИИ].[Субвенции бюджетам субъектов Российской Федерации и муниципальных образований],
            [Период__Период].[Период__Период].CurrentMember     
        )'  
    ------------------------------------------ 
    ---Предельные величины     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля]     
    as 'IIF     
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] < 0 and [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > [КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита],
            IIF     
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > 0 and [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] > 0,
                (
                    -[КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] - [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] - [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов]     
                ) /     
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                ),
                IIF     
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > 0,
                    (
                        -[КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] - [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] - [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств]     
                    ) /     
                    (
                        [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                    ),
                    IIF     
                    (
                        [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] > 0,
                        (
                            -[КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] - [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов]     
                        ) /     
                        (
                            [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                        ),
                        (
                            -[КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] - [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов]     
                        ) /     
                        (
                            [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                        )     
                    )     
                )     
            ),
            0     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля полож]     
    as 'IIF     
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля] < 0,
            0,
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля]     
        )'
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_нарушение]
    as 'IIF
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля полож] > 0.15,
            "нарушение",
            null
        )'
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_доля]     
    as 'IIF     
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов] > 0,
            IIF     
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем гос.долга] - [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов]     
                ) /     
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                ),
                0     
            ),
            IIF     
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем гос.долга]     
                ) /     
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]     
                ),
                0     
            )     
        )'     
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_нарушение]
    as 'IIF
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_доля] > 1,
            "нарушение",
            null
        )'
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_доля]     
    as 'IIF     
        (
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]     
            ) <> 0,
            [КД__Сопоставимый].[КД__Сопоставимый].[Расходы на обслуживание муниципального долга] /     
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]     
            ),
            0     
        )'
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_нарушение]
    as 'IIF
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_доля] > 0.15,
            "нарушение",
            null
        )'
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг]     
    as 'IIF     
        (
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]     
            ) <> 0,
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]     
            )     
            * 0.15,
            0     
        )' 
    member [КД__Сопоставимый].[КД__Сопоставимый].[PeriodUniqueName]     
    as '
            IIF 
            (
                [Период__Период].[Период__Период].CurrentMember.Level.Name = "Год",
                Tail 
                (
                    Filter 
                    (
                        Descendants 
                        (
                            [Период__Период].[Период__Период].CurrentMember,
                            [Период__Период].[Период__Период].[Месяц],
                            SELF 
                        ),
                        not IsEmpty 
                        (
                            (
                                [КД__Сопоставимый].[КД__Сопоставимый].[Дефицит (профицит) +/-],
                                [Measures].[Факт] 
                            ) 
                        ) 
                    ) 
                ).Item(0).UniqueName,
                [Период__Период].[Период__Период].CurrentMember.UniqueName
            )
        '
    -----------------------  
    member [Measures].[Мера для предела] 
    as 'IIF 
        (
            [Период__Период].[Период__Период].CurrentMember.Name = "Декабрь",
            [Measures].[Факт],
            [Measures].[Годовые назначения]
        )'     
SELECT
    {
        {
            {
                [КД__Сопоставимый].[КД__Сопоставимый].[Дефицит (профицит) +/-],
                [КД__Сопоставимый].[КД__Сопоставимый].[Внутренний долг],
                [КД__Сопоставимый].[КД__Сопоставимый].[Изменение остатков средств на счетах по учету средств бюджета],
                [КД__Сопоставимый].[КД__Сопоставимый].[Расходы на обслуживание государственного внутреннего долга],
                [КД__Сопоставимый].[КД__Сопоставимый].[Расходы на оплату услуг агентов по осуществлению ими функций, предусмотренных агентскими соглашениями, заключенными с целью осуществления государственных внутренних заимствований]  
            }  
            *  
            {
                [Measures].[Годовые назначения],
                [Measures].[Факт]  
            } 
        } + 
        {
            {
                [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_нарушение],
                [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_нарушение],
                [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_нарушение],
                [КД__Сопоставимый].[КД__Сопоставимый].[PeriodUniqueName]
            }
            *
            {
                [Measures].[Мера для предела]
            }
        } 
    } on columns,
    non empty( [Периоды] ) on rows     
FROM [ФО_МесОтч_Доходы]      
  ]]>
</query>
  
