    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_нарушение]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля полож] > 0.15,
            "нарушение",
            null   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита] < 0 and [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > [КД__Сопоставимый].[КД__Сопоставимый].[Объем дефицита],
            IIF   
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > 0 and [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] > 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] + [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] + [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] + [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                )   
                * 0.15,
                IIF   
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] > 0,
                    (
                        [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] + [КД__Сопоставимый].[КД__Сопоставимый].[Снижение остатков средств] + [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                    )   
                    * 0.15,
                    IIF   
                    (
                        [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] > 0,
                        (
                            [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] + [КД__Сопоставимый].[КД__Сопоставимый].[Объем бюджетных кредитов] + [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                        )   
                        * 0.15,
                        (
                            [КД__Сопоставимый].[КД__Сопоставимый].[Доходы от продаж акций] + [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                        )   
                        * 0.15   
                    )   
                )   
            ),
            0   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_доля]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов] > 0,
            IIF   
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем гос.долга] - [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов]   
                ) /   
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ),
                0   
            ),
            IIF   
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем гос.долга]   
                ) /   
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ),
                0   
            )   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_нарушение]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_доля] > 1,
            "нарушение",
            null   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов] > 0,
            IIF   
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ] + [КД__Сопоставимый].[КД__Сопоставимый].[Фактический объем бюджетных кредитов]   
                ),
                0   
            ),
            IIF   
            (
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ) <> 0,
                (
                    [КД__Сопоставимый].[КД__Сопоставимый].[Всего доходов] - [КД__Сопоставимый].[КД__Сопоставимый].[Безвозмезные поступления ]   
                ),
                0   
            )   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_доля]   
    as 'IIF   
        (
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]   
            ) <> 0,
            [КД__Сопоставимый].[КД__Сопоставимый].[Расходы на обслуживание муниципального долга] /   
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]   
            ),
            0   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_нарушение]   
    as 'IIF   
        (
            [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_доля] > 0.15,
            "нарушение",
            null   
        )'   
    member [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг]   
    as 'IIF   
        (
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]   
            ) <> 0,
            (
                [КД__Сопоставимый].[КД__Сопоставимый].[Всего расходов МО] - [КД__Сопоставимый].[КД__Сопоставимый].[Объем субвенций]   
            )   
            * 0.15,
            0   
        )'  ----------------------- 
    member [Measures].[Мера для предела] 
    as 'IIF 
        (
            [Период__Период].[Период__Период].CurrentMember.Name = "Декабрь",
            [Measures].[Факт],
            [Measures].[Годовые назначения]
        )'         
SELECT
    {
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит],
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельный дефицит_доля полож],
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг],
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельный долг_доля],
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг],
        [КД__Сопоставимый].[КД__Сопоставимый].[Предельные расходы на долг_доля]
    } on columns   
FROM [ФО_МесОтч_Доходы]   
WHERE   
    (
        <%selected_period%>,
        [Measures].[Мера для предела]
    )        
  ]]>
</query>

