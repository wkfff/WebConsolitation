<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FO_0003_0008_date">
    <![CDATA[
-- FO_0003_0008_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin
            (
                {
                    Filter
                    (
                        [Период].[Период].Members,
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года"
                    )
                }
            )
        )
    } on rows
from  [ФК_Паспорт субъекта]
  ]]>
  </query>

    <query id="FO_0003_0008_date2">
    <![CDATA[
-- FO_0003_0008_date
with
    member Measures.[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
    member Measures.[PKID]
    as '[Период].[Период].CurrentMember.Properties("PKID") > [Период].[Период].[Данные всех периодов].[<%period_year%>].Properties("PKID")'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            NonEmptyCrossjoin
            (
                {
                    Filter
                    (
                        Descendants
                        (
                            [Период].[Период].[Данные всех периодов],
                            [Период].[Период].[Месяц],
                            SELF
                        ),
                        [Период].[Период].CurrentMember.Name <> "Остатки на начало года" and
                        [Период].[Период].CurrentMember.Name <> "Данные месяца" and
                        [Период].[Период].CurrentMember.Name <> "Данные квартала" and
                        [Период].[Период].CurrentMember.Name <> "Данные полугодия" and
                        [Период].[Период].CurrentMember.Name <> "Данные года" and
                        (
                            not isEmpty
                            (
                                (
                                    [Measures].[Значение],
                                    [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в федеральный бюджет]
                                )
                            ) or not isEmpty
                            (
                                (
                                    [Measures].[Значение],
                                    [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в консолидированный бюджет региона]
                                )
                            )
                        ) and
                        Measures.[ДанныеНа]
                    )
                }
            )
        )
    } on rows
from  [ФК_Паспорт субъекта]
where  
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]    
    ) 
  ]]>
    </query>

  <query id="FO_0003_0008_incomes">
    <![CDATA[
-- FO_0003_0008_incomes  
WITH                
    member [Measures].[Исполнение консолидированного бюджета ] 
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Исполнение консолидированного бюджета], [Measures].[Исполнение консолидированного бюджета] / 1000)' 
    member [Measures].[Исполнение бюджета субъекта ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Исполнение бюджета субъекта], [Measures].[Исполнение бюджета субъекта] / 1000)' 
    member [Measures].[Первоначальный план ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Первоначальный план], [Measures].[Первоначальный план] / 1000)' 
    member [Measures].[Уточненный план консолидированного бюджета ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Уточненный план консолидированного бюджета], [Measures].[Уточненный план консолидированного бюджета] / 1000)' 
    member [Measures].[Уточненный план бюджета субъекта ] 
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Уточненный план бюджета субъекта], [Measures].[Уточненный план бюджета субъекта] / 1000)' 
    member [Measures].[Оценка ]     
    as 'IIF ([Показатели].[Паспорт субъекта].CurrentMember is [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],[Measures].[Оценка], [Measures].[Оценка] / 1000)' 
    member [Measures].[Уровень]
    as '[Показатели].[Паспорт субъекта].Properties("Примечание")'
    member [Measures].[Темп к прошлому году ]
    as 'IIF ([Measures].[Темп к прошлому году] = 0, null, [Measures].[Темп к прошлому году])'
    member [Measures].[Темп уточненного плана к исполнению прошлого года ]
    as  'IIF ([Measures].[Темп уточненного плана к исполнению прошлого года] = 0, null, [Measures].[Темп уточненного плана к исполнению прошлого года])' 
    member [Measures].[Темп оценки к исполнению прошлого года ]
    as 'IIF ([Measures].[Темп оценки к исполнению прошлого года] = 0, null, [Measures].[Темп оценки к исполнению прошлого года])' 
    member [Measures].[Исполнение к плану ]
    as 'IIF ([Measures].[Исполнение к плану] = 0, null, [Measures].[Исполнение к плану])' 
    member [Measures].[Темп к прошлому году ФО ] 
    as '
        (
            [Measures].[Темп к прошлому году ],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].DataMember
        )' 
    member [Measures].[Темп к прошлому году РФ ]
    as '
        (
            [Measures].[Темп к прошлому году ],
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember
        )'
SELECT
    {   
    {
            {
                [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>]    
            }     
            *     
            {
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ],
                [Measures].[Исполнение к плану ],
                [Measures].[Темп к прошлому году ]     
            }    
        } +  
        {
            {                
                [Период].[Период].[Данные всех периодов].[<%period_last_year%>]     
            }     
            *     
            {
                [Measures].[Первоначальный план ],
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ],
                [Measures].[Исполнение к плану ],
                [Measures].[Темп к прошлому году ]     
            }    
        } +     
        {
            {
                [Период].[Период].[Данные всех периодов].[<%period_year%>]     
            }       
            *           
            {
                [Measures].[Первоначальный план ],
                [Measures].[Уточненный план консолидированного бюджета ],
                [Measures].[Уточненный план бюджета субъекта ],
                [Measures].[Темп уточненного плана к исполнению прошлого года ],
                [Measures].[Исполнение консолидированного бюджета ],
                [Measures].[Исполнение бюджета субъекта ],
                [Measures].[Темп к прошлому году ],
                [Measures].[Темп к прошлому году ФО ],
                [Measures].[Темп к прошлому году РФ ],
                [Measures].[Оценка ],
                [Measures].[Темп оценки к исполнению прошлого года ],
                [Measures].[Уровень]
            }          
        }   
    } on columns,
    {
        [Показатели].[Паспорт субъекта].[Все].[ИТОГО ДОХОДОВ],
        [Показатели].[Паспорт субъекта].[Все].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ],
        [Показатели].[Паспорт субъекта].[Все].[Налог на прибыль организаций],
        [Показатели].[Паспорт субъекта].[Все].[Налог на доходы физических лиц],
        [Показатели].[Паспорт субъекта].[Все].[Акцизы],
        [Показатели].[Паспорт субъекта].[Все].[Упрощенная система налогообложения],
        [Показатели].[Паспорт субъекта].[Все].[Налог на вмененный доход],
        [Показатели].[Паспорт субъекта].[Все].[Единый сельскохозяйственный налог],
        [Показатели].[Паспорт субъекта].[Все].[Налог на имущество физических лиц],
        [Показатели].[Паспорт субъекта].[Все].[Налог на имущество организаций],
        [Показатели].[Паспорт субъекта].[Все].[Транспортный налог],
        [Показатели].[Паспорт субъекта].[Все].[Земельный налог],
        [Показатели].[Паспорт субъекта].[Все].[Налог на добычу полезных ископаемых],
        [Показатели].[Паспорт субъекта].[Все].[Прочие налоговые доходы],
        [Показатели].[Паспорт субъекта].[Все].[Неналоговые доходы],
        [Показатели].[Паспорт субъекта].[Все].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ],
        [Показатели].[Паспорт субъекта].[Все].[Средства от возврата остатков субсидий, субвенций и иных межбюджетных трансфертов, имеющих целевое назначение, прошлых лет],
        [Показатели].[Паспорт субъекта].[Все].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации],
        [Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах],
        [Показатели].[Паспорт субъекта].[Все].[Дотации, в т.ч.],
        [Показатели].[Паспорт субъекта].[Все].[на выравнивание бюджетной обеспеченности],
        [Показатели].[Паспорт субъекта].[Все].[на поддержку мер по обеспечению сбалансированности бюджетов],
        [Показатели].[Паспорт субъекта].[Все].[Субсидии, в т.ч.],
        [Показатели].[Паспорт субъекта].[Все].[капитального характера],
        [Показатели].[Паспорт субъекта].[Все].[Субвенции],
        [Показатели].[Паспорт субъекта].[Все].[Иные межбюджетные трансферты],
        [Показатели].[Паспорт субъекта].[Все].[ДОХОДЫ ОТ ПРЕДПРИНИМАТЕЛЬСКОЙ И ИНОЙ ПРИНОСЯЩЕЙ ДОХОД ДЕЯТЕЛЬНОСТИ],
        [Показатели].[Паспорт субъекта].[Все].[Расчетный уровень бюджетной обеспеченности],
        [Показатели].[Паспорт субъекта].[Все].[Фактический уровень бюджетной обеспеченности],
        [Показатели].[Паспорт субъекта].[Все].[Место в РФ по фактическому уровню бюджетной обеспеченности]
  --      [Показатели].[Паспорт субъекта].[Все].[Раздел 1. Социально-значимые расходы],
  --      [Показатели].[Паспорт субъекта].[Все].[Недостаток средств (-) на финансовое обеспечение расходов раздела 1],
   --     [Показатели].[Паспорт субъекта].[Все].[Раздел 2. Первоочередные расходы],
   --     [Показатели].[Паспорт субъекта].[Все].[Недостаток средств (-) на финансовое обеспечение расходов разделов 1 и 2],
   --     [Показатели].[Паспорт субъекта].[Все].[ИТОГО РАСХОДОВ],
   --     [Показатели].[Паспорт субъекта].[Все].[Профицит (+) / дефицит (-)],
    --    [Показатели].[Паспорт субъекта].[Все].[Итого источников финансирования дефицита бюджета],
    --    [Показатели].[Паспорт субъекта].[Все].[Недостаток средств на финансовое обеспечение расходов (с учетом источников финансирования)]   
    }on rows   
FROM [ФК_Паспорт субъекта]           
WHERE           
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]          
    )           
  ]]>
  </query>

<query id="FO_0003_0008_chart">
   <![CDATA[
-- FO_0003_0008_chart 
with              
   member [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций]
    as '[Показатели].[Паспорт субъекта].[Все].[Доля межбюджетных трансфертов из федерального бюджета (за исключением субвенций) в доходах] * 100'
    member [Показатели].[Паспорт субъекта].[Собственные доходы]
    as ' 100 - [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций]'
    member [Показатели].[Паспорт субъекта].[Собственные доходы ]
    as 'IIF
        (
            [Measures].CurrentMember is [Measures].[Первоначальный план],
            [Показатели].[Паспорт субъекта].[Все].[Доля собственных доходов в общей сумме доходов (за исключением субвенций)] * 100,
            100 -
            (
                [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций]
            )
        ) '
    member [Период].[Период].[План ]   
    as '   
        (
            [Период].[Период].[Данные всех периодов].[<%period_year%>],
            [Measures].[Первоначальный план]   
        )'   
    member [Период].[Период].[Уточненный план ]   
    as '   
        (
            <%period_cur_date%>,
            [Measures].[Уточненный план консолидированного бюджета]   
        )'   
    member [Период].[Период].[Текущее исполнение]      
    as '      
        (
            <%period_cur_date%>,
            [Measures].[Исполнение консолидированного бюджета]     
        )'   
    member [Период].[Период].[Прошлый год]    
    as '    
        (
            [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
            [Measures].[Исполнение консолидированного бюджета]    
        )'   
    member [Период].[Период].[2 года назад]    
    as '    
        (
            [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>],
            [Measures].[Исполнение консолидированного бюджета]    
        )'    
select
    {
        [Период].[Период].[План ],
        [Период].[Период].[Уточненный план ],
        [Период].[Период].[Текущее исполнение] ,
        [Период].[Период].[Прошлый год],
        [Период].[Период].[2 года назад]     
    }on columns,
    {
        [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций],
        [Показатели].[Паспорт субъекта].[Собственные доходы ]      
    } on rows              
from  [ФК_Паспорт субъекта] 
where 
    (        
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>] 
    )
    ]]>  
    </query>

    <query id="FO_0003_0008_chart2">
        <![CDATA[
-- FO_0003_0008_chart2
with 
    member [Показатели].[Паспорт субъекта].[Поступление в федеральный бюджет]
    as '[Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в федеральный бюджет] * 100'
    member [Показатели].[Паспорт субъекта].[Поступление в конс. бюджет региона ]
    as '[Показатели].[Паспорт субъекта].[Все].[Доля налогов, поступающих в консолидированный бюджет региона] * 100'
select
    {
        <%period_cur_date%>,
        [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
        [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>]        
    }on columns,
    {
        [Показатели].[Паспорт субъекта].[Поступление в федеральный бюджет],
        [Показатели].[Паспорт субъекта].[Поступление в конс. бюджет региона ]
    } on rows 
from  [ФК_Паспорт субъекта] 
where 
    (
        [Measures].[Значение],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>] 
    ) 
    ]]>
    </query>

    <query id="FO_0003_0008_chart_summ">
        <![CDATA[
-- FO_0003_0008_chart_summ 
with
    member [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций]
    as '[Показатели].[Паспорт субъекта].[Все].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации] - [Показатели].[Паспорт субъекта].[Все].[Субвенции]'
    member [Показатели].[Паспорт субъекта].[Собственные доходы]
    as '[Показатели].[Паспорт субъекта].[Все].[ИТОГО ДОХОДОВ] - [Показатели].[Паспорт субъекта].[Все].[Субвенции]'
    member [Measures].[Исполнение консолидированного бюджета ]
    as '[Measures].[Исполнение консолидированного бюджета] / 1000'

    member [Период].[Период].[План ]
    as '
        (
            [Период].[Период].[Данные всех периодов].[<%period_year%>],
            [Measures].[Первоначальный план]
        ) / 1000'
    member [Период].[Период].[Уточненный план ]
    as '
        (
            <%period_cur_date%>,
            [Measures].[Уточненный план консолидированного бюджета]
        ) / 1000'
    member [Период].[Период].[Текущее исполнение]
    as '
        (
            <%period_cur_date%>,
            [Measures].[Исполнение консолидированного бюджета ]
        )'
    member [Период].[Период].[Прошлый год]
    as '
        (
            [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
            [Measures].[Исполнение консолидированного бюджета ]
        )'
    member [Период].[Период].[2 года назад]
    as '
        (
            [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>],
            [Measures].[Исполнение консолидированного бюджета ]
        )'
select
    {
        [Период].[Период].[План ],
        [Период].[Период].[Уточненный план ],
        [Период].[Период].[Текущее исполнение] ,
        [Период].[Период].[Прошлый год],
        [Период].[Период].[2 года назад]
    }on columns,
    {
        [Показатели].[Паспорт субъекта].[МБТ за исключением субвенций],
        [Показатели].[Паспорт субъекта].[Собственные доходы]
    } on rows
from  [ФК_Паспорт субъекта]
where
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )
    ]]>
    </query>

    <query id="FO_0003_0008_chart2_summ">
        <![CDATA[
-- FO_0003_0008_chart_summ 
with
    member [Показатели].[Паспорт субъекта].[Федеральный бюджет]
    as ' [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в федеральный бюджет]'
    member [Показатели].[Паспорт субъекта].[Поступление в конс. бюджет региона]
    as ' [Показатели].[Паспорт субъекта].[Все].[Объем налоговых поступлений в консолидированный бюджет региона]'
    member [Measures].[Значение ]
    as '[Measures].[Значение] / 1000'
select
    {
        <%period_cur_date%>,        
        [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
        [Период].[Период].[Данные всех периодов].[<%period_last_last_year%>]
        
    }on columns,
    {
        [Показатели].[Паспорт субъекта].[Федеральный бюджет],
        [Показатели].[Паспорт субъекта].[Поступление в конс. бюджет региона]
    } on rows
from  [ФК_Паспорт субъекта]
where
    (
        [Measures].[Значение ],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )
    ]]>
    </query>

</root>