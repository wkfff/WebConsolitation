  <system.diagnostics>
    <sources>
      <source name="Krista.FM.RIA.Extensions.E86N" switchValue="Verbose">
        <listeners>
          <add name="FileTextWriter"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="FileTextWriter" type="System.Diagnostics.TextWriterTraceListener, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" initializeData="logs\logindata.log"/>
      <add name="log4netWriter" type="Krista.Diagnostics.Log4Net.Log4NetTraceListener, Krista.Diagnostics.Log4Net"/>
    </sharedListeners>
  </system.diagnostics>
  <log4net>
    <appender name="LogToFile" type="log4net.Appender.RollingFileAppender">
      <file value="logs\log.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <!-- Value of priority may be ALL, DEBUG, INFO, WARN, ERROR, FATAL, OFF -->
      <priority value="WARN" />
      <appender-ref ref="LogToFile" />
    </root>
  </log4net>

  <system.net>
    <defaultProxy>
      <proxy
        usesystemdefault = "true"
        bypassonlocal = "true"
      />
    </defaultProxy>
