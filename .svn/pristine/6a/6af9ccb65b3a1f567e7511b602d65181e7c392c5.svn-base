<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="FNS_0004_0005_date">
    <![CDATA[
-- FNS_0004_0005_compare_date
-- <#[ФК_МесОтч_Доходы]#>
with
    member [Measures].[ДанныеНа]
    as '[Период].[Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период].[Период].Members,
                not IsEmpty([Measures].[Значение за период])
            ),
            1
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [ФНС_5 МН_Сводный]
  ]]>
  </query>


  <query id="FNS_0004_0005_compare_Grid_1">
    <![CDATA[
-- FNS_0004_0005_compare_Grid  

    WITH     
    member [Показатели].[ФНС 5 МН Сопоставимый].[Налоговая база по налогу]   
    as '   
        (
            [Показатели].[ФНС 5 МН Сопоставимый].[Все показатели].[Раздел I. Отчет о налоговой базе и структуре начислений по земельному налогу по юридическим лицам].[6. Налоговая база]   
        )/1000'    
    member [Показатели].[ФНС 5 МН Сопоставимый].[Сумма исчисленного налога]   
    as '   
        (
            [Показатели].[ФНС 5 МН Сопоставимый].[Все показатели].[Раздел I. Отчет о налоговой базе и структуре начислений по земельному налогу по юридическим лицам].[7. Сумма налога, подлежащая уплате в бюджет]   
        )/1000'    
    member [Показатели].[ФНС 5 МН Сопоставимый].[Средняя расчетная ставка по налогу]   
    as '[Показатели].[ФНС 5 МН Сопоставимый].[Налоговая база по налогу]/[Показатели].[ФНС 5 МН Сопоставимый].[Сумма исчисленного налога]'   
    member [Показатели].[ФНС 5 МН Сопоставимый].[Объем льгот]   
    as '   
        (
            [Показатели].[ФНС 5 МН Сопоставимый].[Все показатели].[Раздел I. Отчет о налоговой базе и структуре начислений по земельному налогу по юридическим лицам].[8. Сумма налога, не поступившая в бюджет в связи с предоставлением налогоплательщикам льгот по налогу - всего, в том числе:]   
        )/1000'    
    member [Measures].[Темп роста] 
    as 'IIF
        (
            (
                [Measures].[Значение за период],
                [Период].[Период].CurrentMember.PrevMember
            ) = null,
            null,
            (
                [Measures].[Значение за период],
                [Период].[Период].CurrentMember
            )/
            (
                [Measures].[Значение за период],
                [Период].[Период].CurrentMember.PrevMember
            )
        )*100' 
SELECT
    non empty     
    {
        {
            <%years%>  
        }  
        *   
        {
            [Measures].[Значение за период],
            [Measures].[Темп роста] 
        }    
    } ON COLUMNS ,
    {
        [Показатели].[ФНС 5 МН Сопоставимый].[Налоговая база по налогу],
        [Показатели].[ФНС 5 МН Сопоставимый].[Сумма исчисленного налога],
        [Показатели].[ФНС 5 МН Сопоставимый].[Средняя расчетная ставка по налогу],
        [Показатели].[ФНС 5 МН Сопоставимый].[Объем льгот]   
    } ON ROWS     
FROM  [ФНС_5 МН_Сводный]   
  ]]>
  </query>

