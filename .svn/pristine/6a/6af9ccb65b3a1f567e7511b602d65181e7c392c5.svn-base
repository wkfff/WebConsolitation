    non empty           
    {
        {
            <%years%>          
        }        
        *         
        {
            [Measures].[Значение за период],
            [Measures].[Темп роста]       
        }          
    } ON COLUMNS ,
    {
        [Показатели].[ФНС_5 НИО Сопоставимый].[Налоговая база по налогу],
        [Показатели].[ФНС_5 НИО Сопоставимый].[Сумма исчисленного налога],
        [Показатели].[ФНС_5 НИО Сопоставимый].[Средняя расчетная ставка по налогу],
        [Показатели].[ФНС_5 НИО Сопоставимый].[Объем льгот] 
    } ON ROWS           
FROM   [ФНС_5 НИО Сводный]          


