            {
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел I. Отчет о структуре начислений по транспортному налогу по юридическим лицам].[5. Количество транспортных средств, по которым налог не начислен к уплате в бюджет в связи с предоставлением налоговых льгот и освобождением от уплаты налога - всего, единиц, в том числе:],
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел II. Отчет о структуре начислений по транспортному налогу по физическим лицам].[3. Количество транспортных средств в базе данных налогового органа - всего, единиц, в том числе:]  
            }   
        )/1000'              
    member [Показатели].[ФНС_5 ТН Сопоставимый].[Сумма исчисленного налога]    
    as 'Aggregate  
        (
            {
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел I. Отчет о структуре начислений по транспортному налогу по юридическим лицам].[4. Сумма налога, подлежащая уплате в бюджет  - всего, тыс.руб., в том числе:],
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел II. Отчет о структуре начислений по транспортному налогу по физическим лицам].[5. Сумма налога, подлежащая уплате в бюджет  - всего, тыс.руб., в том числе:]  
            }  
        )/1000'  
    member [Показатели].[ФНС_5 ТН Сопоставимый].[Средняя расчетная ставка по налогу]    
    as '[Показатели].[ФНС_5 ТН Сопоставимый].[Количество транспортных средств, по которым начислен налог к уплате в бюджет ]  /[Показатели].[ФНС_5 ТН Сопоставимый].[Сумма исчисленного налога]'  
    member [Показатели].[ФНС_5 ТН Сопоставимый].[Объем льгот]    
    as 'Aggregate 
        (
            {
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел I. Отчет о структуре начислений по транспортному налогу по юридическим лицам].[6. Сумма налога, не поступившая в бюджет в связи с предоставлением налогоплательщикам налоговых льгот, в том числе:],
                [Показатели].[ФНС_5 ТН Сопоставимый].[Все].[Раздел II. Отчет о структуре начислений по транспортному налогу по физическим лицам].[6. Сумма налога, не поступившая в бюджет в связи с предоставлением налогоплательщикам налоговых льгот, в том числе:] 
            } 
        )/1000' 
    member [Measures].[Темп роста]           
    as 'IIF          
        (
            (
                [Measures].[Значение],
                [Период].[Период].CurrentMember.PrevMember          
            ) = null,
            null,
            (
                [Measures].[Значение],
                [Период].[Период].CurrentMember          
            )/          
            (
                [Measures].[Значение],
                [Период].[Период].CurrentMember.PrevMember          
            )          
        )*100'           
SELECT
    non empty               
    {
        {
           <%years%>              
        }            
        *             
        {
            [Measures].[Значение],
            [Measures].[Темп роста]           
        }              
    } ON COLUMNS ,
    {
        [Показатели].[ФНС_5 ТН Сопоставимый].[Количество транспортных средств, по которым начислен налог к уплате в бюджет ],
        [Показатели].[ФНС_5 ТН Сопоставимый].[Сумма исчисленного налога],
        [Показатели].[ФНС_5 ТН Сопоставимый].[Объем льгот]    
    } ON ROWS               
FROM    [ФНС_5 ТН_Сводный] 

  ]]>
  </query>

  <query id="FNS_0004_0005_compare_Grid_6">
    <![CDATA[
