  </webReportsSettingsSection>

  <system.diagnostics>
    <sources>
      <source name="Krista.FM.Client.WebReportsHelper" switchValue="Verbose">
        <listeners>
          <add name="ColoredConsole"/>
          <add name="FileTextWriter"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="Krista.FM.Client.ReportsUploader" switchValue="Verbose">
        <listeners>
          <add name="ColoredConsole"/>
          <add name="FileTextWriter"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="Krista.FM.Client.ReportsBootloader" switchValue="Verbose">
        <listeners>
          <add name="ColoredConsole"/>
          <add name="FileTextWriter"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="Krista.FM.Client.ReportsBootloaderConsole" switchValue="Verbose">
        <listeners>
          <add name="ColoredConsole"/>
          <add name="FileTextWriter"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="ColoredConsole" type="Krista.Diagnostics.ColoredConsoleTraceListener, Krista.Diagnostics"/>
      <add name="FileTextWriter" type="System.Diagnostics.TextWriterTraceListener, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" initializeData="WebReportsHelper.log"/>
      <!--
      <add name="ChatServer" type="Krista.Diagnostics.ChatServerTraceListener, Krista.Diagnostics" initializeData="tcp://fmserv:889/Krista.ChatServer/Server.rem"/>
      -->
    </sharedListeners>
  </system.diagnostics>
  <system.runtime.remoting>
    <application name="ReportsBootloaderService">
      <service>
        <wellknown mode="Singleton" type="Krista.FM.Client.WebReportsHelper.SnapshotServer, Krista.FM.Client.WebReportsHelper" objectUri="Server.rem" />
      </service>
      <channels>
        <channel ref="tcp" port="7999">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off"/>
  </system.runtime.remoting>
</configuration>