  <webReportsSettingsSection>
    <!--<reportsHostAddress value="http://arch.krista.ru/site/reports/iPhone/default.aspx" />-->
    <reportsHostAddress value="http://avolkov.krista.ru/test/reports/iphone/default.aspx" />
    <!--<reportsHostAddress value="http://fm1.krista.ru/yaroslavl/reports/iphone/default.aspx" />-->
    <!--<reportsHostAddress value="http://fm1.krista.ru/reports/iphone/default.aspx" />-->
