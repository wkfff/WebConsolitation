<configuration>

  <configSections>
    <section name="webReportsSettingsSection" type="Krista.FM.Client.WebReportsCommon.WebReportsSettingsSection, Krista.FM.Client.WebReportsCommon, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" allowDefinition="Everywhere" allowExeDefinition="MachineToApplication" restartOnExternalChanges="true" />
  </configSections>

  <appSettings>
