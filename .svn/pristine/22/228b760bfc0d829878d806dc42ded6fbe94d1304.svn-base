        } + [Энергетика]+    
        {
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Лесопромышленный комплекс]  
        } + [Лесопромышленный комплекс] +    
        {
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Связь]  
        } + [Связь] +    
        {
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Транспорт]  
        } + [Транспорт]+    
        {
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Строительство]  
        } + [Строительство] +    
