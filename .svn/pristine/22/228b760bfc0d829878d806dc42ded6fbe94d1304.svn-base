<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="STAT_0023_0001_lastDate">
    <![CDATA[
-- STAT_0023_0001_lastDate  
with
    member [Measures].[ДанныеНа]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                OpeningPeriod
                (
                    [Период__Период].[Период__Период].[Месяц],
                    [Период__Период].[Период__Период].[Данные всех периодов].[2010]
                ) : ClosingPeriod
                (
                    [Период__Период].[Период__Период].[Месяц],
                    [Период__Период].[Период__Период].[Данные всех периодов].[2020]
                ),
                not IsEmpty([Measures].[Значение]) and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Остатки на начало года" and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Данные квартала" and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Данные полугодия" and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Данные года" and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Данные месяца" and
                [Период__Период].[Период__Период].CurrentMember.Name <> "Значение не указано"
            )
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from [ОРГАНИЗАЦИИ_Характеристика деятельности]
	]]>
  </query>
  
  

  <query id="STAT_0023_0001_grid">
    <![CDATA[
-- STAT_0023_0001_grid
with     
    set [Показатели]   
    as '{
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников списочного состава на конец отчетного месяца],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля численности работников предприятия от общей численности занятого населения в населенном пункте],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Фонд оплаты труда],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата рабочих],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Задолженность по выплате заработной платы],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, находящихся в простое],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, находящихся в длительных отпусках без сохранения содержания],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, занятых неполный рабочий день],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Количество свободных рабочих мест (вакантных должностей)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата по всем вакантным рабочим местам],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, планируемых к высвобождению],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Наличие/отсутствие неурегулированной задолженности по платежам в бюджет и внебюджетные фонды],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля неурегулированной просроченной дебиторской задолженности от общей величины задолженности],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля неурегулированной просроченной кредиторской задолженности от общей величины задолженности],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Сумма задолженности по кредитным договорам (по полученным кредитам и займам)]
        } '     
    member [Период__Период].[Период__Период].[Начало года]  
    as ' [Период__Период].[Период__Период].[Данные всех периодов].[<%period_year%>].[Полугодие 1].[Квартал 1].[Январь]'  
    member [Период__Период].[Период__Период].[Предыдущий период]  
    as ' <%prev_period%> ' 
    member [Период__Период].[Период__Период].[Текущий период]  
    as ' <%curr_period%>  ' 
    member [Период__Период].[Период__Период].[Отклонение от предыдущего месяца]  
    as 'IIF  
        (
            IsEmpty  
            (
                [Период__Период].[Период__Период].[Текущий период]  
            ) or IsEmpty  
            (
                [Период__Период].[Период__Период].[Предыдущий период]  
            ),
            null,
            [Период__Период].[Период__Период].[Текущий период] - [Период__Период].[Период__Период].[Предыдущий период]  
        )'  
    member [Период__Период].[Период__Период].[Темп роста к предыдущему месяцу] 
    as 'IIF 
        (
            IsEmpty
            (
                [Период__Период].[Период__Период].[Предыдущий период]
            ) or
            ((
                [Период__Период].[Период__Период].[Предыдущий период]
            ) = 0) or
            IsEmpty
            (
                [Период__Период].[Период__Период].[Текущий период]
            ),
            null,
            [Период__Период].[Период__Период].[Текущий период] / [Период__Период].[Период__Период].[Предыдущий период] - 1
        )' 
    member [Период__Период].[Период__Период].[Отклонение от начала года]  
    as 'IIF  
        (
            IsEmpty  
            (
                [Период__Период].[Период__Период].[Текущий период]  
            ) or IsEmpty  
            (
                [Период__Период].[Период__Период].[Начало года] 
            ) or
            (<%curr_period%> is [Период__Период].[Период__Период].[Данные всех периодов].[<%period_year%>].[Полугодие 1].[Квартал 1].[Январь]),
            null,
            [Период__Период].[Период__Период].[Текущий период] - [Период__Период].[Период__Период].[Начало года] 
        )'   
    member [Период__Период].[Период__Период].[Темп роста к началу года] 
    as 'IIF 
        (
            IsEmpty
            (
                [Период__Период].[Период__Период].[Начало года]
            ) or
            ((
                [Период__Период].[Период__Период].[Начало года]
            ) = 0) or
            IsEmpty
            (
                   [Период__Период].[Период__Период].[Текущий период]
            ) or
            (<%curr_period%> is [Период__Период].[Период__Период].[Данные всех периодов].[<%period_year%>].[Полугодие 1].[Квартал 1].[Январь]),
            null,
            [Период__Период].[Период__Период].[Текущий период] / [Период__Период].[Период__Период].[Начало года] - 1
        )' 
        
    member [Период__Период].[Период__Период].[Показатель]        
    as ' [Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name'       
    
    member [Период__Период].[Период__Период].[Показатель с единицой измерения]        
    as ' [Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name + ", " + 
         LCase([Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Единица измерения"))'   
         
    member [Период__Период].[Период__Период].[Единица измерения]    
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Единица измерения")'  
    member [Период__Период].[Период__Период].[Код]    
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Код")'   
select
    {
        [Период__Период].[Период__Период].[Показатель с единицой измерения],
        [Период__Период].[Период__Период].[Текущий период],
        [Период__Период].[Период__Период].[Предыдущий период],
        [Период__Период].[Период__Период].[Начало года],
        [Период__Период].[Период__Период].[Отклонение от предыдущего месяца],
        [Период__Период].[Период__Период].[Темп роста к предыдущему месяцу],
        [Период__Период].[Период__Период].[Отклонение от начала года],
        [Период__Период].[Период__Период].[Темп роста к началу года],
        [Период__Период].[Период__Период].[Код],
        [Период__Период].[Период__Период].[Показатель],
        [Период__Период].[Период__Период].[Единица измерения]
    } on columns,
    { [Показатели] } on rows 
From [ОРГАНИЗАЦИИ_Характеристика деятельности]   
where     
    (
        [Measures].[Значение],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Организации__Реестр организаций].[Организации__Реестр организаций].[<%selected_organization%>]
    )         
  ]]>
  </query>

  <query id="STAT_0023_0001_chart">
    <![CDATA[
-- STAT_0023_0001_chart
with   
    set [Периоды]   
    as 'Tail   
        (
            Filter  
            (
                Descendants  
                (
                    [Период__Период].[Период__Период].[Данные всех периодов],
                    [Период__Период].[Период__Период].[Месяц],
                    SELF  
                ),
                not IsEmpty ( ( [Measures].[Значение] ) ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Остатки на начало года"  
                ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Данные квартала"  
                ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Данные полугодия"  
                ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Данные месяца"  
                ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Данные года"  
                ) and  
                (
                    [Период__Период].[Период__Период].CurrentMember.Name <> "Заключительные обороты"  
                ) and [Период__Период].[Период__Период].CurrentMember.Properties("PKID") <= <%curr_period%>.Properties("PKID")
            ),
            12
        ) '
    member [Организации__Показатели работы].[Организации__Показатели работы].[Период]
    as '[Период__Период].[Период__Период].CurrentMember.Name + " " + [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Parent.Name'
select
    {
        [Организации__Показатели работы].[Организации__Показатели работы].[Период],
        [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[<%selected_grid_indicator%>]
    } on columns,
    {[Периоды]} on rows 
From [ОРГАНИЗАЦИИ_Характеристика деятельности]  
where   
    (
        [Measures].[Значение],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Организации__Реестр организаций].[Организации__Реестр организаций].[<%selected_organization%>]
    )   
  ]]>
  </query>

  <query id="STAT_0023_0001_organizationSelected">
    <![CDATA[
-- STAT_0023_0001_organizationSelected
with
    set [Показатели]
    as '
        {
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников списочного состава на конец отчетного месяца],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля численности работников предприятия от общей численности занятого населения в населенном пункте],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Фонд оплаты труда],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата рабочих],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Задолженность по выплате заработной платы],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, находящихся в простое],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, находящихся в длительных отпусках без сохранения содержания],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, занятых неполный рабочий день],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Количество свободных рабочих мест (вакантных должностей)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата по всем вакантным рабочим местам],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников, планируемых к высвобождению],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Объем отгруженных товаров собственного производства, выполненных работ и услуг собственными силами],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Наличие/отсутствие неурегулированной задолженности по платежам в бюджет и внебюджетные фонды],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля неурегулированной просроченной дебиторской задолженности от общей величины задолженности],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Доля неурегулированной просроченной кредиторской задолженности от общей величины задолженности],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Сумма задолженности по кредитным договорам (по полученным кредитам и займам)]
        } '
    member [Период__Период].[Период__Период].[Текущий период]
    as '  <%curr_period%> '
   set [Реестр организаций]
    as 'Filter
        (
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации].Children,
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Тип организации") = "Системообразующие предприятия"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Нефть и газ]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Нефть и газ"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Геология]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Геология]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Геология"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Производство нефтепродуктов]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Производство нефтепродуктов]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Производство нефтепродуктов"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Энергетика]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Энергетика]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Энергетика"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Лесопромышленный комплекс]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Лесопромышленный комплекс]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Лесопромышленный комплекс"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Связь]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Связь]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Связь"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Транспорт]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Транспорт]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Транспорт"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Строительство]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Строительство]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Строительство"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Агропромышленный комплекс]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Агропромышленный комплекс]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности") = "Агропромышленный комплекс"
        )'

    member [Measures].[Вид деятельности]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Виды деятельности")'
    set [Список организаций]
    as '[Нефть и газ] +
        [Геология] +
        [Производство нефтепродуктов] +
        [Энергетика]+
        [Лесопромышленный комплекс] +
        [Связь] +
        [Транспорт]+
        [Строительство] +
        [Агропромышленный комплекс]'
    member [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели ]
    as 'Aggregate([Показатели])'
select
    {
        [Период__Период].[Период__Период].[Текущий период]       
    } on columns,
    NON EMPTY{ [Список организаций] * {[Организации__Показатели работы].[Организации__Показатели работы].[Все показатели ]} } on rows
From [ОРГАНИЗАЦИИ_Характеристика деятельности]
where
    (
        [Measures].[Значение],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ]
    )
  ]]>
</query>
  <query id="STAT_0023_0001_organizationDigest">
    <![CDATA[
-- STAT_0023_0001_organizationDigest
with     
