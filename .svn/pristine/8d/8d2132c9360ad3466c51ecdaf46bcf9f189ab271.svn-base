select
    {
      {<%kd_group%>}
    } on columns,
    Filter(Generate
    (
        {
            [Период].[Период].[Данные всех периодов].[<%period_first_year%>],
            [Период].[Период].[Данные всех периодов].[<%period_year%>],
            [Период].[Период].[Данные всех периодов].[<%period_end_year%>] 
        },
        Descendants
        (
            [Период].[Период].CurrentMember,
            [Период].[Период].[Месяц]
        )  
    ), ([КД].[Сопоставимый].[Налог на прибыль ] + [КД].[Сопоставимый].[НДФЛ ]) <> 0) on rows
from [ФК_МесОтч_Доходы]  
where  
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация<%subject%>,
        [Measures].[Исполнено за период без -],
        [Уровни бюджета].[СКИФ].[<%skif_level%>]
    )  
  ]]>
  </query>

