.ExpandBlockFirstState
{
    background-image: url(../../images/Collapse.gif);
    cursor: pointer;     
    background-repeat: no-repeat;
    margin-left: 2px;
    width: 15px;
    border-style: none !important;
}
.ExpandBlockSecondState
{
    background-image: url(../../images/Expand.gif);
    cursor: pointer;     
    background-repeat: no-repeat;
    margin-left: 2px;
    width: 15px;
    border-style: none !important;
}

.ExpandBlockRowFirstState
{
    display: block;
}
.ExpandBlockRowSecondState
{
    display: none;
}

