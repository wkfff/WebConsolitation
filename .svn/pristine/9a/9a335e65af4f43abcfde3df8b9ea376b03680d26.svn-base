<?xml version="1.0" encoding="Windows-1251"?>
<root>
    <query id="FST_0001_0001_Inflation">
        <![CDATA[
-- FST_0001_0001_Inflation
WITH  
    SET [NonFilterColumnSet]  
    AS ' {[Measures].[За период]}'  
SELECT
    [NonFilterColumnSet] ON COLUMNS,
    {
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Без варианта]
    } on rows 
FROM [СТАТ_СЭП_Годовой сборник]  
WHERE  
    (
        [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Прогноз инфляции] ,
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember ,
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2011].DataMember  
    ) 
]]>
    </query>

    <query id="SB_0001_0001_chart">
        <![CDATA[
-- SB_0001_0001_chart
with  
    member [ФСТ].[Показатели].[Уровень]  
    as '[Территории].[Сопоставимый].CurrentMember.Level.Name'  
    set [Все регионы]  
    as 'filter  
        (
            Descendants  
            (
                [Территории].[Сопоставимый].[Все территории],
                [Территории].[Сопоставимый].[РФ],
                SELF_AND_AFTER  
            ) ,
            not  
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember  
            ) and not [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[Все территории].[Несопоставленные данные]    and  
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ"  
            )  
        )'           
    member [ФСТ].[Показатели].[Единицы измерения ]  
    as '"<%measure%>"' 
    
     member [ФСТ].[Показатели].[Рост тарифа на <%filter%> ]
    as '[ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] - 1'
    member [ФСТ].[Показатели].[Рост тарифа на <%filter%> для населения ]
    as '[ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%> для населения] - 1'
select
    {
        [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>],
        [ФСТ].[Показатели].[Рост тарифа на <%filter%> ],
        [ФСТ].[Показатели].[Рост тарифа на <%filter%> для населения ]
    }on columns,
    {
        order  
        (
            filter  
            (          
                [Все регионы],
                not isEmpty
                (
                    [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]
                )
            ),
            [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>],
            BASC  
        )  
    } on rows  
from  [ФСТ_ЖКХ_Тарифы]  
WHERE  
    (
        [Measures].[Факт],
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2011]  
    )
]]>
    </query>


    <query id="SB_0001_0001_text">
        <![CDATA[
-- SB_0001_0001_text
with  
    member [ФСТ].[Показатели].[Уровень]  
    as '[Территории].[Сопоставимый].CurrentMember.Level.Name'  
    set [Все регионы]  
    as 'filter  
        (
            Descendants  
            (
                [Территории].[Сопоставимый].[Все территории],
                [Территории].[Сопоставимый].[РФ],
                SELF_AND_AFTER  
            ) ,
            not  
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember  
            ) and not [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[Все территории].[Несопоставленные данные]    and  
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ"  
            )  
        )'  
    member [ФСТ].[Показатели].[Ранг среднеотпускной тариф ФО]  
    as 'IIF  
        (
            IsEmpty  
            (
                [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
            ) or  [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>] = 0,
            null,
            Rank  
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order  
                (
                    Filter  
                    (
                        [Территории].[Сопоставимый].CurrentMember.Siblings ,
                        not IsEmpty  
                        (
                            [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
                        ) and not [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>] = 0  
                    ),
                    [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>],
                    BDESC  
                ),
                [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
            )  
        )'  
    member [ФСТ].[Показатели].[Ранг среднеотпускной тариф РФ]  
    as 'IIF  
        (
            IsEmpty  
            (
                [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
            ) or  [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>] = 0,
            null,
            Rank  
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order  
                (
                    Filter  
                    (
                        [Все регионы],
                        not IsEmpty  
                        (
                            [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
                        ) and not [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>] = 0  
                    ),
                    [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>],
                    BDESC  
                ),
                [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>]  
            )  
        )'  
    member [ФСТ].[Показатели].[Ранг прирост тарифа ФО]  
    as 'IIF  
        (
            IsEmpty  
            (
                [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
            ) or  [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] = 0,
            null,
            Rank  
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order  
                (
                    Filter  
                    (
                        [Территории].[Сопоставимый].CurrentMember.Siblings ,
                        not IsEmpty  
                        (
                            [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
                        ) and not [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] = 0  
                    ),
                    [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>],
                    BDESC  
                ),
                [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
            )  
        )'  
    member [ФСТ].[Показатели].[Ранг прирост тарифа РФ]  
    as 'IIF  
        (
            IsEmpty  
            (
                [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
            ) or  [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] = 0,
            null,
            Rank  
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order  
                (
                    Filter  
                    (
                        [Все регионы],
                        not IsEmpty  
                        (
                            [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
                        ) and not [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] = 0  
                    ),
                    [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>],
                    BDESC  
                ),
                [ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>]  
            )  
        )'  
    member [ФСТ].[Показатели].[Единицы измерения ]  
    as '"<%measure%>"'  
    member [ФСТ].[Показатели].[Худший среднеотпускной тариф ФО]  
    as 'max  
        (
            [Территории].[Сопоставимый].CurrentMember.Siblings,
            [ФСТ].[Показатели].[Ранг среднеотпускной тариф ФО]  
        )'  
    member [ФСТ].[Показатели].[Худший среднеотпускной тариф РФ]  
    as 'max  
        (
            [Все регионы],
            [ФСТ].[Показатели].[Ранг среднеотпускной тариф РФ]  
        )'  
    member [ФСТ].[Показатели].[Худший прирост тарифа ФО]  
    as 'max  
        (
            [Территории].[Сопоставимый].CurrentMember.Siblings,
            [ФСТ].[Показатели].[Ранг среднеотпускной тариф ФО]  
        )'  
    member [ФСТ].[Показатели].[Худший прирост тарифа РФ]  
    as 'max  
        (
            [Все регионы],
            [ФСТ].[Показатели].[Ранг среднеотпускной тариф РФ]  
        )'  
    member [ФСТ].[Показатели].[Рост тарифа на <%filter%> ] 
    as '[ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%>] - 1' 
    member [ФСТ].[Показатели].[Рост тарифа на <%filter%> для населения ] 
    as '[ФСТ].[Показатели].[Все показатели].[Рост тарифа на <%filter%> для населения] - 1' 
select
    {
        [ФСТ].[Показатели].[Все показатели].[Среднеотпускной тариф на <%filter%>],
        [ФСТ].[Показатели].[Рост тарифа на <%filter%> ] ,
        [ФСТ].[Показатели].[Все показатели].[Тариф на <%filter%> для населения],
        [ФСТ].[Показатели].[Рост тарифа на <%filter%> для населения ],
        [ФСТ].[Показатели].[Ранг среднеотпускной тариф ФО],
        [ФСТ].[Показатели].[Худший среднеотпускной тариф ФО],
        [ФСТ].[Показатели].[Ранг среднеотпускной тариф РФ],
        [ФСТ].[Показатели].[Худший среднеотпускной тариф РФ],
        [ФСТ].[Показатели].[Ранг прирост тарифа ФО],
        [ФСТ].[Показатели].[Худший прирост тарифа ФО],
        [ФСТ].[Показатели].[Ранг прирост тарифа РФ],
        [ФСТ].[Показатели].[Худший прирост тарифа РФ],
        [ФСТ].[Показатели].[Единицы измерения ]    
    }on columns,
    {
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]  
    } on rows  
from  [ФСТ_ЖКХ_Тарифы]  
WHERE  
    (
        [Measures].[Факт],
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2011]  
    )  
]]>
    </query>

    <query id="SB_0001_0001_grid">
        <![CDATA[
-- SB_0001_0001_grid
with  
    member [ФСТ].[Показатели].[Количество регулируемых организаций электроснабжения, в т.ч.]  
    as '[ФСТ].[Показатели].[Все показатели].[Количество регулируемых организаций электроснабжения, всего]'  
    member [ФСТ].[Показатели].[Региональная генерация] 
    as '[ФСТ].[Показатели].[Все показатели].[Количество регулируемых организаций региональной генерации]'  
    member [ФСТ].[Показатели].[Энергоснабжающие] 
    as '[ФСТ].[Показатели].[Все показатели].[Количество регулируемых энергоснабжающих организации]' 
    member [ФСТ].[Показатели].[Электросетевые] 
    as '[ФСТ].[Показатели].[Все показатели].[Количество регулируемых организаций обслуживающих сети]' 
    member [ФСТ].[Показатели].[Энергосбытовые] 
    as '[ФСТ].[Показатели].[Все показатели].[Количество регулируемых сбытовых организаций]' 
select
    { [Measures].[Факт] }on columns,
    {
        [ФСТ].[Показатели].[Количество регулируемых организаций электроснабжения, в т.ч.],
        [ФСТ].[Показатели].[Региональная генерация],
        [ФСТ].[Показатели].[Энергоснабжающие],
        [ФСТ].[Показатели].[Электросетевые],
        [ФСТ].[Показатели].[Энергосбытовые],
        [ФСТ].[Показатели].[Все показатели].[Количество регулируемых организаций теплоснабжения],
        [ФСТ].[Показатели].[Все показатели].[Количество регулируемых организаций водоснабжения]  
    } on rows      
from  [ФСТ_ЖКХ_Тарифы]      
WHERE      
    (
        [Период].[Год Квартал Месяц].[Данные всех периодов].[2011],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )  
]]>
</query>

</root>