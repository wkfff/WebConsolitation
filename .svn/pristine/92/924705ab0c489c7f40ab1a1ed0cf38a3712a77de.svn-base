<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[SELECT
    { [Measures].[За период]  } on columns,
    {
        Tail 
        (
            Filter 
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[За период]  > 0 
            ) 
        ).Item(0) 
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows 
FROM [СТАТ_Труд_Трудовые ресурсы]
where 
    (
        [Группировки].[Труд_Трудовые ресурсы].[По видам экономической деятельности, по городам и районам, по видам хозяйствующих субъектов],
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Труд].[Трудовые ресурсы].[Средняя численность работников],
        [Территории].[РФ Карта].[МР ГО].[Саратовский р-н],
        [ОК].[ОКВЭД].[Раздел].[Сельское хозяйство, охота и лесное хозяйство] 
    )]]>
  </query>  
  <query id="last_date2">
    <![CDATA[SELECT
    { [Measures].[За период]  } on columns,


{
    Tail
    (
        Filter
        (
            [Период].[Год Квартал Месяц].[Год].Members,
            [Measures].[За период]  > 0
        )
    ).Item(0)
} DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [СТАТ_Труд_Заработная плата]
where
    (
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Труд_Заработная плата].[Группировка].[По городам и районам],
        [Труд].[Заработная плата].[Показатель].[Среднемесячная начисленная заработная плата],
        [Территории].[РФ Карта].[Субъект РФ].[Саратовская обл.]
    )]]>
  </query>
  <query id="TG">
    <![CDATA[select     non empty 
{
    [Период].[Год Квартал Месяц].[<%lastdate%>]     
}on columns ,
non empty 
{
    [Территории].[РФ Карта].[МР ГО].Members     
} on rows       
from [СТАТ_Труд_Заработная плата]         
where      
    (
        [Источники данных].[Источник].[<%1%>],
        [Группировки].[Труд_Заработная плата].[Группировка].[По городам и районам],
        [Труд].[Заработная плата].[Показатель].[Среднемесячная начисленная заработная плата],
        [Measures].[За период]          
    ) ]]>
  </query>
  <query id="TC">
    <![CDATA[
select
    non empty  
    {
         [Период].[Год Квартал Месяц].[<%firstyear%>]:[Период].[Год Квартал Месяц].[<%lastdate%>]     
    }  on rows ,
    non empty   
    {
    
        [Территории].[РФ Карта].[МР ГО].[<%pokaz%>]    
    }  on columns  
from [СТАТ_Труд_Заработная плата]     
where     
    (
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Группировки].[Труд_Заработная плата].[Группировка].[По городам и районам],
        [Труд].[Заработная плата].[Показатель].[Среднемесячная начисленная заработная плата],
        [Measures].[За период]   
    )]]>
  </query>

  <query id="DL">
    <![CDATA[WITH
    MEMBER [Measures].[ ]
    as 'null'
    SET [NonFilterColumnSet]
    AS '{[Measures].[ ]}'

SELECT
    [NonFilterColumnSet]on rows ,
    {

                [ОК].[Виды хозяйствующих субъектов].[Вид хозяйствующего субъекта].Members

    }  on columns
FROM [СТАТ_Труд_Трудовые ресурсы]]]>
  </query>

  <query id="CC">
    <![CDATA[select
    non empty  
    {
       [Период].[Год Квартал Месяц].[<%lastdate%>]     
    }on rows ,
    non empty  
    {
        [ОК].[ОКВЭД].[Раздел].Members   
    }    on columns  
from [СТАТ_Труд_Трудовые ресурсы]   
where  
    (
        [Группировки].[Труд_Трудовые ресурсы].[По видам экономической деятельности, по городам и районам, по видам хозяйствующих субъектов],
        [Источники данных].[Источник].[СТАТ Отчетность - Облстат],
        [Труд].[Трудовые ресурсы].[Средняя численность работников],
        [Measures].[За период],
        [Территории].[РФ Карта].[МР ГО].[<%pokaz%>],
        [ОК].[Виды хозяйствующих субъектов].[Вид хозяйствующего субъекта].[<%pokaz2%>])
     ]]>
  </query>
  <query id="BG">
    <![CDATA[select
    non empty  
    {
        [Период].[Год Квартал Месяц].[1998]:[Период].[Год Квартал Месяц].[<%lastdate%>]  
    }on columns ,
    non empty  
    {
        [ОК].[ОКФС].[Уровень 1].Members  
    } on rows  
from [СТАТ_Труд_Трудовые ресурсы]  
where  
    (
        [Группировки].[Труд_Трудовые ресурсы].[По формам собственности],
