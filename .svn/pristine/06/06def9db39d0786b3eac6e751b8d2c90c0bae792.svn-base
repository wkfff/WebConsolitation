  <query id="iPad_0001_0002_table">
    <![CDATA[
    --iPad_0001_0002_table
select
    {
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских цен на продовольственные товары],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских цен на непродовольственные товары],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских цен (тарифов) на услуги],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс цен производителей промышленных товаров],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс цен производителей сельскохозяйственной продукции],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс тарифов на грузовые перевозки],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских тарифов на жилищно-коммунальнальные услуги],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских тарифов на отопление],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских тарифов на горячее водоснабжение],
  [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Индекс потребительских тарифов на холодное водоснабжение и водоотведение]

    } on rows,
    {
        [Территории].[СЭП].[Все территории].[<%state_area%>],
        [Территории].[СЭП].[Все территории].[<%region%>],
        [Территории].[СЭП].[Все территории].[Российская Федерация]
    } on columns 
from [СТАТ_СЭП_Годовой сборник] 
where 
    (
        [Measures].[За период],        
        <%period_year%>,
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Без варианта],
        [ОК].[ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [ОК].[ОКФС].[Все формы собственности].[Значение не указано],
        [ОК].[Основные возрастные группы].[Все возрастные группы].[Значение не указано],
        [Половая принадлежность].[Все типы].[Значение не указано],
        [СЭП].[Источник информации].[Все].[1],
        [Тип местности].[Все типы местности].[Значение не указано]  
    ) 
  ]]>
  </query>

  <query id="iPad_0001_0002_chart">
    <![CDATA[
    --iPad_0001_0002_chart
with   
    member [СЭП].[Годовой сборник].[Имя года] 
    as '[Период].[Год Квартал Месяц].CurrentMember.Parent.Parent.Parent.Name '  
select
    {
        [СЭП].[Годовой сборник].[Имя года],
        [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Уровень жизни населения].[Среднедушевые денежные доходы],
        [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Стоимость фиксированного набора товаров и услуг]
    } on rows,
    {
        Tail
        (
            Filter 
            (
                [Период].[Год Квартал Месяц].Members,
                (not IsEmpty
                (
                    (
                        [Measures].[За период],
                        [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Уровень жизни населения].[Среднедушевые денежные доходы]
                    )
                ) or not IsEmpty 
                (
                    (
                        [Measures].[За период],
                        [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Стоимость фиксированного набора товаров и услуг]
                    )
                ))and 
                (
                    [Период].[Год Квартал Месяц].CurrentMember.Level.Name = "Месяц" 
                )                 
            ),
            12
        )
    } on columns
from [СТАТ_СЭП_Годовой сборник]
where
    (
        [Measures].[За период],
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Без варианта],
        [ОК].[ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [ОК].[ОКФС].[Все формы собственности].[Значение не указано],
        [ОК].[Основные возрастные группы].[Все возрастные группы].[Значение не указано],
        [Половая принадлежность].[Все типы].[Значение не указано],
        [СЭП].[Источник информации].[Все].[1],
        [Тип местности].[Все типы местности].[Значение не указано],
        [Территории].[СЭП].[Все территории].[<%state_area%>]
    )
  ]]>
  </query>

  <query id="iPad_0001_0002_labels">
    <![CDATA[
    --iPad_0001_0002_labels
    with         
    set [Территории ]         
    as 'Filter         
        (
            [Территории].[СЭП].Children,
            not IsEmpty ( [Measures].[За период] ) and         
            (
                [Территории].[СЭП].CurrentMember.Properties("Принадлежность") = [Территории].[СЭП].[Все территории].[<%state_area%>].Properties("Принадлежность")         
            )         
        )'         
    set [Территории РФ]         
    as 'Filter         
        (
            [Территории].[СЭП].Children,
            not IsEmpty ( [Measures].[За период] ) and not         
            (
                [Территории].[СЭП].CurrentMember.Properties("Принадлежность") = "РФ"         
            ) and not         
            (
                [Территории].[СЭП].CurrentMember is [Территории].[СЭП].[Все территории].[Российская Федерация]         
            )         
        )'         
    member measures.[За период ]       
    as '[Территории].[СЭП].CurrentMember.Properties("Принадлежность")'         
    member measures.[Принадлежность]       
    as '[Территории].[СЭП].CurrentMember.Properties("Принадлежность")'       
   member measures.[Ранг по РФ]  
    as 'IIF  
        (
            (
                [СЭП].[Годовой сборник].CurrentMember is [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Стоимость фиксированного набора товаров и услуг]  
            ),
            IIF  
            (
                not IsEmpty ( [Measures].[За период] ),
                Rank  
                (
                    [Территории].[СЭП].CurrentMember,
                    Order  
                    (
                        [Территории РФ],
                        ( [Measures].[За период] ),
                        ASC  
                    ),
                    ( [Measures].[За период] )  
                ),
                null 
            ),
            IIF 
            (
                not IsEmpty ( [Measures].[За период] ),
                Rank 
                (
                    [Территории].[СЭП].CurrentMember,
                    Order  
                    (
                        [Территории РФ],
                        ( [Measures].[За период] ),
                        DESC  
                    ),
                    ( [Measures].[За период] )  
                ),
                null
            ) 
        ) '  
    member Measures.[Ранг по ФО]  
    as 'IIF  
        (
            (
                [СЭП].[Годовой сборник].CurrentMember is [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Стоимость фиксированного набора товаров и услуг]  
            ),
            IIF
            (
                not IsEmpty ( [Measures].[За период] ),
                Rank
                (
                    [Территории].[СЭП].CurrentMember,
                    Order
                    (
                        [Территории ],
                        ( [Measures].[За период] ),
                        ASC
                    ),
                    ( [Measures].[За период] )
                ),
                null
            ),
            IIF
            (
                not IsEmpty ( [Measures].[За период] ),
                Rank
                (
                    [Территории].[СЭП].CurrentMember,
                    Order
                    (
                        [Территории ],
                        ( [Measures].[За период] ),
                        DESC
                    ),
                    ( [Measures].[За период] )
                ),
                null
            )
        )'  
    member measures.[Худший ранг ФО]
    as ' max
        (
            [Территории ],
            Measures.[Ранг по ФО]
        ) '
    member measures.[Худший ранг РФ]
    as ' max
        (
            [Территории РФ],
            Measures.[Ранг по РФ]
        ) '
    member measures.[Прошлый период]  
    as '
        (
            [Measures].[За период],
            [Период].[Год Квартал Месяц].[Данные всех периодов]<%period_last_date%>
        )' 
        member measures.[Разница]
    as '
        [Measures].[За период] - measures.[Прошлый период]'
select
    {
        {
            [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Цены и тарифы].[Стоимость фиксированного набора товаров и услуг],
            [СЭП].[Годовой сборник].[Данные всех источников].[СТАТ\0001 Отчетность - вариант: Без варианта].[Уровень жизни населения].[Среднедушевые денежные доходы]       
        }          
    } on columns,
    {
        { [Территории].[СЭП].[Все территории].[<%state_area%>]  }
        *  
        {
            measures.[Ранг по ФО],
            measures.[Ранг по РФ],
            [Measures].[За период],
            measures.[Худший ранг ФО],
            measures.[Худший ранг РФ],
            measures.[Разница]
        }         
    }on rows         
from  [СТАТ_СЭП_Годовой сборник]         
where     
    (        
        <%period_year%>,
        [Источники данных].[Все источники данных].[СТАТ Отчетность - Без варианта],
        [ОК].[ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [ОК].[ОКФС].[Все формы собственности].[Значение не указано],
        [ОК].[Основные возрастные группы].[Все возрастные группы].[Значение не указано],
        [Половая принадлежность].[Все типы].[Значение не указано],
        [СЭП].[Источник информации].[Все].[1],
        [Тип местности].[Все типы местности].[Значение не указано]     
    ) 
  ]]>
  </query>

