<%@ Control Language="C#" AutoEventWireup="true" Codebehind="ContainerPanel.ascx.cs"
    Inherits="Krista.FM.Server.Dashboards.Components.ContainerPanel" %>
<table style="border-collapse: collapse; width: 100%; margin-top: 5px;" class="GadgetContainerPanel">
    <tr>
        <td class="topleft">
        </td>
        <td class="top">
        </td>
        <td class="topright">
        </td>
    </tr>
    <tr runat="server" id="headerTr">
        <td class="headerleft">
        </td>
        <td class="header">
            <div id="HeaderDiv" style="vertical-align:top;  float: left;" runat="server"></div>
            <img id="HeaderImage" style="margin-top: -13px; position: absolute; z-index: 100" runat="server" src="" alt=""/>
        </td>
        <td class="headerright">
        </td>
    </tr>
    <tr>
        <td class="left">
        </td>
        <td style="overflow: visible;" runat="server" class="PartStyle">
            <asp:PlaceHolder ID="ContentPlaceHolder" runat="server"></asp:PlaceHolder>
        </td>
        <td class="right">
        </td>
    </tr>
    <tr>
        <td class="bottomleft">
        </td>
        <td class="bottom">
        </td>
        <td class="bottomright">
        </td>
    </tr>
</table>

