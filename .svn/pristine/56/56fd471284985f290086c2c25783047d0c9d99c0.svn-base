<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[ -- name: last_date   
SELECT
    { [Measures].[Значение]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[Значение] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        <%baseRegion%>,
        [Источники данных].[Все источники данных],
        [Показатели].[ВедСтат_Свод].[Все показатели].<%way_last_year%>
    )  
	  ]]>
  </query>


  <query id="grid">
    <![CDATA[ WITH
    MEMBER [Measures].[Единица измерения]
    AS  '[Показатели].[ВедСтат_Свод].CurrentMember.Properties("Единица измерения")'
    select
    {
      
            Filter
            (<%gridMarks%>,
                
                [Measures].[Значение] > 0
            )
    
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows,
    non empty  { [Measures].[Значение],[Measures].[Единица измерения] } on columns
FROM [ВЕДСТАТ_Свод]
where
    (
       [Период].[Год Квартал Месяц].[Данные всех периодов].[<%lastdate%>]
    )
]]>
</query>
  <query id="Chart">
    <![CDATA[/*select
    non empty      
    {
       <%chartMarks%>.[<%Pokaz%>]      
    } on columns     ,
    non empty       
    {
        [Период].[Год Квартал Месяц].[Данные всех периодов].[1998]:[Период].[Год Квартал Месяц].[Данные всех периодов].[<%lastdate%>] 
    } on rows  
FROM [ВЕДСТАТ_Свод]      
where      
    (
        <%baseRegion%>    
    )    */	
     select
non empty
{
[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество единиц хранения].[Управленческая документация],
[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество единиц хранения].[Документы личного происхождения],
[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество единиц хранения].[Документы по личному составу],
[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество единиц хранения].[Фотодокументы],
[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество единиц хранения].[Прочие документы]
}  on columns,
non empty {[Период].[Год Квартал Месяц].[Данные всех периодов].children
  }on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].[Город Губкинский],
[measures].[значение]
    ) 	 ]]>
  </query>  
  <query id="text">
    <![CDATA[select
    non empty   
    {
        <%textMarks%>
} on rows,     
non empty   
{
    [Период].[Год Квартал Месяц].[Данные всех периодов].[<%postdate%>]:[Период].[Год Квартал Месяц].[Данные всех периодов].[<%postdate%>]
} on columns   
FROM [ВЕДСТАТ_Свод]   
where   
    (
        <%baseRegion%> 
    )   ]]>
  </query>
    <query id="text2">
    <![CDATA[with            
    member [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_0]            
    as '100           
        *[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая ремонта]/[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь]'             
    member [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_1]            
    as '100           
        *[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая оснащения спецоборудованием (стеллажи, лестницы, подъемники и т.д.)]/[Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь]'            
select
    --non empty             
    {
        --        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ_],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество муниципальных архивов],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь].[Площадь архивохранилищ],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая ремонта],
        [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_0],
        [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая оснащения спецоборудованием (стеллажи, лестницы, подъемники и т.д.)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_1]            
    } on rows,
  --  non empty            
    {
    BottomCount(
        Filter          
        (
            [Период].[Год Квартал Месяц].[Данные всех периодов].children,
            (
                not IsEmpty       
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество муниципальных архивов] 
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Количество муниципальных архивов]                
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь]                
                )/*and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Занимаемая архивами площадь].[Площадь архивохранилищ]                
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая ремонта]                
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_0]                
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[ОРГАНИЗАЦИЯ СОДЕРЖАНИЯ МУНИЦИПАЛЬНЫХ АРХИВОВ].[Площадь архивов, требующая оснащения спецоборудованием (стеллажи, лестницы, подъемники и т.д.)]                
                )and not IsEmpty
                (
                    [Показатели].[ВедСтат_Свод].[Все показатели].[вычисляемое значение_1]
                )*/
            )  
        ),1)
    } on columns             
FROM [ВЕДСТАТ_Свод]             
where             
    (
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].[Город Губкинский],
        [measures].[значение]            
    )]]>
  </query>
</root>