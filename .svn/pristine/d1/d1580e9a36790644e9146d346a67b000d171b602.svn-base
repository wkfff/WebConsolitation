namespace Krista.FM.RIA.Extensions.E86N.Utils
{
    /// <summary>
    ///   Перечисление состояний документов
    /// </summary>
    public enum StatesType : byte
    {
        /// <summary>
        ///   состояние не указано
        /// </summary>
        NoState = 0, 

        /// <summary>
        ///   состояние Заблокирован
        /// </summary>
        Locked = 1, 

        /// <summary>
        ///   состояние Создан
        /// </summary>
        Сreated = 2, 

        /// <summary>
        ///   На редактировании
        /// </summary>
        OnEditing = 3, 

        /// <summary>
        ///   На рассмотрении
        /// </summary>
        UnderConsideration = 4, 

        /// <summary>
        ///   На доработке
        /// </summary>
        OnCompletion = 5, 

        /// <summary>
        ///   На выполнении
        /// </summary>
        OnPerforming = 6, 

        /// <summary>
        ///   состояние Завершено
        /// </summary>
        Completed = 7, 

        /// <summary>
        ///   состояние Экспортирован
        /// </summary>
        Exported = 8
    }
}
