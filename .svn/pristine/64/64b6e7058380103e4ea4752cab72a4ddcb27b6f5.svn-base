<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FO_0006_0003_date">
    <![CDATA[
-- FO_0006_0003_date
with
    member [Measures].[ДанныеНа]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период__Период].[Период__Период].Members,
                not IsEmpty([Measures].[С начала года])
            ),
            1
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [ФНС_4 НМ_Районы]  
  ]]>
  </query>

  <query id="FO_0006_0003_compare_grid">
    <![CDATA[       
-- FO_0006_0003_compare_Grid  
with  
    set [Города]  
    as 'Filter  
        (
            [Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край].[Городские округа Алтайского края].Children,
            not  
            (
                [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember is [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember.Parent.DataMember  
            ) and  
            (
                [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember.Properties("Тип территории") = "ГО"  
            )  
        )'  
    set [Районы]  
    as 'Filter  
        (
            [Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край].[Районы Алтайского края].Children,
            not  
            (
                [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember is [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember.Parent.DataMember  
            ) and  
            (
                [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember.Properties("Тип территории") = "МР"  
            )  
        )'  
    member [Measures].[Худший Ранг по абсолютной сумме недоимки]  
    as '""'  
    member [Measures].[Ранг по абсолютной сумме недоимки]  
    as '""'  
    set [периоды]  
    as '{<%months%>}'  
    member [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по краю]  
    as '[Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край]'  
    member [Районы__Сопоставимый].[Районы__Сопоставимый].[По крупнейшим налогоплательщикам]  
    as '[Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край].[Крупнейшие налогоплательщики]'  
    member [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по муниципальным районам]  
    as '[Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край].[Районы Алтайского края]'  
    member [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по городским округам]  
    as '[Районы__Сопоставимый].[Районы__Сопоставимый].[Все районы].[Алтайский край].[Городские округа Алтайского края]'  
    member [Доходы__Группы].[Доходы__Группы].[Всего ]  
    as '  
        (
            [Доходы__Группы].[Доходы__Группы].[Все показатели].[Всего],
            [Measures].[С начала года]  
        )/<%mul%>'  
    member [Доходы__Группы].[Доходы__Группы].[Федеральные налоги]  
    as '  
        (
            [Доходы__Группы].[Доходы__Группы].[Все показатели].[Всего].[Федеральные налоги и сборы],
            [Measures].[С начала года]  
        )/<%mul%>'  
    member [Доходы__Группы].[Доходы__Группы].[Региональные налоги]  
    as '  
        (
            [Доходы__Группы].[Доходы__Группы].[Все показатели].[Всего].[Региональные налоги и сборы],
            [Measures].[С начала года]  
        )/<%mul%>'  
    member [Доходы__Группы].[Доходы__Группы].[Местные налоги]  
    as '  
        (
            [Доходы__Группы].[Доходы__Группы].[Все показатели].[Всего].[Местные налоги и сборы],
            [Measures].[С начала года]  
        )/<%mul%>'  
    member [Доходы__Группы].[Доходы__Группы].[Налоги со спец. режимом]  
    as '  
        (
            [Доходы__Группы].[Доходы__Группы].[Все показатели].[Всего].[Налоги со специальным налоговым режимом],
            [Measures].[С начала года]  
        )/<%mul%>'  
    member [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период]  
    as 'Sum 
        (
            Tail 
            (
                [периоды],
                1 
            ) 
        ) -  [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь]'
        member [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период, %]    
    as '(IIF   
        (
             [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь] = 0 OR ([Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь],
             [Районы__Сопоставимый].[Районы__Сопоставимый].CurrentMember,[Доходы__Группы].[Доходы__Группы].CurrentMember) = null OR [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь] = "",
             "null",
            Sum    
            (
                Tail    
                (
                    [периоды],
                    1    
                )    
            ) /  [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь]   
        )*<%mul%> - <%mul%>)*100'   
    member [Период__Период].[Период__Период].[Данные всех периодов].[Ранг по приросту недоимки]   
    as '""'   
    member [Период__Период].[Период__Период].[Данные всех периодов].[худший Ранг по приросту недоимки]  
    as '""'
    member [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по муниципальным районам и городским округам]
    as '[Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по муниципальным районам]+
        [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по городским округам]'
select
    {
        {
            [Доходы__Группы].[Доходы__Группы].[Всего ]      
        }     
        *     
        {
            [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь],
            [периоды],
            [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период],
            [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период, %],
            [Период__Период].[Период__Период].[Данные всех периодов].[Ранг по приросту недоимки]  
        } +     
        {
            [Доходы__Группы].[Доходы__Группы].[Федеральные налоги],
            [Доходы__Группы].[Доходы__Группы].[Региональные налоги],
            [Доходы__Группы].[Доходы__Группы].[Местные налоги],
            [Доходы__Группы].[Доходы__Группы].[Налоги со спец. режимом]     
        }     
        *     
        {
            [Период__Период].[Период__Период].[Данные всех периодов].[<%predYear%>].[Полугодие 2].[Квартал 4].[Декабрь],
            [периоды],
            [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период],
            [Период__Период].[Период__Период].[Данные всех периодов].[Прирост(снижение) недоимки за период, %]
        } + 
        {
            [Доходы__Группы].[Доходы__Группы].[Налоги со спец. режимом]
        } 
        *
        {
            [Период__Период].[Период__Период].[Данные всех периодов].[худший Ранг по приросту недоимки]
        } 
    } on columns,
    {
        [Районы],
        [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по муниципальным районам],
        [Города],
        [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по городским округам],
        [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по муниципальным районам и городским округам],
        [Районы__Сопоставимый].[Районы__Сопоставимый].[По крупнейшим налогоплательщикам],
        [Районы__Сопоставимый].[Районы__Сопоставимый].[Всего по краю]             
    } on rows
from  [ФНС_4 НМ_Районы]  
where  
    (
        [Задолженность__ФНС Сопоставимый].[Задолженность__ФНС Сопоставимый].[Все показатели].[Раздел I. Задолженность по налогам и сборам в бюджетную систему Российской Федерации].[ЗАДОЛЖЕННОСТЬ ПЕРЕД БЮДЖЕТОМ ПО НАЛОГАМ И СБОРАМ ВСЕГО В ТОМ ЧИСЛЕ:].[ВОЗМОЖНАЯ К ВЗЫСКАНИЮ ЗАДОЛЖЕННОСТЬ].[НЕДОИМКА]  
    )  
  ]]>
  </query>
  
</root>