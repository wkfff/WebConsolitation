<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="mapa">
    <![CDATA[with
    member [Показатели].[Рынок труда].[Все].[Уровень безработицы, процент]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность безработных граждан]
                )/
                (
                      [Показатели].[Рынок труда].[Показатели].[Численность экономически активного населения]
                )
            )
            *100
        )'
member [Показатели].[Рынок труда].[ydal] 
    as '[Территории].[РФ].CurrentMember.Properties("Краткое наименование")' 
select
   -- non empty 
    {
        [Территории].[РФ].[РФ].[Российская  Федерация].[Центральный федеральный округ].[Костромская область].[Костромская область].children
     -- [2010].[Полугодие 1].[Квартал 1].[Февраль].lag(1).children
    } on rows ,
    non empty                       
    {
        [Показатели].[Рынок труда].[Все].[Уровень безработицы, процент],
        [Показатели].[Рынок труда].[ydal]
    }  on columns                      
from [СТАТ_Мониторинг ситуации на рынке труда]                       
where                       
    (
        [Measures].[Значение],<%p1%>
    )]]>
  </query>  
  <query id="mapo">
    <![CDATA[with
    member [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность вакансий]
                )
            )            
        )'
            member [Показатели].[Рынок труда].[ydal] 
    as '[Территории].[РФ].CurrentMember.Properties("Краткое наименование")' 
select
    --non empty 
    {
        [Территории].[РФ].[РФ].[Российская  Федерация].[Центральный федеральный округ].[Костромская область].[Костромская область].children
     -- [2010].[Полугодие 1].[Квартал 1].[Февраль].lag(1).children
    } on rows ,
    non empty                       
    {
        [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек],
        [Показатели].[Рынок труда].[ydal] 
    }  on columns                      
from [СТАТ_Мониторинг ситуации на рынке труда]                       
where                       
    (
        [Measures].[Значение],<%p1%>
    )]]>
  </query>
  <query id="ld">
    <![CDATA[with
    member [Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность незанятых граждан, состоящих на учёте в органах службы занятости населения]
                )/
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность вакансий]
                )
            )            
        )'
    member [Показатели].[Рынок труда].[Все].[Уровень безработицы, процент]
    as '
        (
            (
                (
                    [Показатели].[Рынок труда].[Показатели].[Численность безработных граждан]
                )/
                (
                      [Показатели].[Рынок труда].[Показатели].[Численность экономически активного населения]
                )
            )
            *100
        )'
select
    non empty
    {
            [Показатели].[Рынок труда].[Все].[Уровень безработицы, процент],
[Показатели].[Рынок труда].[Все].[Коэффициент напряженности на рынке труда,  на 1 вакансию/человек]
     
    }   on columns,
    non empty
    {
        [Период].[Период].[День].members
    } on rows
from [СТАТ_Мониторинг ситуации на рынке труда]
where
    (
        [Measures].[Значение],[Территории].[РФ].[РФ].[Российская  Федерация].[Центральный федеральный округ].[Костромская область].[Костромская область]
    )]]>
  </query>
</root>