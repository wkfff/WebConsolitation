    member [КД].[Сопоставимый].[Единый сельскохозяйственный налог ]
    as '[КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА СОВОКУПНЫЙ ДОХОД].[Единый сельскохозяйственный налог]' 
    member [КД].[Сопоставимый].[Государственная пошлина ]
    as '[КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[ГОСУДАРСТВЕННАЯ ПОШЛИНА]' 
    member [КД].[Сопоставимый].[Задолженность и перерасчеты по отмененным налогам, сборам и иным обязательным платежам ]
    as '[КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[ЗАДОЛЖЕННОСТЬ И ПЕРЕРАСЧЕТЫ ПО ОТМЕНЕННЫМ НАЛОГАМ, СБОРАМ И ИНЫМ ОБЯЗАТЕЛЬНЫМ ПЛАТЕЖАМ]'
    ---- 

    set [Группы КД] 
    as '
        {
            [КД].[Сопоставимый].[Налог на прибыль организаций ],
            [КД].[Сопоставимый].[Налог на доходы физических лиц ],
