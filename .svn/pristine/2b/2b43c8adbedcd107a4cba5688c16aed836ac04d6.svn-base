<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="Grid">
    <![CDATA[with 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Сметная стоимость;в прогнозном уровне цен или предполагаемая (предельная) стоимость строительства] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Сметная стоимость].[в прогнозном уровне цен или предполагаемая (предельная) стоимость строительства]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Сметная стоимость;% освоения] 
    as '
    iif([Строительство__Ход работ].[Строительство__Ход работ].[Сметная стоимость;в прогнозном уровне цен или предполагаемая (предельная) стоимость строительства]<>0,
    [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;ВСЕГО]/ [Строительство__Ход работ].[Строительство__Ход работ].[Сметная стоимость;в прогнозном уровне цен или предполагаемая (предельная) стоимость строительства],
    null)' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;ВСЕГО] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Освоено капитальных вложений в текущих ценах за отчетный период текущего года].DataMember +[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.11 г.].[ВСЕГО:]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.12 г.].[ВСЕГО:]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.13 г.].[ВСЕГО:]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;окружного бюджета] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Освоено капитальных вложений в текущих ценах за отчетный период текущего года].[в том числе за счёт средств:].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.11 г.].[в том числе за счёт средств:].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.12 г.].[в том числе за счёт средств:].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.13 г.].[в том числе за счёт средств:].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.10 г.].[в том числе за счёт средств:].[окружного бюджета]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;местного бюджета] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Освоено капитальных вложений в текущих ценах за отчетный период текущего года].[в том числе за счёт средств:].[местного бюджета]+ 
        [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.11 г.].[в том числе за счёт средств:].[бюджетов муниципальных образований]+ 
        [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.12 г.].[в том числе за счёт средств:].[бюджетов муниципальных образований]+
        [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.13 г.].[в том числе за счёт средств:].[бюджетов муниципальных образований]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;прочих источников] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Освоено капитальных вложений в текущих ценах за отчетный период текущего года].[в том числе за счёт средств:].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.11 г.].[в том числе за счёт средств:].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.12 г.].[в том числе за счёт средств:].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем незавершенного строительства в текущих ценах на 01.01.13 г.].[в том числе за счёт средств:].[прочих источников]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;ВСЕГО] 
    as '
        (
            [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Финансирование за отчётный период текущего года].DataMember
        ) + 
        (
            [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.11 года].DataMember+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.12 года].DataMember+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.13 года].DataMember
        )' 
        
    member [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;всего окружной бюджет] 
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Финансирование за отчётный период текущего года].[в том числе за счёт средств].[всего окружной бюджет]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.11 года].[в том числе за счёт средств].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.12 года].[в том числе за счёт средств].[окружного бюджета]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.13 года].[в том числе за счёт средств].[окружного бюджета]' 
    member [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;бюджетов муниципальных образований] 
    as ' [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.11 года].[в том числе за счёт средств].[бюджетов муниципальных образований]+
     [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.12 года].[в том числе за счёт средств].[бюджетов муниципальных образований]+
     [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.13 года].[в том числе за счёт средств].[бюджетов муниципальных образований]+
     [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Финансирование за отчётный период текущего года].[в том числе за счёт средств].[бюджетов муниципальных образований]'
    member [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;прочих источников]
    as '[Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.11 года].[в том числе за счёт средств].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.12 года].[в том числе за счёт средств].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Объем финансирования с начала строительства на 01.01.13 года].[в том числе за счёт средств].[прочих источников]+ [Строительство__Ход работ].[Строительство__Ход работ].[Данные всех источников].[Финансирование за отчётный период текущего года].[в том числе за счёт средств].[прочих источников]'  
    member [Строительство__Ход работ].[Строительство__Ход работ].[% освоенных капитальных вложений от финансирования;ВСЕГО] 
    as ' iif
        (
            not isempty
            (
                [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;ВСЕГО]
            ),
            (
                [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;ВСЕГО]/ [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;ВСЕГО]
            ),
            null
        )'  
    member [Строительство__Ход работ].[Строительство__Ход работ].[% освоенных капитальных вложений от финансирования;всего окружной бюджет] 
    as ' iif
        (
            not isempty
            (
                [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;всего окружной бюджет]
            ),
            (
                [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;окружного бюджета]/ [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;всего окружной бюджет]
            ),
            null
        )'  /**/

select
    {
        [Сметная стоимость;в прогнозном уровне цен или предполагаемая (предельная) стоимость строительства],
        [Строительство__Ход работ].[Строительство__Ход работ].[Сметная стоимость;% освоения],
        [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;ВСЕГО],
        [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;окружного бюджета],
        [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;местного бюджета],
        [Строительство__Ход работ].[Строительство__Ход работ].[Освоено капитальных вложений в текущих ценах с начала строительства;в том числе за счет средств:;прочих источников],
        [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;ВСЕГО],
        [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;всего окружной бюджет],
        [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;бюджетов муниципальных образований],
        [Строительство__Ход работ].[Строительство__Ход работ].[Финансирование за отчетный период в текущих ценах с начала строительства;в том числе за счет средств:;прочих источников],
        [Строительство__Ход работ].[Строительство__Ход работ].[% освоенных капитальных вложений от финансирования;ВСЕГО],
        [Строительство__Ход работ].[Строительство__Ход работ].[% освоенных капитальных вложений от финансирования;всего окружной бюджет]

    } on columns,
    non empty{        
<%FilterGrid%> 
    } on rows
from [ЭО_Мониторинг строительства_Объекты строительства]
where ([Measures].[Отчет],
<%ChosenCurPeriod%>)
]]>
  </query>
  <query id="ComboCustomers">
    <![CDATA[with
member  [Источники данных].[Источники данных].[uname]
as '[Строительство__Заказчики].[Строительство__Заказчики].currentmember.unique_name'
member  [Источники данных].[Источники данных].[mounth]
as '[Строительство__Заказчики].[Строительство__Заказчики].currentmember.name'
member  [Источники данных].[Источники данных].[year]
as '[Строительство__Заказчики].[Строительство__Заказчики].currentmember.parent.name'

select
non empty
{
filter(
[Строительство__Заказчики].[Строительство__Заказчики].[(All)].[Все заказчики].children,
[Measures].[Отчет]> 0)
    } on rows,
    {
    [Источники данных].[Источники данных].[mounth],
        [Источники данных].[Источники данных].[year],
    [Источники данных].[Источники данных].[uname],

 [Источники данных].[Источники данных].[Все источники данных]
    } on columns
from [ЭО_Мониторинг строительства_Объекты строительства]
where ( [Measures].[Отчет])
]]>
  </query>
  <query id="ComboYear">
    <![CDATA[
    with
member  [Источники данных].[Источники данных].[uname]
as '[Период__Период].[Период__Период].currentmember.unique_name'
member  [Источники данных].[Источники данных].[mounth]
as '[Период__Период].[Период__Период].currentmember.name'
member  [Источники данных].[Источники данных].[year]
as '[Период__Период].[Период__Период].currentmember.parent.parent.parent.name'
select
non empty   {
filter(
[Период__Период].[Период__Период].[Месяц].members,
[Measures].[Отчет]> 0)
    } on rows,
    {
    [Источники данных].[Источники данных].[mounth],
        [Источники данных].[Источники данных].[year],
    [Источники данных].[Источники данных].[uname],

 [Источники данных].[Источники данных].[Все источники данных]
    } on columns
from [ЭО_Мониторинг строительства_Объекты строительства]
where ( [Measures].[Отчет],[Строительство__Заказчики].[Строительство__Заказчики].[Все заказчики] )
]]>
  </query>


