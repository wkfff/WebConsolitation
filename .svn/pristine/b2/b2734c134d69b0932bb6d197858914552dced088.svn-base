  <query id="STAT_0002_0011_item">
    <![CDATA[
--STAT_0002_0011_chart
 with 
    member [Measures].[Единицы] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Единица измерения")' 
   
    member  [Период__Период].[Период__Период].[Период] 
    as 'Aggregate({<%periods%>})' 
    member [Measures].[Прирост] 
    as 'IIF 
        (
            [Measures].[Значение ] = null and [Период__Период].[Период__Период].CurrentMember <> [Период__Период].[Период__Период].[Тип строки],
            null,
            IIF 
            (
                [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].[Тип строки],
                2,
                IIF 
                (
                    [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].[Показатель],
                    [Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name + "," + " "+[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Единица измерения"),
                    IIF 
                    (
                        [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].[Показатель без ед],
                        [Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name,
                        [Measures].[Темп прироста АППГ] 
                    ) 
                ) 
            ) 
        )'   
    member [Период__Период].[Период__Период].[Показатель] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name + "," + " "+[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Единица измерения")'  
    set [Показатели] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].Children'  
    member [Период__Период].[Период__Период].[Тип строки] 
    as 'IIF 
        (
            [Measures].CurrentMember is [Measures].[Значение ],
            "0",
            IIF 
            (
                [Measures].CurrentMember is [Measures].[Прирост],
                "1",
                "2" 
            ) 
        )' 
    member [Период__Период].[Период__Период].[Показатель без ед] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name'  
    set [Организации]
    as 'Filter([Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации].Children,
    [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Тип организации") = "Основные предприятия" and
    [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "<%kind%>"
    
    )'
select
    non empty   
    {
     [Организации]
    } on columns,
    non empty
    {
     
     [Measures].[Единицы]
            
    } on rows 
From  [ОРГАНИЗАЦИИ_Характеристика деятельности] 
where 
    (
    <%measure%>,
    <%period%>,
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ] 
    ) 
    ]]>
  </query>


