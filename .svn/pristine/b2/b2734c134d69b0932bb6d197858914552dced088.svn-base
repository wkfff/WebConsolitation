<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="STAT_0002_0011_Date">
    <![CDATA[ 
    with 
    member [Measures].[Имя] 
    as 'IIF 
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            [Период__Период].[Период__Период].CurrentMember.Name + " год",
            IIF 
            (
                [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 1",
                "1 квартал" + " " + [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Name + " года" ,
                IIF 
                (
                    [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 2",
                    "2 квартал" + " " + [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Name + " года" ,
                    IIF 
                    (
                        [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 3",
                        "3 квартал" + " " + [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Name + " года" ,
                        IIF 
                        (
                            [Период__Период].[Период__Период].CurrentMember.Name = "Квартал 4",
                            "4 квартал" + " " + [Период__Период].[Период__Период].CurrentMember.Parent.Parent.Name + " года" ,
                            nill 
                        ) 
                    ) 
                ) 
            ) 
        )' 
    member [Measures].[Уникальное имя] 
    as ' [Период__Период].[Период__Период].CurrentMember.UniqueName ' 
    member [Measures].[Уровень] 
    as 'IIF 
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            0,
            IIF 
            (
                [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Квартал],
                1,
                2 
            ) 
        )' 
    set [Периоды] 
    as 'Generate 
        (
            {
                [Период__Период].[Период__Период].[Данные всех периодов].[1998] : [Период__Период].[Период__Период].[Данные всех периодов].[2020] 
            },
            {
                [Период__Период].[Период__Период].CurrentMember 
            } + Filter 
            (
                Descendants 
                (
                    [Период__Период].[Период__Период].CurrentMember,
                    [Период__Период].[Период__Период].[Квартал],
                    SELF 
                ),
                not 
                (
                    [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].CurrentMember.Parent.DataMember 
                ) and not 
                (
                    [Период__Период].[Период__Период].CurrentMember.Name = "Остатки на начало года" 
                ) and not 
                (
                    [Период__Период].[Период__Период].CurrentMember.Name = "Данные года" 
                ) and not 
                (
                    [Период__Период].[Период__Период].CurrentMember.Name = "Данные полугодия" 
                ) 
            ) 
        )' 
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Организации] 
    as 'Aggregate({[Реестр организаций]})' 
    set [Реестр организаций] 
    as 'Filter 
        (
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации].Children,
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Тип организации") = "Основные предприятия" 
        )' 
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень],
        [Measures].[Значение] 
    } on columns,
    Filter 
    (
        [Периоды],
        not IsEmpty([Measures].[Значение]) 
    ) on rows 
from [ОРГАНИЗАЦИИ_Характеристика деятельности] 
where 
    (
        [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели],
        [Организации__Реестр организаций].[Организации__Реестр организаций].[Организации],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ] 
    ) ]]>
  </query>

  <query id="STAT_0002_0011_Kinds">
    <![CDATA[
with
    set [Реестр организаций]
    as 'Filter
        (
            [Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации].Children,
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Тип организации") = "Основные предприятия"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Нефть и газ]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Нефть и газ"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Геология]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Геология]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Геология"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Производство нефтепродуктов]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Производство нефтепродуктов]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Производство нефтепродуктов"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Энергетика]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Энергетика]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Энергетика"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Лесопромышленный комплекс]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Лесопромышленный комплекс]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Лесопромышленный комплекс"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Связь]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Связь]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Связь"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Транспорт]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Транспорт]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Транспорт"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Строительство]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Строительство]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Строительство"
        )'
    member [Организации__Реестр организаций].[Организации__Реестр организаций].[Агропромышленный комплекс]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].[Все организации]'
    set [Агропромышленный комплекс]
    as 'Filter
        (
            [Реестр организаций],
            [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение") = "Агропромышленный комплекс"
        )'
    member [Measures].[Имя]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Name '
    member [Measures].[Уникальное имя]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.UniqueName'
    member [Measures].[Тип]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Тип организации")'
    member [Measures].[Уровень]
    as 'IIF
        (
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Геология]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Производство нефтепродуктов]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Энергетика]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Лесопромышленный комплекс]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Связь]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Транспорт]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Строительство]
            ) or
            (
                [Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember is [Организации__Реестр организаций].[Организации__Реестр организаций].[Агропромышленный комплекс]
            ) ,
            0,
            1
        )'
    member [Measures].[Вид деятельности]
    as '[Организации__Реестр организаций].[Организации__Реестр организаций].CurrentMember.Properties("Подчинение")'
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень]
    } on columns,
    {
     
   
            {
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Нефть и газ],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Геология],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Производство нефтепродуктов],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Энергетика],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Лесопромышленный комплекс],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Связь],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Транспорт],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Строительство],
                [Организации__Реестр организаций].[Организации__Реестр организаций].[Агропромышленный комплекс]  
            }
    } on rows
From  [ОРГАНИЗАЦИИ_Характеристика деятельности]
  ]]>
  </query>


  <query id="STAT_0002_0011_Measures">
    <![CDATA[
with  
    member [Measures].[Имя] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Name '
    member [Measures].[Уникальное имя] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.UniqueName'
    member [Measures].[Тип] 
    as '[Организации__Показатели работы].[Организации__Показатели работы].CurrentMember.Properties("Тип организации")'
    member [Measures].[Уровень] 
    as '0' 
    set [Показатели]
    as '{
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Численность работников списочного состава на конец отчетного месяца],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Фонд начисленной заработной платы работников за отчетный период],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Средняя заработная плата],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Просроченная задолженность по заработной плате работников списочного и несписочного состава, включая внешних совместителей, а также уволенных работников],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Количество свободных рабочих мест (вакантных должностей)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Объем проданных товаров несобственного производства],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Объем неурегулированной задолженности по платежам в бюджет и внебюджетные фонды],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Кредиторская задолженность (без НДС)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Кредиторская задолженность (без НДС): в т. ч. просроченная задолженность],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Дебиторская задолженность (без НДС)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Дебиторская задолженность (без НДС): в т. ч. просроченная задолженность],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Инвестиции в основной капитал, всего],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Инвестиции в основной капитал: в т.ч. в природоохранные объекты],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Наличае инвестиционной программы: объем финансирования (из всех источников финансирования)],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Наличае инвестиционной программы: исполнение на конец отчетного периода],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Наличае инвестиционной программы: количество созданых в период реализации программы рабочих мест],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Стоимость основных фондов по полной учетной стоимости на конец отчетного периода],
            [Организации__Показатели работы].[Организации__Показатели работы].[Все показатели].[Прибыль (убыток) до налогообложения за отчетный период]
        }'
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень] 
    } on columns,
    {
        [Показатели]
    }  on rows 
From  [ОРГАНИЗАЦИИ_Характеристика деятельности] 
  ]]>
  </query>
  
  <query id="STAT_0002_0011_lastDate">
    <![CDATA[
-- STAT_0002_0011_lastDate  
with
    member Measures.[ДанныеНа]
    as '[Период__Период].[Период__Период].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    Tail
    (
        NonEmptyCrossjoin
        (
