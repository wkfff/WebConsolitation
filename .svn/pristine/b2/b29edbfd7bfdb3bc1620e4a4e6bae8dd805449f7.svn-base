  <query id="iPad_0001_0001_GosDebts_v">
    <![CDATA[
--iPad_0001_0001_GosDebts_v
with     
    member Measures.[Численность постоянного населения]     
    as 'LookupCube     
        (
            "[ФСГС_Численность населения]",
            "     
            (
                [Measures].[Факт],
                [Вариант].[ФСГС Население].[Все].[Численность населения на 1 января],
                [ФСГС].[Численность населения].[Все].[Все население],
                " + MemberToStr     
                (
                    [Территории].[Сопоставимый].CurrentMember     
                ) + ",
                " + MemberToStr     
                (
                    [Период].[Период].CurrentMember.Parent.Parent.Parent
                ) + "     
            )"     
        ) / 1000'     
    member [МФ РФ].[Долговые обязательства_Сопоставимый].[Бюджет субъекта]     
    as 'LookupCube     
        (
            "[ФК_МесОтч_Доходы]",
            "     
            (
                [Measures].[Исполнено],
                [КД].[Сопоставимый].[Все коды доходов].[Доходы - всего в том числе:],
                [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта],
                " + MemberToStr     
                (
                    [Территории].[Сопоставимый].CurrentMember     
                ) + ",
                " + MemberToStr     
                (
                    [Период].[Период].CurrentMember     
                ) + "     
            )"     
        ) / 1000000'     
    member [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.]     
    as '     
        (
            [МФ РФ].[Долговые обязательства_Сопоставимый].[Все].[Объем государственного долга субъектов Российской Федерации]     
        ) / 1000000' 
        member [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга прошлый месяц, млн.руб.]
    as '
        (
            [МФ РФ].[Долговые обязательства_Сопоставимый].[Все].[Объем государственного долга субъектов Российской Федерации]
        ) / 1000000'
    member [МФ РФ].[Долговые обязательства_Сопоставимый].[Долговая нагрузка]     
    as 'IIF     
        (
            IsEmpty     
            (
                [МФ РФ].[Долговые обязательства_Сопоставимый].[Бюджет субъекта]     
            ) or [МФ РФ].[Долговые обязательства_Сопоставимый].[Бюджет субъекта] = 0,
            null,
            [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.] / [МФ РФ].[Долговые обязательства_Сопоставимый].[Бюджет субъекта]     
        )'     
    member  [Measures].[Значение]     
    as ' IIF   
        (
            
            (
                [МФ РФ].[Долговые обязательства_Сопоставимый].CurrentMember is [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.] or [МФ РФ].[Долговые обязательства_Сопоставимый].CurrentMember is [МФ РФ].[Долговые обязательства_Сопоставимый].[Государственный долг на душу населения, тыс.руб/чел.]  
            ),
            (
                [Measures].[Объем долговых обязательств],
                [Период].[Период].[Данные всех периодов]<%period_year%>    
            ),
            (
                [Measures].[Объем долговых обязательств],
                [Период].[Период].[Данные всех периодов]<%period_first_year%>  
            )  
        )'  
    member  [Measures].[Значение прошлый год]     
    as 'IIF  
        (
            (
                [МФ РФ].[Долговые обязательства_Сопоставимый].CurrentMember is [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.] or [МФ РФ].[Долговые обязательства_Сопоставимый].CurrentMember is [МФ РФ].[Долговые обязательства_Сопоставимый].[Государственный долг на душу населения, тыс.руб/чел.]  
            ),
            (
                [Measures].[Объем долговых обязательств],
                [Период].[Период].[Данные всех периодов]<%period_last_date%> 
            ),
            (
                [Measures].[Объем долговых обязательств],
                [Период].[Период].[Данные всех периодов]<%period_last_year%>
            )  
        )'  
    member  [Measures].[Темп роста к прошлому году]     
    as 'IIF     
        (
            IsEmpty     
            (
                (
                    [Measures].[Значение прошлый год]     
                )     
            ) or     
            (
                [Measures].[Значение прошлый год]     
            ) <= 0,
            null,
            ( [Measures].[Значение] ) /     
            (
                [Measures].[Значение прошлый год]     
            )     
        )'     
    member [МФ РФ].[Долговые обязательства_Сопоставимый].[Государственный долг на душу населения, тыс.руб/чел.]     
    as 'IIF     
        (
            IsEmpty     
            (
                Measures.[Численность постоянного населения]     
            ) or Measures.[Численность постоянного населения] = 0,
            null,
            [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.] /     
            (
                Measures.[Численность постоянного населения]     
            )     
            * 1000     
        )'     
    member Measures.[Ранг государственный долг]     
    as 'IIF     
        (
            IsEmpty ( [Measures].[Значение] )  or ([Measures].[Значение] <= 0),
            null,
            Rank     
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order     
                (
                    Filter     
                    (
                        [Территории].[Сопоставимый].CurrentMember.Siblings,
                         not IsEmpty ( [Measures].[Значение] )  and not ([Measures].[Значение] <= 0)
                    ),
                    [Measures].[Значение]     ,
                    BASC     
                ),
                [Measures].[Значение]     
            )     
        )'     
    member [Measures].[Налоговые и неналоговые доходы]     
    as 'LookupCube     
        (
            "[ФК_МесОтч_Доходы]",
            "     
            (
                [Measures].[Назначено],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ],
                [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта],
                " + MemberToStr     
                (
                    [Территории].[Сопоставимый].CurrentMember     
                ) + ",
                " + MemberToStr     
                (
                    [Период].[Период].CurrentMember     
                ) + "     
            )"     
        )'     
    member [Measures].[Государственный долг]     
    as '     
        (
            [Measures].[Объем долговых обязательств] ,
            [МФ РФ].[Долговые обязательства_Сопоставимый].[Все].[Объем государственного долга субъектов Российской Федерации]     
        )'     
    member [Measures].[Доходы от предпринимательской деятельности]     
    as 'LookupCube     
        (
            "[ФК_МесОтч_Доходы]",
            "     
            (
                [Measures].[Назначено],
                [КД].[Сопоставимый].[Все коды доходов].[ДОХОДЫ ОТ ПРЕДПРИНИМАТЕЛЬСКОЙ И ИНОЙ ПРИНОСЯЩЕЙ ДОХОД ДЕЯТЕЛЬНОСТИ],
                [Уровни бюджета].[СКИФ].[Все].[Бюджет субъекта],
                " + MemberToStr     
                (
                    [Территории].[Сопоставимый].CurrentMember     
                ) + ",
                " + MemberToStr     
                (
                    [Период].[Период].CurrentMember     
                ) + "     
            )"     
        )'     
    member [Measures].[Утвержденные доходы]     
    as '[Measures].[Налоговые и неналоговые доходы]+ [Measures].[Доходы от предпринимательской деятельности]'     
    member [Measures].[Привлечение бюджетных кредитов]     
    as 'LookupCube     
        (
            "[ФК_МесОтч_Источники финансирования]",
            "     
            (
                [Measures].[Исполнено],
                [КИФ].[Сопоставимый].[Все источники финансирования].[ИСТОЧНИКИ ВНУТРЕННЕГО ФИНАНСИРОВАНИЯ ДЕФИЦИТОВ БЮДЖЕТОВ].[Бюджетные кредиты от других бюджетов бюджетной системы Российской Федерации].[Получение бюджетных кредитов от других бюджетов бюджетной системы Российской Федерации в валюте Российской Федерации],
                " + MemberToStr     
                (
                    [Территории].[Сопоставимый].CurrentMember     
                ) + ",
                " + MemberToStr     
                (
                    [Период].[Период].CurrentMember     
                ) + "     
            )"     
        )'     
    member [МФ РФ].[Долговые обязательства_Сопоставимый].[Предельный объем долга]     
    as 'IIF     
        (
            IsEmpty     
            (
                [Measures].[Утвержденные доходы]     
            ) or     
            (
                [Measures].[Утвержденные доходы]     
            ) = 0,
            null,
            (
                [Measures].[Государственный долг] - [Measures].[Привлечение бюджетных кредитов]     
            ) / [Measures].[Утвержденные доходы]     
        )'     
    member Measures.[Ранг государственный долг РФ]   
    as 'IIF   
        (
            IsEmpty ( [Measures].[Значение] )  or ([Measures].[Значение] <= 0),
            null,
            Rank   
            (
                [Территории].[Сопоставимый].CurrentMember,
                Order   
                (
                    Filter   
                    (
                        [Территории].[Сопоставимый].[Субъект РФ].Members,
                        not IsEmpty ( [Measures].[Значение] )  and not ([Measures].[Значение] <= 0)
                    ),
                    [Measures].[Значение],
                    BASC   
                ),
                [Measures].[Значение]   
            )   
        )'  
    member Measures.[Худший ранг государственный долг]     
    as 'max     
        (
            [Территории].[Сопоставимый].CurrentMember.Siblings,
            Measures.[Ранг государственный долг]     
        )'     
    member Measures.[Худший ранг государственный долг РФ]     
    as 'max     
        (
            [Территории].[Сопоставимый].[Субъект РФ].Members,
            Measures.[Ранг государственный долг РФ]     
        )'     
    member [Measures].[Темп роста к прошлому году ]     
    as '[Measures].[Темп роста к прошлому году]     
        * 100'     
SELECT
    {
        [Measures].[Значение],
        [Measures].[Ранг государственный долг],
        [Measures].[Ранг государственный долг РФ],
        [Measures].[Темп роста к прошлому году ],
        Measures.[Худший ранг государственный долг],
        Measures.[Худший ранг государственный долг РФ]     
    } on columns,
    {
        [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга, млн.руб.],
        [МФ РФ].[Долговые обязательства_Сопоставимый].[Объем долга прошлый месяц, млн.руб.],
        [МФ РФ].[Долговые обязательства_Сопоставимый].[Предельный объем долга],
        [МФ РФ].[Долговые обязательства_Сопоставимый].[Государственный долг на душу населения, тыс.руб/чел.],
        [МФ РФ].[Долговые обязательства_Сопоставимый].[Долговая нагрузка]     
    } on rows     
FROM  [МФ РФ_Долговые обязательства]     
where     
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[<%region%>].[<%state_area%>]
    )                  
 ]]>
  </query>

