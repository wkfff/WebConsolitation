<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="last_date">
    <![CDATA[ -- name: last_date   
SELECT
    { [Measures].[Значение]  } on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Год].Members,
                [Measures].[Значение] > 0
            )
        ).Item(0)
    } DIMENSION PROPERTIES MEMBER_UNIQUE_NAME on rows
FROM [ВЕДСТАТ_Свод]
where
    (
        <%baseRegion%>,
        [Источники данных].[Все источники данных],
        [Показатели].[ВедСтат_Свод].[Все показатели].[<%way_last_year%>]
    )  
	  ]]>
  </query>

 
  <query id="grid1">
    <![CDATA[ -- name: grid1
select
    non empty   
    {
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ ДЛЯ ОБЕСПЕЧЕНИЯ НАСЕЛЕНИЯ УСЛУГАМИ ТОРГОВЛИ, ОБЩЕСТВЕННОГО ПИТАНИЯ И БЫТОВОГО ОБСЛУЖИВАНИЯ].[Количество объектов розничной торговли (магазины павильоны, киоски)],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ ДЛЯ ОБЕСПЕЧЕНИЯ НАСЕЛЕНИЯ УСЛУГАМИ ТОРГОВЛИ, ОБЩЕСТВЕННОГО ПИТАНИЯ И БЫТОВОГО ОБСЛУЖИВАНИЯ].[Торговая площадь объектов розничной торговли],
        [Показатели].[ВедСтат_Свод].[Все показатели].[СОЗДАНИЕ УСЛОВИЙ ДЛЯ ОБЕСПЕЧЕНИЯ НАСЕЛЕНИЯ УСЛУГАМИ ТОРГОВЛИ, ОБЩЕСТВЕННОГО ПИТАНИЯ И БЫТОВОГО ОБСЛУЖИВАНИЯ].[Оборот розничной торговли]     
    } DIMENSION PROPERTIES [Показатели].[ВедСтат_Свод].[Единица измерения] on rows,
    non empty   
    {
        <%last_year%>.Lag(9):<%last_year%>
    } on columns   
FROM [ВЕДСТАТ_Свод]   
where   
    (
        [Источники данных].[Все источники данных],
        <%baseRegion%>,
        [Measures].[Значение]   
    )     
    ]]>
  </query>

