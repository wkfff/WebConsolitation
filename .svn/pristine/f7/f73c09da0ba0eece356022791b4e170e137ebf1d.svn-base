h2
{
    border-bottom: 1px solid #5c87b2;
    margin: 10px 25px 5px 0px;
    padding: 0px 0px 1px 0px;
}
.GroupReportTitle
{
    font-size: 14px;
    font-family: Verdana;
    font-weight: bold;
    cursor: pointer;    
}
.ReportTitle
{
    font-size: 14px;
    font-family: Verdana;
    vertical-align: top;
    vertical-align: bottom;
    font-weight: bold;    
}
.ReportCode
{
    color: silver;
    font-size: 10px;
    font-family: Verdana;  
    vertical-align: baseline;  
    padding-left: 5px;
}
.ReportDescription
{
    font-size: 12px;
    font-family: Verdana;    
}