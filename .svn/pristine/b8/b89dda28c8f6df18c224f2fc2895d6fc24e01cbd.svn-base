<RegionSettings>
  <!--
  // тип документа СКИФ для консолидированного уровня
  ConsolidateDocumentSKIFType,
  // тип документа СКИФ для уровня районов
  ConsolidateDocumentSKIFType,  
  // тип документа СКИФ для собственного бюджета
  OwnBudgetDocumentSKIFType,
  // уровень бюджета СКИФ для уровня районов
  RegionBudgetSKIFLevel,
  // начальный год для выбора в параметре
  BeginPeriodYear,
  // использовать куб [ФО_Первоначальный план доходов] для колонки уточненного плана
  OriginalPlanEnable,  
  // фильтр по КОСГУ
  KosguFilter
  -->
  <Region id="HMAO">
    <ConsolidateDocumentSKIFType>[ТипДокумента].[СКИФ].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов]</ConsolidateDocumentSKIFType>
    <RegionDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Данные конс. отчета субъекта в разрезе городов и районов (5+7+10)]</RegionDocumentSKIFType>
    <OwnBudgetDocumentSKIFType>[ТипДокумента].[СКИФ].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов]</OwnBudgetDocumentSKIFType>
    <RegionBudgetSKIFLevel>
      Aggregate
      (
      {
        [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет района],
        [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет поселения],
        [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет гор.округа]
      }
      )
    </RegionBudgetSKIFLevel>
    <BeginPeriodYear>2009</BeginPeriodYear>
    <OriginalPlanEnable>true</OriginalPlanEnable>
    <KosguFilter>[КОСГУ].[Сопоставимый].DefaultMember</KosguFilter>
  </Region>
  <Region id="Sakhalin">
    <ConsolidateDocumentSKIFType>[ТипДокумента].[СКИФ].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов]</ConsolidateDocumentSKIFType>
    <RegionDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Данные конс. отчета субъекта в разрезе городов и районов (5+7+10)]</RegionDocumentSKIFType>
    <OwnBudgetDocumentSKIFType>[ТипДокумента].[СКИФ].[Консолидированная отчетность и отчетность внебюджетных территориальных фондов]</OwnBudgetDocumentSKIFType>
    <RegionBudgetSKIFLevel>
      Aggregate
      (
      {
      [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет района],
      [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет поселения],
      [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет гор.округа]
      }
      )
    </RegionBudgetSKIFLevel>
    <BeginPeriodYear>2009</BeginPeriodYear>
    <OriginalPlanEnable>false</OriginalPlanEnable>
    <KosguFilter>[КОСГУ].[Сопоставимый].[Все коды ЭКР].[Неуказанная экономическая статья]</KosguFilter>
  </Region>
</RegionSettings>
