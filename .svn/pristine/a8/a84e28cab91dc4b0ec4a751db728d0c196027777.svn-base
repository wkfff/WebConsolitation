<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="DynamicText">
    <![CDATA[
     --EO/EO_00011_4 DynamicText
select
    {
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства],
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства].[Добыча топливно-энергетических полезных ископаемых],
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства].[Обрабатывающие производства],
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства].[Производство и распределение электроэнергии, газа и воды]
    } on columns,
    {
        Tail
        (
            { 
                filter
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members,
                    not isempty([Measures].[Отчет]) 
                )  
            },
            2   
        )  
    } on rows 
from   [ЭО_СЭП МО] 
where 
    (
        [Measures].[Отчет],
        [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Значение не указано],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [СЭП__Предприятия],
       <%baseRegion%>
    ) ]]>
  </query>
  <query id="Chart">
    <![CDATA[
    --EO/EO_0011_4 chart
with 
    member [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Население муниципального образования].[Года] 
    as ' [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.name ' 
select
    {
         [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства].Children ,
         [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Население муниципального образования].[Года] 
    } on columns,
    non empty 
    {
         filter
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал].members,
            not isempty
            (
                (
                    [Measures].[Отчет],
                    [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Объем промышленного производства]
                )
            )
        )
    } on rows 
from   [ЭО_СЭП МО] 
where 
    (
        [Measures].[Отчет],
        [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Значение не указано],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [СЭП__Предприятия],
        <%baseRegion%>
    )  ]]>
  </query>



</root>