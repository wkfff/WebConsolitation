        [Уровни бюджетов].[Местные бюджеты ]
    } ON rows ,
    {
        [Measures].[Проект ],
        [Measures].[Проект (прошлый год) ]
    }ON columns 
FROM  [ФО_Результат доходов с расщеплением] 
  
    ]]>
</query>

  
