            [Вариант__Проект доходов].[Вариант__Проект доходов].[Все].[<%period_pred_year%>].[<%incomes2%>]
        )/<%mul%>' 
    member [Measures].[Проект ]
    as 'LookupCube 
        (
            "[ФО_Результат расходов]",
            " 
            (
                [Measures].[Прогноз],
                [Период__Период].[Период__Период].[Данные всех периодов].[<%period_year%>],
                [Вариант__Проект расходов].[Вариант__Проект расходов].[Все варианты].[<%outcomes1%>],
                " + MemberToStr 
                (
                    IIF
                    (
                        [Уровни бюджетов].CurrentMember is [Уровни бюджетов].[Местные бюджеты ],
                        [Уровни бюджетов].[Все].[Все уровни].[Конс.бюджет субъекта].[Конс.бюджет МО],
                        [Уровни бюджетов].CurrentMember
                    ) 
                ) +" 
            )" 
        )/<%mul%>' 
    member [Measures].[Проект (прошлый год) ]
    as '
        LookupCube 
        (
            "[ФО_Результат расходов]",
            " 
            (
                [Measures].[Прогноз],
                [Период__Период].[Период__Период].[Данные всех периодов].[<%period_pred_year%>],
                [Вариант__Проект расходов].[Вариант__Проект расходов].[Все варианты].[<%outcomes2%>],
                " + MemberToStr 
                (
                    IIF
                    (
                        [Уровни бюджетов].CurrentMember is [Уровни бюджетов].[Местные бюджеты ],
                        [Уровни бюджетов].[Все].[Все уровни].[Конс.бюджет субъекта].[Конс.бюджет МО],
                        [Уровни бюджетов].CurrentMember
                    )
                ) +" 
            )" 
        )/<%mul%>' 
    member [Measures].[Темп роста ] 
    as 'IIF([Measures].[Проект (прошлый год) ] = null,null,[Measures].[Проект ]/[Measures].[Проект (прошлый год) ])'
    member [Measures].[Сравнение темпов роста доходов и расходов]
    as 'null'
     member [Районы__Сопоставимый].[Все районы].[Районы ]
    as 'Aggregate([Районы__Сопоставимый].[Все районы].[Собственно областной бюджет],
[Районы__Сопоставимый].[Все районы].[Бюджеты городов и районов])'
member [Уровни бюджетов].[Местные бюджеты ]
as '[Уровни бюджетов].[Все].[Все уровни].[Конс.бюджет субъекта].[Конс.бюджет МО]'
SELECT
    NON EMPTY 
    {
        [Уровни бюджетов].[Все].[Все уровни].[Конс.бюджет субъекта],
        [Уровни бюджетов].[Все].[Все уровни].[Конс.бюджет субъекта].[Бюджет субъекта],
        [Уровни бюджетов].[Местные бюджеты ]
    } ON rows ,
    {
        [Проект (прошлый год)],
        [Measures].[Проект],
        [Measures].[Темп роста],
        [Measures].[Проект (прошлый год) ],
        [Measures].[Проект ],
        [Measures].[Темп роста ],
        [Measures].[Сравнение темпов роста доходов и расходов] 
    }ON columns 
FROM  [ФО_Результат доходов с расщеплением] 

    
  ]]>
  </query>
  <query id="FO_0001_0008_compare_Grid_Kind">
    <![CDATA[
  -- FO_0001_0008_compare_Grid_Kind
   With
  member [Measures].[Names]
  as '[Вариант__Проект доходов].CurrentMember.name'
  SELECT
  NON EMPTY
  {
  [Вариант__Проект доходов].[Вариант__Проект доходов].[Все].children
  } ON rows ,
  { [Measures].[Names] }ON columns
  FROM  [ФО_Результат доходов с расщеплением]
  ]]>
  </query>
    <query id="FO_0001_0008_compare_Grid_Kind_a">
      <![CDATA[
  -- FO_0001_0008_compare_Grid_Kind_a
 With
  member [Measures].[UniqNames]
  as '[Вариант__Проект доходов].CurrentMember.Uniquename'
  member [Measures].[Names]
  as '[Вариант__Проект доходов].CurrentMember.name'
  SELECT
  NON EMPTY
  {
  [Вариант__Проект доходов].[Вариант__Проект доходов].[Вариант]
  } ON rows ,
  { [Measures].[Names],[Measures].[UniqNames] }ON columns
  FROM  [ФО_Результат доходов с расщеплением]
  ]]>
    
  </query>
  <query id="FO_0001_0008_compare_Grid_Kinds">
    <![CDATA[
  -- FO_0001_0008_compare_Grid_Kinds
  With
  member [Measures].[Names]
  as '[Вариант__Проект расходов].CurrentMember.name'
  SELECT
  NON EMPTY
  {
  [Вариант__Проект расходов].[Вариант__Проект расходов].[Вариант]
  } ON rows ,
  { [Measures].[Names] }ON columns
  FROM   [ФО_Результат расходов]
  ]]>
