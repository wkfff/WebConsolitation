<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="STAT_0002_0009_Date">
    <![CDATA[
-- STAT_0002_0009_Date  
with  
    member [Measures].[Имя]  
    as 'IIF  
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            [Период__Период].[Период__Период].CurrentMember.Name + " год",
            [Период__Период].[Период__Период].CurrentMember.Name  
        )'  
    member [Measures].[Уникальное имя]  
    as 'IIF  
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            Tail  
            (
                Filter  
                (
                    Descendants  
                    (
                        [Период__Период].[Период__Период].CurrentMember,
                        [Период__Период].[Период__Период].[Месяц],
                        SELF  
                    ),
                    not  
                    (
                        [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].CurrentMember.Parent.DataMember  
                    ) and  not IsEmpty( [Measures].[Факт] )  
                )  
            ).Item(0).UniqueName,
            [Период__Период].[Период__Период].CurrentMember.UniqueName  
        )'  
    member [Measures].[Уровень]  
    as 'IIF  
        (
            [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Год],
            0,
            IIF
            (
                [Период__Период].[Период__Период].CurrentMember.Level is [Период__Период].[Период__Период].[Месяц],
                1,
                2 
            )
        )' 
    set [Периоды]  
    as 'Generate  
        (
            {
                [Период__Период].[Период__Период].[Данные всех периодов].[2011] : [Период__Период].[Период__Период].[Данные всех периодов].[2020]  
            },
            {
                [Период__Период].[Период__Период].CurrentMember  
            } + Filter  
            (
                Descendants  
                (
                    [Период__Период].[Период__Период].CurrentMember,
                    [Период__Период].[Период__Период].[Месяц],
                    SELF_AND_AFTER 
                ),
                not  
                (
                    [Период__Период].[Период__Период].CurrentMember is [Период__Период].[Период__Период].CurrentMember.Parent.DataMember  
                )  
            )  
        )'  
select
    {
        [Measures].[Имя],
        [Measures].[Уникальное имя],
        [Measures].[Уровень]  
    } on columns,
    Filter  
    (
        [Периоды],
        not IsEmpty([Measures].[Значение])  
    ) on rows  
from  [ЭО_Банки]  
where  
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Банки - ДЭ Ежемесячная отч],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ]  
    )  
	]]>
  </query>
  <query id="STAT_0002_0009_list_of_dates">
    <![CDATA[ 
select
    {[Measures].[Значение]} on columns,
    {
        filter
        (
            [Период__Период].[Период__Период].[День].members,
            [Measures].[Значение] > 0
        )
    } on rows
from [ЭО_Банки] 
where
    (
       [Источники данных].[Источники данных].[Все источники данных].[ЭО Банки - Управление ЦБРФ] ,
[Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ] ,
[Банки__Реестр банков].[Банки__Реестр банков].[Все банки].[Значение не указано]
    )
    ]]>
  </query>
  <query id="STAT_0002_0009_list_of_dates_first">
    <![CDATA[ 
--STAT_0002_0009_list_of_dates_first
select
    {[Measures].[Значение]} on columns,
    {
       Filter  
            (
                [Период__Период].[Период__Период].[Данные всех периодов].[1998].[Полугодие 1].[Квартал 2].[Апрель].[12] : <%current_period%>,
                not isempty ([Measures].[Значение]) and not [Период__Период].[Период__Период].CurrentMember.Name = "Заключительные Обороты" 
            )
    } on rows
from [ЭО_Банки] 
where
    (
[Источники данных].[Источники данных].[Все источники данных].[ЭО Банки - Управление ЦБРФ] ,
[Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ] ,
[Банки__Реестр банков].[Банки__Реестр банков].[Все банки].[Значение не указано]
    )
    ]]>
  </query>
  <query id="STAT_0002_0009_prev_date">
    <![CDATA[
    with   
    member Measures.[ДанныеНа]   
    as '  
        (
            [Период__Период].[Период__Период].CurrentMember.UniqueName 
        )'   
select
    {Measures.[ДанныеНа]} on columns,
    Tail   
    (
        NonEmptyCrossjoin   
        (
            [Период__Период].[Период__Период].[Данные всех периодов].[1998].[Полугодие 1].[Квартал 1].[Январь].[1] : <%current_period%>   
        ),
        2  
    ) on rows   
From [ЭО_Банки]   
where   
    (
        [Источники данных].[Источники данных].[Все источники данных].[ЭО Банки - Управление ЦБРФ] ,
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ] ,
        [Банки__Реестр банков].[Банки__Реестр банков].[Все банки].[Значение не указано]   
    )    
   ]]>
  </query>
  <query id="STAT_0002_0009_grid">
    <![CDATA[
-- STAT_0002_0009_grid
    with    
    member [Measures].[Значение ]    
    as '    
        (
            [Measures].[Значение],
            <%current_period%>    
        )'    
    member [Measures].[Отклонение]    
    as 'IIF    
                (
                    [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки],
                    0,
                    IIF (<%current_period%> is <%last_period%>,
    null,
    IIF    
        (
            isempty    
            (
                (
                    [Measures].[Значение],
                    <%current_period%>
                )    
            ) OR isempty    
            (
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            ) OR    
            (
                [Measures].[Значение],
                <%current_period%>    
            ) = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                (
                    [Measures].[Значение],
                    <%current_period%>   
                )-    
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            )    
        )))'    
    member [Measures].[Прирост]    
    as 'IIF    
                (
                    [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки],
                    1,
                    IIF (<%current_period%> is <%last_period%>,
    null,
    IIF    
        (
            isempty( [Measures].[Значение ]) OR [Measures].[Значение ] = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                
                    IIF    
                    (
                        (
                            [Measures].[Значение]
                            ,<%last_period%>    
                        ) = 0,
                        null,
                        (
                            (
                                (
                                    [Measures].[Значение],
                                    <%current_period%>    
                                )/    
                                (
                                    [Measures].[Значение]
                                    ,<%last_period%>    
                                )    
                            ) - 1    
                        )    
                    )    
                )    
            )    
        )
        )'    
    set [Кредиты]    
    as '    
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты юр. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам, в т.ч. ипотечные кредиты]    
        }'    
    set [Показатели Кредиты]    
    as '    
        {
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Задолженность по кредитам на начало отчетного периода],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Выдано кредитов за отчетный период],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Погашено кредитов за отчетный период],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Задолженность по кредитам на конец отчетного периода],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь выданным за отчетный период кредитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь выданным за отчетный период кредитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь выданным за отчетный период кредитам в инвалюте],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь выданным за отчетный период кредитам в инвалюте]    
        } '     
    set [Депозиты]    
    as '     
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Депозиты юр. лиц],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Вклады физ. лиц (включая карточные счета)],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Расчетные счета клиентов]   
        }  '   
    set [Показатели Депозиты]    
    as '    
        {
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Остаток на начало отчетного периода],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Поступило на счет за отчетный период],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Снято со счетов за отчетный период],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Остаток на конец отчетного периода],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в инвалюте],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в инвалюте] 
        }'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Наименование]    
    as '[Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки]    
    as 'IIF    
        (
            [Measures].CurrentMember is [Measures].[Значение ],
            "0",
            IIF    
            (
                [Measures].CurrentMember is [Measures].[Прирост],
                "1",
                "2"    
            )    
        )'
        member [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в рублях]
        as '""'
        member [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в инвалюте]
        as '""'
select
    non empty    
    {
        {
            [Банки__Основные показатели].[Банки__Основные показатели].[Наименование]    
        } + [<%columnset%>] +    
        {
            [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в инвалюте],
            [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки]
        }    
    } on columns,
   
    {
        Filter    
        (
            [<%rowset%>]    
            *    
            {
                [Measures].[Значение ],
                [Measures].[Отклонение],
                [Measures].[Прирост]    
            },
            not isempty ([Measures].[Значение])    
        )    
    } on rows    
From  [ЭО_Банки]    
where    
    (
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ]    
    ) 
    
    ]]>
  </query>

  <query id="STAT_0002_0009_chart1">
    <![CDATA[
-- STAT_0002_0009_chart1
  with    
    member [Measures].[Значение ]    
    as '    
        (
            [Measures].[Значение],
            <%current_period%>    
        )'    
    member [Measures].[Отклонение]    
    as ' IIF    
        (
            isempty    
            (
                (
                    [Measures].[Значение],
                    <%current_period%>
                )    
            ) OR isempty    
            (
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            ) OR    
            (
                [Measures].[Значение],
                <%current_period%>    
            ) = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                (
                    [Measures].[Значение],
                    <%current_period%>   
                )-    
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            )    
        )'    
    member [Measures].[Прирост]    
    as 'IIF    
        (
            isempty( [Measures].[Значение ]) OR [Measures].[Значение ] = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                IIF    
                (
                    [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки],
                    1,
                    IIF    
                    (
                        (
                            [Measures].[Значение]
                            ,<%last_period%>    
                        ) = 0,
                        null,
                        (
                            (
                                (
                                    [Measures].[Значение],
                                    <%current_period%>    
                                )/    
                                (
                                    [Measures].[Значение]
                                    ,<%last_period%>    
                                )    
                            ) - 1    
                        )    
                    )    
                )    
            )    
        )'    
    set [Кредиты]    
    as '    
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты юр. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам, в т.ч. ипотечные кредиты]    
        }'    
    set [Показатели Кредиты]    
    as '    
        {
            
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь выданным за отчетный период кредитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь выданным за отчетный период кредитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь выданным за отчетный период кредитам в инвалюте],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь выданным за отчетный период кредитам в инвалюте]    
        } '     
    set [Депозиты]    
    as '     
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Депозиты юр. лиц],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Вклады физ. лиц (включая карточные счета)],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Расчетные счета клиентов]   
        }  '   
    set [Показатели Депозиты]    
    as '    
        {
           
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в рублях],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Минимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в инвалюте],
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Максимальная процентная ставка по вновь привлеченным за отчетный период срочным депозитам в инвалюте] 
        }'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Наименование]    
    as '[Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки]    
    as 'IIF    
        (
            [Measures].CurrentMember is [Measures].[Значение ],
            "0",
            IIF    
            (
                [Measures].CurrentMember is [Measures].[Прирост],
                "1",
                "2"    
            )    
        )'
