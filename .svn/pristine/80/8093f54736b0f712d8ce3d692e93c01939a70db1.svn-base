  <query id="STAT_0002_0009_chart2">
    <![CDATA[
-- STAT_0002_0009_chart2
  with    
    member [Measures].[Значение ]    
    as '    
        (
            [Measures].[Значение],
            <%current_period%>    
        )'    
    member [Measures].[Отклонение]    
    as ' IIF    
        (
            isempty    
            (
                (
                    [Measures].[Значение],
                    <%current_period%>
                )    
            ) OR isempty    
            (
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            ) OR    
            (
                [Measures].[Значение],
                <%current_period%>    
            ) = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                (
                    [Measures].[Значение],
                    <%current_period%>   
                )-    
                (
                    [Measures].[Значение]
                    ,<%last_period%>    
                )    
            )    
        )'    
    member [Measures].[Прирост]    
    as 'IIF    
        (
            isempty( [Measures].[Значение ]) OR [Measures].[Значение ] = 0,
            null,
            IIF    
            (
                [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Наименование],
                [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name,
                IIF    
                (
                    [Банки__Основные показатели].[Банки__Основные показатели].CurrentMember is [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки],
                    1,
                    IIF    
                    (
                        (
                            [Measures].[Значение]
                            ,<%last_period%>    
                        ) = 0,
                        null,
                        (
                            (
                                (
                                    [Measures].[Значение],
                                    <%current_period%>    
                                )/    
                                (
                                    [Measures].[Значение]
                                    ,<%last_period%>    
                                )    
                            ) - 1    
                        )    
                    )    
                )    
            )    
        )'    
    set [Кредиты]    
    as '    
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты юр. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Кредиты физ. лицам, в т.ч. ипотечные кредиты]    
        }'    
    set [Показатели Кредиты]    
    as '    
        {
            
            [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Выдано кредитов за отчетный период],
        [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Погашено кредитов за отчетный период]
        } '     
    set [Депозиты]    
    as '     
        {
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Депозиты юр. лиц],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Вклады физ. лиц (включая карточные счета)],
            [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[Расчетные счета клиентов]   
        }  '   
    set [Показатели Депозиты]    
    as '    
        {
           
           [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Поступило на счет за отчетный период],
    [Банки__Основные показатели].[Банки__Основные показатели].[Все показатели].[Снято со счетов за отчетный период]
        }'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Наименование]    
    as '[Банки__Типы банковских услуг].[Банки__Типы банковских услуг].CurrentMember.Name'    
    member [Банки__Основные показатели].[Банки__Основные показатели].[Тип строки]    
    as 'IIF    
        (
            [Measures].CurrentMember is [Measures].[Значение ],
            "0",
            IIF    
            (
                [Measures].CurrentMember is [Measures].[Прирост],
                "1",
                "2"    
            )    
        )'
        member [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в рублях]
        as '""'
        member [Банки__Основные показатели].[Банки__Основные показатели].[Процентная ставка в инвалюте]
        as '""'
        set [Периоды] 
    as 'Tail
        (
        Filter 
            (
            <%current_period%> : <%first_period%>,
            not isempty ([Measures].[Значение])
            and not [Период__Период].[Период__Период].CurrentMember.Name = "Заключительные Обороты" 
            ),
            <%num%>
        )'
select
    non empty    
    {
       [<%columnset%>] 
   
    } on rows,
    non empty    
    {
[Периоды]
                
            
    } on columns    
From  [ЭО_Банки]    
where    
    (
        [Банки__Типы банковских услуг].[Банки__Типы банковских услуг].[Все].[<%face%>],
        [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ],
        [Measures].[Значение]
    ) 
  ]]>
  </query>
