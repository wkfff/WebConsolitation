                  <igGaugeProp:SolidFillBrushElement Color="20, 0, 0, 0" />
                </BrushElements>
              </Shadow>
              <Response />
              <StrokeElement Color="106, 154, 172">
                <BrushElements>
                  <igGaugeProp:SolidFillBrushElement Color="106, 154, 172" />
                </BrushElements>
              </StrokeElement>
              <BrushElements>
