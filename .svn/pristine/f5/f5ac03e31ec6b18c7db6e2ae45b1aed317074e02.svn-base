<root>
  <query id="mfrf_0001_0004_ff_grid">
    <![CDATA[
-- mfrf_0001_0004_ff_grid        
With 
    member [Measures].[ShortName]
    as '[Территории].[Сопоставимый].CurrentMember.Properties("Сокращенное наименование")'
    member [Measures].[ParentShortName]
    as '[Территории].[Сопоставимый].CurrentMember.Parent.Properties("Сокращенное наименование")'
    member [Measures].[ФО]
    as 'IIF
        (
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ" or [Территории].[Сопоставимый].CurrentMember.Level.Name = "Федеральный округ"
            ),
            IIF
            (
                [Территории].[Сопоставимый].CurrentMember.Level.Name = "Субъект РФ",
                [Measures].[ParentShortName],
                [Measures].[ShortName]
            ),
            null
        )'
    member [Measures].[Исполнено текущий год]    
    as '            
        (
            [Measures].[Прогноз_очередной год],
            [Период].[Период].[Данные всех периодов].[<%period_year%>]
        )'     
    member [Measures].[Исполнено прошлый год]   
    as '   
        (
            [Measures].[Прогноз_очередной год],
            [Период].[Период].[Данные всех периодов].[<%period_last_year%>],
            <%filter%>
        )'   
    member [Measures].[Темп роста_]    
    as 'IIF   
        (
            IsEmpty  
            (
                [Measures].[Исполнено прошлый год]   
            ) or [Measures].[Исполнено прошлый год] = 0,
            null,
            (
                [Measures].[Исполнено текущий год] / [Measures].[Исполнено прошлый год] 
            )  
        )' 
   set [Субъекты]
    as 'Filter
        (
            Generate
            (
                Descendants
                (
                    [Территории].[Сопоставимый].[Все территории].[Российская  Федерация]<%select_FO%>,
                    [Территории].[Сопоставимый].[Федеральный округ],
                    SELF
                ),
                {
                    [Территории].[Сопоставимый].CurrentMember
                } +  Descendants
                (
                    [Территории].[Сопоставимый].CurrentMember,
                    [Территории].[Сопоставимый].[Субъект РФ],
                    SELF
                )
            ),  not ([Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember)
            and not IsEmpty
            (
                (
                    [Measures].[Прогноз_очередной год],
                    [МФ РФ].[Сопост Показатели].[Аггрегированные фонды]
                )
            ) and 
            (
                [Measures].[Прогноз_очередной год],
                [МФ РФ].[Сопост Показатели].[Аггрегированные фонды]
            ) <> 0 
        )'  
    member [МФ РФ].[Сопост Показатели].[Аггрегированные фонды] 
    as 'Aggregate 
        (
            Except  
            (
                [МФ РФ].[Сопост Показатели].[Тип].AllMembers,
                {
                    [МФ РФ].[Сопост Показатели].[Все показатели].[Несопоставленные данные]  
                }  
            ) 
        )'         
SELECT
    {
        {
            {
                [МФ РФ].[Сопост Показатели].[Все показатели]
            } 
            *
            {
                [Measures].[ФО]
            } 
        } + 
        {
            Except 
            (
                [МФ РФ].[Сопост Показатели].[Тип].AllMembers,
                {
                    [МФ РФ].[Сопост Показатели].[Все показатели].[Несопоставленные данные] 
                } 
            ) 
        } 
        * 
        {
            [Measures].[Прогноз_очередной год],
            [Measures].[Темп роста_] 
        } 
    } on columns,
        {
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация] 
        } + [Субъекты] 
    } on rows  
    
FROM [МФ РФ_Фонды] 
WHERE 
    (
       [Период].[Период].[Данные всех периодов].[<%period_year%>],
       <%variant_mes_otch%>
    )  
	]]>
      </query>

      <query id="mfrf_0001_0004_ff_chart1">
        <![CDATA[    
-- mfrf_0001_0004_ff_chart1    
with
    member Measures.[ФФФПР_Empty]
    as '
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов]
        ) / 1000000'
    member Measures.[ФФФПР]
    as 'IIF(IsEmpty(Measures.[ФФФПР_Empty]), 0, Measures.[ФФФПР_Empty])'
    member Measures.[ФФК_Empty]
    as '
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций]
        ) / 1000000'
    member Measures.[ФФК]
    as 'IIF(IsEmpty(Measures.[ФФК_Empty]), 0, Measures.[ФФК_Empty])'
    member Measures.[ФФСР_Empty]
    as '
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд софинансирования расходов]
        ) / 1000000'
    member Measures.[ФФСР]
    as 'IIF(IsEmpty(Measures.[ФФСР_Empty]), 0, Measures.[ФФСР_Empty])'
    member Measures.[ФФРР_Empty]
    as '
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд регионального развития]
        ) / 1000000'
    member Measures.[ФФРР]
    as 'IIF(IsEmpty(Measures.[ФФРР_Empty]), 0, Measures.[ФФРР_Empty])'
    member Measures.[Иные_Empty]
    as '
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Иные межбюджетные трансферты]
        ) / 1000000'
    member Measures.[Иные]
    as 'IIF(IsEmpty(Measures.[Иные_Empty]), 0, Measures.[Иные_Empty])'
    member [Measures].[Исполнено текущий год]    
    as '            
        (
            [Measures].[Прогноз_очередной год],
            [Период].[Период].[Данные всех периодов].[<%period_year%>]   
        )'     
    member [Measures].[Исполнено прошлый год]   
    as '   
        (
            [Measures].[Прогноз_очередной год],
            [Период].[Период].[Данные всех периодов].[<%period_last_year%>]   
        )'   
    member [Measures].[Темп роста_]    
    as 'IIF   
        (
            IsEmpty  
            (
                [Measures].[Исполнено прошлый год]   
            ) or [Measures].[Исполнено прошлый год] = 0,
            null,
            (
                [Measures].[Исполнено текущий год] / [Measures].[Исполнено прошлый год] 
            )  
        )' 
SELECT
    {
        Measures.[ФФФПР],
        Measures.[ФФК],
        Measures.[ФФСР],
        Measures.[ФФРР],
        Measures.[Иные]
    } on columns,
    Filter
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация<%subject_FO%>.Children,
        not
        (
            [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
        )
    ) on rows
FROM [МФ РФ_Фонды]
WHERE
    (
       [Период].[Период].[Данные всех периодов].[<%period_year%>],
       <%variant_mes_otch%>
    )
	]]>
      </query>
  
  <query id="mfrf_0001_0004_ff_chart2">
        <![CDATA[
-- mfrf_0001_0004_ff_chart2
    
with 
    member Measures.[ФФФПР_Empty] 
    as ' 
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов] 
        )' 
    member Measures.[ФФФПР] 
    as 'IIF
        (
            IsEmpty(Measures.[ФФФПР_Empty]),
            0,
            Measures.[ФФФПР_Empty]
        )' 
    member Measures.[ФФК_Empty] 
    as ' 
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций] 
        )' 
    member Measures.[ФФК] 
    as 'IIF
        (
            IsEmpty(Measures.[ФФК_Empty]),
            0,
            Measures.[ФФК_Empty]
        )' 
    member Measures.[ФФСР_Empty] 
    as ' 
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд софинансирования расходов] 
        )' 
    member Measures.[ФФСР] 
    as 'IIF
        (
            IsEmpty(Measures.[ФФСР_Empty]),
            0,
            Measures.[ФФСР_Empty]
        )' 
    member Measures.[ФФРР_Empty] 
    as ' 
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд регионального развития] 
        )' 
    member Measures.[ФФРР] 
    as 'IIF
        (
            IsEmpty(Measures.[ФФРР_Empty]),
            0,
            Measures.[ФФРР_Empty]
        )' 
    member Measures.[Иные_Empty] 
    as ' 
        (
            [Measures].[Прогноз_очередной год],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Иные межбюджетные трансферты] 
        )' 
    member Measures.[Иные] 
    as 'IIF
        (
            IsEmpty(Measures.[Иные_Empty]),
            0,
            Measures.[Иные_Empty]
        )'  
    member [Период].[Период].[2007_]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2007],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2007 год"]
        )'          
    member [Период].[Период].[2008_]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2008],
                [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2008 год и на плановый период 2009 и 2010 годов"]
        )'  
    member [Период].[Период].[2009_]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2009],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2009 год и на плановый период 2010 и 2011 годов"].[ФЗ с изменениями от 2 декабря 2009 г. № 309-ФЗ]
        )'  
    member [Период].[Период].[2010_]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2010],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2010 год и на плановый период 2011 и 2012 годов"].[ФЗ от 3 ноября 2010 г. № 278-ФЗ]
        )'  
        
    member [Период].[Период].[2011brпланbrдекабрьbr2010]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2011],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2011 год и на плановый период 2012 и 2013 годов"].[ФЗ от 13 декабря 2010 г. № 357-ФЗ]
        )' 
    member [Период].[Период].[2012brпланbrдекабрьbr2010]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2012],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2011 год и на плановый период 2012 и 2013 годов"].[ФЗ от 13 декабря 2010 г. № 357-ФЗ]
        )' 
    member [Период].[Период].[2013brпланbrдекабрьbr2010]
    as '
        (
            [Период].[Период].[Данные всех периодов].[2013],
            [Вариант].[МФРФ Фонды].[Все].[Данные, утвержденные Федеральным законом "О федеральном бюджете"].[ФЗ "О федеральном бюджете на 2011 год и на плановый период 2012 и 2013 годов"].[ФЗ от 13 декабря 2010 г. № 357-ФЗ]
        )' 
select
    {
       <%period%>
       
    } on columns,
    {
        Measures.[ФФФПР],
        Measures.[ФФК],
        Measures.[ФФСР],
        Measures.[ФФРР],
        Measures.[Иные]
    } on rows
from [МФ РФ_Фонды] 
where 
    (
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация<%subject%>
    )         
	]]>
  </query>
  
  <query id="FFKMasterTable">
    <![CDATA[
    SELECT
    non empty{
        [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].children
        *
        {
            [Measures].[Прогноз_очередной год],
            [Measures].[Темп роста]
        }
    } on columns,
    Except
    (
        [Территории].[Сопоставимый].[Федеральный округ].Members,
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DATAMEMBER
        }
    ) on rows
FROM [МФ РФ_Фонды]
WHERE
    (
       <%period_year%>
    )
	]]>
  </query>

  <query id="FFKDetailTable">
    <![CDATA[
    With  
    member [Measures].[FK]  
    as '[Территории].[Сопоставимый].CurrentMember.Parent.Name'   
SELECT
    non empty {
        {
            {
                [МФ РФ].[Сопост Показатели].[Все показатели] 
            } 
            *{[Measures].[FK]} 
        } +  
        {
            {
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].children
            }   
            *   
            {
                [Measures].[Прогноз_очередной год],
                [Measures].[Темп роста]   
            }   
        } 
    } on columns,
    Filter   
    (
        [Территории].[Сопоставимый].[Субъект РФ].AllMembers,
        (
            not   
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember   
            )and   
            (
                not IsEmpty([Measures].[Прогноз_очередной год])  
            )   
        )   
    ) on rows    
FROM [МФ РФ_Фонды]   
WHERE   
    (
        <%period_year%>
    )   
    ]]>
  </query>

  <query id="FFKChart1">
    <![CDATA[
with member [Measures].[Значение0] as 'IIF(IsEmpty([Measures].[Прогноз_очередной год]), 0, [Measures].[Прогноз_очередной год])'
SELECT
    non empty  except
    (
        [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].Children,
        {
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].[Всего трансферт из Федерального фонда компенсаций]
        }) on columns,
        filter([Период].[Период].[Год].members, not (IsEmpty([Measures].[Прогноз_очередной год]) or [Measures].[Прогноз_очередной год] = 0)) on rows
FROM [МФ РФ_Фонды]   
WHERE   
    (
        [Measures].[Значение0],  
        <%region%>        
    )
    ]]>
  </query>
  <query id="FFKChart2">
    <![CDATA[
    SELECT
    non empty  except 
    (
        [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].Children,
        {
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд компенсаций].[Всего трансферт из Федерального фонда компенсаций] 
        }
    ) on columns,
    filter
    (
        <%state_area%>.children,
        not 
        (
            IsEmpty([Measures].[Прогноз_очередной год]) or [Measures].[Прогноз_очередной год] = 0
        )
    ) on rows 
FROM [МФ РФ_Фонды]    
WHERE    
    (
         <%period_year%>,
        [Measures].[Прогноз_очередной год]
    )
    ]]>
  </query>

  <query id="FFPRMasterTable">
    <![CDATA[
    with  
    member measures.[Значение ] 
    as 'null' 
    member measures.[Темп роста ] 
    as 'null'  
SELECT
    {
        {
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Численность постоянного населения],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Индекс налогового потенциала],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Индекс бюджетных расходов],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (до распределения ФФПР)],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Всего ФФПР],
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (после распределения ФФПР)] 
        }  
        *   
        {
            [Measures].[Значение ],
            [Measures].[Темп роста ] 
        }  
    } on columns,
    Except  
    (
        [Территории].[Сопоставимый].[Федеральный округ].Members,
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DATAMEMBER  
        }  
    ) on rows  
FROM [МФ РФ_Фонды]   
WHERE   
    (
        <%period_year%>
    )    
	]]>
  </query>

