    } on columns,
    Except 
    (
        [Территории].[Сопоставимый].[Федеральный округ].Members,
        {
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DATAMEMBER 
        } 
    ) on rows 
FROM [МФ РФ_Фонды] 
WHERE 
    (
         <%period_year%>
    ) 
	]]>
  </query>

  <query id="FFSSRDetailTable">
    <![CDATA[
    With  
    member [Measures].[FK]  
    as '[Территории].[Сопоставимый].CurrentMember.Parent.Name'   
SELECT
    {
        {
            {
                [МФ РФ].[Сопост Показатели].[Все показатели] 
            } 
            *{[Measures].[FK]} 
        } +  
        {
            {
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд софинансирования социальных расходов].children
            }   
            *   
            {
                [Measures].[Прогноз_очередной год],
                [Measures].[Темп роста]   
            }   
        } 
    } on columns,
    Filter   
    (
        [Территории].[Сопоставимый].[Субъект РФ].AllMembers,
        (
            not   
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember   
            )and   
            (
                not IsEmpty([Measures].[Прогноз_очередной год])  
            )   
        )   
    ) on rows    
FROM [МФ РФ_Фонды]   
WHERE   
    (
         <%period_year%>
    )   
    ]]>
  </query>

  <query id="FFSSRChart1">
    <![CDATA[
    with member [Measures].[Значение0] as 'IIF(IsEmpty([Measures].[Прогноз_очередной год]), 0, [Measures].[Прогноз_очередной год])'
   SELECT
    non empty  except   
    (
        [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд софинансирования социальных расходов].Children,
        {
            [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд софинансирования социальных расходов].[Всего трансферт из Федерального фонда софинансирования социальных расходов]   
        }  
    ) on columns,
    NON EMPTY Filter 
    (
        [Период].[Период].[Год].members,
        [Measures].[Прогноз_очередной год] <> 0 
    ) on rows  
FROM [МФ РФ_Фонды]   
WHERE   
    (
        [Measures].[Значение0],
        <%region%>
    )   
    ]]>
  </query>
