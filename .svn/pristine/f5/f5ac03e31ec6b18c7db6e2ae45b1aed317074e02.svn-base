  <query id="FFPRAveragefonds">
    <![CDATA[
  with
    member measures.[Средеяя до ФО]
    as 'Avg
        (
            [Территории].[Сопоставимый].Currentmember.Children,
            (
                [Measures].[Прогноз_очередной год],
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (до распределения ФФПР)]
            )
        )'
    member measures.[Средеяя до РФ]
    as 'Avg
        (
            Filter
            (
                [Территории].[Сопоставимый].[Субъект РФ].AllMembers,
                (
                    not
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
                    )
                )
            ),
            (
                [Measures].[Прогноз_очередной год],
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (до распределения ФФПР)]
            )
        )'
    member measures.[Средеяя после ФО]
    as 'Avg
        (
            [Территории].[Сопоставимый].Currentmember.Children,
            (
                [Measures].[Прогноз_очередной год],
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (после распределения ФФПР)]
            )
        )'
    member measures.[Средеяя после РФ]
    as 'Avg
        (
            Filter
            (
                [Территории].[Сопоставимый].[Субъект РФ].AllMembers,
                (
                    not
                    (
                        [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
                    )
                )
            ),
            (
                [Measures].[Прогноз_очередной год],
                [МФ РФ].[Сопост Показатели].[Все показатели].[Федеральный фонд финансовой поддержки регионов].[Реальная бюджетная обеспеченность (после распределения ФФПР)]
            )
        )'
SELECT
    {
        measures.[Средеяя до ФО],
        measures.[Средеяя после ФО],
        measures.[Средеяя до РФ],
        measures.[Средеяя после РФ]
    } on columns,
   {<%state_area%>} on rows
FROM [МФ РФ_Фонды]
WHERE
    (
        <%period_year%>
    )
    ]]>
  </query>

