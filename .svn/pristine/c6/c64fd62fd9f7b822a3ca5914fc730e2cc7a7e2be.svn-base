<?xml version="1.0" encoding="Windows-1251"?>
<root>
  <query id="FK_0001_0014_date">
    <![CDATA[
-- FK_0001_0014_compare_date   
-- <#[ФК_МесОтч_Доходы]#>
with
    member [Measures].[ДанныеНа]
    as '[Период].[Месяц].CurrentMember.UniqueName'
    set [NonEmptySet]
    as 'Tail
        (
            Filter
            (
                [Период].[Месяц].Members,
                not IsEmpty([Measures].[Исполнено])
            ),
            1
        )'
select
    { [Measures].[ДанныеНа] } on columns,
    { [NonEmptySet] } on rows
from  [ФК_МесОтч_Доходы]
  ]]>
  </query>

  <query id="FK_0001_0014_Grid_incomes">
    <![CDATA[
-- FK_0001_0014_Grid_incomes
-- <#[ФК_МесОтч_Доходы]#>
with  
    member [КД].[Сопоставимый].[Общий объем доходов,<br/>в том числе за счет:]  
    as '[КД].[Сопоставимый].[Все коды доходов].[Доходы - всего в том числе:]'
    member [КД].[Сопоставимый].[налоговых поступлений ] 
    as 'Aggregate 
        (
            {
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ПРИБЫЛЬ, ДОХОДЫ],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[СТРАХОВЫЕ ВЗНОСЫ НА ОБЯЗАТЕЛЬНОЕ СОЦИАЛЬНОЕ СТРАХОВАНИЕ],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ТОВАРЫ (РАБОТЫ, УСЛУГИ), РЕАЛИЗУЕМЫЕ НА ТЕРРИТОРИИ РОССИЙСКОЙ ФЕДЕРАЦИИ],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ТОВАРЫ, ВВОЗИМЫЕ НА ТЕРРИТОРИЮ РОССИЙСКОЙ ФЕДЕРАЦИИ],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА СОВОКУПНЫЙ ДОХОД],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ НА ИМУЩЕСТВО],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[НАЛОГИ, СБОРЫ И РЕГУЛЯРНЫЕ ПЛАТЕЖИ ЗА ПОЛЬЗОВАНИЕ ПРИРОДНЫМИ РЕСУРСАМИ],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[ГОСУДАРСТВЕННАЯ ПОШЛИНА],
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ].[ЗАДОЛЖЕННОСТЬ И ПЕРЕРАСЧЕТЫ ПО ОТМЕНЕННЫМ НАЛОГАМ, СБОРАМ И ИНЫМ ОБЯЗАТЕЛЬНЫМ ПЛАТЕЖАМ]
            } 
        )'
    member [КД].[Сопоставимый].[безвозмездных поступлений от других бюджетов бюджетной системы РФ ] 
    as '[КД].[Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации]'
    member [Measures].[Утвержденные бюджетные назначения, тыс. руб.]
    as '[Measures].[Назначено] / 1000'
select
    {[Measures].[Утвержденные бюджетные назначения, тыс. руб.]} on columns,
    {
        [КД].[Сопоставимый].[Общий объем доходов,<br/>в том числе за счет:],
        [КД].[Сопоставимый].[налоговых поступлений ],
        [КД].[Сопоставимый].[безвозмездных поступлений от других бюджетов бюджетной системы РФ ]
    } on rows   
from  [ФК_МесОтч_Доходы]     
where     
    (
        [Период].[Месяц].[Данные всех периодов]<%period_cur_date%>,
        [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>
    )
  ]]>
  </query>

  <query id="FK_0001_0014_Grid_outcomes">
    <![CDATA[
-- FK_0001_0014_Grid_outcomes
-- <#[ФК_МесОтч_Доходы]#>
with
    member [Measures].[Утвержденные бюджетные назначения, тыс. руб.]
    as '[Measures].[Назначено] / 1000'
    
    member [Расходы].[ФК_МесОтч_Сопоставимый].[Общий объем расходов,<br/>в том числе:]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Расходы бюджета - Итого]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[общегосударственные расходы ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Общегосударственные вопросы]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[национальная оборона ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная оборона]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[национальная безопасность и правоохранительная деятельность ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная безопасность и правоохранительная деятельность]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[национальная экономика ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная экономика]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[жилищно-коммунальное хозяйство ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Жилищно-коммунальное хозяйство]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[охрана окружающей среды ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Охрана окружающей среды]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[образование ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Образование]'
     member [Расходы].[ФК_МесОтч_Сопоставимый].[культура, кинематография, СМИ ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Культура, кинематография, средства массовой информации]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[здравоохранение, физическая культура и спорт ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Здравоохранение, физическая культура и спорт]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[социальная политика ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Социальная политика]'
    member [Расходы].[ФК_МесОтч_Сопоставимый].[межбюджетные трансферты ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Межбюджетные трансферты]'
select
    {[Measures].[Утвержденные бюджетные назначения, тыс. руб.]} on columns,
    {
         [Расходы].[ФК_МесОтч_Сопоставимый].[Общий объем расходов,<br/>в том числе:],
         [Расходы].[ФК_МесОтч_Сопоставимый].[общегосударственные расходы ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[национальная оборона ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[национальная безопасность и правоохранительная деятельность ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[национальная экономика ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[жилищно-коммунальное хозяйство ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[охрана окружающей среды ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[образование ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[культура, кинематография, СМИ ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[здравоохранение, физическая культура и спорт ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[социальная политика ],         
         [Расходы].[ФК_МесОтч_Сопоставимый].[межбюджетные трансферты ]          
    } on rows   
from   [ФК_МесОтч_Расходы]  
where     
    (
        [Период].[Месяц].[Данные всех периодов]<%period_cur_date%>,
        [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>
    ) 
  ]]>
  </query>


  <query id="FK_0001_0014_Grid_deficite">
    <![CDATA[
-- FK_0001_0014_Grid_deficite
-- <#[ФК_МесОтч_Доходы]#>
with
member [Уровни бюджета].[СКИФ].[Дефицит (-)/профицит (+)]
    as '[Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта]'
member [Measures].[Утвержденные бюджетные назначения, тыс. руб.]
    as '[Measures].[Назначено] / 1000'
select
    { [Measures].[Утвержденные бюджетные назначения, тыс. руб.] } on columns,
    {[Уровни бюджета].[СКИФ].[Дефицит (-)/профицит (+)]} on rows
from   [ФК_МесОтч_Дефицит Профицит]
where      
    (
        [Период].[Месяц].[Данные всех периодов]<%period_cur_date%>,
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>      
    ) 
  ]]>
  </query>


  <query id="FK_0001_0014_Grid_bkku">
    <![CDATA[
-- FK_0001_0014_Grid_bkku
-- <#[ФК_МесОтч_Доходы]#>
with        
    member [КД].[Сопоставимый].[применяются санкции]    
    as 'LookupCube    
            (
                "[МФ РФ_Индикаторы БК и КУ]",
                "    
                (
                    [МФ РФ].[Сопоставимый индикаторы БККУ].[Все индикаторы].[Группа, присвоенная субъекту РФ по доле МБТ],
                    [Вариант].[БККУ].[Все].[По данным месячной отчетности],
                    [Measures].[Значение Факт],
                    " + MemberToStr    
                    (
                        [Территории].[Сопоставимый].CurrentMember    
                    ) + ",
                    " + MemberToStr    
                    (
                        [Период].[Месяц].CurrentMember    
                    ) + "    
                )"    
            )'
    member [КД].[Сопоставимый].[* к субъекту РФ применяются санкции, утвержденные пунктом 4 статьи 130 БК РФ]
    as '[КД].[Сопоставимый].[применяются санкции]'
    member [КД].[Сопоставимый].[Предельный объем государственного долга субъекта]    
    as 'IIF  
        (

            [КД].[Сопоставимый].[применяются санкции] <> 1,
            [КД].[Сопоставимый].[Все коды доходов].[Доходы - всего в том числе:] - [КД].[Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации],
            (
                [КД].[Сопоставимый].[Все коды доходов].[Доходы - всего в том числе:] - [КД].[Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации] 
            )  
            * 0.5 
        )'
    member [Measures].[Утвержденные бюджетные назначения, тыс. руб.] 
    as 'IIF  
        (
            [КД].[Сопоставимый].CurrentMember is [КД].[Сопоставимый].[* к субъекту РФ применяются санкции, утвержденные пунктом 4 статьи 130 БК РФ],
            [Measures].[Назначено],
            [Measures].[Назначено] / 1000
        ) ' 
select
    { [Measures].[Утвержденные бюджетные назначения, тыс. руб.] } on columns,
    {
        [КД].[Сопоставимый].[Предельный объем государственного долга субъекта],
        [КД].[Сопоставимый].[* к субъекту РФ применяются санкции, утвержденные пунктом 4 статьи 130 БК РФ]    
    } on rows     
from  [ФК_МесОтч_Доходы]      
where      
    (
        [Период].[Месяц].[Данные всех периодов]<%bkkuDate%>,
        [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>      
    ) 
  ]]>
  </query>

  <query id="FK_0001_0014_chart1">
    <![CDATA[
-- FK_0001_0014_chart1
with
    member [КД].[Сопоставимый].[Налоговые и неналоговые доходы ]
    as 'Aggregate
        (
            {
                [КД].[Сопоставимый].[Все коды доходов].[НАЛОГОВЫЕ И НЕНАЛОГОВЫЕ ДОХОДЫ]
            }
        )'
    member [КД].[Сопоставимый].[Безвозмездные поступленияbrот других бюджетов бюджетной системы РФ]
    as '[КД].[Сопоставимый].[Все коды доходов].[БЕЗВОЗМЕЗДНЫЕ ПОСТУПЛЕНИЯ].[Безвозмездные поступления от других бюджетов бюджетной системы Российской Федерации]'
    member [Measures].[Утвержденные бюджетные назначения, тыс. руб.]
    as '[Measures].[Назначено] / 1000'
select
    {[Measures].[Утвержденные бюджетные назначения, тыс. руб.]} on columns,
    {   
    [КД].[Сопоставимый].[Налоговые и неналоговые доходы ],
        [КД].[Сопоставимый].[Безвозмездные поступленияbrот других бюджетов бюджетной системы РФ]
        
        
    } on rows
from  [ФК_МесОтч_Доходы]
where
    (
        [Период].[Месяц].[Данные всех периодов]<%period_cur_date%>,
        [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>
    )
  ]]>
  </query>

  <query id="FK_0001_0014_chart2">
    <![CDATA[
-- FK_0001_0014_chart2
with
    member [Measures].[Утвержденные бюджетные назначения, тыс. руб.]
    as '[Measures].[Назначено] / 1000'
    
    member [Расходы].[ФК_МесОтч_Сопоставимый].[Общегосударственные расходы]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Общегосударственные вопросы]'    
     member [Расходы].[ФК_МесОтч_Сопоставимый].[Культура, кинематография, СМИ]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Культура, кинематография, средства массовой информации]'   
     member [Расходы].[ФК_МесОтч_Сопоставимый].[Национальная безопасность]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная безопасность и правоохранительная деятельность]'
     member [Расходы].[ФК_МесОтч_Сопоставимый].[Здравоохранение,brфизическая культура и спорт]
    as '[Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Здравоохранение, физическая культура и спорт]'
select 
    {[Measures].[Утвержденные бюджетные назначения, тыс. руб.]} on columns,
    {
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная оборона],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Общегосударственные расходы],                        
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Социальная политика],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Охрана окружающей среды],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Образование],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Культура, кинематография, СМИ], 
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Национальная экономика],        
        [Расходы].[ФК_МесОтч_Сопоставимый].[Национальная безопасность],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Жилищно-коммунальное хозяйство],                
        [Расходы].[ФК_МесОтч_Сопоставимый].[Здравоохранение,brфизическая культура и спорт],
        [Расходы].[ФК_МесОтч_Сопоставимый].[Данные всех источников].[Межбюджетные трансферты]
    } on rows
from   [ФК_МесОтч_Расходы]
where
    (
        [Период].[Месяц].[Данные всех периодов].[<%period_year%>],
        [Уровни бюджета].[СКИФ].[Все].[Конс.бюджет субъекта],
        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].<%subject%>
    )
  ]]>
  </query>
</root>