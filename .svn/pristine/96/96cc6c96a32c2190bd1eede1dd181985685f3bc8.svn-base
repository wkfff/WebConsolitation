<RegionSettings>
  <!--
  // тип документа СКИФ для уровня районов
  RegionDocumentSKIFType,
  // уровень бюджета СКИФ для уровня районов
  RegionBudgetSKIFLevel    
  -->
  <Region id="HMAO">
    <RegionDocumentSKIFType>[ТипДокумента].[СКИФ].[Все].[Данные конс. отчета субъекта в разрезе городов и районов (5+7+10)]</RegionDocumentSKIFType>
    <RegionBudgetSKIFLevel>
          Aggregate
          (
            {
              [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет района],
              [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет поселения],
              [Уровни бюджета__СКИФ].[Уровни бюджета__СКИФ].[Все].[Бюджет гор.округа]
            }
          )
    </RegionBudgetSKIFLevel>
  </Region>
</RegionSettings>
