/*
    Components.GaugeIndicator
*/
.ElementTitle
{
    font-size: 14px;
    font-family: Verdana;
    font-weight: bold;   
}
.ElementSubTitle
{
    font-size: 12px;
    font-family: Verdana;
}

/*
    Components.CustomLogin, Logon.aspx
*/
.ErrorTableCell
{
    border-left: LightGray 2px solid;
    border-right: LightGray 2px solid;
    padding: 5px;
    font-size: 12px;
    font-family: Verdana;
    color: firebrick;
    background-color: White;
}
.ShadowLeft
{
    border-right: medium none;
    background-position: right bottom;
    border-top: medium none;
    background-image: url(../../images/ShadowLeft.gif);
    border-left: medium none;
    width: 123px;
    border-bottom: medium none;
    background-repeat: no-repeat;
}
.ShadowRight
{
    border-right: medium none;
    background-position: left bottom;
    border-top: medium none;
    background-image: url(../../images/ShadowRight.gif);
    border-left: medium none;
    width: 123px;
    border-bottom: medium none;
    background-repeat: no-repeat;
}

/*
    Components.Header
*/
.ErrorText
{    
    font-size: 14px;
    font-family: Verdana;
    color: firebrick;
    background-color: White;
}

/*
    Components.GridSearch
