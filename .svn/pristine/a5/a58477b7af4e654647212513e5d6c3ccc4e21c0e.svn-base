<query id="STAT_0002_0007_DataTotalNumber">
<![CDATA[
  with 
    member Measures.[ДанныеНа] 
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName' 
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail 
        (
            Filter 
            (
                [Период].[Год Квартал Месяц].[Месяц].Members,
                not IsEmpty
                (
                    (
                        [Measures].[Нарастающий итог],
                        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ],
                        [Труд].[Трудовые ресурсы].[Все показатели].[Общая численность безработных (на конец месяца)]
                    )
                ) 
            ),
 1
        ) 
    } on rows 
from  [СТАТ_Труд_Трудовые ресурсы] 
  ]]>
</query>

<query id="STAT_0002_0007_DataCount">
<![CDATA[
 --STAT_0002_0007_Data
   with
    member Measures.[ДанныеНа]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
select
    {Measures.[ДанныеНа]} on columns,
    {
        Tail
        (
            Filter
            (
                [Период].[Год Квартал Месяц].[Месяц].Members,
                not IsEmpty
                (
                    (
                        [Measures].[Нарастающий итог],
                        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ],
                        [Труд].[Трудовые ресурсы].[Все показатели].[Число безработных, зарегистрированных в службе занятости (на конец месяца)]
                    )
                )
               
            ),
 1
        )
    } on rows
from  [СТАТ_Труд_Трудовые ресурсы]
  ]]>
</query>


<query id="STAT_0002_0007_Chart1">
<![CDATA[
 --STAT_0002_0007_Chart1
WITH
 
  Member [Measures].[Нарастающий итог ] 
    as 'IIF
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
            )  
        )'
 member [Measures].[Период ]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
SELECT
  {
     [Measures].[Нарастающий итог ],
     [Measures].[Период ]
  } ON COLUMNS,
  {
    Filter
        (
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].Children,
            not
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
            )
            and ([Measures].[Нарастающий итог ] <>0)
        )
  } ON ROWS
FROM [СТАТ_Труд_Трудовые ресурсы]
WHERE
  (
    <%index%>,
    <%date%>
  )
  ]]>
</query>


<query id="STAT_0002_0007_Chart2">
<![CDATA[
--STAT_0002_0007_Chart2
WITH    
        
    MEMBER [Measures].[RegionName]
    AS ' IIF 
        (
            [Территории].[РФ].CurrentMember is [Территории].[РФ].CurrentMember.Parent.DataMember,
            [Территории].[РФ].CurrentMember.Parent.Name,
            [Территории].[РФ].CurrentMember.Name 
        )'  
        
  Member [Measures].[Нарастающий итог ] 
    as 'IIF
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
            )  
        )'    
         
SELECT
    {
        [Measures].[RegionName],
        [Measures].[Нарастающий итог ]
    } ON COLUMNS ,
    {
        [Территории].[РФ].[Все территории].[Российская Федерация].DATAMEMBER,
        [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER    
    } ON ROWS    
FROM [СТАТ_Труд_Трудовые ресурсы]    
WHERE    
    (
       
        <%index%>,
        <%date%>  
    )     
  ]]>
</query>

<query id="STAT_0002_0007_Chart">
<![CDATA[
--STAT_0002_0007_Chart
WITH    
     
    MEMBER [Measures].[RegionName]
    AS ' IIF 
        (
            [Территории].[РФ].CurrentMember is [Территории].[РФ].CurrentMember.Parent.DataMember,
            [Территории].[РФ].CurrentMember.Parent.Name,
            [Территории].[РФ].CurrentMember.Name 
        )'
        
      member [Территории].[РФ].[Другие федеральные округа] 
    as ' 
        (
            [Территории].[РФ].[Все территории].[Российская Федерация].DATAMEMBER,
            [Measures].[Нарастающий итог ]
        ) - 
        (
            [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER,
            [Measures].[Нарастающий итог ]
        ) '
      Member [Measures].[Нарастающий итог ] 
    as 'IIF
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
            )  
        )'
     member [Measures].[Период ]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'    
SELECT
    {
        
        [Measures].[Нарастающий итог ],
        [Measures].[Период ] 
    } ON COLUMNS ,
    {
         [Территории].[РФ].[Другие федеральные округа],
         [Территории].[РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].DATAMEMBER
    } ON ROWS    
FROM [СТАТ_Труд_Трудовые ресурсы]    
WHERE    
    (
        <%index%>,
        <%date%> 
    )     
  ]]>
  
</query>
 <query id="STAT_0001_0012_Operative">
   <![CDATA[ 
   --STAT_0002_0007_Operative
    with
  
    member [Показатели].[Рынок труда].[Период ]
    as '[Период].[Период].CurrentMember.Parent.UniqueName'
    member [Measures].[Значение ]
    as '[Measures].[Значение] / 100'


SELECT
    non Empty
    {
      {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация]}
      *
       {
        [Показатели].[Рынок труда].[Период ]
       }
       +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
    } ON columns,

    filter
    (
        Descendants
        (
            [Период].[Период].[Данные всех периодов],
             [Период].[Период].[День],
            SELF
        ),
        not isEmpty ([Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные месяца"
        ) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные года"
        )
    ) ON rows
FROM  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE
    (
       [Measures].[Значение],
       [Источники данных].[Все источники данных].[СТАТ Отчетность - Минсоцразвития]
    )
  ]]>
</query>


 <query id="STAT_0001_0012_unjobedOperative">
  <![CDATA[ 
--STAT_0002_0007_unjobedOperative
   
with
  Member  [Показатели].[Рынок труда].[УрФО]
    as '[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].DataMember'
    Member [Территории].[Сопоставимый].[РФед]
    as '[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember'
    member [Показатели].[Рынок труда].[Период ]
    as '[Период].[Период].CurrentMember.Parent.UniqueName'
    member [Measures].[Значение ]
    as '[Measures].[Значение] / 100'
    
  
SELECT
    non Empty
    {
      {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Курганская область]}
       *
       {
        [Показатели].[Рынок труда].[Период ]
       }
       +

       {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Курганская область]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
       +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Свердловская область]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
       +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Тюменская область]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
        +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ханты-Мансийский автономный округ]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
        +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Челябинская область]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
        +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].[Ямало-Ненецкий автономный округ]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
        +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
        +
        {[Территории].[Сопоставимый].[Все территории].[Российская  Федерация]}
       *
       {
        [Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]
       }
    } ON columns,

    filter
    (
        Descendants
        (
            [Период].[Период].[Данные всех периодов],
             [Период].[Период].[День],
            SELF
        ),
        not isEmpty ([Показатели].[Рынок труда].[Все].[Общая численность зарегистрированных безработных граждан]) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные месяца"
        ) and not
        (
            [Период].[Период].CurrentMember.Name = "Данные года"
        )
    ) ON rows
FROM  [СТАТ_Мониторинг ситуации на рынке труда]
WHERE
    (
       [Measures].[Значение]
    )

   ]]>
  </query>

<query id="STAT_0002_0007_Grid">
<![CDATA[
--STAT_0002_0007_Grid
WITH

    Member [Территории].[Сопоставимый].[УрФО]
    as '[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].DataMember'
    Member [Территории].[Сопоставимый].[РФед]
    as '[Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember'
   member [Территории].[Сопоставимый].[Период ]
    as '[Период].[Год Квартал Месяц].CurrentMember.UniqueName'
    
    Member [Measures].[Нарастающий итог ] 
    as 'IIF
        (
            IsEmpty 
            (
                (
                    [Measures].[Нарастающий итог],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Труд].[Трудовые ресурсы].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember,
                    [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
                )
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС]  
            ),
            (
                [Measures].[Нарастающий итог],
                [Источники данных].[Все источники данных].[СТАТ Отчетность - ФСГС по Свердловской области] 
            )  
        )'
        
    set [Месяцы]
    as 'Filter
        (
            Descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов].[2009],
                [Период].[Год Квартал Месяц].[Месяц],
                Self_and_before
            )  + Descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов].[2010],
                [Период].[Год Квартал Месяц].[Месяц],
                Self_and_before
            )
            + Descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов].[2011],
                [Период].[Год Квартал Месяц].[Месяц],
                Self_and_before
            )  
            + Descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов].[2012],
                [Период].[Год Квартал Месяц].[Месяц],
                Self_and_before
            ) 
            - descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов],
                [Период].[Год Квартал Месяц].[Полугодие],
                SELF
            ) - descendants
            (
                [Период].[Год Квартал Месяц].[Данные всех периодов],
                [Период].[Год Квартал Месяц].[Квартал],
                SELF
            ),
            [Период].[Год Квартал Месяц].CurrentMember.Name <> "Остатки на начало года"
             and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Данные квартала"
             and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Данные полугодия" 
             and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Данные года" 
             and [Период].[Год Квартал Месяц].CurrentMember.Name <> "Данные месяца" 
            and  not IsEmpty(                         
                              [Measures].[Нарастающий итог ]
                             )
          
                                
        )'
    member [Measures].[ДоляВРФ]
    as ' IIF
        (
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[РФед]
            ),
            null,
            
                    IIF
                    (
                        (
                            [Measures].[Нарастающий итог ],
                             [Группировки].[Труд_Трудовые ресурсы].[Все группировки].[Без группировки],
                            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember,
                            [Период].[Год Квартал Месяц].CurrentMember
                        ) = 0,
                        null,
                        (
                            (
                                [Measures].[Нарастающий итог ],
                                [Территории].[Сопоставимый].CurrentMember,
                                [Период].[Год Квартал Месяц].CurrentMember
                            ) /
                            (
                                [Measures].[Нарастающий итог ],
                                [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].DataMember,
                                [Период].[Год Квартал Месяц].CurrentMember
                            )
                        )
                    )
               

        )'
    member [Measures].[ДоляВУрФО]
    as 'IIF
        (
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[РФед]
            ) or
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].[УрФО]
            ),
            null,
              IIF
                (
                    (
                        [Measures].[Нарастающий итог ],
                        [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].DataMember,
                        [Период].[Год Квартал Месяц].CurrentMember
                    ) = 0,
                    null,
                    (
                        (
                            [Measures].[Нарастающий итог ],
                            [Территории].[Сопоставимый].CurrentMember,
                            [Период].[Год Квартал Месяц].CurrentMember
                        ) /
                        (
                            [Measures].[Нарастающий итог ],
                            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].DataMember,
                            [Период].[Год Квартал Месяц].CurrentMember
                        )
                    )
                )
        ) '
 member [Measures].[Темп Роста]     
    as 'IIF     
        (
            [Период].[Год Квартал Месяц].CurrentMember.PrevMember is  [Период].[Год Квартал Месяц].CurrentMember.PrevMember.Parent.DataMember,
            IIF 
            (
                (
                    [Measures].[Нарастающий итог ],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember.PrevMember.PrevMember
                )=0,
                null,
                (
                    (
                        [Measures].[Нарастающий итог ],
                        [Территории].[Сопоставимый].CurrentMember,
                        [Период].[Год Квартал Месяц].CurrentMember     
                    ) /     
                    (
                        [Measures].[Нарастающий итог ],
                        [Территории].[Сопоставимый].CurrentMember,
                        [Период].[Год Квартал Месяц].CurrentMember.PrevMember.PrevMember
                    )     
                )     
            ),
            IIF 
            (
                (
                    [Measures].[Нарастающий итог ],
                    [Территории].[Сопоставимый].CurrentMember,
                    [Период].[Год Квартал Месяц].CurrentMember.PrevMember
                )=0,
                null,
                (
                    (
                        [Measures].[Нарастающий итог ],
                        [Территории].[Сопоставимый].CurrentMember,
                        [Период].[Год Квартал Месяц].CurrentMember 
                    ) / 
                    (
                        [Measures].[Нарастающий итог ],
                        [Территории].[Сопоставимый].CurrentMember,
                        [Период].[Год Квартал Месяц].CurrentMember.PrevMember
                    ) 
                ) 
            )
        )'
SELECT
    {
        Filter
        (
            [Территории].[Сопоставимый].[Все территории].[Российская  Федерация].[Уральский федеральный округ].Children,
            not
            (
                [Территории].[Сопоставимый].CurrentMember is [Территории].[Сопоставимый].CurrentMember.Parent.DataMember
            )
        )
