  <query id = "ORG_0001_0005_chart_part2">
    <![CDATA[ 
    -- Query name: ORG_0001_0005_chart_part2
with

member [Measures].[Цена ]
    as 'Aggregate
        (
            filter
            (
                except
                (
                    {
                        [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
                    },
                    {
                        [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
                    }
                ),
                [Measures].[Цена] > 0
            ),
            [Measures].[Цена]
        )/ filter
        (
            except
            (
                {
                    [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
                },
                {
                    [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
                }
            ),
            [Measures].[Цена] > 0
        ).count'


    set [Территории]
    as 'filter
        (
            {
                [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].children
            },
            not IsEmpty ( [Measures].[Цена ] ) and ([Measures].[Цена ] > 0)
        )'
member [Организации__Предприятия торговли].[Организации__Предприятия торговли].[1]
    as 'Aggregate
        (
            filter
            (
                except
                (
                    {
                        [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
                    },
                    {
                        [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
                    }
                ),
                [Measures].[Цена] > 0
            ),
            [Measures].[Цена]
        )/ filter
        (
            except
            (
                {
                    [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
                },
                {
                    [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
                }
            ),
            [Measures].[Цена] > 0
        ).count'
    member [Территории__РФ].[Территории__РФ].[sum]
    as ' Aggregate
        (
            [Территории],
            [Measures].[Цена ]
        ) / [Территории].count'
  member [Территории__РФ].[Территории__РФ].[Ямало-Ненецкий автономный округ]
   as 'Aggregate(filter
        (
            except
            (
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].Children
                },
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].DataMember
                }
            ),
            not IsEmpty
            (

                [Measures].[Цена]
            ) and ([Measures].[Цена] > 0)

        ))/filter
        (
            except
            (
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].Children
                },
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].DataMember
                }
            ),
            not IsEmpty
            (

                [Measures].[Цена]
            ) and ([Measures].[Цена] > 0)

        ).count'
select
    {
    <%selected_food%>
    } on columns,
    non empty
    {
[Организации__Предприятия торговли].[Организации__Предприятия торговли].[1]
    } on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
         [Территории__РФ].[Территории__РФ].[Ямало-Ненецкий автономный округ],
         <%date%>,
         [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Розничная цена]

    )
    ]]>
  </query>

  <query id = "HMAO-VAL-CUR">
    <![CDATA[  -- Query name: HMAO-VAL-CUR
with
    set [Территории]
    as 'filter
        (
            {
                [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].children
            },
            not IsEmpty ( [Measures].[Цена] ) and ([Measures].[Цена] > 0)
        )'
    member [Территории__РФ].[Территории__РФ].[sum]
    as '
    Aggregate([Территории],
    [Measures].[Цена])
    /
    [Территории].count
    '
select
    {
    
    <%selected_food%>
    } on columns,
    non empty { [Территории__РФ].[Территории__РФ].[sum]} on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        --[Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        --<%date%>,
        <%selected_date%>,
        
        [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Розничная цена]
    )
    ]]>
  </query>

