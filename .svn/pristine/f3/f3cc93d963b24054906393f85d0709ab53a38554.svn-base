<?xml version="1.0" encoding="Windows-1251"?>
<root>

  <query id="ORG_0001_0005_list_of_dates">
    <![CDATA[ -- name: ORG_0001_0005_list_of_dates
select
    {[Measures].[Цена]} on columns,
    {
        filter
        (
            [Период__День].[Период__День].[День].members,
            [Measures].[Цена] > 0
        )
    } on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        --[Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Департамент экономики],
        [Организации__Товары и услуги].[Организации__Товары и услуги].[Все товары и услуги].[Продовольственные товары],
        [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации],
        [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Розничная цена],
        [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ]
    )
    ]]>
  </query>
  
  <query id = "ORG_0001_0005_data_part1">
    <![CDATA[ -- ORG_0001_0005_data
with
    member [Measures].[Цена на выбранную дату]
    as '
        (
            [Measures].[Цена],
            <%selected_date%>
        )'
    member [Measures].[Цена на дату для сравнения]
    as '
        (
            [Measures].[Цена],
            <%compare_date%>
        )'
    set [Территории]
    as 'filter
        (
            except
            (
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].Children
                },
                {
                    [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].DataMember
                }
            ),
            not IsEmpty
            (
                [Measures].[Цена на выбранную дату]
            ) and not IsEmpty
            (
                [Measures].[Цена на дату для сравнения]
            ) and ([Measures].[Цена на выбранную дату] > 0) and ([Цена на дату для сравнения] > 0)
        )'
    set [Организации]
    as 'filter(except
        (
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
            },
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
            }
        ),
            not IsEmpty
            (
                [Measures].[Цена на выбранную дату]
            ) or not IsEmpty
            (
                [Measures].[Цена на дату для сравнения]
            ) and ([Measures].[Цена на выбранную дату] > 0) and ([Цена на дату для сравнения] > 0)
        )'
        
         member [Организации__Предприятия торговли].[Организации__Предприятия торговли].[megoorg]
    as 'Aggregate(filter(except
        (
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
            },
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
            }
        ), [Measures].[Цена] > 0),  [Measures].[Цена])/ filter(except
        (
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].Children
            },
            {
                [Организации__Предприятия торговли].[Организации__Предприятия торговли].[Все организации].[Значение не указано]
            }
        ), [Measures].[Цена] > 0).count'
    set [Даты]
    as '
        {
            <%compare_date%>,
            <%selected_date%>
        }'
select
    [Даты] * 
    {[Организации__Предприятия торговли].[Организации__Предприятия торговли].[megoorg]}
    --[Организации]
    on columns,
    [Территории] on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        [Measures].[Цена],
        --[Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - Департамент экономики],
        <%selected_food%>,
        [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Розничная цена]--,[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ]
    )
    ]]>
  </query>

  <query id = "ORG_0001_0005_data_part2">
    <![CDATA[ -- Query name: ORG_0001_0005_data_part2
with
    member [Measures].[Цена на выбранную дату]
    as '
        (
            [Measures].[Цена],
            <%selected_date%>
        )'
    member [Measures].[Цена на дату для сравнения]
    as '
        (
            [Measures].[Цена],
            <%compare_date%>
        )'
    set [Территории]
    as 'filter(
        {
            --[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].[Город Ханты-Мансийск],
            --[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].[Город Нижневартовск],
            --[Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ханты-Мансийский автономный округ - Югра].[Город Сургут]
            [Территории__РФ].[Территории__РФ].[Все территории].[Российская Федерация].[Уральский федеральный округ].[Тюменская область с Ханты-Мансийским автономным округом, Ямало-Ненецким автономным округом].[Ямало-Ненецкий автономный округ].children
        },
            not IsEmpty
            (
                [Measures].[Цена на выбранную дату]
            ) or not IsEmpty
            (
                [Measures].[Цена на дату для сравнения]
            ) and ([Measures].[Цена на выбранную дату] > 0) and ([Цена на дату для сравнения] > 0)
        )'
select
    {
        [Measures].[Цена на выбранную дату],
        [Measures].[Цена на дату для сравнения]
    } on columns,
    non empty [Территории] on rows
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Розничная цена],
        --[Источники данных].[Источники данных].[Все источники данных].[СТАТ Отчетность - ХМАО-Югра],
        <%selected_food%>
    )    
    ]]>
  </query>
  
  <query id = "ORG_0001_0005_unit">
    <![CDATA[ -- ORG_0001_0005_unit
with
    member [Measures].[Имя]
    as '[Организации__Товары и услуги].[Организации__Товары и услуги].CurrentMember.UniqueName'
    member [Measures].[Единица измерения]
    as '[Организации__Товары и услуги].[Организации__Товары и услуги].CurrentMember.Properties("Единица измерения")'
select
    {
        [Measures].[Имя],
        [Measures].[Единица измерения]
    } on columns
from [ОРГАНИЗАЦИИ_Цены и тарифы]
where
    (
        <%selected_food%>
    )
    ]]>
  </query>

  <query id = "ORG_0001_0005_chart_part1">
