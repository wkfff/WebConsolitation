  <query id = "ORG_0001_0005_chart_part2">
    <![CDATA[
  SELECT
  {
  [Источники данных].[Источники данных].[Все источники данных].[ФСГС Цены РФ - 11.07.2011],
  [Источники данных].[Источники данных].[Все источники данных].[ФСГС Цены РФ - 11.07.2011]
  }
  ON ROWS ,
  NON EMPTY
  {
      [Территории__Сопоставимый].[Территории__Сопоставимый].[Все территории].[Российская  Федерация].datamember
  } ON COLUMNS
  FROM [ФСГС_Цены РФ]
  WHERE
  ([Период__Период].[Период__Период].[Данные всех периодов],
  [Measures].[Цена],
  [Продукция__Сопоставимый].[Продукция__Сопоставимый].[Все виды продукции].[Говядина (кроме бескостного мяса)],
  [Организации__Тип цены].[Организации__Тип цены].[Все типы цен].[Средняя розничная цена])
  ]]>
</query>
  <query id = "textovkadata">
    <![CDATA[
 -- ORG_0001_0005_text
with
    member [Measures].[Цена на выбранную дату]
    as '
        (
            [Measures].[Цена],
            <%selected_date%>
        )'
    member [Measures].[Цена на дату для сравнения]
    as '
        (
