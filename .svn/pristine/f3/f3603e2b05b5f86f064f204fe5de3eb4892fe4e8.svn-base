<?xml version="1.0" encoding="windows-1251"?>
<root>
  <query id="DynamicText">
    <![CDATA[
     --EO/EO_0001 DynamicText
with 
    member [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячная заработная плата на 1 работающего крупные предприятия] 
    as '[СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячная заработная плата на 1 работающего]' 
select
    {
        {
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячная заработная плата на 1 работающего],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].[Трудоспособное население],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячный доход на 1 работающего],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Просроченная задолженность по оплате труда],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Средний размер назначенных месячных пенсий],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].[Все население],
            
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].[Пенсионеры],
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].[Дети] 
        }  
        *  
        {
            [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Значение не указано]  
        },
        {
            [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячная заработная плата на 1 работающего крупные предприятия] 
        }  
        *  
        {
            [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Крупные и средние предприятия и организации]  
        }  
    } on columns,
    {
        Tail
        (
            { 
                filter
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members,
                    not isempty([Measures].[Отчет]) 
                )  
            },
            2   
        )  
    } on rows 
from   [ЭО_СЭП МО] 
where 
    (
        [Measures].[Отчет],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        
        [СЭП__Предприятия],
       <%baseRegion%>
    ) ]]>
  </query>
  <query id="Chart">
    <![CDATA[
    --EO/EO_0001 chart
with 
    member [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Население муниципального образования].[Года] 
    as ' [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.name ' 
select
    {
         [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Среднемесячный доход на 1 работающего (по отраслям)].children
    } on columns,
    non empty 
    {
         Tail
        (
            { 
                filter
                (
                    [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Год].members,
                    not isempty([Measures].[Отчет]) 
                )  
            },
            1   
        ) 
    } on rows 
from   [ЭО_СЭП МО] 
where 
    (
        [Measures].[Отчет],
        [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Значение не указано],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [СЭП__Предприятия],
        <%baseRegion%>
    )  ]]>
  </query>

  <query id="Chart2">
    <![CDATA[
  --EO/EO_0001 chart --EO/EO_0001 chart  
with  
    member [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Население муниципального образования].[Года]  
    as ' [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].currentmember.parent.parent.name '  
select
    non empty
    {
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].children,
        [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Население муниципального образования].[Года]  
    } on columns,
    non empty
    {
        filter
        (
            [Период__Год Квартал Месяц].[Период__Год Квартал Месяц].[Квартал].members,
            not isempty
            (
                (
                    [Measures].[Отчет],
                    [СЭП__Показатели МО].[СЭП__Показатели МО].[Все показатели].[Прожиточный минимум].[Все население]
                )
            )
        )
    } on rows  
from   [ЭО_СЭП МО]  
where  
    (
        [Measures].[Отчет],
        [ОК__Виды организаций].[ОК__Виды организаций].[Все виды организаций].[Значение не указано],
        [ОК__ОКВЭД].[ОК__ОКВЭД].[Все виды экономической деятельности].[Значение не указано],
        [СЭП__Предприятия],
        <%baseRegion%>
    )    ]]>
  </query>

</root>