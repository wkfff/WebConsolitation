    <CubeCommand method="Add" name="Balancing" ClassType="13" SubClassType="0" Description="" CommandType="1">
      <property name="Statement"><![CDATA[CREATE MEMBER CURRENTCUBE.Measures.[Balancing] AS 'IIF(Left([Период].[Период].CurrentMember.Name, 6)= "Данные"
or  Left([Период].[Период].CurrentMember.Name, 8)= "Значение"
or  Left([Период].[Период].CurrentMember.Name, 7)= "Остатки"
or  Left([Период].[Период].CurrentMember.Name, 14)= "Заключительные"
,null,1)', VISIBLE = '0']]></property>
    </CubeCommand>
    <CubeCommand method="Add" name="PredPeriodValuePos" ClassType="13" SubClassType="0" Description="" CommandType="1">
      <property name="Statement"><![CDATA[CREATE MEMBER CURRENTCUBE.Measures.[PredPeriodValuePos] AS 'IIF ((not IsEmpty(([Период].[Период].CurrentMember,[Measures].[Balancing]))),
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(1),[Measures].[Balancing])),
1,
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(2),[Measures].[Balancing])),
2,
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(3),[Measures].[Balancing])),
3,
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(4),[Measures].[Balancing])),
4,
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(5),[Measures].[Balancing])),
5,
IIF (not IsEmpty(([Период].[Период].CurrentMember.Lag(6),[Measures].[Balancing])),
6,
IIF(not IsEmpty(([Период].[Период].CurrentMember.Lag(7),[Measures].[Balancing])),
7,0)))))))
,0)', VISIBLE = '0']]></property>
    </CubeCommand>
    <CubeCommand method="Add" name="Прирост" ClassType="13" SubClassType="0" Description="" CommandType="1">
      <property name="Statement"><![CDATA[CREATE MEMBER CURRENTCUBE.Measures.[Прирост] AS '([Период].[Период].CurrentMember,[Measures].[Факт]) 
- ([Период].[Период].CurrentMember.Lag(([Период].[Период].CurrentMember,[Measures].[PredPeriodValuePos])),[Measures].[Факт])']]></property>
    </CubeCommand>
    <CubeCommand method="Add" name="Темп прироста" ClassType="13" SubClassType="0" Description="" CommandType="1">
      <property name="Statement"><![CDATA[CREATE MEMBER CURRENTCUBE.Measures.[Темп прироста] AS 'IIF (([Период].[Период].CurrentMember.Lag(([Период].[Период].CurrentMember,[Measures].[PredPeriodValuePos])),[Measures].[Факт]) <> 0,
([Период].[Период].CurrentMember,[Measures].[Факт]) 
/ ([Период].[Период].CurrentMember.Lag(([Период].[Период].CurrentMember,[Measures].[PredPeriodValuePos])),[Measures].[Факт])
-1,
null)', FORMAT_STRING = 'Percent']]></property>
    </CubeCommand>
    <CubeCommand method="Add" name="Процент выполнения плана" ClassType="13" SubClassType="0" Description="" CommandType="1">
      <property name="Statement"><![CDATA[CREATE MEMBER CURRENTCUBE.Measures.[Процент выполнения плана] AS 'IIF((([Measures].[План]) = 0) 
